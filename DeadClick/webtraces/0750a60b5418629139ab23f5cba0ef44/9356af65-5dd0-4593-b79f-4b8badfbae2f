 	$(document).ready(function() { 
        console.log('into the ready');
      var requiredForm = 1;

      $('.contact-form .required').each(function(){
		  var formRequiredValue = $.trim($(this).val());
          if(formRequiredValue == '') {
              requiredForm = 0;
       	}      
      }); 

		if(requiredForm == 1){//send button
			$(this).removeClass('disabled').addClass('enabled').find('input, select, textarea').prop('disabled',false);
			$('.contact-form .submit').prop('disabled',false);
		}

	}); 

 	
	var emailRegEx = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
	
	$('.contact-form input, .contact-form select, .contact-form textarea').bind('input propertychange change',function(){
        if ($('#licenseRenewal').is(':checked')){  
        	$('#cardType').addClass("required");
            $('#cardNumber').addClass("required");
            $('#cardName').addClass("required");
            $('#expirationMonth').addClass("required");
            $('#expirationYear').addClass("required"); 
    	} else {
             //alert('imp else checked');
            $('#cardType').removeClass("required");
            $('#cardNumber').removeClass("required");
            $('#cardName').removeClass("required");
            $('#expirationMonth').removeClass("required");
            $('#expirationYear').removeClass("required");            
    	} 
		var currentInput = $(this);
		var currentContainer = currentInput.closest(".section");
		var allRequired = 1;
		var requiredForm = 1;
		currentContainer.find('.required').each(function(){//check required fields
			var currentRequiredValue = $.trim($(this).val());
            if(currentRequiredValue == '') {
				allRequired = 0;
                if ((document.documentMode || 100) > 8)
                    $(this).addClass('error');
            } else {
                if ((document.documentMode || 100) > 8) 
                	$(this).removeClass('error');
            }

            if(this.id == 'email'){
                if (!emailRegEx.test($(this).val())) {
                    allRequired = 0;
					if ((document.documentMode || 100) > 8) 
                    	$(this).addClass('error');
                } else {
                    if ((document.documentMode || 100) > 8)
                    	$(this).removeClass('error');
                } 
            }
		}); 
		if(allRequired == 1) {//if not empty
			// var nextSection = $('.section.disabled');
			var nextSection = currentContainer.next();
			nextSection.removeClass('disabled').addClass('enabled').find('input, select, textarea').prop('disabled',false);
		}else{//if empty
			var enabledSections = currentContainer.nextAll('.section.enabled');
			enabledSections.removeClass('enabled').addClass('disabled').find('input, select, textarea').prop('disabled',true);
		}
        $('.contact-form .required').each(function(){//check all required fields
            var formRequiredValue = $.trim($(this).val());
            if(formRequiredValue == '') {
                requiredForm = 0;
                if ((document.documentMode || 100) > 8) 
                    $(this).addClass('error');
            } else {
                if ((document.documentMode || 100) > 8)
                    $(this).removeClass('error');
            }

            if(this.id == 'email'){
                if (!emailRegEx.test($(this).val())) {
                    requiredForm = 0;
                    if ((document.documentMode || 100) > 8)
                        $(this).addClass('error');
                } else {
                    if ((document.documentMode || 100) > 8)
                        $(this).removeClass('error');
                } 
            }
        });

		/*if(document.getElementById('email')){

			if (!emailRegEx.test($('#email').val())) {
		    	requiredForm = 0;
		    }
	    }*/
		
		if(requiredForm == 1){//send button
			$('.contact-form .submit').prop('disabled',false);
		}else{
			$('.contact-form .submit').prop('disabled',true);
		}

	});
