	color:			#FFFFFF;

}

.sf-menu li {

	background:		none;

}

.sf-menu li ul a {

	display:block; padding:5px 0 5px 15px; margin:0; font:normal 16px/22px 'AlegreyaSCRegular'; color:#FFF;  text-decoration:none; border-bottom:1px solid #879abb; background:url(../images/hover-arrow.gif) no-repeat 0px 10px; height:100%; margin-left:8px; 

}

