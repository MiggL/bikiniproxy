ation,Configuration:Configuration,HtmlTag:HtmlTag,ImageTag:ImageTag,VideoTag:VideoTag,ClientHintsMetaTag:ClientHintsMetaTag,Layer:Layer,TextLayer:TextLayer,SubtitlesLayer:SubtitlesLayer,Cloudinary:Cloudinary,VERSION:"2.3.0",CloudinaryJQuery:CloudinaryJQuery};return cloudinary});var IdeaBookUploadPhotos=function ideaBookUploadPhotos(){function cloudinaryUploadCallback(error,result){if(error===null&&result!==null){$("form.new_idea_book_photo").submit()}}function openCloudinaryWidget(cloudinaryOptions){$("#widget-container").empty();$("#upload_to_ideabook .lightbox--container").removeClass("-hidden-");cloudinary.openUploadWidget(cloudinaryOptions,cloudinaryUploadCallback);openLightbox("upload_to_ideabook");$("#widget-container iframe").load(function(){$("#upload_to_ideabook .lightbox--container").addClass("-hidden-")})}function bindWidgetOpenerButton(cloudinaryOptions){$("#upload_widget_opener").on("click",function(){closeLightbox("ideabook-selection").then(function(){openCloudinaryWidget(cloudinaryOptions)})})}function bindIdeabookSelectionForm(){var $dropdown=$('select[name="idea_book_photo[idea_book_id]"]');var $titleField=$('input[name="idea_book_photo[title]"]');var $ideabookId=$("#idea_book_id");var $title=$("#idea_book_title");$dropdown.on("change",function(event){$ideabookId.val(event.target.value)});$titleField.on("change",function(event){$title.val(event.target.value)})}function main(cloudinaryOptions){bindWidgetOpenerButton(cloudinaryOptions);bindIdeabookSelectionForm()}return{main:main}}();