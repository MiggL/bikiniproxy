<script type="text/javascript" src="http://www.prisonstudies.org/sites/default/files/js/js_FD8b20J2-JOeWVVzSx5eAk3kuTt16c5Is1iWHR3xy58.js"></script>
<script type="text/javascript">
<!--//--><![CDATA[//><!--

	jQuery(document).ready(function() {
   
	// add row numbers for results
	jQuery("#views-aggregator-datatable tbody tr").each(function (i) {
   jQuery("td:first", this).html(i + 1);
});

	});
	
	// Alter rankings column to reflect joint rankings
	(function ($) {
$(document).ready(function() {
    var tableRows = $("#views-aggregator-datatable tbody tr"); //find all the rows
    var rowValues = []; //to keep track of which values appear more than once
    tableRows.each(function() { 
        var rowValue = $(this).find(".views-field:last").html();
		var lastRank = $(this).closest("tr").prev().children("td:first").html();
        if (!rowValues[rowValue]) {
            var rowComposite = new Object();
            rowComposite.count = 1;
            rowComposite.row = this;
            rowValues[rowValue] = rowComposite;
        } else {
            var rowComposite = rowValues[rowValue];
			$(this).closest("tr").prev().children("td:first").addClass( "joint" );
			$(this).closest("tr").children("td:first").html(lastRank).addClass( "joint" );
            rowComposite.count++;
        }
    });

// now add an asterix for results which are approximate figures
    jQuery(".circaAdd").prepend("c.");   

// set chart area to transparent to allow watermarking in #chart_div
jQuery("#chart_div svg > rect").attr("fill", "transparent");
	
// Add thousands separator	
	
function hyphenate(i, v) {
    return v.replace(/\B(?=(\d{3})+(?!\d))/g, function (c, i, a) {
        return i && c !== "." && !((a.length - i) % 3) ? " " + c : c;
    })
}

$(function () {
var resultCell = $("tbody>tr>td");
    resultCell.each(function () {
		if ($(this).index() >= 2) $(this).text(hyphenate)
    })
})
// thousands end
	
});
})(jQuery);
	
//--><!]]>
</script>
