								});
							} else {
								$('<div class="stats_content"></div>')
								.html(
									'<span class="no_data">No data available</span>'
								)
								.appendTo('#tooltip_career_'+pid);
							}
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
			$("#more_career_vs_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center left',
				offset: [-3, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/xml/named.sit_bvp_5y_date.bam?vs_pitcher_id="+pid+"&game_type='R'&date_num='2018/02/15'&team_id="+tid+"&sport_code='mlb'&cur_seas_sw='"+include_curr_season+"'",
						dataType: "xml",
		  				success: function(xml) {
							$(xml).find('row').each(function(){
								var player = $(this).attr('player_first_last');
								if ($(this).attr('avg') != '') {var avg = $(this).attr('avg');} else {var avg = '-';}
								if ($(this).attr('ab') != '') {var ab = $(this).attr('ab');} else {var ab = '-';}
								if ($(this).attr('hr') != '') {var hr = $(this).attr('hr');} else {var hr = '-';}
								if ($(this).attr('rbi') != '') {var rbi = $(this).attr('rbi');} else {var rbi = '-';}
								$('.loading').addClass('hide');
								$('<tr class="stats_content"></tr>')
								.html(
									'<td class="first">'+player+'</td><td>'+avg+'</td><td>'+ab+'</td><td>'+hr+'</td><td>'+rbi+'</td>'
								)
								.appendTo('#tooltip_career_vs_'+pid+' tbody');
								$('.pitcher tr:odd').addClass('odd');
							});
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
		});
	}
	$(".tooltip .close").click(function() {
		$(this).parent().prev().tooltip().hide();
		return false;
	});
});
</script>

