<script>
		var slideIndex = 0;
		var _timeout;
		carousel();
		function showDivs(n) {
			var i;
			var x = document.getElementsByClassName("top-slide");
			var slideTab = document.getElementsByClassName("slideDiv");
			if (n > x.length) {slideIndex = 1} 
			if (n < 1) {slideIndex = x.length} ;
			for (i = 0; i < x.length; i++) {
				x[i].style.display = "none"; 
				slideTab[i].style.background = "#ffffff";
			}
			x[slideIndex-1].style.display = "block"; 
			slideTab[slideIndex-1].style.background = "#cccccc";
		}
		function carousel() {
			var i;
			var x = document.getElementsByClassName("top-slide");
			var slideTab = document.getElementsByClassName("slideDiv");
			for (i = 0; i < x.length; i++) {
			  x[i].style.display = "none"; 
			  slideTab[i].style.background = "#ffffff";
			}
			slideIndex++;
			if (slideIndex > x.length) {slideIndex = 1} 
			x[slideIndex-1].style.display = "block"; 
			slideTab[slideIndex-1].style.background = "#cccccc";
			_timeout = setTimeout(carousel, 3000); // Change image every 2 seconds
		}
		
		var slideTab = document.getElementsByClassName("slideDiv");
		for(var t = 0; t < slideTab.length; t ++ )
		{
			(function(index) {
				slideTab[t].onmouseover = function() {
					showDivs(slideIndex = index +1);
					clearTimeout(_timeout);
				};
				slideTab[t].onmouseout = function() {
					_timeout = setTimeout(carousel, 3000); 
				}
			})(t);
		}
	</script>
