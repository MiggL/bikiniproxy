cart_header=$('#head_shopping_cart');var t_cart_dropdown=$('#dropdown_shopping_cart');var t_top=Number($(t_cart_header).offset().top)+Number($(t_cart_header).height())+Number($(t_cart_header).css('padding-top').replace('px',''))+Number($(t_cart_header).css('padding-bottom').replace('px',''));$(t_cart_dropdown).css('left',$(t_cart_header).offset().left).css('top',t_top+'px');};}
