if (d){
	d=d.substr(1);
	var arr=d.split("&"),l=arr.length;
	for (var i=0;i<l;i++){
		var kv=arr[i],kvs=kv.split("=");
		if (kvs[0]=='kvcmsid'){
			kvcmsid=kvs[1];
		}
		else if (kvs[0]=='kvsubj'){
			kvsubj=kvs[1];
		}
		else if (kvs[0]=='kvent'){
			kvent=kvs[1];
		}
		else if (kvs[0]=='ESN'){
			sn=kvs[1];
		}
		else if (kvs[0]!=""){
			tacProp[kvs[0]]=unescape(kvs[1])
		}
	}
}
