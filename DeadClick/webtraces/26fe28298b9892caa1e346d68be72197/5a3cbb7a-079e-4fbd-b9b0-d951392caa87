function makeWin2(url) {

              agent = navigator.userAgent;
              windowName = "Sitelet";

              params  = "";
              params += "toolbar=0,";
              params += "location=0,";
              params += "directories=0,";
              params += "status=0,";
              params += "menubar=0,";
              params += "scrollbars=0,";
              params += "resizable=0,";
              params += "width=220,";
              params += "height=310";

              win = window.open(url, windowName , params);

              if (agent.indexOf("Mozilla/2") != -1 &&
agent.indexOf("Win") == -1) {
                  win = window.open(url, windowName , params);
              }

              if (!win.opener) {
                  win.opener = window;
              }

          };	
