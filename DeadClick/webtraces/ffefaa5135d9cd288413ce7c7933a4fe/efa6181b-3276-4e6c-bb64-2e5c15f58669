(function(f){var i=document.createElement("input");var a="oninput" in i&&(!("documentMode" in document)||document.documentMode>9);var k=function(l){return l.nodeName==="INPUT"&&(l.type==="text"||l.type==="password")};var c=null;var j=null;var d=null;var h={get:function(){return d.get.call(this)},set:function(l){j=l;d.set.call(this,l)}};var b=function(l){c=l;j=l.value;d=Object.getOwnPropertyDescriptor(l.constructor.prototype,"value");Object.defineProperty(c,"value",h);c.attachEvent("onpropertychange",g)};var e=function(){if(!c){return}delete c.value;c.detachEvent("onpropertychange",g);c=null;j=null;d=null};var g=function(l){if(l.propertyName!=="value"){return}var m=l.srcElement.value;if(m===j){return}j=m;f(c).trigger("textchange")};if(a){f(document).on("input",function(l){if(l.target.nodeName!=="TEXTAREA"){f(l.target).trigger("textchange")}})}else{f(document).on("focusin",function(l){if(k(l.target)){e();b(l.target)}}).on("focusout",function(){e()}).on("selectionchange keyup keydown",function(){if(c&&c.value!==j){j=c.value;f(c).trigger("textchange")}})}})(jQuery);