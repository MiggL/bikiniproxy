!function(o){o.fn.showUp=function(s,n){n=n||{};var a=o(s),l=n.down||"navbar-hide",t=n.up||"navbar-show",e=n.btnHideShow||".btn-hide-show",c=n.offset||110,d=0;o(window).scroll(function(){o(this).scrollTop()>c&&(o(this).scrollTop()>d?a.removeClass(t).addClass(l):a.removeClass(l).addClass(t)),d=o(this).scrollTop()}),o(e).click(function(){a.hasClass(l)?a.removeClass(l).addClass(t):a.removeClass(t).addClass(l)})}}(jQuery),$(document).ready(function(){var o=420,s=220,n=".btn-fixed-bottom",a=".back-to-top";$(window).scroll(function(){$(this).scrollTop()>s?$(n).fadeIn(o):$(n).fadeOut(o)}),$(a).click(function(s){return s.preventDefault(),$("html, body").animate({scrollTop:0},o),!1})});