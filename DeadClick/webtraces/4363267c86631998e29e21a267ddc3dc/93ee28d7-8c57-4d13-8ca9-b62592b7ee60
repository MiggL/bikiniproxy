.button,
input[type="button"],
input[type="reset"],
input[type="submit"] {
	background-color: #d67247;
	border: 0;
	border-radius: 2px;
	color: #fff;
	font-size: 12px;
	font-weight: 700;
	padding: 10px 30px 11px;
	text-transform: uppercase;
	vertical-align: bottom;
}

