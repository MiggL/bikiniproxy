<script type="text/javascript">

    jQuery(document).ready(function () {
        jQuery('#txtCriteria').keypress(function (e) {
            if (e.which == 13) {
                search();
                return false;
            }
        });
    });

    function doClear(inputField) {
        if (inputField.value == inputField.defaultValue) {
            inputField.value = "";
            inputField.style.color = "black";
        }
    }
    function search() {
        var str = document.getElementById('txtCriteria').value;
        str = encodeURIComponent(str);
        // some chars do not get URLENcoded
        str = str.replace(/\(/g, '%28').replace(/\)/g, '%29'); // ()
	    window.location.href = "https://www.translink.ca/Standard_Items/Search_Results.aspx?searchStr=" + str;
	}
</script>
