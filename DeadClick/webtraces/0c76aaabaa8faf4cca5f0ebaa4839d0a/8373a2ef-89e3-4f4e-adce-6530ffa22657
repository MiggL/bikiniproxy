function addLoadEvent(o){var n=window.onload;"function"!=typeof window.onload?window.onload=o:window.onload=function(){n&&n(),o()}}function generateUUID(){var e=(new Date).getTime();window.performance&&"function"==typeof window.performance.now&&(e+=performance.now());var n="xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx".replace(/[xy]/g,function(n){var r=(e+16*Math.random())%16|0;return e=Math.floor(e/16),("x"==n?r:3&r|8).toString(16)});return n}function getCookie(e){for(var n=e+"=",r=document.cookie.split(";"),t=0;t<r.length;t++){for(var o=r[t];" "==o.charAt(0);)o=o.substring(1);if(0==o.indexOf(n))return o.substring(n.length,o.length)}return""}function setCookie(e,n,r){var t=new Date;t.setTime(t.getTime()+24*r*60*60*1000);var o="expires="+t.toUTCString();document.cookie=e+"="+n+"; "+o+"; path=/"}addLoadEvent(function(){if(null!=document.body){var e=getCookie("brandcdn_uid"),n="";"undefined"!=typeof document.referrer&&document.referrer.length>2&&(n=document.referrer.split("/")[2]);var r=location.hostname;("undefined"==typeof e||""==e)&&(e=generateUUID(),setCookie("brandcdn_uid",e,365));var t=document.createElement("iframe");t.src="https://adservices.brandcdn.com/pixel/cv?aid="+adv_id+"&cv_ck="+e+"&m="+r+"&r="+n,t.style.display="none",document.body.appendChild(t)}});
