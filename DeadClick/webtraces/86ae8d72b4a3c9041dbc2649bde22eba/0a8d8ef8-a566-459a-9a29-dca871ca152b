var blackberrycom=blackberrycom?blackberrycom:{};
blackberrycom.getLocale=function(){var b="en_gb";
if(document.location.search.indexOf("currentLocale")>-1){b=document.location.search.match(/\?currentLocale=(.._..)/)[1]
}else{if(typeof $.cookie("currentLocale")!=="undefined"){b=$.cookie("currentLocale")
}else{if($("body").attr("class").match(/\locale\-(.._..)/)!==null){b=$("body").attr("class").match(/\locale\-(.._..)/)[0].replace("locale-","")
}else{if(typeof bbcom!=="undefined"&&typeof bbcom.siteConfig!=="undefined"&&typeof bbcom.siteConfig.currentLocale!=="undefined"){b=bbcom.siteConfig.currentLocale
}}}}return b
};
blackberrycom.location={};
blackberrycom.location.locale=blackberrycom.getLocale();
blackberrycom.location.country=location.pathname.indexOf("country-")>-1?location.pathname.replace(/.+?country-(..).+?$/,"$1"):blackberrycom.location.locale.replace(/.._/,"");
blackberrycom.location.language=location.pathname.indexOf("language-")>-1?location.pathname.replace(/.+?language-(..).+?$/,"$1"):blackberrycom.location.locale.replace(/_../,"");
(function(b){b(document).ready(function(){b("html, body").removeClass("no-js");
var f=typeof CQ!=="undefined"?CQ.WCM.isEditMode():false;
if(!f){var d=document.createElement("script");
d.src="https://www.youtube.com/iframe_api";
var e=document.getElementsByTagName("script")[0];
e.parentNode.insertBefore(d,e);
b("script[type='text/x-component-embed']").each(function(h,j){var g=document.createElement("script");
g.type="text/javascript";
g.text=j.innerHTML;
j.parentNode.insertBefore(g,j);
b(j).remove()
});
console.log("Ready")
}})
})(jQuery);
$("#back-top").css("display","none");
$(window).scroll(function(){if($(window).scrollTop()+$(window).height()>($(document).height()/3)){$("#back-top").css("display","block")
}else{$("#back-top").css("display","none")
}});
$("#back-top").click(function(){$("html, body").animate({scrollTop:0},"slow");
return false
});
var blackberrycom=blackberrycom?blackberrycom:{};
blackberrycom.getLocale=function(){var b="en_gb";
if(document.location.search.indexOf("currentLocale")>-1){b=document.location.search.match(/\?currentLocale=(.._..)/)[1]
}else{if(typeof $.cookie("currentLocale")!=="undefined"){b=$.cookie("currentLocale")
}else{if($("body").attr("class").match(/\locale\-(.._..)/)!==null){b=$("body").attr("class").match(/\locale\-(.._..)/)[0].replace("locale-","")
}else{if(typeof bbcom!=="undefined"&&typeof bbcom.siteConfig!=="undefined"&&typeof bbcom.siteConfig.currentLocale!=="undefined"){b=bbcom.siteConfig.currentLocale
}}}}return b
};
blackberrycom.location={};
blackberrycom.location.locale=blackberrycom.getLocale();
blackberrycom.location.country=location.pathname.indexOf("country-")>-1?location.pathname.replace(/.+?country-(..).+?$/,"$1"):blackberrycom.location.locale.replace(/.._/,"");
blackberrycom.location.language=location.pathname.indexOf("language-")>-1?location.pathname.replace(/.+?language-(..).+?$/,"$1"):blackberrycom.location.locale.replace(/_../,"");
(function(b){b(document).ready(function(){b("html, body").removeClass("no-js");
var d=typeof CQ!=="undefined"?CQ.WCM.isEditMode():false
})
})(jQuery);
$("#back-top").css("display","none");
$(window).scroll(function(){if($(window).scrollTop()+$(window).height()>($(document).height()/3)){$("#back-top").css("display","block")
}else{$("#back-top").css("display","none")
}});
$("#back-top").click(function(){$("html, body").animate({scrollTop:0},"slow");
return false
});
