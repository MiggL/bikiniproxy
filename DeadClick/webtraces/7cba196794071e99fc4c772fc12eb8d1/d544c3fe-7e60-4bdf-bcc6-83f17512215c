    <script>
                (function runRequireQueue() {
            var zeroPriorityQueue = [];
            BI.requireQueue.forEach(function(requireObj, index, queue) {
                // All high priority requires called first
                if(requireObj.priority && requireObj.priority === 'high') {
                    requireObj.requireFunc();
                } else {
                    zeroPriorityQueue.push(requireObj);
                }
                // if all the high priority calls have been fired, then call the rest
                if(index === queue.length-1) {
                    zeroPriorityQueue.forEach(function(r) {
                        r.requireFunc();
                    });
                }
            });
        })();
    </script>
