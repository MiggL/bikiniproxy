<script type="text/javascript">
//<![CDATA[



$(window).resize(function(){     
   
      if ($("#content_left").css("float") == "none" ){
             $(".content_left").remove().insertAfter($("#pageHeading"));
             $( ".center_text" ).addClass( "col" );
             $( ".center_buttons" ).addClass( "col" );
        }
        
         else if ($("#content_left").css("float") == "left" ){ 
            $(".content_left").remove().insertBefore($(".content_main"));
           
             };
             
       if ($("#content_right").css("float") == "left" ){
           $(".content_right").appendTo(".tablet_right_col");
        }
        
        else  if ($("#content_right").css("float") == "right" ){
            $(".content_right").remove().insertAfter($(".content_main"));
           
             };   
       


            
});
  //]]>
</script>
 <script type="text/javascript">
//<![CDATA[
(function () {
        
    // Create mobile element
    var mobile = document.createElement('div');
    mobile.className = 'nav-mobile';
    document.querySelector('.nav').appendChild(mobile);

    // hasClass
    function hasClass(elem, className) {
        return new RegExp(' ' + className + ' ').test(' ' + elem.className + ' ');
    }

    // toggleClass
    function toggleClass(elem, className) {
        var newClass = ' ' + elem.className.replace(/[\t\r\n]/g, ' ') + ' ';
        if (hasClass(elem, className)) {
            while (newClass.indexOf(' ' + className + ' ') >= 0) {
                newClass = newClass.replace(' ' + className + ' ', ' ');
            }
            elem.className = newClass.replace(/^\s+|\s+$/g, '');
        } else {
            elem.className += ' ' + className;
        }
    }

    // Mobile nav function
    var mobileNav = document.querySelector('.nav-mobile');
    var toggle = document.querySelector('.nav-list');
    mobileNav.onclick = function () {

        toggleClass(this, 'nav-mobile-open');
        toggleClass(toggle, 'nav-active');
    };
})();

          //]]>
        </script>
 <script type="text/javascript">
//<![CDATA[
(function () {
        
    // Create mobile element
    var mobilesub = document.createElement('div');
    mobilesub.className = 'subnav-mobile';
    document.querySelector('.subnav').appendChild(mobilesub);
     

    // hasClass
    function hasClass(elem, className) {
        return new RegExp(' ' + className + ' ').test(' ' + elem.className + ' ');
    }

    // toggleClass
    function toggleClass(elem, className) {
        var newClass = ' ' + elem.className.replace(/[\t\r\n]/g, ' ') + ' ';
        if (hasClass(elem, className)) {
            while (newClass.indexOf(' ' + className + ' ') >= 0) {
                newClass = newClass.replace(' ' + className + ' ', ' ');
            }
            elem.className = newClass.replace(/^\s+|\s+$/g, '');
        } else {
            elem.className += ' ' + className;
        }
    }

    // Mobile nav function
    var mobileNavsub = document.querySelector('.subnav-mobile');
    var toggle = document.querySelector('.subnav-list');
    mobileNavsub.onclick = function () {
        toggleClass(this, 'subnav-mobile-open');
        toggleClass(toggle, 'subnav-active');
    };
})();

          //]]>
        </script>
 
