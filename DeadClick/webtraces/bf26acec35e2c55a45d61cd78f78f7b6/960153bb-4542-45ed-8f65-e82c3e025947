		<script type="text/javascript">
		$(document).ready(function(){

			var divs = $('li.cf'),
				NUMBER_TO_SHOW = 5,
				randomItem,
				randomItemArray = [];

			divs.css('display','none');

			numberOfItems = divs.length;

			//console.log('Number of items: ' + numberOfItems);

			for( var x = 0, len = NUMBER_TO_SHOW; x < len; x++){
				
				function addItem(){
					// Generate random number between 0 & 1, then multiply by total
					randomItem = Math.floor( Math.random() * numberOfItems);
					// If this number hasn't already been generated
					if( !randomItemArray[randomItem] ){
						// Then set that upsell item to display block
						$(divs[randomItem]).css('display', 'block');
						// And save a record to make sure we don't use it again
						randomItemArray[randomItem] = true;
						//console.log('Added: ' + randomItem);
					}else{
						addItem();
					}
				}
				addItem();
			}
		});
		</script>
