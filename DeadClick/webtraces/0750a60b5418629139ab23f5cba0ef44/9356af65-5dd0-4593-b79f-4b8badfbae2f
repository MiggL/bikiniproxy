	/*show hide comments on mobile*/
	var show = 0;
	$('.mobile-comments').click(function(){
		var $comments = $('.comments-container');
		var $this = $(this);
		if (show == 0) {
			$comments.slideDown('fast');
			$this.html('<span class="triangleUp"></span>Hide Comments');
			show = 1;
		} else{
			$comments.slideUp('fast');
			$this.html('<span class="triangleDown"></span>Show Comments');
			show = 0;
		}
		return false;
	});//end click
	
	/*product literature accordion*/
	$('.accordion-container .top-level .title').click(function(){
		/* The bullet list text are not shown when accordion is open. Issue No.95(Testing QA/Prod)*/
