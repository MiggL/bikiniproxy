define(function(){var a;a=function(b){if(!b||typeof b!=="object"){return false}if(window.Promise&&b instanceof Promise){return true}return typeof b.then==="function"};return{load:function(b,c,d){c([b],function(e){var h,g,f;h=function(){d.error.apply(null,arguments)};g=function(){d.apply(null,arguments)};if(a(e)){f=e.done||e.then;if(typeof e.fail==="function"){f.call(e,g);e.fail(h)}else{f.call(e,g,h)}}else{d(e)}})}}});