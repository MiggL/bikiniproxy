        function getRRDisplayCode() {
            try {
                switch(TagManagerData.Locale) {
			case "de_AT":
				return '7517-de_at';
				break;
			case "de_DE":
				return '7517-de_de';
				break;	
			case "en_GB":
				return '7517-en_gb';
				break; 
			case "en_BE":
				return '7517-en_be';
				break;
			case "en_IE":
				return '7517-en_ie';
				break;
			case "en_NL":
				return '7517-en_nl';
				break;
			case "es_ES":
				return '7517-es_es';
				break;
			case "fr_BE":
				return '7517-fr_be';
				break;
			case "fr_CA":
				return '7517-fr_ca';
				break;
			case "fr_FR":
				return '7517-fr_fr';
				break;
			case "it_IT":
				return '7517-it_it';
				break;
			case "en_FI":
				return '7517-en_fi';
				break;
		}
            } catch(e){
                //The JS in the try-catch above is user-definable.  Catch and suppress any errors and just return a default.
            }
            return '7517-en_nl';
        }

        function getRRDisplayType() {
            try {
                return "";
            } catch (e) {
                //The JS in the try-catch above is user-definable.  Catch and suppress any errors and just return a default.
            }
            return "";
        }

        var configData = {};

        configData.displayCode = getRRDisplayCode();
        configData.productId = "CU9108";
        configData.summaryContainerDiv = "BVCustomerRatings";
        configData.contentContainerDiv = "BVReviewsContainer";
        configData.onEvent = function(json) {
            //no-op
        };

        if (getRRDisplayType() != "") {
            configData.displayType = getRRDisplayType();
        }

        configData.doShowContent = function(json) {
            

        };

        
            $BV.ui("rr", "show_reviews", configData);
        
    });
</script> 
      
