		<script type="text/javascript" src="http://www.millenniumbulk.com/wp-content/themes/millenniumUT/library/js/jquery.isotope.min.js"></script>
		<script src="//code.jquery.com/ui/1.11.2/jquery-ui.js"></script>
		<script>
		jQuery(function(){
		
		  var checkTimelines = document.getElementById('total-timeline');
		  if(checkTimelines != null)
		  {
				  var ChooseOne = 1;
				  var special = false;
				  var ZoomNumber = 1;
				  var countTimeLine = document.getElementById('total-timeline').innerHTML*1;
				  var totalWidth = 880;
				  var oneItemWidth = Math.round(totalWidth/countTimeLine);
				  var $container = jQuery('#container-timeline'),
					  $items = jQuery('.item');
				  
				  $container.isotope({
					itemSelector: '.item',
					layoutMode : 'fitColumns',
					resizesContainer : false,
					getSortData : {
					  fitOrder : function( $item ) {
						var order,
							index = $item.index();
						
						if ( $item.hasClass('large') && index < 6 ) {
						  order = index + (6 - index);
						} 
						else if( $item.hasClass('large') && index >= 6 )
						{
						  order = index + (12 - index);
						}
						else {
						  order = index;
						}
						return order;
					  }
					},
					sortBy : 'fitOrder'
				  });
				  
				  $items.click(function(){
					var $this = jQuery(this);
					
					// nothing to change if this already has large class
					if ( $this.hasClass('large') ) {
					  return;
					}
					var $previousLargeItem = $items.filter('.large');
					var getId = $this.attr('data-id');
					var getWidthScrollBar = (oneItemWidth * (getId-1) + (oneItemWidth/2));
					if(getId == 1)
					{
						getWidthScrollBar = 0;
					}
					else if(getId == countTimeLine)
					{
						getWidthScrollBar = (oneItemWidth * (getId));
					}
					
					ChooseOne = getId;
					
					jQuery('.timeline-content').addClass('hide');
					jQuery('#timeline-content-'+getId).removeClass('hide');
					
					document.getElementById('scrolltab').style.left = getWidthScrollBar+'px';
					/*document.getElementById('container-timeline').style.zoom = 3;*/
					
					$previousLargeItem.removeClass('large');
					
					$this.addClass('large');
					
					$container
					  // update sort data on changed items
					  .isotope('updateSortData', $this )
					  .isotope('updateSortData', $previousLargeItem )
					  // trigger layout and sort
					  .isotope();
					  
				  });
				  
				  
				  jQuery('#scrolltab').draggable({
					containment: "timeline-linetab",
					axis: "x",
					drag: function( event, ui ) {
						if(ui.position.left >= 0 && ui.position.left <= totalWidth)
						{
							ChooseNumber = Math.round( ui.position.left / oneItemWidth );
							if(ChooseNumber <= 0)
							{
								ChooseNumber = 1;
							}
							if(ChooseOne != ChooseNumber)
							{
								ChooseOne = ChooseNumber;
								$items[(ChooseOne-1)].click();
							}
							
							return true;
						}
						else
						{
							return false;
						}
					}
				  });
				  
				  jQuery("#timeline-linetab").click(function(event) {
					if(event.target.offsetLeft > 0)
					{
						var left = event.pageX - event.target.offsetLeft - 35;
						if(left <= 0)
						{
							left = 0;
						}
						else if(left >= 880)
						{
							left = 880;
						}
						ChooseNumber = Math.round( left / oneItemWidth );
						if(ChooseNumber <= 0)
						{
							ChooseNumber = 1;
						}
						if(ChooseOne != ChooseNumber)
						{
							ChooseOne = ChooseNumber;
							$items[(ChooseOne-1)].click();
						}
						document.getElementById('scrolltab').style.left = left+'px';
					}
				  });

				  
				  $items[0].click();
				
		  }
		  
		});
		
		
		function hidePlayNStop() {
			var playStopSlider = document.getElementsByClassName('ls-bottom-nav-wrapper');
			/* console.log(playStopSlider);
			console.log(playStopSlider.length); */
			if(typeof playStopSlider[0] !== 'undefined' && playStopSlider[0])
			{
				clearInterval(refreshIntervalId);
				playStopSlider[0].style.display = "none";
			}
		}
		
		var refreshIntervalId = setInterval(function(){ hidePlayNStop(); }, 1000);
		
		</script>
		<script type="text/javascript">
			/*
