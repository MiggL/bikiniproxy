<script type="text/javascript">
    $(function () {
        $('.datetimepicker-search').datetimepicker({
            useCurrent: false,
            inline: true,
            sideBySide: false,
            locale: 'en',
            format: 'y-M-d',
            minDate: moment(),
            viewMode: 'days'
        });

        $(".picker-switch").removeAttr('title')
            .css('cursor', 'default')  
            .css('background', 'inherit')
            .on('click', function (e) {
                e.stopPropagation();
            });
        var minPrice = "65€";
        var brunchPrice = "41€";
        var maxPrice = "67€";
        function addPrice() {
            var divSam = $('<div class="best-price" />');
            var a = $('<a disabled="disabled" href="#" class="" />');
            a.html(minPrice);
            divSam.append(a);

            var divDim = $('<div class="best-price" />');
            var a = $('<a disabled="disabled" href="#" class="" />');
            a.html(brunchPrice);
            divDim.append(a);


            var div = $('<div class="best-price" />');
            a = $('<a disabled="disabled" href="#" class="" />');
            a.html(maxPrice);
            div.append(a);

            $('.day:not(.disabled) .best-price').remove();

                            $('.day:not(.disabled):not(:nth-child(7),:nth-child(1))').append(div);
                $('.day:not(.disabled):nth-child(7)').append(divSam);
                $('.day:not(.disabled):nth-child(1)').append(divDim);
            
            // saint valentin
            sv = $(".day[data-day='14/02/2018']");
            if (sv.length == 1) {
                sv.find('.best-price').remove();
                div = $('<div class="best-price" />');
                a = $('<a disabled="disabled" href="#" class="" />');
                a.html('89€');
                div.append(a);
                sv.append(div);
            }

            $('.day:not(.disabled)').on('click', function(e){
                var data = $(this).attr('data-day').split('/');
                                var date = data[2]+'/'+data[0]+'/'+data[1];
                                $('#dateSearch').val(date);
                $('#formSearch').submit();
                return false;
            });
        }

        addPrice();
        $(".datetimepicker-search").on("dp.update", function(e) {
            addPrice();
        });

    });

</script>	      </div>
