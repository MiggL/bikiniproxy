	jQuery('#subprod_colourdropdown').change(function(){
		var x = jQuery('#subprod_colourdropdown').val();

		// Disable the add to cart button and other controls here [Rees]

		jQuery('#subprod_colourdropdown').attr('disabled', true);
		jQuery('#subprod_colourdropdown').css('background-color','lightgray');
		jQuery('#subprod_colourdropdown').css('color','gray');
		jQuery('#subprod_colourdropdown').css('cursor','progress');

		jQuery('.add-to-cart button.button span span').text('Loading...');
		jQuery('.add-to-cart button.button').css('background-color','lightgray');
		jQuery('.add-to-cart button.button').css('cursor','progress');
		jQuery('.add-to-cart button.button span span').css('color','gray');
		jQuery('.add-to-cart button.button span').css('border','1px dotted gray');
		jQuery('.add-to-cart button.button').attr('disabled',true);
		jQuery('.add-to-cart button.button').attr('title','Please wait...');

		jQuery('#subprod_dyelotdropdown').attr('disabled', true);
		jQuery('#subprod_dyelotdropdown').css('background-color','lightgray');
		jQuery('#subprod_dyelotdropdown').css('color','gray');
		jQuery('#subprod_dyelotdropdown').css('cursor','progress');

		jQuery('#qty').attr('disabled', true);
		jQuery('#qty').css('background-color','lightgray');
		jQuery('#qty').css('color','gray');
		jQuery('#qty').css('cursor','progress');

		window.location.href=x;
	});
