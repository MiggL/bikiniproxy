<script type="text/javascript">
function set_attribute(option_id, selected_val)
{
	//alert(option_id);
	//alert(selected_val);

	/*if(selected_val != "")
	    document.getElementByName('id['+option_id+']').value = selected_val;
	else
		document.getElementByName('id['+option_id+']').value = 0;*/
    updateConfigPrice();
}
function updateConfigPrice()
{
	params = $('#myForm').serializeArray();
	//alert(params);
	$.ajax({

		type: 'post',
		url: 'ajax_handler.php?action=config-price',
		data: params,
		success: function(retVal){
			//alert(retVal);
			$('#idx_products_price').html(retVal);
		}

	})
}

</script>
<script type="text/javascript">
//initiate the plugin and pass the id of the div containing gallery images
$(".zoom_01").elevateZoom({gallery:'gallery_01', cursor: 'pointer', galleryActiveClass: 'active', imageCrossfade: true, loadingIcon: 'http://www.elevateweb.co.uk/spinner.gif'});

//pass the images to Fancybox
$(".zoom_01").bind("click", function(e) {
  var ez =   $('.zoom_01').data('elevateZoom');
	$.fancybox(ez.getGalleryList());
  return false;
});
</script>
