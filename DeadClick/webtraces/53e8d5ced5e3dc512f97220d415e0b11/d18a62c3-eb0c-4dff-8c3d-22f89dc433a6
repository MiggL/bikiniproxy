</div><script type="text/javascript">
    $(document).ready(function() {
        jQuery.fn.extend({
            prependClass: function(newClasses) {
                return this.each(function() {
                    var currentClasses = $(this).prop("class");
                    $(this).removeClass(currentClasses).addClass(newClasses + " " + currentClasses);
                });
            }
        });

        var requestData = {"pageId":"freecharts","fromCurrency":"GBP","toCurrency":"MMK"};

        getRoadRunnerRequest(requestData).done(function(response) {
            var rr = JSON.parse(response);
            var cards = rr.cards;
            var ad = rr.ad;
            var modules = rr.modules;
            // Must collect the overwritingCampaignIds before the ad slot is overwritten
            var adFrame = $(".textAdWrap").find("iframe");
            var overwritingCampaignIds = getOverwritingCampaignIds(adFrame);

            for (var key in cards) {
                if (cards.hasOwnProperty(key)) {
                    $("#" + key).html(cards[key].content);
                    if(cards[key].parentClass) {
                        $("#" + key).parent("div").prependClass(cards[key].parentClass);
                    }
                    if(cards[key].hasOwnProperty("afterLoadScripts")) {
                        new Function("rr", cards[key].afterLoadScripts.join(""))(rr);
                    }
                }
            }

            if (ad !== undefined && ad.hasOwnProperty("afterLoadScripts")) {
                new Function("rr", ad.afterLoadScripts.join(""))(rr);
            }

            for (var moduleId in modules) {
                if (modules.hasOwnProperty(moduleId)) {
                    if (modules[moduleId].hasOwnProperty("beforeLoadScripts")) {
                        new Function("rr", modules[moduleId].beforeLoadScripts.join(""))(rr);
                    }
                    $("#" + moduleId).html(modules[moduleId].content);
                    if (modules[moduleId].hasOwnProperty("afterLoadScripts")) {
                        new Function("rr", modules[moduleId].afterLoadScripts.join(""))(rr);
                    }
                }
            }

            if (rr.hasOwnProperty("afterLoadScripts")) {
                new Function("rr", rr.afterLoadScripts.join(""))(rr);
            }

            var impressions = [];
            $("[data-sendimpression='true']").each(function(){
                impressions.push(JSON.parse($(this).attr("data-impressionevent")));
            });
            adFrame
                .contents()
                .find("[data-sendimpression='true']")
                .each(function () {
                    var imp = JSON.parse($(this).attr("data-impressionevent"));
                    if (overwritingCampaignIds.length > 0) {
                        imp.eventData.overwritingCampaignIds = overwritingCampaignIds;
                    }
                    impressions.push(imp);
                });
            sendBatchXeidEvents(impressions);
        });
    });

    function getRoadRunnerRequest(requestData) {
        var defaultData = {
            "url": window.location.href
        };

        requestData = $.extend({}, defaultData, requestData);
        return $.ajax({
            type: "GET",
            url: "/a/b/rr.php",
            data: requestData,
            dataType: "text"
        });
    }

    function getOverwritingCampaignIds(adFrame) {
        var overwritingCampaignIds = [];
        if (adFrame.length > 0) {
            var match;
            var campaignRegex = /WT\.seg_1=([a-zA-Z\d\-]*)/ig;
            var adContents = "";
            adFrame.contents().find("body").each(function() {
                adContents += $(this).html();
            });

            while ((match = campaignRegex.exec(adContents)) !== null) {
                overwritingCampaignIds.push(match[1]);
            }
        }

        return overwritingCampaignIds;
    }
</script><script type="text/javascript">
	var runMasonry = function(){
		$('.grid-layout').masonry({
			itemSelector:'.grid-item',
			percentPosition: true,
			columnWidth: '.grid-item',
			transitionDuration:0
		});
	};
</script>
			
			
