<script>
    $(window).load(function() {
		
        /************** For New Additional Menu ***************/        
        //$('.Taccordion').each(function () {
            //var $accordian = $(this);
            $('.Taccordion-head').click(function () {
                $(this).removeClass('Topen').addClass('Tclose');
                $('.Taccordion-body').slideUp();
                if (!$(this).next().is(':visible')) {
                    $(this).removeClass('Tclose').addClass('Topen');
                    $(this).next().slideDown();
                }
            });
        //});
        /*****************************/
    });
	
