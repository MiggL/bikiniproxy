        try {
           googleDiv = parent.document.createElement('div');
           googleDiv.id = 'div-gpt-ad-TaboolaFeed-1';
           //googleDiv.style.width='300px';
           //googleDiv.style.height='250px';
           frameElement.parentNode.appendChild(googleDiv);
           var googletag = parent.googletag || {};
           googletag.cmd = parent.googletag.cmd || [];
           googletag.cmd.push(function() {
               googletag.display('div-gpt-ad-TaboolaFeed-1');
               googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                   if (!event.isEmpty && event.slot.getSlotElementId() === 'div-gpt-ad-TaboolaFeed-1') {
			//console.log('render');
 			parent.TRC.dispatch('trcContentReady', {container: frameElement.parentNode});
                   }
               });
           });
        }
        catch(e){
        }
    }
