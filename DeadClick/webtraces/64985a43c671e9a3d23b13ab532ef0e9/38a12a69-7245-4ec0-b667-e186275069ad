var prefixe=document.getElementById("prefixe");var textPrefixe="";if(prefixe!=null&&prefixe.innerText!=undefined){textPrefixe=prefixe.innerText;}
else if(prefixe!=null){textPrefixe=prefixe.textContent;}
else{textPrefixe="";}
if(textPrefixe==""){textPrefixe="/fr";}
var lang=textPrefixe.substring(1,3);lang=lang+'_'+lang.toUpperCase();var _channelUrl="/"+"/"+"static.pecheur.com/js/channel.html",_facebookUrl="/"+"/"+"connect.facebook.net/"+lang+"/all.js";window.fbAsyncInit=function(){FB.init({appId:appId,channelUrl:_channelUrl,status:true,cookie:true,xfbml:true});$(document).trigger('FBSDKLoaded');};(function(d){var js,id='facebook-jssdk',ref=d.getElementsByTagName('script')[0];if(d.getElementById(id)){return;}
js=d.createElement('script');js.id=id;js.async=true;js.src=_facebookUrl;ref.parentNode.insertBefore(js,ref);}(document));