		<script type="text/javascript">
			var productAddToCartForm = new VarienForm('product_addtocart_form');
            productAddToCartForm.submit = function(){
					// This is the function that runs when we add a product to the basket (not one of the 600 others defined elsewhere)
                    if (this.validator.validate()) {
						var form = this.form;
						var oldUrl = form.action;
						var e = null;
						var data = jQuery('#product_addtocart_form').serialize();
						data += '&isAjax=1';

						jQuery('#ajax_loader').show();
						jQuery('#addToCartButton').hide();

						if(jQuery('.email-alert')){
							jQuery('.email-alert .email-form').hide();
							jQuery('.email-alert .email-submitted').show();
						}

						var url = form.action.replace("checkout/cart","ajax/index");

						try {
							jQuery.ajax({
								url: url,
								dataType: 'json',
								type : 'post',
								data: data,
								success: function(data){

									jQuery('#ajax_loader').hide();
									jQuery('#ajax_add_status').show();

									// Remove the hidden form element for the dye lot
									jQuery('#product_addtocart_form [name^="super_group"]').remove();

									window.setTimeout(function () {
										jQuery('#ajax_add_status,#basketmessage').fadeOut('slow',function() {

											if(jQuery('.header .gbp-price-cart')){
												jQuery('.header .gbp-price-cart').text(data.gbppricecartcontent);
											}
											if(jQuery('.header .currency-price-cart')){
												jQuery('.header .currency-price-cart').text(data.currencypricecartcontent);
											}

											jQuery('#addToCartButton').fadeIn('slow');
											jQuery('#basketmessage').fadeIn('slow');
										});
									}, 2000);
								}
							});
						} catch (e) {
						}
						// We might want some slightly cleverer error handling here - maybe show a message to the customer or log it?

						this.form.action = oldUrl;
						if (e) {
							throw e;
						}
					}
				}.bind(productAddToCartForm);
    </script>
