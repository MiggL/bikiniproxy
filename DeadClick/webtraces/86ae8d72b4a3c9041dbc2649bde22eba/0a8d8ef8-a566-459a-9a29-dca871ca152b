h.prototype._handleBlur=function(k){var m=this;
window.setTimeout(function(){if((document.activeElement==m.$selection[0])||(j.contains(m.$selection[0],document.activeElement))){return
}m.trigger("blur",k)
},1)
};
h.prototype._attachCloseHandler=function(k){var m=this;
