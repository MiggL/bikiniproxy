function isInList(url,li){
 var l=li.length;
 for (var i=0; i<l; i++) {
   if(url.indexOf(li[i])>-1){return i+1;}
 }
 return false;
}
function readCk(name){
 var c=document.cookie,b=c.indexOf(name+'=')
 if(b>=0){
  b=c.indexOf('=',b)+1
  if(b>0){
   var e=c.indexOf(';',b)
   if(e==-1)e=c.length
   return c.substring(b,e)
  }
 }
 return ''
}
function optoutFn(){
	try {
		if (cap_optout===1){
			var i1=document.createElement('iframe');
			i1.id='optout';
			if (!window.adsSaf)i1.style.display="none";
			i1.style.width='0px';
			i1.style.height='0px';
			i1.src='//optout3.privacy.aol.com/adapter.aspx?p_id=2&action_id=9';
			document.body.appendChild(i1);
			clearInterval(x);
		}
		else if (cap_optout===0){
			clearInterval(x);
		}
 	}
	catch (e){}
	if (t>50){
		clearInterval(x);
	}
	else t++;
}
