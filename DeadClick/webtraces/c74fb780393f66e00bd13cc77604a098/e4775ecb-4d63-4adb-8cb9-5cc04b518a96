   var microsoftLineItem = ADventori.utils.map(MicrosoftFixeData[eadLocation], function(data) {
   if (+dateToday >= +ADventori.date.parse(data.StartDate) && +dateToday <= +ADventori.date.parse(data.EndDate)) {
    return data;
   }
});
//    debug.log('microsoftLineItem :', microsoftLineItem);
