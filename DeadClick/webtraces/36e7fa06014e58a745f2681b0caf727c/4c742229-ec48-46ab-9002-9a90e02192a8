<script type="text/javascript">
function validate1()
{
	var f=document.form1;
	var msg="";
	var child=f.txtchildren.value;
	var adult=f.txtadult.value;
	var children=0;
	var cat=0;
	var rt=0;
	if (f.radiocategory3.checked==true)
	{
		if (adult=="" || adult=="0")
		{
			msg=msg+"No.of students cannot be blank.\n";
		}
		if (isNaN(adult))
		msg=msg+" Please re-enter no. of students.\n";	

	}
	
	if (f.radiocategory1.checked==true || f.radiocategory2.checked==true)
	{
		if (adult=="" || adult=="0")
		{
			msg=msg+" No.of adults cannot be blank.\n";
		}
		else
		{
			if (adult.substring(1,2)=="." || adult.substring(2,3)==".")
			{
				msg=msg+"Invalid no.of adults\n";
			}
		}
		if (isNaN(adult))
		msg=msg+" Please re-enter no. of adults.\n";	
	
		if (child!="")
		{
		
			if (isNaN(child))
			{
				msg=msg+" Please re-enter no. of children.\n";	
			}
			else

			{
				children=parseInt(adult)*5;
				if (child.substring(1,2)=="." || child.substring(2,3)==".")
				{
					msg=msg+"Invalid no.of children\n";
				}
		

				if 	(child>children)
				{
					msg=msg+" One Adult can have max 5 children.\n";	
				}	
			}
		}
	}
	if (f.radiocategory3.checked==true)
	{
		var students=document.getElementById("txtadult").value;
		if (students<20)
		{	
			msg=msg+"Minimum 20 students are required for booking under this category.\n";
		}
		if (students.substring(1,2)=="." || students.substring(2,3)==".")
		{
			msg=msg+"Invalid no.of students\n";
		}

	}

	var date1=f.date1.value;
	if (date1=="")
	{	
		msg=msg+"Tour date cannot be blank.\n";
	}

	var currentTime = new Date();
	var month = currentTime.getMonth() + 1;
	var day = currentTime.getDate();
	var year = currentTime.getFullYear();
	var curdate=day+ "/" + month + "/" + year;

	
	var d1=f.date1.value;
	var d3=curdate;
	//alert(curdate);
	var strdate1=d1.split("/");
	var strdate3=d3.split("/");
if (getDayOfDate(d1)=="0")
{
		msg=msg+"Invalid Date.";
}
if (parseInt(strdate1[2],10)==2012 && parseInt(strdate1[1],10)==4 && parseInt(strdate1[0],10)==16)
	{
		msg=msg+"Invalid Date.";
	}	

	if (parseInt(strdate3[2],10)>parseInt(strdate1[2],10))
	{
		msg=msg+"Tour date cannot be before / equal to today.";
	}
	else if (parseInt(strdate3[2],10)==parseInt(strdate1[2],10))
	{
		if (parseInt(strdate3[1],10)>parseInt(strdate1[1],10))
		{
			msg=msg+"Tour date cannot be before / equal to today.";
		}
		else if (parseInt(strdate3[1],10)==parseInt(strdate1[1],10))
		{
			if (parseInt(strdate3[0],10)>parseInt(strdate1[0],10))
			{
					msg=msg+"Tour date cannot be before / equal to today.";
			}
		}			
		
	}

	if (msg!="")
	{
		alert(msg);
		return false;
	}
	else
	{
		document.getElementById("hidden_search_type").value="1";
//		document.form1.submit();
		if (f.radiocategory1.checked==true)
		{
			cat=1;
		}
		else if (f.radiocategory2.checked==true)
		{
			cat=2;
		}
		else if (f.radiocategory3.checked==true)
		{
			cat=3;
		}

		if (f.radioroute1.checked==true)
		{
			rt=1;
		}
		else if (f.radioroute2.checked==true)
		{
			rt=2;
		}
		else if (f.radioroute3.checked==true)
		{
			rt=2;
		}

//				$_SESSION['new_search']="S".","."0".",".$nationality.",".$route_skier.",".$way_skier.","."0".",".$persons.",".$children_skier.",".$dd_skier.",".$mm_skier.",".$yy_skier;
//		$_SESSION['new_search']="G".",".$ticket_category.","."0".",".$route_general.",".$way_general.",".$adults.","."0".",".$children_general.",".$dd_general.",".$mm_general.",".$yy_general;

		document.getElementById("hidden_string").value="G"+","+cat+","+"0"+","+rt+","+"7"+","+adult+","+"0"+","+child+","+parseInt(strdate1[0],10)+","+parseInt(strdate1[1],10)+","+parseInt(strdate1[2],10);
		
//		alert(document.getElementById("hidden_string").value);
		document.form1.submit();
		document.location.href="searchbookings.php";
		

	}
}

function validate2()
{
	var f=document.form1;
	var adult=f.txtpersons.value;
	var msg="";
	var nationality="";
	var route="";
	if (f.selectnationality.value=="" || f.selectnationality.value=="0")
	{
		msg="Select the nationality.\n";
	}	
	if (f.selectroute.value=="" || f.selectroute.value=="0")
	{
		msg=msg+"Select the route.\n";
	}	
	
	if (f.selectroute.value=="1")
	{
		if (f.selectpasstype.value=="" || f.selectpasstype.value=="0")
		{
			msg=msg+"Select the pass type.\n";
		}
	}	
	
	if (isNaN(adult))
    msg=msg+" Please re-enter no. of persons.\n";	
	if (adult=="" || adult=="0")
	{
		msg=msg+"No.of persons cannot be blank.\n";
	}
	if (adult.substring(1,2)=="." || adult.substring(2,3)==".")
	{
		msg=msg+"Invalid no.of persons\n";
	}
	if (f.dateskier.value=="")
	{	
		msg=msg+"Tour date cannot be blank.\n";
	}
	var currentTime = new Date();
	var month = currentTime.getMonth() + 1;
	var day = currentTime.getDate();
	var year = currentTime.getFullYear();
	var curdate=day+ "/" + month + "/" + year;

	
	var d1=f.dateskier.value;
	var d3=curdate;
	//alert(curdate);
	var strdate1=d1.split("/");
	var strdate3=d3.split("/");

if (getDayOfDate(d1)=="0")
{
		msg=msg+"Invalid Date.";
}
	if (parseInt(strdate1[2],10)==2012 && parseInt(strdate1[1],10)==4 && parseInt(strdate1[0],10)==16)
	{
		msg=msg+"Invalid Date.";
	}

	if (parseInt(strdate3[2],10)>parseInt(strdate1[2],10))
	{
		msg=msg+"Tour date cannot be before / equal to today.";
	}
	else if (parseInt(strdate3[2],10)==parseInt(strdate1[2],10))
	{
		if (parseInt(strdate3[1],10)>parseInt(strdate1[1],10))
		{
			msg=msg+"Tour date cannot be before / equal to today.";
		}
		else if (parseInt(strdate3[1],10)==parseInt(strdate1[1],10))
		{
			if (parseInt(strdate3[0],10)>parseInt(strdate1[0],10))
			{
					msg=msg+"Tour date cannot be before / equal to today.";
			}
		}			
		
	}



	if (msg!="")
	{
		alert(msg);
		return false;
	}
	else
	{
//				$_SESSION['new_search']="S".","."0".",".$nationality.",".$route_skier.",".$way_skier.","."0".",".$persons.",".$children_skier.",".$dd_skier.",".$mm_skier.",".$yy_skier;

		document.getElementById("hidden_string").value="S"+","+"0"+","+f.selectnationality.value+","+f.selectroute.value+","+f.selectpasstype.value+","+"0"+","+adult+","+"0"+","+parseInt(strdate1[0],10)+","+parseInt(strdate1[1],10)+","+parseInt(strdate1[2],10);
		
//		alert(document.getElementById("hidden_string").value);
		document.form1.submit();
		document.location.href="searchbookings.php";


	}

}

function getCategory()
{

	if ((document.getElementById("radiocategory1").checked==true) || (document.getElementById("radiocategory2").checked==true))
	{
		document.getElementById("tdadult").innerHTML="Adults:";
		document.getElementById("tdchild").style.visibility="visible";
		document.getElementById("spantxtchild").style.visibility="visible";
	}
	else
	{
		document.getElementById("tdadult").innerHTML="Students:";
		document.getElementById("tdchild").style.visibility="hidden";
		document.getElementById("spantxtchild").style.visibility="hidden";
	}

	if ((document.getElementById("radioroute1").checked==true) || (document.getElementById("radioroute2").checked==true))
	{
		document.getElementById("tdchild").style.visibility="visible";
		document.getElementById("spantxtchild").style.visibility="visible";
	}
	else
	{
		document.getElementById("tdchild").style.visibility="hidden";
		document.getElementById("spantxtchild").style.visibility="hidden";
	}
	if ((document.getElementById("radioroute3").checked==true) || (document.getElementById("radiocategory3").checked==true))
	{
		document.getElementById("tdchild").style.visibility="hidden";
		document.getElementById("spantxtchild").style.visibility="hidden";
	}

}
		function changepasstype(id)
		{
			var passtype=document.getElementById("selectpasstype");
			var x;
			var i=0;
			if (id=="1" || id=="2")
			{
				var theOption=new Option;
	
				theOption.text="One Way";
				theOption.value="1";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="One Day";
				theOption.value="2";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Weekly";
				theOption.value="4";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Seasonal Pass";
				theOption.value="5";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Seasonal Pass (Premium)";
				theOption.value="6";
				passtype.options[i]=theOption;
				i++;

				
			}
			if (id=="23")
			{
				var theOption=new Option;
	
				theOption=new Option;
				theOption.text="One Day";
				theOption.value="2";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Weekly";
				theOption.value="4";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Seasonal Pass";
				theOption.value="5";
				passtype.options[i]=theOption;
				i++;
				theOption=new Option;
				theOption.text="Seasonal Pass (Premium)";
				theOption.value="6";
				passtype.options[i]=theOption;
				i++;

				
			}


		}

function getDayOfDate(dt) {  
var givenDate=dt;  
  
weekDays=["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"]  
var dArray = givenDate.split("/");  
// You have to write code that will check whether the date is valid or not because February cannot be 30 or 31, etc  
  
// months in Javascript are 0-11  
myDate=new Date(dArray[2],dArray[1]-1,dArray[0]);  
var dayCode = myDate.getDay(); // dayCode 0-6  
var dayIs=weekDays[dayCode]; //It will contain the required day, in this example it will be friday  
if (dayIs=="Monday")
{
	var strdate_monday_check=dt.split("/");
	var chk_date= parseInt(strdate_monday_check[0]);
	if ((chk_date>=1 && chk_date<8) || (chk_date>=15 && chk_date<22))
	{
		return "0";
	}
	else
	{
		return "1";
	}

}
else
	{
	return "1";
	}
} 
