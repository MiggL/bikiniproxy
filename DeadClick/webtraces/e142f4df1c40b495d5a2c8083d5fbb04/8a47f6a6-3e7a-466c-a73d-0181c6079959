          start = $(div).offset().top;
   
      $.event.add(window, "scroll", function() {
          var p = $(window).scrollTop();
          $(div)
            .css('position',((p)>start -52) ? 'fixed' : 'static');
          $(div).css('top',((p)>start -52) ? '52px' : '');
      });
   
