        var orgOpen = window.open;
        window.open = function (url, windowName, windowFeatures) {
            if (window.innerWidth < 767) {
                var chatWindowHeight = 300; /* Width of the chat window. Use your own value. */
                var chatWindowWidth = 240;  /* Height of the chat window. Use your own value. */

                /* Calculate the top and left position to keep the chatwindow in center */
                var top = 200;
                var left = 200;

                /* Create the arguments for window.open method. */
                var windowFeatures = "width=" + chatWindowWidth + ",height=" + chatWindowHeight + ",status,resizable,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
            }
            else {
                var chatWindowHeight = 500; /* Width of the chat window. Use your own value. */
                var chatWindowWidth = 400;  /* Height of the chat window. Use your own value. */

                /* Calculate the left and top position to keep the chatwindow in bottom right */
                var left = 935; //parseInt((screen.availWidth / 2));
                var top = 165; //parseInt((screen.availHeight / 2) - (chatWindowHeight / 2));

                /* Create the arguments for window.open method. */
                var windowFeatures = "width=" + chatWindowWidth + ",height=" + chatWindowHeight + ",status,resizable,left=" + left + ",top=" + top + "screenX=" + left + ",screenY=" + top;
            }
            return orgOpen(url, windowName, windowFeatures);
        }
        $(document).ready(function () {
            var userCountry = 'fr';
            console.log('con:' + userCountry);
            if (userCountry != '')
            {
                if (userCountry != 'us' && userCountry != 'ca')
                {
                    $('.live_chat').remove();
                }
            }
            var futureDate = new Date();
            futureDate.setDate(futureDate.getDate() + 120);
            var cookieDate = futureDate.toUTCString();
            cmpID = getParamByName('cmpid');

            if (cmpID == "") {
                var cookies = document.cookie;
                var cookiearray = cookies.split("; ");
                cookiearray.forEach(function (cookie) {
                    if (cookie.substring(0, 6) == 'cmpid=') {
                        // Found CMPID cookie 					
                        cmpID = cookie.substring(6);
                    }
                });
            } else {
                // CMPID found in query string - save a cookie			
                document.cookie = "cmpid=" + cmpID + "; expires=" + cookieDate + "; path=/";
            }
            console.log(cmpID);
            $('.hid-cmpid').val(cmpID);
        });
    </script>
