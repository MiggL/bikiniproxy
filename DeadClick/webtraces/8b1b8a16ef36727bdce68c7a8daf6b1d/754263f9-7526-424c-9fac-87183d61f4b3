<script type='text/javascript'>
requirejs.onError = function (err) {
    logger.log('Exception Occurred: '+err.requireType, 'Error', 'JavascriptIncrementalLoader');
    if (err.requireType === 'timeout') {
       logger.log('Timed out trying to load modules: ' + err.requireModules, 'Error', 'JavascriptIncrementalLoader');
       var exception = new GrahamTechnology.util.ExceptionErrorMessage();
       exception.exceptionName = 'Timed out trying to load modules'
       exception.errorMessage = err.requireModules;
       GrahamTechnology.action.setRefresh(true,exception);
       GrahamTechnology.action.refresh(true);
    } else if (err.requireType === 'scripterror') {
       logger.log('File not found or Javascript error detected in one of the following files: ' + err.requireModules, 'Error', 'JavascriptIncrementalLoader');
       var exception = new GrahamTechnology.util.ExceptionErrorMessage();
       exception.exceptionName = 'File not found or Javascript error detected in one of the following files'
       exception.errorMessage = err.requireModules;
       GrahamTechnology.action.setRefresh(true,exception);
       GrahamTechnology.action.refresh(true);
    }
};
</script>
