<style>
.pitcher .more {background-image:url(/images/icons/related_link/hou.gif)};
</style>

<script type="text/javascript">
$(document).ready(function () {
	bam.loadSync(bam.homePath + 'bam.datetime.js');


	/*
		Always display the current season stats unless it's between Opening Day and All-Star per MLBPRODBAM-6557
	*/
	$.ajax({
		type: "GET",
			url: "/lookup/xml/named.org_game_type_date_info.bam?sport_code=%27mlb%27&current_sw=%27Y%27&game_type=%27S%27&game_type=%27R%27&game_type=%27A%27&game_type=%27F%27&game_type=%27D%27&game_type=%27L%27&game_type=%27W%27&season=2018",
		dataType: "xml",
			success: function(xml) {
				var include_curr_season = 'N';
				if ($(xml).find('row').length != 0) {
					var today = new Date;
					var dataYYYY = today.getFullYear();
					today = today.getTime();
					var allStarStartDate;
					var regSeasonStartDate;
					var seasonYYYY;
					$(xml).find('row').each(function(){
						var game_type = $(this).attr('game_type');
						if (game_type=='A') {
							allStarStartDate = new Date( $(this).attr('first_game_date') );
							allStarStartDate = allStarStartDate.getTime();
							seasonYYYY = $(this).attr('season');
						}
					});
					if(today > allStarStartDate && dataYYYY == seasonYYYY) {
						include_curr_season = 'Y';
					}
					initPitchers(include_curr_season);
				} else {
					initPitchers(include_curr_season);
				}
			}
	});

	function initPitchers(include_curr_season) {
		$('.pitcher.first').each(function(index){
			var pid = $(this).attr("pid");
			var time, time_zone;

			if ("hou" === "mlb") {
				time = $(this).attr("eastern_time");
				time_zone = "EDT";
			} else {
				if ("hou" == $(this).attr("home_file_code")) {
					time = $(this).attr("game_time_home");
					time_zone = $(this).attr("time_zone_home");
				} else {
					time = $(this).attr("game_time_away");
					time_zone = $(this).attr("time_zone_away");
				}
			}

			var formatDate = bam.datetime.parseISODate(time);
			if (formatDate.formatDate("h:mm A") == "3:33 AM") {
				time = formatDate.formatDate("EEEE, MMMM d, ") + 'TBD';
			} else {
				time = formatDate.formatDate("EEEE, MMMM d, h:mm A") + ' ' + time_zone;
			}
			var tid = $(this).attr("tid");
			var game_pk = $(this).attr("game_pk");
			var gid = $(this).attr("gid");
			gid = gid.replace(/-/g, "_");
			gid = gid.replace(/\//g, "_");
			bam.showLineup = function(gid){
				bam.load(bam.homePath+"bam.popModule.js", function(){
					var newHTML  = "<iframe frameborder='0' width='725' height='438' scrolling='no' src='/mlb/lineups/index.jsp?game_id="+gid+"'></iframe>";
					bam.popModule.init({
						css        : "/mlb/lineups/lineups.popModule.css", // this is the default css, but custom css can be set here like so
						overlayCss : "/shared/css/bam/bam.overlay.css"    // (same as above)
					});
					bam.popModule.show({
						htmlContent    : newHTML,
						width          : 725, // default is 500 (set in CSS file)
						height         : 438,
						overlayOpacity : 0.8,
						preShow        : function(){},
						postShow       : function(){}
					});
				});
			};
			$('<span></span>').html(time).prependTo('#date_'+game_pk);
			$('<span></span>').html('<a href="javascript:bam.showLineup(\''+gid+'\')">Lineups</a>').appendTo('.lineups_container_'+game_pk);
			$('<span></span>').html('<a href="http://www.mlb.com/r/game?gid='+gid+'">Preview</a>').appendTo('.preview_container_'+game_pk);
			$.ajax({
				type: "GET",
	       		url: "/ticketing-client/xml/Game.tiksrv?team_id="+tid+"&begin_date=20180215&end_date=20180219&site_section=%27DEFAULT%27",
				dataType: "xml",
	 			success: function(xml) {
					$(xml).find('game').each(function(){
						var tlink = $(this).find('ticket_link').attr('tlink');
						var sid = $(this).attr('schedule_id');
						var home_team_id = $(this).attr('home_team_id');
						if (game_pk === sid) {
							if (home_team_id == "117") {var home_or_away = 'home';} else {var home_or_away = 'away';}
							var tracking = "hou-news-probable_pitchers-na-X0";
							$('<span></span>').html('<a class="bam-button bam-button-tickets bam-button-small tickets_link" href="javascript:void(openTIXXWindow( \''+tlink+'\', \''+home_or_away+'\', \''+tracking+'\', false ));">Buy Tickets <span>&raquo;</span></a>').appendTo('.tickets_container_'+game_pk);
						}
					});
		    	}
	    	});
			$.ajax({
				type: "GET",
	       		url: "/lookup/xml/named.sit_pitcher_date.bam?player_id="+pid+"&sport_code='mlb'&season=2018&game_type='F'&game_type='D'&game_type='L'&game_type='W'&date_num='2018/02/15'&vs_team_id="+tid,
				dataType: "xml",
	 			success: function(xml) {
					$(xml).find('row[sit_code="cur"]').each(function(){
						var wins = $(this).attr('p_win');
						var losses = $(this).attr('p_loss');
						var era = $(this).attr('p_era');
						$('<div class="rs_stats"></div>').html('<dl><dt>Postseason stats</dt><dd>'+wins+'-'+losses+', '+era+' ERA</dd></dl>').appendTo('.rs_stats_container_'+pid);
					});
		    	}
	    	});
			$("#more_last_three_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center right',
				offset: [39, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/xml/named.mlb_individual_pitching_last_x_total.bam?results=3&game_type=%27R%27&game_type=%27D%27&game_type=%27L%27&game_type=%27W%27&season=2018&player_id="+pid,
						dataType: "xml",
		  				success: function(xml) {
							if ($(xml).find('row').length != 0) {
								$(xml).find('row').each(function(){
									var wins = $(this).attr('w');
									var losses = $(this).attr('l');
									var era = $(this).attr('era');
									var ip = $(this).attr('ip');
									var so = $(this).attr('so');
									var whip = $(this).attr('whip');
									var g = $(this).attr('g');
									var game_type = $(this).attr('game_type');
									if (game_type=="R") var gt_text = "regular-season";
									if (game_type=="D") var gt_text = "division series";
									if (game_type=="L") var gt_text = "league championship series";
									if (game_type=="W") var gt_text = "world series";
									$('.loading').addClass('hide');
									if (ip > 0) {
										$('<div class="stats_content list"></div>')
										.html('<span class="header">'+ip+' '+gt_text+' innings pitched</span><dl><dt>Wins</dt><dd>'+wins+'</dd></dl><dl><dt>Losses</dt><dd>'+losses+'</dd></dl><dl><dt>ERA</dt><dd>'+era+'</dd></dl><dl><dt>Ks</dt><dd>'+so+'</dd></dl>'
										)
										.appendTo('#tooltip_last_three_'+pid);
										$('.stats_content dl:odd').addClass('odd');
									} else {
										$('<div class="stats_content list"></div>')
										.html(
											'<span class="no_data">No data available</span>'
										)
										.appendTo('#tooltip_last_three_'+pid);
										$('.stats_content dl:odd').addClass('odd');
									}
								});
							} else {
								$('<div class="stats_content list"></div>')
								.html(
									'<span class="no_data">No data available</span>'
								)
								.appendTo('#tooltip_last_three_'+pid);
								$('.stats_content dl:odd').addClass('odd');
							}
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
			$("#more_career_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center right',
				offset: [18, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/xml/named.sit_pitcher_date.bam?player_id="+pid+"&sport_code=%27mlb%27&season=2018&game_type=%27R%27&date_num='2018/02/15'&vs_team_id="+tid,
						dataType: "xml",
		  				success: function(xml) {
							if ($(xml).find('row[sit_code="cur"]').length != 0) {
								$(xml).find('row[sit_code="cur"]').each(function(){
									var wins = $(this).attr('p_win');
									var losses = $(this).attr('p_loss');
									var era = $(this).attr('p_era');
									var ip = $(this).attr('p_innings');
									var so = $(this).attr('p_strikeout');
									$('.loading').addClass('hide');
									$('<div class="stats_content list"></div>')
									.html(								'<span class="header">'+ip+' innings pitched</span><dl><dt>Wins</dt><dd>'+wins+'</dd></dl><dl><dt>Losses</dt><dd>'+losses+'</dd></dl><dl><dt>ERA</dt><dd>'+era+'</dd></dl><dl><dt>Ks</dt><dd>'+so+'</dd></dl>'
									)
									.appendTo('#tooltip_career_'+pid);
								});
							} else {
								$('<div class="stats_content"></div>')
									.html(
										'<span class="no_data">No data available</span>'
									)
								.appendTo('#tooltip_career_'+pid);
							}
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
			$("#more_career_vs_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center right',
				offset: [-3, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/named.sit_bvp_5y_date.bam?vs_pitcher_id="+pid+"&game_type='R'&date_num='2018/02/15'&team_id="+tid+"&sport_code='mlb'&cur_seas_sw='"+include_curr_season+"'",
						dataType: "xml",
		  				success: function(xml) {
							$(xml).find('row').each(function(){
								var player = $(this).attr('player_first_last');
								if ($(this).attr('avg') != '') {var avg = $(this).attr('avg');} else {var avg = '-';}
								if ($(this).attr('ab') != '') {var ab = $(this).attr('ab');} else {var ab = '-';}
								if ($(this).attr('hr') != '') {var hr = $(this).attr('hr');} else {var hr = '-';}
								if ($(this).attr('rbi') != '') {var rbi = $(this).attr('rbi');} else {var rbi = '-';}
								$('.loading').addClass('hide');
								$('<tr class="stats_content"></tr>')
								.html(
									'<td class="first">'+player+'</td><td>'+avg+'</td><td>'+ab+'</td><td>'+hr+'</td><td>'+rbi+'</td>'
								)
								.appendTo('#tooltip_career_vs_'+pid+' tbody');
								$('.pitcher tr:odd').addClass('odd');
							});
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
		});
		$('.pitcher.last').each(function(index){
			var pid = $(this).attr("pid");
			var tid = $(this).attr("tid");
			$.ajax({
				type: "GET",
	       		url: "/lookup/xml/named.sit_pitcher_date.bam?player_id="+pid+"&sport_code='mlb'&season=2018&game_type='F'&game_type='D'&game_type='L'&game_type='W'&date_num='2018/02/15'&vs_team_id="+tid,
				dataType: "xml",
	 			success: function(xml) {
					$(xml).find('row[sit_code="cur"]').each(function(){
						var wins = $(this).attr('p_win');
						var losses = $(this).attr('p_loss');
						var era = $(this).attr('p_era');
						$('<div class="rs_stats"></div>').html('<dl><dt>Postseason stats</dt><dd>'+wins+'-'+losses+', '+era+' ERA</dd></dl>').appendTo('.rs_stats_container_'+pid);
					});
		    	}
	    	});

			$("#more_last_three_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center left',
				offset: [39, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/xml/named.mlb_individual_pitching_last_x_total.bam?results=3&game_type=%27R%27&game_type=%27D%27&game_type=%27L%27&game_type=%27W%27&season=2018&player_id="+pid,
						dataType: "xml",
		  				success: function(xml) {
							if ($(xml).find('row').length != 0) {
								$(xml).find('row').each(function(){
									var wins = $(this).attr('w');
									var losses = $(this).attr('l');
									var era = $(this).attr('era');
									var ip = $(this).attr('ip');
									var so = $(this).attr('so');
									var whip = $(this).attr('whip');
									var g = $(this).attr('g');
									var game_type = $(this).attr('game_type');
									if (game_type=="R") var gt_text = "regular-season";
									if (game_type=="D") var gt_text = "division series";
									if (game_type=="L") var gt_text = "league championship series";
									if (game_type=="W") var gt_text = "world series";
									$('.loading').addClass('hide');
									if (ip > 0) {
										$('<div class="stats_content list"></div>')
										.html('<span class="header">'+ip+' '+gt_text+' innings pitched</span><dl><dt>Wins</dt><dd>'+wins+'</dd></dl><dl><dt>Losses</dt><dd>'+losses+'</dd></dl><dl><dt>ERA</dt><dd>'+era+'</dd></dl><dl><dt>Ks</dt><dd>'+so+'</dd></dl>'
										)
										.appendTo('#tooltip_last_three_'+pid);
									} else {
										$('<div class="stats_content"></div >')
										.html(
											'<span class="no_data">No data available</span>'
										)
										.appendTo('#tooltip_last_three_'+pid);
									}
								});
							} else {
								$('<div class="stats_content"></div >')
								.html(
									'<span class="no_data">No data available</span>'
								)
								.appendTo('#tooltip_last_three_'+pid);
							}
				    	}
			    	});
				},
				onHide: function() {
					$('.stats_content').remove();
					$('.loading').removeClass('hide');
				}
			});
			$("#more_career_"+pid).tooltip({
				events: {def: "click,mouseout"},
				effect: 'fade',
				relative: true,
				position: 'center left',
				offset: [18, 0],
				predelay:200,
				delay:1200,
				onBeforeShow: function(){
					$.ajax({
						type: "GET",
			       		url: "/lookup/xml/named.sit_pitcher_date.bam?player_id="+pid+"&sport_code=%27mlb%27&season=2018&game_type=%27R%27&date_num='2018/02/15'&vs_team_id="+tid,
						dataType: "xml",
		  				success: function(xml) {
							if ($(xml).find('row[sit_code="cur"]').length != 0) {
								$(xml).find('row[sit_code="cur"]').each(function(){
									var wins = $(this).attr('p_win');
									var losses = $(this).attr('p_loss');
									var era = $(this).attr('p_era');
									var ip = $(this).attr('p_innings');
									var so = $(this).attr('p_strikeout');
									$('.loading').addClass('hide');
									$('<div class="stats_content list"></div>')
									.html('<span class="header">'+ip+' innings pitched</span><dl><dt>Wins</dt><dd>'+wins+'</dd></dl><dl><dt>Losses</dt><dd>'+losses+'</dd></dl><dl><dt>ERA</dt><dd>'+era+'</dd></dl><dl><dt>Ks</dt><dd>'+so+'</dd></dl>'
									)
