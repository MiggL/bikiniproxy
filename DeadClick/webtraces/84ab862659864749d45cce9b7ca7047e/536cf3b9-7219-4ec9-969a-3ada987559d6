    var doi = "10.1108%2FJTMC-08-2014-0050";
    if (! citart) { citart = "citart1"; }
    if (! ptype) { ptype = ""; }
    var pt = rid.charAt(0) + "JTMC0820140050" + ptype;
    return popupRef(citart, rid, doi, pt, area, width, height);
}



