    $(document).on('click', '#load-more-photos', function (event) {
      event.preventDefault()
      $('#load-more-photos').text("Chargement...")

      var path = "/projets/19616/load_more_photos"
      var viewMode = "xl"
      var loadedPhotosCount = $('#sortable-project-photo-item li').length

      return $.ajax({
        url: path,
        type: 'GET',
        data: { m: viewMode, loaded_photos_count: loadedPhotosCount },
        success: function (data) {
          $('#sortable-project-photo-item').append(data.photos_html)

          if (data.remove_load_more_button === true) {
            $('#load-more-photos-button').empty()
          }
          else {
            $('#load-more-photos').text("Ajouter plus de photos")
          }
        }
      })
    })
  </script>
