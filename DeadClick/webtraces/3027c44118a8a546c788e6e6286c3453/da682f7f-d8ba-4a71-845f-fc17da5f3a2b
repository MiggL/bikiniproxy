function addFlashMovie(id, flv) {
    var flashvars = {file: flv ,type: 'flv'};
    var params = {allowfullscreen :true};
    var attributes = {};
    swfobject.embedSWF('/flvplayer.swf', id, "352", "288", "7.0.0", false, flashvars, params, attributes);
}

