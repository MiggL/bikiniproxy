    }
    clientAPI.idleTimeoutHandler.start(4500000, 0);
    clientAPI.setPageRendered(true);
    GrahamTechnology.util.AddDisableBackSpaceForHistoryListener();
}

function ajaxOnUnload() {
    clientAPI.postPageUnloadExpiryCommand(true);
    clientAPI.setPageRendered(false);
    GrahamTechnology.ui.Form.sendUnsentComponentValues();
    GrahamTechnology.util.RemoveDisableBackSpaceForHistoryListener();
}

