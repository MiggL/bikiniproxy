function isInList(url,li){
	var l=li.length;
	for (var i=0; i<l; i++){
		if(url.indexOf(li[i])>-1)return i+1;
	}
	return false;
}
function readCk(name){
	var c=document.cookie,b=c.indexOf(name+'=');
	if(b>=0){
		b=c.indexOf('=',b)+1;
		if(b>0){
			var e=c.indexOf(';',b);
			if(e==-1)e=c.length;
			return c.substring(b,e);
		}
	}
	return '';
}
