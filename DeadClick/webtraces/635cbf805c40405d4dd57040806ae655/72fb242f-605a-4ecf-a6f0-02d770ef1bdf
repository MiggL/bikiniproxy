<script type="text/javascript">
  var query_url = window.location.href;
  var admitad_uid	= _GET('admitad_uid', location.hash.indexOf('admitad_uid=') > 0 ? '?' + location.hash.substr(1) : null);
  var ftduid	= _GET('tduid', location.hash.indexOf('tduid=') > 0 ? '?' + location.hash.substr(1) : null);
  var utm_campaign = _GET('utm_campaign', location.hash.indexOf('utm_campaign=') > 0 ? '&' + location.hash.substr(1) : null);
  var utm_medium = _GET('utm_medium', location.hash.indexOf('utm_medium=') > 0 ? '&' + location.hash.substr(1) : null);

  if(utm_campaign){

    $.cookie('utm_campaign',utm_campaign,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  if(ftduid)
  {

    $.cookie('ftduid',ftduid,{expires: 30, path: '/',domain:COOKIESDIAMON});

  }
  var utm_source	= _GET('utm_source', location.hash.indexOf('utm_source=') > 0 ? '?' + location.hash.substr(1) : null);
  if(utm_source)
  {

    if(utm_source=='lomadee'){
	$.cookie("lomadee_traffic",utm_source,{expires: 30, path: '/',domain:COOKIESDIAMON});
	}
    $.cookie("utm_source",utm_source,{expires: 30, path: '/',domain:COOKIESDIAMON});

    // uniq, only reserve the last Commission platform 2016.06.23
	  $.cookie("linkid",'',{expires: -1, path: '/',domain:COOKIESDIAMON});
  }

  if(utm_source && utm_medium && utm_campaign) {
    $.cookie('mail_channel', utm_medium, {expires: 0, path: '/', domain: COOKIESDIAMON})
    $.cookie('mail_campaign', utm_campaign, {expires: 0, path: '/', domain: COOKIESDIAMON})
  }

  if(admitad_uid)
  {
    $.cookie("utm_source",'admitad_uid',{expires: 30, path: '/',domain:COOKIESDIAMON});
    $.cookie("admitad_uid",admitad_uid,{expires: 30, path: '/',domain:COOKIESDIAMON});

    // uniq, only reserve the last Commission platform 2016.06.23
	  $.cookie("linkid",'',{expires: -1, path: '/',domain:COOKIESDIAMON});
  }

  // "cityads" union platform "click_id"
  var click_id = _GET('click_id', query_url);
  var click_uid = _GET('click_id', location.hash.indexOf('click_id=') > 0 ? '&' + location.hash.substr(1) : null);
  if(click_id)
  {

    $.cookie('click_id', click_id, {expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  if(click_uid)
  {

    $.cookie('click_uid', click_uid, {expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  //aff source end
  var oid	= _GET('oid', location.hash.indexOf('oid=') > 0 ? '?' + location.hash.substr(1) : null);
  var rqid	= _GET('rqid', location.hash.indexOf('rqid=') > 0 ? '&' + location.hash.substr(1) : null);
  if(oid)
  {

    $.cookie("oid",oid,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  if(rqid)
  {

    $.cookie("rqid",rqid,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var af_id	= _GET('af_id', location.hash.indexOf('af_id=') > 0 ? '&' + location.hash.substr(1) : null);
  if(af_id){
    $.cookie("af_id",af_id,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var pl_id	= _GET('pl_id', location.hash.indexOf('pl_id=') > 0 ? '&' + location.hash.substr(1) : null);
  if(pl_id){
    $.cookie("pl_id",pl_id,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var click_id	= _GET('click_id', location.hash.indexOf('click_id=') > 0 ? '&' + location.hash.substr(1) : null);
  if(click_id){
    $.cookie("click_id",click_id,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var pl_type	= _GET('pl_type', location.hash.indexOf('pl_type=') > 0 ? '&' + location.hash.substr(1) : null);
  if(pl_type){
    $.cookie("pl_type",pl_type,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var actionpay	= _GET('actionpay', location.hash.indexOf('actionpay=') > 0 ? '&' + location.hash.substr(1) : null);
  if(actionpay){
    $.cookie("actionpay",actionpay,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var utm_term	= _GET('utm_term', location.hash.indexOf('utm_term=') > 0 ? '&' + location.hash.substr(1) : null);
  if(utm_term){
    $.cookie("utm_term",utm_term,{expires: 30, path: '/',domain:COOKIESDIAMON});
  }
  var offer_type	= _GET('offer_type', location.hash.indexOf('offer_type=') > 0 ? '&' + location.hash.substr(1) : null);
  if(offer_type){
    if(offer_type=='null' || offer_type=='30days'){
	    $.cookie("offer_type",offer_type,{expires: 30, path: '/',domain:COOKIESDIAMON});
	}
	if(offer_type=='session'){
		$.ajax({
			type: "get",
			dataType: "json",
			url: "/fun/index.php?act=ajax_offer_type&offer_type=" + offer_type,
			async: false,
			success: function(res){
			}
		});
	}

  }
</script>



<script type="text/javascript">

    function loadScript(url,callBack) {
        var scrs = document.getElementsByTagName('script');
        var last = scrs[scrs.length - 1];
        var scr = document.createElement('script');
        scr.src = url;
        scr.async = true;
        last.parentNode.insertBefore(scr, last); 

        if (scr.readystate) {
            scr.onreadystatechange = function() {
                if (scr.readyState == 'loaded' || scr.readyState == 'complete') {    
                    callBack();
                    scr.onreadystatechange = null;   
                }
            }
        } else {
            scr.onload = function(e) {
                callBack();
            }
        }          
    }

    function googleTranslateElementInit() {
        new google.translate.TranslateElement({pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.SIMPLE}, 'google_translate_element');
    }

    $(function() {
        $('#js_topLanguageBox').on('click', function() {
            var _thatCon = $(this),
                _dataComplete = _thatCon.attr('data-load');

            if (_dataComplete == '0') {
                loadScript('//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit');
                setTimeout(function() {
                    _thatCon.attr('data-load','1');
                },1000);
            } 
        });
    });

    // $(window).on('load', function(){
    //     $LAB.script("//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit")
    // });
</script>

