	$(window).resize(function(){
		//carouselResize();
		var winW = $(window).width();
		if(winW >= 767){
			$('#mobile-nav a').removeClass('active');
			$('#main-nav').removeAttr('style');
			//show comments and set mobile comments trigger
			$('.comments-container').removeAttr('style');
			$('.mobile-comments').html('<span class="triangleDown"></span>Show Comments');
			show = 0;
		}
		if(winW <= 767){

			$('#top-nav .triangleUp').removeClass('triangleUp').addClass('triangleDown');
			$('#country-list').css({'display':'none'});
			countryOpen = 0;
		}
