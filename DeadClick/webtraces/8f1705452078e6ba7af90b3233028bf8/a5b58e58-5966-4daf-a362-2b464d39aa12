<script>
    // clone and prep menu for jquery mmenu plugin
    (function () {
        var pageNav = document.getElementById('page-nav').cloneNode(true),
            navList = pageNav.querySelector('ul'),
            pageContainer = document.getElementById('page-container'),
            trigger = document.createElement('a'),
			api, $current;

        pageNav.id = 'mobile-nav';
        pageNav.className = 'mobile-nav';

        trigger.id = 'mobile-nav-toggle';
        trigger.href = '#mobile-nav';
//        trigger.className = 'mobile-nav-toggle hamburger hamburger--squeeze';
//        trigger.innerHTML = '<div class="hamburger-box"><div class="hamburger-inner"></div></div>';
        trigger.className = 'mobile-nav-toggle';
        trigger.innerHTML = '<a class="hamburger-box btn btn-custom btn-sm">menu</a>';

        // fast iteration helper
        function forEach(collection, fn) {
            if (!collection) { return false; }
            var i = 0, limit = collection.length;
            for (; i < limit; i++) {
                fn(collection[i], i);
            }
        }

        // move extra items into main nav tree
        forEach(pageNav.querySelectorAll('.navbar-right a'), function (a) {
            var li = document.createElement('li');
            li.appendChild(a);
            navList.appendChild(li);
        });

        // remove unused elements
        forEach(pageNav.querySelectorAll('.navbar-right, .page-nav__toggle'), function (el) {
            el.parentNode.removeChild(el);
        });

        // dissolve interstitial divs
        forEach(pageNav.querySelectorAll('.page-nav__dropdown'), function (el) {
            var parent = el.parentNode;
            while (el.firstChild) {
                parent.insertBefore(el.firstChild, el);
            }
            parent.removeChild(el);
        });

        // remove classes that may interfere with plugin styles
        forEach(pageNav.querySelectorAll('.page-nav__drilldown-toggle'), function (el) {
            el.className = '';
        })

        // replace drilldown buttons with spans to accommodate plugin
        forEach(pageNav.querySelectorAll('button'), function (el) {
            var replacement = document.createElement('span');
            replacement.textContent = el.textContent;
            el.parentNode.insertBefore(replacement, el);
            el.parentNode.removeChild(el);
        });

        document.body.insertBefore(pageNav, document.body.firstChild);
        pageContainer.insertBefore(trigger, pageContainer.firstChild);
        $(pageNav).mmenu({
            /* options */
            'offCanvas': {
                'position': 'right'
            }
        }, {
            /* config */
            'offCanvas': {
                'pageSelector': '#page-container'
            }
        });

		api = $(pageNav).data('mmenu');
		$current = $(pageNav).find('.current').last();
		if ($current) {
            api.setSelected($current, true);
            api.openPanel($current.parents('.mm-panel').first());
        }
    })();
</script>
    <!-- Javascript -->
    <script type="text/javascript">

        $('#form-signup_v1').validate({
            submit: {
                settings: {
                    inputContainer: '.field'
                },
                callback: {
                    onBeforeSubmit: function (node) {

                        myBeforeSubmitFunction(':D', ':)', node);

                    },
                    onSubmit: function (node) {

                        console.log('#' + node.id + ' has a submit override.');

                        //node.submit();

                    }
                }
            },
            debug: true
        });

        function myBeforeSubmitFunction(a, b, node) {

            console.log(a, b);

            $(node).find('input:not([type="submit"]), select, textarea').attr('readonly', 'true');
            $(node).append('<div class="ui active loader"></div>');

        }

        $('#prefill-signup_v1').on('click', function () {

            var form = $(this).closest('form');

            form.find('#signup_v1-name').val('John Doe');
            form.find('#signup_v1-username').val('RocketJoe');
            form.find('#signup_v1-password').val('test123');
            form.find('#signup_v1-password-confirm').val('test123');
            form.find('#signup_v1-email').val('test@test.test');
            form.find('#signup_v1-email-confirm').val('test@test.test');

        });

    </script>    
