    <script type="text/javascript">

        function collapseMenu()
        {
            if($(window).scrollTop() > 210)
            {
                $("#header").addClass("menu-collapse");
            }
            else
            {
                $("#header").removeClass("menu-collapse");
            }
        }

        function setEqualHeight(selector) {
            var heights = new Array();

            $(selector).each(function() {
                $(this).css('min-height', '0');
                $(this).css('max-height', 'none');
                $(this).css('height', 'auto');
                heights.push($(this).height());
            });

            var max = Math.max.apply( Math, heights );
            
            $(selector).each(function() {
                $(this).css('height', max + 'px');
            }); 
        }

   	    $(document).ready(function(e) { 
        
            collapseMenu();	
			$('.search_div span.search_icon').click(function(e) {
    			$('.search_over_div').toggle("slide",{ direction: "right" });
    			$(this).css('visibility', 'hidden');
    		});
            var liLeng = $('#slider ul li img.hero_img').length;
            for (i=0; i<=liLeng; i++)
            {
                var imgsrc = $('#slider ul li:nth-child('+i+') img.hero_img').attr('src');
                $('#slider ul li:nth-child('+i+') img.hero_img').css('opacity','0');
                $('#slider ul li:nth-child('+i+')').css('background-image','url('+imgsrc+')');
                $('#slider ul li:nth-child('+i+')').css('background-position','center center');
                $('#slider ul li:nth-child('+i+')').css('background-size','cover');
            }
            var imgsrc = $('.agency_banner img').attr('src');
            $('.agency_banner img').css('opacity','0');
            $('.agency_banner').css('background-image','url('+imgsrc+')');
            $('.agency_banner').css('background-position','center center');
            $('.agency_banner').css('background-size','cover');

			var imgsrc = $('.map_img img').attr('src');
            $('.map_img img').css('opacity','0');
            $('.map_img').css('background-image','url('+imgsrc+')');
            $('.map_img').css('background-position','center center');
            $('.map_img').css('background-size','cover');

			var liLeng = $('.brand_dev ul li img').length;
			for (i=0; i<=liLeng; i++)
            {
                var imgsrc = $('.brand_dev ul li:nth-child('+i+') img').attr('src');
                $('.brand_dev ul li:nth-child('+i+') img').css('opacity','0');
                $('.brand_dev ul li:nth-child('+i+')').css('background-image','url('+imgsrc+')');
                $('.brand_dev ul li:nth-child('+i+')').css('background-position','center center');
                $('.brand_dev ul li:nth-child('+i+')').css('background-size','cover');
            }
				
            $('.sub-menu').parent().addClass('sub_menu_li');
            var pull = $('#pull');menu= $('#nav_1');menuHeight= menu.height();
            pull.on('click', function(e) {
                e.preventDefault();
                menu.slideToggle();
                pull.toggleClass('active');
            });
		
    		$('#slider').flexslider();

    		$('#testimonial_slide').flexslider();

    		$('.clent_testimonial_carousel').flexslider();
    		$(".talent_carousel").owlCarousel({
                items: 3,
                pagination: false,
                loop: true,
                nav: true,
                margin: 70,
                autoWidth: false,
                merge: true,
                responsive: {
                    1269: {},
                    1024: {},
                    767: {items: 3, margin: 20},
                    480: {items: 2},
                    320: {items: 1}
                }
            });
    		$(".stay_carousel").owlCarousel({
                items: 5,
                pagination: false,
                loop: true,
                nav: true,
                margin: 30,
                autoWidth: false,
                merge: true,
                responsive: {
                    1269: {},
                    1024: {},
                    767: {items: 3, margin: 10},
                    480: {items: 2, margin: 10},
                    320: {items: 1, margin: 10}
                }
            });
            // This is the slider visible on the portfolio and case study pages
        	var workPortfolioCarousel = $(".work-portfolio-carousel");
            workPortfolioCarousel.owlCarousel({
                items: 1,
                center: true,
                loop: true,
                autoWidth: false,
                merge: true,
                dots: true 
        	});
            $('.work-portfolio-carousel-left').on('click', function() {
                workPortfolioCarousel.trigger('prev.owl.carousel');
            });
            $('.work-portfolio-carousel-right').on('click', function() {
                workPortfolioCarousel.trigger('next.owl.carousel');
            });
        
        	$('#fullpage').smartscroll({
                section: '.section', 
                anchor: '.scroll', 
                fullscreen: false, 
                activelink: 'active', 
                nav: true, 
                controls: true, 
                totop: false, 
                speed: 1000, 
                totopspeed: 1000
            });
	
	
            $('input.radio_input').wrap('<span class="radio_span">');	
			$('input.radio_input:checked').parent().addClass("selected");
			$('input.radio_input[type="radio"]').click(function () {
    			$('input.radio_input:not(:checked)').parent().removeClass("selected");
    			$('input.radio_input:checked').parent().addClass("selected");	
    		});	
            if($('.quiz_fancy').length) {
                $('.quiz_fancy').fancybox({});
            }
        });
	

        $('#fullpage').smartscroll({
            section: '.section', 
            anchor: '.scroll', 
            fullscreen: false, 
            activelink: 'active', 
            nav: true, 
            controls: true, 
            totop: false, 
            speed: 1000, 
            totopspeed: 1000
        });
	
        $(window).scroll(function(){
            collapseMenu();
        });
        $(window).load(function(){
            $("#tab_1").hide();
            // Set the slides to equal height so we can vertically center them
            setEqualHeight('#testimonial_slide .slides li');
        });

        
        $(window).resize(function() {
            // Follow through equal heights on window resize
            setTimeout(function() {
                setEqualHeight('#testimonial_slide .slides li');
            }, 120);
            // Fix menu when going from mobile to desktop sizes
            if ($(window).width() >= 768) {
                $('#nav_1').css('display', '');
            }
        });
