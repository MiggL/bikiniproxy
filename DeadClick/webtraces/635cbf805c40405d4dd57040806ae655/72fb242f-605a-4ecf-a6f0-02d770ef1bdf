<script>
    (function() {
        var isHttpss = 'https:' == document.location.protocol ? true : false;
        var path = (isHttpss ? 'https://' : 'http://') + 'analytics.logsss.com';
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;

        ga.src = path + '/logsss20.min.js';

        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
</script>


