        function ShowDiv() {

            if (m == 1) {

                document.getElementById("divShow").className = "showDisplay"
                m = 0;
                document.getElementById('lblshowall').innerHTML = 'show less properties...';
                document.getElementById('imgshowall').src = '/images/minus.jpg';
