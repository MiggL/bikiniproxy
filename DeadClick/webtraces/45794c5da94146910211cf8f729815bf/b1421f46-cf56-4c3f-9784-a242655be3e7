    window.onload = function() {
        
        var tl = new TimelineMax({repeat:1, repeatDelay:2});
        var immeuble = document.getElementById("immeuble");
        var reflet = document.getElementById("reflet");
        var avion = document.getElementById("avion");
        var bagages = document.getElementById("bagages");
        var couple = document.getElementById("couple");
        var accroche1 = document.getElementById("accroche1");
        var accroche2 = document.getElementById("accroche2");
        var trajet = document.getElementById("trajet");
        var depart = document.getElementById("depart");
        var hop = document.getElementById("hop");
        var desti1 = document.getElementById("desti1");
        var desti2 = document.getElementById("desti2");
        var desti3 = document.getElementById("desti3");
        var prix = document.getElementById("prix");
        var cta = document.getElementById("cta");
        var ml = document.getElementById("ml");
        //var logo_baseline = document.getElementById("logo_baseline");

        tl.set(immeuble,{x:"0px", opacity:0});
        tl.set(reflet,{x:"0px", opacity:0});
        tl.set(bagages,{x:"0px", opacity:0});
        tl.set(couple,{x:"0px", opacity:0});
        tl.set(avion,{x:-53, y:32});
        tl.set(depart,{opacity:0});
        tl.set(hop,{opacity:0});
        tl.set(desti1,{opacity:0});
        tl.set(desti2,{opacity:0});
        tl.set(desti3,{opacity:0});
        tl.set(cta,{x:+55, y:7, scale:0.5, opacity:0},0);
        
        tl.to(cta,0.5, {opacity:1, scale:1.25, ease:Cubic.easeOut},0);
        tl.to(immeuble,6, {x:300, ease:Power0.easeNone},0);
        tl.to(reflet,6, {x:300, ease:Power0.easeNone},0);
        tl.to(bagages,6, {x:390, ease:Power0.easeNone},0);
        tl.to(couple,6, {x:450, ease:Power0.easeNone},0);
        tl.to(immeuble,0.5, {opacity:1, ease:Power0.easeNone},0);
        tl.to(reflet,0.5, {opacity:1, ease:Power0.easeNone},0);
        tl.to(bagages,0.5, {opacity:1, ease:Power0.easeNone},0);
        tl.to(couple,0.5, {opacity:1, ease:Power0.easeNone},0);
        tl.to(avion,3.5, {x:111, y:-50, ease:Power0.easeNone},0);
        tl.from(accroche2,1, {opacity:0, ease:Cubic.easeOut},1.5);
        //tl.set(depart,{opacity:1},6);
        //tl.set(hop,{opacity:1},6.8);
        tl.set(desti1,{opacity:1},2.5);
        tl.set(desti2,{opacity:1},2.9);
        tl.set(desti3,{opacity:1},3.3);
        tl.to(bagages,0.5, {opacity:0, ease:Power0.easeNone},2.5);
        tl.to(depart,0.5, {opacity:1, y:-10, ease:Cubic.easeOut},5.5);
        tl.to(hop,0.5, {opacity:1, y:-10, ease:Cubic.easeOut},5.5);
        tl.to(desti1,0.5, {y:-10, ease:Cubic.easeOut},5.5);
        tl.to(desti2,0.5, {y:-10, ease:Cubic.easeOut},5.5);
        tl.to(desti3,0.5, {y:-10, ease:Cubic.easeOut},5.5);
        tl.to(accroche1,0.5, {opacity:0, y:-20, ease:Cubic.easeOut},5.5);
        tl.to(accroche2,0.5, {opacity:0, y:-20, ease:Cubic.easeOut},5.5);
