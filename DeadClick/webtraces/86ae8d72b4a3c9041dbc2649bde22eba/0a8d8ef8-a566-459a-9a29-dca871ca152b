s.prototype.reset=function(){function N(P){function O(Q){return L[Q]||Q
}return P.replace(/[^\u0000-\u007E]/g,O)
}function M(T,S){if(i.trim(T.term)===""){return S
}if(S.children&&S.children.length>0){var O=i.extend(true,{},S);
for(var V=S.children.length-1;
V>=0;
V--){var U=S.children[V];
