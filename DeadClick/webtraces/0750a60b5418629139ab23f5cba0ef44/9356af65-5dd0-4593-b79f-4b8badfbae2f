	/* carousel swipe for mobile*/
	$('#slides .slide').on('swipeleft',function(){
		alert('swipeleft');
		var current = $('#slides .slide.current-slide');
		var currentTrigger = $('#triggers .current-trigger');
		var selectedItem = $(this).index();
		var slideWidth = current.outerWidth();
		var nextSlide = current.next('.slide').length ? current.next('.slide') : $('#slides .slide:last-child'); 
		var nextTrigger = currentTrigger.next('li').length ? currentTrigger.next('li') : $('#triggers li:last-child');
		current.animate({'margin-left':'-100%','width':slideWidth},function(){
				$(this).removeClass('current-slide').attr('style','');
			});
		nextSlide.css({'position':'absolute','top':0,'width':slideWidth,'display':'block','left':'100%'}).animate({'left':'0%'},function(){
			$(this).addClass('current-slide').attr('style','');
		});
		currentTrigger.removeClass('current-trigger');
		nextTrigger.addClass('current-trigger');
	});

	$('#slides .slide').on('swiperight',function(){
		var current = $('#slides .slide.current-slide');
		var currentTrigger = $('#triggers .current-trigger');
		var selectedItem = $(this).index();
		var slideWidth = current.outerWidth();
		var prevSlide = current.prev('.slide').length ? current.prev('.slide') : $('#slides .slide:last-child'); 
		var prevTrigger = currentTrigger.prev('li').length ? currentTrigger.prev('li') : $('#triggers li:last-child');
		current.animate({'margin-left':'100%','width':slideWidth},function(){
				$(this).removeClass('current-slide').attr('style','');
			});
		prevSlide.css({'position':'absolute','top':0,'width':slideWidth,'display':'block','left':'-100%'}).animate({'left':'0%'},function(){
			$(this).addClass('current-slide').attr('style','');
		});
		currentTrigger.removeClass('current-trigger');
		prevTrigger.addClass('current-trigger');
	});




	//uncomment for text bubble
	$('.quick-view').css({'display':'block','opacity':0})
	$('#triggers li').hover(function(){
		var $this = $(this).not('.current-trigger');
		var quickView = $this.find('.quick-view');
		var windowW = $(window).width();
		if(windowW >= 767)
			$this.addClass('overflow');//remove overflow hidden
			quickView.stop().animate({'opacity':1,'bottom':'100%'});
	},function(){
		var $this = $(this).not('.current-trigger');
		var quickView = $this.find('.quick-view');
		var windowW = $(window).width();
		if(windowW >= 767)
			// quickView.stop().animate({'opacity':0,'bottom':'0%'},function(){
			// 	$(this).parent().removeClass('overflow');
			// });
			$this.removeClass('overflow');//add overflow hidden class to only trigger pop-up when mouse is over the trigger <li>
			quickView.css({'opacity':0,'bottom':'0%'});
	});

	//move item up on hover
	// $('#triggers li').hover(function(){
	// 	var $this = $(this);
