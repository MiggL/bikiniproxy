define(["jquery","wcs.context"],function(d,a){var c=a.getCookie("cn3"),b=a.getCookie("kiosk_id");if(c==="1"){d(".clear-session-template-js").appendTo("#body-container");d(".clear-session-js").removeClass("hide");d(".clear-session-js a").on("click",function(f){f.preventDefault();a.removeCookies(".*");if(typeof b!=="undefined"){location.href=WCS_CONFIG.get("global::kioskClearSessionReturnURL")+"&kioskId="+b}else{if(d("body#checkout").size()>0){location.href=location.origin}else{location.reload()}}})}});