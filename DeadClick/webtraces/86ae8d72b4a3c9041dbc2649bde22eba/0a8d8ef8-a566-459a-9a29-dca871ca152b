}if(!s&&q&&!w){t="above"
}else{if(!q&&s&&w){t="below"
}}if(t=="above"||(w&&t!=="below")){p.top=k.top-j.top-v.height
}if(t!=null){this.$dropdown.removeClass("select2-dropdown--below select2-dropdown--above").addClass("select2-dropdown--"+t);
