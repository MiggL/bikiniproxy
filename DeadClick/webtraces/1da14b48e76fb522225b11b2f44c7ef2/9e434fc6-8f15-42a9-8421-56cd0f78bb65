var getQueryParam = function getQueryParam(paramKey) {
    if (typeof window !== 'undefined') {
        return (window.location.search.match(new RegExp('[?&]' + paramKey + '=([^&]+)')) || [null])[1];
    } else {
        return;
    }
};

// Removes invalid characters from query parameter
function sanitizeQueryParam(str) {
    if (str) {
        return str.replace(new RegExp(/[^-A-Za-z0-9+&@#/%?=~_|!:,.;\(\)]/, ''));
    } else {
        return;
    }
}
