jQuery(function($){$(document).on('click',"button[class*=single_add_to_cart_button]:not(disabled)",function(e){var btn=$(this);if(!$(btn).data('redirecting_link')){track_add_to_cart_for_button(btn);}});$(document).on('item-added-via-ajax','form',function(){track_add_to_cart_for_button($(this).find('.single_add_to_cart_button'));});$(document).on('click','.mini_cart_item a.remove, .cart_item a.remove',function(e){var btn=$(this);if(!$(btn).data('redirecting_link')){track_remove_from_cart_for_button(btn);}});$(document).on('found_variation','.variations_form',function(event,variation){var form=$(this);var btn=$(form).find('button[class*=single_add_to_cart_button]');var prod_data=$(btn).data('ga-prod-details');if(variation.original_variation_sku){prod_data.variant=variation.original_variation_sku;}
else{prod_data.variant=variation.sku;}
prod_data.price=variation.display_price;$(btn).data('ga-prod-details',prod_data);});});function track_add_to_cart_for_button(btn){if(typeof(ga)=='function'){try{ga("require","ec");}catch(ex){}
var prod=$(btn).data('ga-prod-details');if(prod){prod.quantity=jQuery(btn).parent().find("input[name=quantity]").val();ga("ec:addProduct",prod);ga("ec:setAction","add");ga("send","event",{eventCategory:'Enhanced-Ecommerce',eventAction:'click',eventLabel:'add_to_cart_click',nonInteraction:1,transport:'beacon'});}}}
function track_remove_from_cart_for_button(btn){if(typeof(ga)=='function'){try{ga("require","ec");}catch(ex){}
var prod=$(btn).data('ga-prod-details');if(prod){ga("ec:addProduct",prod);ga("ec:setAction","remove");ga("send","event",{eventCategory:'Enhanced-Ecommerce',eventAction:'click',eventLabel:'remove_from_cart_click',nonInteraction:1,transport:'beacon',});}}}
function proceedToUrl(btn){if(!$(btn).data('redirecting_link')){$(btn).data('redirecting_link',true);window.location=$(btn).attr('href');}}
function proceedWithFormSubmit(btn){if(!$(btn).data('redirecting_link')){$(btn).data('redirecting_link',true);$(btn).closest('form').submit();}}
function ue_prod_exists_in_JSON(t_url,t_json_name){if(t_json_name.hasOwnProperty(t_url)){return t_json_name[t_url];}}