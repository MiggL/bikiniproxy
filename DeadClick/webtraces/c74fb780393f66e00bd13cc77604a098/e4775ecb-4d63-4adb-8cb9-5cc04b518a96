  // debug.log("Condition xsell_adsl");
    audience = 'audience_xsell_adsl';
    flag_audience = true;
  } else if  ((arrayIndexOf(audiences_joined, 'SFR-SEGMENT-MULTI')!=-1 || audience_multi ==1)&& Flag_audience_multi ===true){
  //debug.log("Condition audience_multi");
    audience = 'audience_multi';
    flag_audience = true;
