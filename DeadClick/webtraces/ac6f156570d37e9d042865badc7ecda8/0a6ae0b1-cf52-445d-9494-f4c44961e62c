var IncodedAniMenu=function(a,b){return void 0==a||0==$(a).length?(alert("Please set correct menu element!"),!1):($iam=this,$iam.options=$.extend({theme:"lozenge",font:"",animation:"gravity",button_position:"left",opacity:!1,"class":"iamenu",button:".iamenu-btn",menu:"ul",submenu:".iamenu-sub",button_close:"iamenu-btn-close",menu_show:"iamenu-show",menu_hide:"iamenu-hide",submenu_show:"iamenu-sub-show",submenu_hide:"iamenu-sub-hide"},b),$iam.el=a,$iam.btn=a+" "+$iam.options.button,$iam.menu=a+" ul",$iam.submenu_holder=a+" "+$iam.options.submenu,$iam.submenu_btn=a+" "+$iam.options.submenu+" a",$iam.submenu=a+" "+$iam.options.submenu+" ul",$iam.themes={lozenge:"iamenu-lozenge",wood:"iamenu-wood",birds:"iamenu-birds",food:"iamenu-food",navyblue:"iamenu-navyblue",photography:"iamenu-photography",restaurant:"iamenu-restaurant",typography:"iamenu-typography",dark:"iamenu-dark",grey:"iamenu-grey",blue:"iamenu-blue",violet:"iamenu-violet",seeblue:"iamenu-seeblue",coal:"iamenu-coal",white:"iamenu-white",sand:"iamenu-sand"},$iam.fonts={"droid-serif":"iamenu-droid-serif","noticia-text":"iamenu-noticia-text",oxygen:"iamenu-oxygen",raleway:"iamenu-raleway",roboto:"iamenu-roboto","roboto-slab":"iamenu-roboto-slab","source-sans-pro":"iamenu-source-sans-pro","source-serif-pro":"iamenu-source-serif-pro"},$iam.animations={gravity:"iamenu-gravity","shift-right":"iamenu-shift-right","shift-left":"iamenu-shift-left",fade:"iamenu-fade"},$iam.options.font=""==$iam.options.font?"":$iam.fonts[$iam.options.font],$iam.opacity_class="",$iam.options.opacity&&($iam.opacity_class=" iamenu-opacity"),$($iam.el).addClass($iam.options["class"]+" "+$iam.themes[$iam.options.theme]+" "+$iam.options.font+" "+$iam.animations[$iam.options.animation]+$iam.opacity_class),$($iam.el).append('<a href="#" class="iamenu-btn"><span></span></a>'),""==$iam.options.button_position||"left"==$iam.options.button_position?$($iam.btn).addClass(""):"right"==$iam.options.button_position?$($iam.btn).addClass("iamenu-btn-right"):"bottom-left"==$iam.options.button_position?$($iam.btn).addClass("iamenu-btn-bottom-left"):"bottom-right"==$iam.options.button_position?$($iam.btn).addClass("iamenu-btn-bottom-right"):(void 0!=$iam.options.button_position.top&&$($iam.btn).css("top",$iam.options.button_position.top+"px"),void 0!=$iam.options.button_position.left&&$($iam.btn).css("left",$iam.options.button_position.left+"px")),$($iam.btn).click(function(a){a.preventDefault(),$($iam.menu).first().hasClass($iam.options.menu_show)?($($iam.menu).first().removeClass($iam.options.menu_show).addClass($iam.options.menu_hide),$($iam.btn).removeClass($iam.options.button_close)):($($iam.menu).first().removeClass($iam.options.menu_hide).addClass($iam.options.menu_show),$($iam.btn).addClass($iam.options.button_close))}),$($iam.submenu_btn).click(function(a){a.preventDefault(),$($iam.submenu).hasClass($iam.options.submenu_show)?$($iam.submenu).removeClass($iam.options.submenu_show).addClass($iam.options.submenu_hide):$($iam.submenu).removeClass($iam.options.submenu_hide).addClass($iam.options.submenu_show)}),$iam)};