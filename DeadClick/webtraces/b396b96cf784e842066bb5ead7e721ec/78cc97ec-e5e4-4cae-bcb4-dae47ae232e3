        "ecommerce_action" : "checkout",
        "event_name"       : "paypal"
    };
    utag_data.ecommerce_action = data.ecommerce_action;
 utag_data.event_name = data.event_name;
 utag.link(data);
});

//checkout_complete
jQuery(window).on("load",function () {
setTimeout(function () {
    if(jQuery(".confirmation .order-confirmation-details .orderdetails .order-number").length && utag_data != null) {
        var merchandise_total = 0;
        for(var price in utag_data.product_price){
            merchandise_total += Number(price);
        }

        var promo_code = "";
        if(utag_data.coupon_codes != null && utag_data.coupon_codes.length > 0){
            promo_code = utag_data.coupon_codes.join();
        }
        var data = {
                "category_id"              : "root",
                "country_code"             : utag_data.customer_country,
                "customer_email"           : utag_data.customer_email,
                "customer_id"              : utag_data.customer_id,
                "ecommerce_action"         : "checkout",
                "event_name"               : "checkout_complete",
                "order_currency_code"      : utag_data.order_currency,
                "order_discount_amount"    : utag_data.order_discount,
                "order_grand_total"        : utag_data.order_grand_total,
                "order_id"                 : utag_data.order_id,
                "order_merchandise_total"  : "" + merchandise_total,
                "order_payment_type"       : utag_data.order_payment_type,
                "order_promo_code"         : promo_code,
                "order_shipping_amount"    : utag_data.order_shipping_amount,
                "order_shipping_type"      : utag_data.order_shipping_type,
                "order_subtotal"           : utag_data.order_subtotal,
                "order_tax_amount"         : utag_data.order_tax_amount,
                "product_category"         : utag_data.product_category,
                "product_id"               : utag_data.product_id,
                "product_price"            : utag_data.product_price,
                "product_quantity"         : utag_data.product_quantity,
                "product_sku"              : utag_data.product_sku
        };
        utag_data.ecommerce_action = "checkout";
        utag_data.event_name = "checkout_complete";
        utag_data.category_id = "root";
        utag_data.order_merchandise_total = "" + merchandise_total;
        utag_data.order_promo_code = promo_code;

    }
}, 300); // trigger the utag.link after 300 miliseconds.

});

</script>



<script type="text/javascript">
_etmc.push(["setOrgId", "7210969"]);
_etmc.push(["trackPageView"]);
var exactSearchCookie = document.cookie.split(';');
for(var i=0; i < exactSearchCookie.length; ++i){
var cookieVal = exactSearchCookie[i].split("="),
c = cookieVal[0],
d = cookieVal[1],
dLen = d.length;
if(c.indexOf('exactSearch') != -1 && dLen != 0) {
_etmc.push(["setOrgId", "7210969"]);
_etmc.push(["trackPageView", {"search" : d}]);
document.cookie="exactSearch=; path=/";
}
}
</script>


