		<script type="text/javascript">
    function processdyelots() {

    	if(jQuery('#subprod_dyelotdropdown').val() == "anydyelot")
    		{
			    	// pick up quantity from quantity box
			    	var qty = jQuery('#qty').val();
		    		var qtyremaining = qty;
					var qtyadded = 0;
					var provisionalbestid = 0;
					var matchfound = false;

					// Sort by product ID so we are always taking older products first
					dyelotarray.sort(function(a,b) {
					    return parseInt(a[0]) - parseInt(b[0]);
					});
			    	
					for(var i = 0, len = dyelotarray.length; i < len; i++) {
						if(dyelotarray[i][1] > 0)
						{
							if(qty == dyelotarray[i][1])
							{
								addtobasketform(qty,dyelotarray[i][0]);
								i = len;
								matchfound = true;
							}
							else if(qty < dyelotarray[i][1] && provisionalbestid == 0)
							{
								provisionalbestid = dyelotarray[i][0];
							}
						}
					}
			
					if(matchfound || provisionalbestid != 0)
					{
						if(matchfound)
						{
							productAddToCartForm.submit();
							return false; // For IE8, to prevent multiple submits [Rees]
						}
						else
						{
							addtobasketform(qty,provisionalbestid);
							productAddToCartForm.submit();
							return false; // For IE8, to prevent multiple submits [Rees]
						}
			
						qtyremaining = 0;
					}
					else
					{
						for(var i = 0, len = dyelotarray.length; i < len; i++) {
			
							if(qtyremaining > 0)
							{
								if(qtyremaining > dyelotarray[i][1])
								{
									addtobasketform(dyelotarray[i][1],dyelotarray[i][0]);
									qtyremaining = qtyremaining - dyelotarray[i][1];
								}
								else
								{
									addtobasketform(qtyremaining,dyelotarray[i][0]);
									qtyremaining = 0;
								}
							}
							else
							{
								i = len;
							}
						}
					}
			
					if(qtyremaining > 0)
					{
						 window.alert("Insufficient stock of this item remaining. Sorry for the inconvenience.");
						 jQuery('.dyelotstockqty').remove();
					}
    			}
    	else
    	{
            var allowaddtobasket = false;
        	// Deal with products selected directly from the dropdown
            for(var j = 0, len = dyelotarray.length; j < len; j++) {
                if(dyelotarray[j][0] == jQuery('#subprod_dyelotdropdown').val()){
					if(jQuery('#qty').val()<= dyelotarray[j][1])
					{
						allowaddtobasket = true;
					}
                }

            }
            if(allowaddtobasket){
                addtobasketform(jQuery('#qty').val(),jQuery('#subprod_dyelotdropdown').val());
                productAddToCartForm.submit(this);
            }
            else{
				window.alert("Insufficient stock of this item remaining. Sorry for the inconvenience.");
			}


    		return false; // For IE8, to prevent multiple submits [Rees]
    	}

    	if(qtyremaining == 0)
    	{
    		//productAddToCartForm.submit(this);
    		//return false; // For IE8, to prevent multiple submits [Rees]
    	}
    };
	jQuery(document).ready(function() {
		jQuery("#product_addtocart_form").submit(function (event) {
			processdyelots();
			return false;
		});
	});
	</script>
			<script type="text/javascript">
			function addtobasketform(qtytoadd,productid) {
				// Add an input field for this product
				jQuery('<input>').attr({
					type: 'hidden',
					value: qtytoadd,
					name: 'super_group[' + productid + ']'
				}).appendTo('#product_addtocart_form');
			};
			function addoptiontobasketform(optiontypeid,optiontid) {
				// Add an input field for this product
				jQuery('<input>').attr({
					type: 'hidden',
					value: optiontypeid,
					name: 'options[' + optiontid + ']'
				}).appendTo('#product_addtocart_form');
			};
		</script>
