})
};
g.prototype.getHighlightedResults=function(){var j=this.$results.find(".select2-results__option--highlighted");
return j
};
g.prototype.destroy=function(){this.$results.remove()
};
g.prototype.ensureHighlightVisible=function(){var k=this.getHighlightedResults();
if(k.length===0){return
}var j=this.$results.find("[aria-selected]");
var m=j.index(k);
