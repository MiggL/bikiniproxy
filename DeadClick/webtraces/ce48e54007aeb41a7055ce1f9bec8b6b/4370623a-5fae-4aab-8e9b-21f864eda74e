<script type="text/javascript">
    var pathname = window.location.pathname;
	var item202checker = jQuery('ul.menu').children('li.item202').children('a').attr('href');
	
	
	if (item202checker == window.location.pathname) {
	

		jQuery('ul.menu').children('li.item202').children('a').css('font-weight', 'bold');
		var pathname = "";
	}

	if(pathname.match('who-we-are') != null)
		jQuery(".item160").addClass("active");
	else if(pathname.match('what-we-do') != null)
		jQuery(".item161").addClass("active");
	else if(pathname.match('donate-now') != null)
		jQuery(".item162").addClass("active");
	else if(pathname.match('get-help') != null)
		jQuery(".item163").addClass("active");
	else if(pathname.match('reports-stats') != null)
		jQuery(".item338").addClass("active");

</script>

<script type="text/javascript">
	var h1checker = jQuery("h1").html();
	jQuery("#ja-mainnav").children("ul").children("li").each(function(index) {
		if (jQuery(this).children("a").children("span").html() == jQuery("h1").html())
			jQuery(this).addClass("active");
	});
	
</script>

<script type="text/javascript">
	var whowearechecker = jQuery("h1.contentheading").html();
	var whowearechecker2 = jQuery(".item235 a span").html();
	if ( whowearechecker == whowearechecker2 ) {
		jQuery(".item235 a span").css("font-weight", "bold");
	}
</script>

<script type="text/javascript">
	var thispage = jQuery("h1:first").html();
	var thispagesimple = thispage.replace(/\s/g,'-').toLowerCase();
	jQuery("body").addClass(thispagesimple);
	
	jQuery("ul.menu").children("li").each(function() {
		if (jQuery(this).children("a").children("span").html() == thispage) jQuery(this).addClass("active");
	});
	
</script>

