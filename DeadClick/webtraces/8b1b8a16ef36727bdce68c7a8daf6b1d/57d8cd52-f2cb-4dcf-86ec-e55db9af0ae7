_satellite.pushAsyncScript(function(event, target, $variables){
  if (_satellite.getVar('platform') != "unknown") {
  console.log("platform: "+_satellite.getVar('platform'));
}

if (_satellite.getVar('platform') == "drupal") {
  var logos = document.getElementsByClassName("xrx_bnrv4_logo disable");
  for (var i = 0; i < logos.length; i++) {
		logos[i].classList.remove("disable");
  }
}

if (_satellite.getVar('platform') == "lamp" || _satellite.getVar('platform') == "drupal") {
		if (typeof xrx_vars['gtm-id'] !== "undefined") {
  		(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer',xrx_vars['gtm-id']);
			var gtmevent = {"event": "xrx_vars_loaded", "xrx_vars": xrx_vars };
			dataLayer.push(gtmevent);  
		}
}

if (false && _satellite.getVar('platform') == "support") {
		if (typeof digitalData.locale['gtm-id'] !== "undefined") {
  		(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer',digitalData.locale['gtm-id']);
		}
}

if (_satellite.getVar('platform') == "lamp" || _satellite.getVar('platform') == "drupal" || _satellite.getVar('platform') == "wordpress" || _satellite.getVar('platform') == "ipr") {
		// load munchkin tracker

		(function ()
		{
    	// Adding the script tag to the head as suggested before
    	var head = document.getElementsByTagName('head')[0];
    	var script = document.createElement('script');
    	script.type = 'text/javascript';
  		script.src = "https://www.xerox.com/assets/js/xrx/marketo/munchkin.min.js";
      // Fire the loading
   	 head.appendChild(script);
		}());
}
});
