_satellite.pushAsyncScript(function(event, target, $variables){
  /* New Relic test */
if (typeof newrelic == 'object') {
  newrelic.setCustomAttribute('conversion', 'true');
  newrelic.setCustomAttribute('orderNumber', TagManagerData.OrderNumber);
  newrelic.setCustomAttribute('locale', TagManagerData.Locale);
  newrelic.setCustomAttribute('brand', TagManagerData.SiteID);
  newrelic.setCustomAttribute('pageType', TagManagerData.PageType);
  newrelic.setCustomAttribute('isInternal', TagManagerData.isInternal);
  newrelic.setCustomAttribute('cartTotal', TagManagerData.CartMerchTotal);
  newrelic.setCustomAttribute('newRelicId', TagManagerData.NewRelicID);
  if(TagManagerData.productID){
  	newrelic.setCustomAttribute('productID', TagManagerData.productID);
  	newrelic.setCustomAttribute('productGender', TagManagerData.ProductECgender );
  	newrelic.setCustomAttribute('productName', TagManagerData.ProductName  );
  }
  if(TagManagerData.categoryID){
  	newrelic.setCustomAttribute('categoryID', TagManagerData.categoryID);
  }
  newrelic.setCustomAttribute('customerId', TagManagerData.CustomerID);
  newrelic.setCustomAttribute('softwareVersion ', TagManagerData.SoftwareVersion );
}
});
