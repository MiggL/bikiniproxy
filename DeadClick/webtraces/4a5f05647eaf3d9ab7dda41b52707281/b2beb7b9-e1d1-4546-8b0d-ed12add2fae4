<script type="text/javascript">
//<![CDATA[
jQuery(function(){
    openNewsletter();
    jQuery('#news_home_sub').on('click', function() {
        var formValidator = new Validation($('newsletterhome-validate-detail'));
        if(formValidator && formValidator.validate()){
            jQuery.ajax({
                url: "https://oxygencollections.com/newsletter/subscriber/new/",
                type: "POST",
                data: {
                    is_ajax : 1,
                    email   : jQuery('.input-box #newsletter').val()
                },
                dataType:'json',
                success: function(data) {
                    if(data['error']) {
                        jQuery('.nl-form-wrapper').html('<p class="result-msg">'+data['error']+'</p>');
                    } else if(data['success']) {
                        jQuery('.nl-form-wrapper').html('<p class="result-msg">'+data['success']+'</p>');
                        setTimeout(function(){
                          jQuery('.ui-dialog-titlebar-close').trigger('click');
                        }, 1500);
                    }
                }
            });
        }
    });
});

//]]>
</script>
