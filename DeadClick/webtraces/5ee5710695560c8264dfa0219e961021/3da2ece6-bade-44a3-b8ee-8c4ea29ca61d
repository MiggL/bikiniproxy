<!-- e business/templates/hearst/home/HNavigation.tpl -->        <script type="text/javascript">jQuery(function($){$(".hst-siteheader a, .topnav a, .sitenav a, .subnav li a").attr("target","_top");});</script>
        <script type="text/javascript">
            var siteHeader = {
                openMenu: function() {
                    // Don't open the menu for the third party header
                    var header = $('#menu-open').closest('header');
                    if (header.hasClass('noflyouts')) {
                        return;
                    }
                    // Analytics switch
                    var t = $("html").hasClass("menu-open") ? 1 : 0,
                        a = $("html").hasClass("menu-open") ? 'close' : 'open',
                        p = $(".menu-bar").offset().top;
                    try {
                        HDN.sendAnalyticsData(a + '|hamburger-menu|hamburger-menu|0');
                    } catch (e) {
                    }
                    if (0 !== p) {
                        if ($("html").hasClass("webkit")) {
                            $("body").animate({ scrollTop: p + 2 }, 500, function() {
                                var navPos = $(".menu-bar").offset().top;
                                $("html").toggleClass("menu-open");
                            });
                        } else {
                            $("html").animate({ scrollTop: p + 2 }, 500, function() {
                                $("html").toggleClass("menu-open");
                            });
                        }
                    } else {
                        $("html").toggleClass("menu-open");
                    }
                }
            };
        </script>
    </body>
</html>
