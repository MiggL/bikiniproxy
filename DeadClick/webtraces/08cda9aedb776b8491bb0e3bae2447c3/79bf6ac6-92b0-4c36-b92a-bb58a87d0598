		<script>

			$(document).ready(function()
			{

                $('.collapsibleListHeader, .collapsibleListHeaderLast').css('cursor', 'pointer');

                $('.collapsibleList').hide();

                $('div.legislationModule .collapsibleListHeader, div.legislationModule .collapsibleListHeaderLast').toggle(function()
                {
                    $(this).next('div.legislationModule .collapsibleList, div.legislationModule .collapsibleList').slideDown();
                    $(this).css("background-image", "url(Assets/Members/LegislationListHeaderBackgroundOpen.png)");
                },
                function()
                {
                    $(this).next('div.legislationModule .collapsibleList').slideUp();
                    $(this).css("background-image", "url(Assets/Members/LegislationListHeaderBackgroundClosed.png)");
                }); // end toggle

                $('div.committeeTestimony .collapsibleListHeader, div.committeeTestimony .collapsibleListHeaderLast').toggle(function()
                {
                    $(this).next('div.committeeTestimony .collapsibleList, div.committeeTestimony .collapsibleList').slideDown();
                    $(this).css("background-image", "url(../Assets/Members/LegislationListHeaderBackgroundOpen.png)");
                },
                function()
                {
                    $(this).next('div.committeeTestimony .collapsibleList').slideUp();
                    $(this).css("background-image", "url(../Assets/Members/LegislationListHeaderBackgroundClosed.png)");
                }); // end toggle


			}); // end ready

		</script>

		<script type="text/javascript">
			$(function(){
