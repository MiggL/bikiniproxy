function load_comment_form(){var form_wrap=$('#comments-form-wrap');if($(form_wrap).length>0){var pid=$(form_wrap).data('pid');jQuery.ajax({type:'POST',url:'/wp-admin/admin-ajax.php',data:{action:'lazy_load_comment_form',post_id:pid},beforeSend:function(){$(form_wrap).addClass('ajax-loading-wrap');},success:function(response){$(form_wrap).replaceWith(response);},complete:function(){$(form_wrap).removeClass('ajax-loading-wrap');}});}}
function get_recently_viewed_products(){var wrap=$('.recently-viewed-placeholder');if($(wrap).length>0){$(wrap).each(function(idx,item){var pid=$(item).data('product');jQuery.ajax({type:'GET',url:'/wp-admin/admin-ajax.php?action=lazy_load_recently_viewed&&pid='+ pid,success:function(response){$(item).html(response);}});});}}
function login_logout_toggle(){var logged_in=false;if($.cookie('cp_my_account_display')){$('body').addClass('is-logged-in');logged_in=true;}
$('body').trigger('login_set',[logged_in]);}
var fnbTimer;fnbTimer=false;function footer_bar_notice(msg,type){type=typeof type!=='undefined'?type:'notice';$('#footer-'+ type+'-bar .row > span').html(msg);$('#footer-'+ type+'-bar').addClass('show');if(fnbTimer){clearTimeout(fnbTimer);}
fnbTimer=setTimeout(function(){$('#footer-notice-bar').removeClass('show');$('#footer-message-bar').removeClass('show');},8000);}
$('html').click(function(){clearTimeout(fnbTimer);$('#footer-notice-bar').removeClass('show');$('#footer-message-bar').removeClass('show');});$('#footer-notice-bar, #footer-message-bar').click(function(event){event.stopPropagation();});function trigger_data_layer_event(event){try{dataLayer.push({'event':event});}catch(err){}}
function trigger_myaccount_data_layer_event(event,category){if(typeof category==='undefined')category='my-account-action';data={'my-account-action-name':event,'event':category};if(typeof ga_user_id!=='undefined'){data['ga-user-id']=ga_user_id;}
try{dataLayer.push(data);}catch(err){}}
function init_date_pickers(){if($('.ue-datepicker-front').length>0){$('.ue-datepicker-front').each(function(i,itm){var min_date=new Date();min_date.setDate(min_date.getDate()+1);var max_date=new Date();max_date.setDate(max_date.getDate()+93);$(itm).datepicker({showOn:"both",buttonImage:"/wp-content/themes/underwear-expert-flatsome-child/img/calendar.png",buttonImageOnly:true,dateFormat:"mm/dd/yy",buttonText:"Select date",minDate:min_date,maxDate:max_date});$(itm).datepicker('setDate',min_date);});}}