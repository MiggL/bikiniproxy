   <script src="/879366/Enabler_01_177.js"></script>
    <script>
	
		/*Dynamic Content Enable code for Profile: 1111095*/
		
		var animOn = 1;

			window.onload = function() {
			  if (Enabler.isInitialized()) {
				Enabler.setProfileId(1111095);
				enablerInitHandler();
			  } else {
				Enabler.addEventListener(studio.events.StudioEvent.INIT, enablerInitHandler);
			  }
			}
	
		// Dynamic Content variables and sample values
			function enablerInitHandler() {
				var devDynamicContent = {};

				     devDynamicContent.DCO_TUI_DCO_Prospection= [{}];
					devDynamicContent.DCO_TUI_DCO_Prospection[0]._id = 0;
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Type_de_campagnes = "Trading ville d\u00E9part+ destination";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Logo = "PDI";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Texte_principal = "Envolez-vous de Nantes pour Marrakech";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Texte_secondaire = "0";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].BG_300x600 = {};
					devDynamicContent.DCO_TUI_DCO_Prospection[0].BG_300x600.Url = "http://etudes.netbooster.com/tui/BG/300x600/300x600-marrakech.jpg";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Affiche_prix_barré = 0;
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Mentions_hors_flux = "A partir de";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Prix_barré_hors_flux = 0;
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Meilleur_Prix_hors_flux = "";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].Devise_hors_flux = "0";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].recup_flux_depart = "500;500";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].recup_flux_destination = "1000000";
					devDynamicContent.DCO_TUI_DCO_Prospection[0].URL = {};
					devDynamicContent.DCO_TUI_DCO_Prospection[0].URL.Url = "https://www.tui.fr/voyages-maroc/marrakech/?departurecitycode=NTE";
					Enabler.setDevDynamicContent(devDynamicContent);

				var test = dynamicContent.DCO_TUI_DCO_Prospection[0].ID;
				var titre = dynamicContent.DCO_TUI_DCO_Prospection[0].Texte_principal;
				var Titre_bis = dynamicContent.DCO_TUI_DCO_Prospection[0].Texte_secondaire;
				var bg = dynamicContent.DCO_TUI_DCO_Prospection[0].BG_300x600.Url;
				var thePrice = dynamicContent.DCO_TUI_DCO_Prospection[0].recup_flux_depart;
				var SplitePrice = thePrice.split(';'); 
				clickTag = dynamicContent.DCO_TUI_DCO_Prospection[0].URL.Url;
				var mentionBannière = dynamicContent.DCO_TUI_DCO_Prospection[0].Mentions_hors_flux;
				var extension = bg.split('.');
				
				if(extension[(extension.length)-1]=='gif'){
					animOn = 0;
				}
				var lelogo = dynamicContent.DCO_TUI_DCO_Prospection[0].Logo;
				if(lelogo == "CM"){
					$("#logoPartner").html('<img src="logo_club.png">');
				}
				else if(lelogo == "NF"){
					$("#logoPartner").html('<img src="logo_NF.svg">');
				}else if(lelogo == "PDI"){
					$("#logoPartner").html('<img src="logo_PDI.png">');
				}		
					if(dynamicContent.DCO_TUI_DCO_Prospection[0].Affiche_prix_barré== 1 ){
					var leprixbarre = "au lieu de <span>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Prix_barré_hors_flux+"<sup>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Devise_hors_flux+"</sup></span>";
				}
				else{
					if(dynamicContent.DCO_TUI_DCO_Prospection[0].Meilleur_Prix_hors_flux != ""){
						if(dynamicContent.DCO_TUI_DCO_Prospection[0].Prix_barré_hors_flux == dynamicContent.DCO_TUI_DCO_Prospection[0].Meilleur_Prix_hors_flux){
							var leprixbarre = "au lieu de <span>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Prix_barré_hors_flux+"<sup>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Devise_hors_flux+"</sup></span>";
						}
					}else{
						if(SplitePrice[0]!=SplitePrice[1]){
							var leprixbarre = "au lieu de <span>"+SplitePrice[0]+"<sup>€</sup></span>";
						}
					
					}
					
				}
				if(dynamicContent.DCO_TUI_DCO_Prospection[0].Meilleur_Prix_hors_flux != ""){
					var leprixTui = ""+mentionBannière+" <span>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Meilleur_Prix_hors_flux+"<sup>"+dynamicContent.DCO_TUI_DCO_Prospection[0].Devise_hors_flux+"</sup></span>";
				}else{
					var leprixTui = ""+mentionBannière+" <span>"+SplitePrice[1]+"<sup>€</sup></span>";
				}
				$("#titrePrincipal").html(titre);
				if(Titre_bis != "0"){
					$("#titreSecondaire").html(Titre_bis);
				}
				$("#prixBarre").html(leprixbarre);
				$("#prixTui").html(leprixTui);
				$("#backgroundImage").html('<img src="'+bg+'">');
				anim();	
			}

	

		function bgExitHandler(e) {
		  Enabler.exitOverride('Background Exit', clickTag);
		}

		document.getElementById('globalAd').addEventListener('click', bgExitHandler, false);
	
	
        $(document).ready(function(){
         
        });		
        
        departAnim = 100;
        
        
        function anim(){
			if(dynamicContent.DCO_TUI_DCO_Prospection[0].Type_de_campagnes != "Opération"){
				if(animOn==1){
					setTimeout(function(){
						$("#backgroundImage").transition({scale:2, duration: 0}, 'linear');
					},0);	
					setTimeout(function(){
						$("#backgroundImage").transition({scale:1, duration: 6500}, 'linear');
					},departAnim);
				}
			}
			        
        }
		
		
    </script>
