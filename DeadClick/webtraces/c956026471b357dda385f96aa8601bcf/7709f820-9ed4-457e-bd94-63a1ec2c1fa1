<script>
  jQuery(function($) {
    ajaxCart.init({
      formSelector: '#AddToCartForm',
      cartContainer: '#CartContainer',
      addToCartSelector: '#AddToCart',
      enableQtySelectors: true,
      moneyFormat: "£{{amount}}"
  });
  });
</script>




