	<script type="text/javascript">
		function plusQty(qtyInput){
			if(jQuery('#'+qtyInput).attr('disabled')!='disabled'){
				qty = $(qtyInput).value;
				qty = parseInt(qty);
								qty++;
								$(qtyInput).value = qty;
			}
		}
		
		function minusQty(qtyInput){
			if(jQuery('#'+qtyInput).attr('disabled')!='disabled'){
				qty = $(qtyInput).value;
				qty = parseInt(qty);
									if(qty>1){
						qty--;
						$(qtyInput).value = qty;
					}
							}
		}
	</script>
