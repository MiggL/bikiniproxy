    } else {
      return '<div class="dfr-carousel-pagination" ng-class="{\'hidden\' : !$ctrl.items.length}">\n          <div class="inner layout-row layout-align-center-center" ng-if="$ctrl.items.length < 8 && $ctrl.itemsPerPage == 1">\n            <span category="Carousel"\n            action="Select"\n            label="{{ $index+1}}" ng-repeat="$$slide in $ctrl.items track by $index" class="pagination-full visible-above-sm-block">\n                <span class="breaker pagination-el" ng-if="::$index !== 0">/</span>\n                <span \n                  class="pagination-el"\n                  ng-class="{\'current\' : $ctrl.current == $index }" \n                  ng-click="$ctrl.goTo($index)" \n                  tabindex="-1"\n                  ng-bind="::($index + 1 < 10 ? \'0\' : \'\') + ($index + 1)"\n                  >\n                </span>\n              </span>\n          </div>\n          <span class="pagination-slim" ng-class="{\'visible-below-sm-initial\' : $ctrl.items.length < 8 && $ctrl.itemsPerPage == 1}">\n            <span class="current bold" ng-bind="$ctrl.suffixer($ctrl.current + 1)"></span>\n            <span ng-if="$ctrl.itemsPerPage > 1" class="current bold" \n            ng-bind="\' - \' + $ctrl.suffixer($ctrl.current + $ctrl.itemsPerPage)"></span>\n            <span class="breaker">/</span>\n            <span class="total bold" ng-bind="$ctrl.suffixer($ctrl.items.length)"></span>\n          </span>\n        </div>';
    }
  }],
  controller: ['$scope', '$element', '$attrs', '$dfrUtil', '$timeout', '$rootScope', '$transclude', function controller($scope, $element, $attrs, $dfrUtil, $timeout, $rootScope, $transclude) {
    'ngInject';

    var $ctrl = this;
    var thumbnailWidth = void 0,
        thumbnails = void 0;

    this.$onInit = function () {
      $scope.$ctrl = $ctrl.$dfrCarousel;
      $scope.$ctrl.breached = false;
      // $scope.$watch('$ctrl.current', refresh);
      $rootScope.$on('window:resize', $dfrUtil.debounce(function () {
        refresh($scope.$ctrl.current);
      }, 300));
    };

    this.$doCheck = $dfrUtil.debounce(function () {
      if ($attrs.type == 'thumbnails' || $attrs.type == 'thumbnails-condensed') {
        thumbnails = angular.element($element[0].querySelectorAll('.thumbnail'));
        if (!thumbnails.length) return;
        thumbnailWidth = thumbnails[0].clientWidth + parseInt(window.getComputedStyle(thumbnails[0]).marginRight) * 2;
        refresh($scope.$ctrl.current);
      }
    });

    function refresh(newCurrent) {
      if (angular.isUndefined(thumbnailWidth) || angular.isUndefined(newCurrent)) return;
      thumbnailWidth = thumbnails[0].clientWidth + parseInt(window.getComputedStyle(thumbnails[0]).marginRight) * 2;
      var parent = angular.element($element[0].querySelector('.thumbnail-inner'));
      var maxThumbailsBeforeButtons = $scope.$ctrl.bp.bpSm ? 4 : $scope.$ctrl.bp.bpMd ? 5 : 6;
      $scope.$ctrl.breached = ($scope.$ctrl.items.length * thumbnailWidth > thumbnailWidth * maxThumbailsBeforeButtons || $scope.$ctrl.bp.bpMd) && $scope.$ctrl.items.length > maxThumbailsBeforeButtons;
      if (!$scope.$ctrl.breached) return;
      var multiplier = 0;
      parent.parent().css('width', thumbnailWidth * maxThumbailsBeforeButtons + 'px');
      multiplier = newCurrent - newCurrent % (maxThumbailsBeforeButtons - 1);
      if (multiplier < 0) multiplier = 0;
      if (multiplier >= $scope.$ctrl.items.length - maxThumbailsBeforeButtons) multiplier = $scope.$ctrl.items.length - maxThumbailsBeforeButtons;
      var tx = -(multiplier * (100 / maxThumbailsBeforeButtons));
      if (Modernizr.csstransforms3d) {
        $scope.$ctrl.setVendor(parent[0], 'Transform', 'translate3d(' + tx + '%, 0, 0)');
      } else {
        $scope.$ctrl.setVendor(parent[0], 'Transform', 'translate( ' + tx + '%, 0)');
      }
    }
  }]
};

