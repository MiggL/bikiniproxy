  <script>
    $(document).ready(function() {
      ajaxCart.init({
        cartContainer: '.side-cart-container',
        cartCountSelector: '.cart-count',
        addToCartSelector: '[data-action=add-to-cart]',
        moneyFormat: "${{amount}}"
