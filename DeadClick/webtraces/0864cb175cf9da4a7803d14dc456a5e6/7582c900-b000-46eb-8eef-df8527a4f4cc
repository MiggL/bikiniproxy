  require: {
    '$dfrCarousel': '^dfrCarousel'
  },

  template: ['$element', '$attrs', function template($element, $attrs) {
    'ngInject';

    if ($attrs.type == 'thumbnails') {
      return '<div class="dfr-carousel-pagination thumbnails ' + $attrs.type + '" layout="row" layout-align="center start"  ng-class="{\'items-breached\' : $ctrl.breached}">\n          <span class="pagination-control prev" ng-click="$ctrl.prev();" tabindex="-1" layout="row" layout-align="center center"\n          category="Carousel"\n          action="Button click"\n          label="{{::modelId +\'-\'+ \'Thumb Previous\'}}">\n            <dfr-svg icon="chevron-left" class="stroked" size="14"></dfr-svg>\n          </span>\n          <div class="thumbnail-wrapper flex-noshrink flex-nogrow">\n            <div class="thumbnail-inner" layout="row" layout-align="start center">\n              <span ng-repeat="$$slide in $ctrl.thumbs track by $index" >\n                <span \n                  category="Carousel"\n                  action="Button click"\n                  label="{{::modelId + \'-SelectThumb-\' + $index}}"\n                  tabindex="-1"\n                  ng-click="$ctrl.goTo($index)" \n                  ng-class="{\'current\' : $ctrl.current == $index, \'is-first\' : $first  }" \n                  class="thumbnail flex-noshrink" dfr-bg-img="{{$$slide}}">\n                </span>\n              </span>\n            </div>\n          </div>\n          <span class="pagination-control next" ng-click="$ctrl.next();" tabindex="-1" layout="row" layout-align="center center" category="Carousel"\n                  action="Button click"\n                  label="{{::modelId +\'-\'+ \'Thumb Next\'}}">\n            <dfr-svg icon="chevron-right"  class="stroked" size="14"></dfr-svg>\n          </span>\n          \n        </div>';
