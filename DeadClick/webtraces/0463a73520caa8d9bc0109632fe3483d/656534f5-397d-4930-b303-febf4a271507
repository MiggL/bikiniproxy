                                (function (lib, img, cjs, ss) {

var p; // shortcut to reference prototypes

// library properties
lib.properties = {
    width: 970,
    height: 250,
    fps: 24,
    color: '#FFFFFF'};

// symbols:



(lib.bg = function() {
	this.initialize(img.bg);
}).prototype = p = new cjs.Bitmap();
p.nominalBounds = new cjs.Rectangle(0,0,970,250);





(lib.white_bg = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// Layer 1
	this.shape = new cjs.Shape();
	this.shape.graphics.f("#FFFFFF").s().p("AyRQPMAAAggeMAkjAAAMAAAAgeg");
	this.shape.setTransform(117,104);

	this.timeline.addTween(cjs.Tween.get(this.shape).wait(1));

}).prototype = p = new cjs.MovieClip();
p.nominalBounds = new cjs.Rectangle(0,0,234,208);


