<script type="text/javascript">
			var artile_img = document.getElementById("article_img");
			var temp_artile_img = document.getElementById("temp_article_img");

			var widthWindow= (window.innerWidth > 0) ? window.innerWidth : screen.width;
			if ((widthWindow >= 320) && (widthWindow <= 600)) {
			    artile_img.src =  temp_artile_img.value.replace(/1024\./g, "600.");
