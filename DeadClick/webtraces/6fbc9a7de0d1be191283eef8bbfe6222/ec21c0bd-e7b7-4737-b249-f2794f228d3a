				closeBtn: false,
								 width:250,				 autoHeight:true,							});
			if (lineven_warningmessage_read_cookie("warning_message_accepted") == null || lineven_warningmessage_read_cookie("warning_message_accepted") == 0) 
			{
				warningmessage_openpopupmessage();
			}
		});
		function warningmessage_openpopupmessage() 
		{
			$('.warningmessage_message_open').trigger('click');
		}
		function warningmessage_closepopupmessage(popup_type)
		{
			var close = true;
			
						
			if (close)
			{
				document.cookie = 'warning_message_accepted=1; path=/';
				$.fancybox.close();
	    	}
		}
		function warningmessage_show_termsandconditions()
		{
			$('.warningmessage_termsandconditions').trigger('click');
		}
		function warningmessage_redirection()
		{
			document.cookie = 'warning_message_accepted=0; path=/';
			document.location="http://www.google.fr";
		}
		
		function lineven_warningmessage_read_cookie(name) 
		{
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) 
			{
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
					if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}
