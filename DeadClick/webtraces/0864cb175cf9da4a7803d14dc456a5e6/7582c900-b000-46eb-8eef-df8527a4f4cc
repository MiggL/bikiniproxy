/***/ (function(module, exports, __webpack_require__) {

"use strict";


__webpack_require__(/*! ./_dfr-carousel.scss */ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel.scss");

var dfrCarouselControlsDirective = __webpack_require__(/*! ./_dfr-carousel-controls.component.js */ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel-controls.component.js"); /* eslint-disable */


angular.module('dfr.feature.content.carousel-simple', []).component('dfrCarouselChildren', __webpack_require__(/*! ./_dfr-carousel-children.component.js */ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel-children.component.js")).component('dfrCarouselNext', dfrCarouselControlsDirective).component('dfrCarouselPrev', dfrCarouselControlsDirective).component('dfrCarouselPagination', __webpack_require__(/*! ./_dfr-carousel-pagination.component.js */ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel-pagination.component.js")).component('dfrCarousel', __webpack_require__(/*! ./_dfr-carousel.component.js */ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel.component.js"));

