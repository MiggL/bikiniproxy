var o=n.closest(".select2");
var p=j(".select2.select2-container--open");
p.each(function(){var s=j(this);
if(this==o[0]){return
}var r=s.data("element");
r.select2("close")
})
})
};
h.prototype._detachCloseHandler=function(k){j(document.body).off("mousedown.select2."+k.id)
};
h.prototype.position=function(k,n){var m=n.find(".selection");
m.append(k)
};
h.prototype.destroy=function(){this._detachCloseHandler(this.container)
};
h.prototype.update=function(k){throw new Error("The `update` method must be defined in child classes.")
};
return h
});
