	function showOverview(){
		$('#noticeoverview').show();
		$('#overviewId').addClass("selected");
		$('#noticedetailsId').removeClass("selected");
		$('#notes').hide();
	}
	function showDetails(){
		$('#noticeoverview').hide();
		$('#overviewId').removeClass("selected");
		$('#noticedetailsId').addClass("selected");
		$('#notes').show();
	}
	showOverview();

	function openExtWeb(webUrl) {
		var pattern = /^((http|https):\/\/)/;
			if(!pattern.test(webUrl)) {
			    webUrl = "http://" + webUrl;
			}
			var win=window.open(webUrl, '_blank');
  			win.focus();
	}

