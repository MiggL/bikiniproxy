



var metal,fire,TL;
    function init(){
        one = document.getElementById("slide1") ;
        two = document.getElementById("slide2") ;
       

        //
        TL = new TimelineMax({
			repeat:2,
            delay:3,
            yoyo:true,
            repeatDelay:3,
            onComplete:function(){
                console.log("OVER");
            }
        });
       
        
        
        TL.to(one,1,{autoAlpha:1,ease:Power1.easeInOut},"-=0")
        .to(two,1,{autoAlpha:1,ease:Power1.easeInOut},"-=0")
     
       
        
                
        
        


        
        ;
        
        

    }
    
    
    
    window.onload=function(){
        init();
}

