function getParameterByName(names){var results;for(name in names){if(name!="indexOf"){names[name]=names[name].replace(/[\[]/,"\\[").replace(/[\]]/,"\\]");var regex=new RegExp("[\\?&]"+names[name]+"=([^&#]*)"),results=regex.exec(location.search);}
if(results){res=decodeURIComponent(results[1].replace(/\+/g," "));var nomParam=names[name];if(nomParam=="affilie"){nomParam="af";}
if(info!=""){info+="&"+nomParam+"="+res;}else{info=nomParam+"="+res;}}}
return info;}
if(navigator.appVersion.indexOf("MSIE 7.")==-1){info=getParameterByName(['af','affilie','kenid']);}
if(info!=""){var prefixe=$("#prefixe").text();if(prefixe=='/fr/fr')
prefixe='';$.ajax({url:prefixe+"/ParamUrl.html?"+info,cache:false});}
