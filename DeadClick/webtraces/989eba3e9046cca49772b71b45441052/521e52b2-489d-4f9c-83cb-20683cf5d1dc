<script>
var $window = $(window),
$content = $("#contents-container"),
$headerin = $("#headerin"), 
$headerhome = $("#homeheader"), 
$headersub = $("#subpheader"), 
topContent = $content.offset().top;
 
var sticky = false;
 
$window.on("scroll", function () {
	if ($window.scrollTop() > topContent) {
		if ( sticky === false ){
			$headerin.fadeIn(0);
			$headerhome.fadeOut(0);
			$headersub.fadeOut(0);
			sticky = true;
		}
	} else {
		if ( sticky === true ){
			$headerin.fadeOut(0);
			$headerhome.fadeIn(0);
			$headersub.fadeIn(0);
			sticky = false;
		}
	}
});
$window.trigger("scroll");
</script>
 
 
 
 




