// This code keeps slideshow slides heights responsive.
  function getActiveSlideHeight() {
    var $slide = jQuery(".views_slideshow_slide").filter(function(){
      if ( jQuery(this).css("display") == "block" )
      {
        return jQuery(this)
      }
    });
   
    return $slide.height();
  };
 
  function resizeSlideContainer() {
    var $slideContainer = jQuery(".views-slideshow-cycle-main-frame");
    var slideHeight = getActiveSlideHeight();
    var slideContainerHeight = $slideContainer.height();
   
    if (slideContainerHeight != slideHeight)
    {
      $slideContainer.height(slideHeight);
    }
  };

  /* Show sidebar menu as expanded for current trail, also added "slide" effect */
function init_sideBarSubmenu() {
  jQuery('#block-menu-block-1 .expanded').each(function( index ) {
    //console.log( index + ": " + jQuery( this ).text() );
    jQuery( this ).prepend('<div class="expander" onclick="toggleSubMenu(\'' + jQuery( this ).attr('class') + '\');">&nbsp;</div>');
  });
}

function toggleSubMenu(obj){
  obj = obj.replace(/ /g, '.');
  obj = '#block-menu-block-1 .' + obj + ' ul';
  displayed = jQuery( obj ).css('display');
  if (displayed=='none') {
    jQuery( obj ).slideDown(100);
  }
  else jQuery( obj ).slideUp(100);
}

jQuery( function() {
  init_sideBarSubmenu();
  jQuery('#header').data('size','large'); // set data flags for stickyHeader()
  stickyHeader();
});

function stickyHeader() {
  if(jQuery(document).scrollTop() > 140) {
    
    if(jQuery('#header').data('size') == 'large') {
        jQuery('#header').data('size','small');
        jQuery('#header').addClass('small');
     
        jQuery('#footer').addClass('small');
        jQuery('ul.sf-menu').css("top","30px");
        // only show if mobile nav is not displaying superfish-1-select = main nav, superfish-3-select = meetings
		/*
        if (jQuery('#superfish-1-select').is(":visible") == false && jQuery('#superfish-3-select').is(":visible") == false) {
          //jQuery('#header').fadeOut(0);
          //jQuery('#block-block-1').fadeOut(0);
          current_header_height = parseInt(jQuery('#header').css('height'));
          //console.log(current_header_height);
          if (current_header_height>=80 ) new_menu_position = 60;
          else new_menu_position = 30;
          jQuery('ul.sf-menu').fadeOut(0).css({
            "top": new_menu_position + 'px'
          }).fadeIn();
          jQuery('#block-block-1').fadeIn();
          jQuery('#header').fadeIn();
        }
        */
    }
    
    jQuery("#inntopia_book_your_trip").hide();/*remove this uncomment stuffs below for sticky inntopia*/
    jQuery("#inntopia_book_your_activity").hide();
    /* if we ever want in sticky view
    if (jQuery('#superfish-1-select').is(":visible") == true && jQuery('#inntopia_book_your_trip').is(":visible") == true) {
      jQuery("#inntopia_book_your_trip").css("top","-40px");
      jQuery('#superfish-1-select').css('margin-top','-20px !important');
    }*/
    jQuery("#innotopia-spacer-toggle").hide();
  }
  else {
    if(jQuery('#header').data('size') == 'small') {
        jQuery('#header').data('size','large');
        jQuery('#header').removeClass('small');
        jQuery('#footer').removeClass('small');
    }
    //console.log(100 - (jQuery(document).scrollTop() ));
    jQuery('ul.sf-menu').css({
      "top": (100 - jQuery(document).scrollTop() ) + 'px'
    });
    jQuery("#inntopia_book_your_trip").show();
     jQuery("#inntopia_book_your_activity").show();
    jQuery("#innotopia-spacer-toggle").show();
  }
  
} 

 /*add has class method to javascript*/
 Element.prototype.hasClass = function(className) {
     return this.className && new RegExp("(^|\\s)" + className + "(\\s|$)").test(this.className);
 };
 
