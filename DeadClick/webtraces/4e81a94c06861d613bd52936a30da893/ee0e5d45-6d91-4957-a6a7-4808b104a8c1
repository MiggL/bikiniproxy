  function fb_share() { 

    var ShareUrl;    // 공유 주소 
    var DocTitle;    // 공유 제목 
    var DocSummary;  // 공유 내용 간략 설명 
    var DocImage;    // 공유 썸네일 ( 80x80 사이즈 추천 ) 

    ShareUrl    = ''; 		//현재 페이지 또는 퍼갈 주소를 설정 
    DocTitle    = "Jangsaengpo Memory of Whale "; 
    DocSummary  = "Jangsaengpo Memory of Whale"; 
    DocImage    = ""  

    newwindow = window.open('http://www.facebook.com/sharer.php?app_id=1930931397137540'+'&s=100&p[url]='+encodeURIComponent(ShareUrl)+'&p[title]='+encodeURIComponent(DocTitle)+"&p[summary]="+encodeURIComponent(DocSummary)+"&p[images][0]="+encodeURIComponent(DocImage),'facebookpopup', 'toolbar=0, status=0, width=626, height=436'); 

    if (window.focus) {newwindow.focus();} 

  } 
</script>







