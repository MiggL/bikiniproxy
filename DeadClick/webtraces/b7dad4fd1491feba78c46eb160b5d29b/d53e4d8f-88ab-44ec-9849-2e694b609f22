    if(TRCImpl && TRCImpl.modes[data.mode_name] && !TRCImpl.modes[data.mode_name]['mode-is-responsive']) {
	for(var i = 0; i < data.boxes.length; i++) {
	    data.boxes[i].style.height = 'auto';
    	    if((i % (data.boxes.length / data.rows) == 0) && i != 0) {
		var clearDiv = document.createElement('div');
        	clearDiv.style.clear = 'both';
	        clearDiv.style.width = '100%';
	    	itemsContainer.insertBefore(clearDiv, data.boxes[i]);
	    }
	}
    }
/***** Vertical Carousel *****/
    try {
    	var viewport = TRC.Browser.ieUpto(8) ? document.documentElement.clientWidth : window.innerWidth;
	    if(viewport >= 769) {
		    /*** Dynamic Height ***/
		    var targetContainer = data.container._trc_container,
		    	widgetContainer = itemsContainer.parentNode,
		    	widgetContainerParent = widgetContainer.parentNode,
		    	containerHeight = targetContainer.offsetHeight,
		    	widgetContainerHeight = containerHeight/2.7,
		    	containerRight = -1,
		    	scrollButton = document.createElement('div'),
		    	isArrowChange = false
		    	passTheBottom = false,
		    	passTheTop = true;
		    //Created bottons up and down
			scrollButton.className ='trc_scroll_button_wrapper';
			scrollButton.innerHTML = '<div class=\'trc_button_up\'></div><div class=\'trc_button_down\'></div>';
			widgetContainer.parentNode.insertBefore(scrollButton, widgetContainer.nextSibling);
			widgetContainer.scrollTop = 0;
		    //Set wrapper height
			widgetContainer.style.height  = widgetContainerHeight + 'px';
			/*** Scroll  ***/
			widgetContainer.onscroll = function() {
				if (widgetContainer.scrollTop >= 0 && widgetContainer.scrollTop <= 1) {
					isArrowChange = false;
				}
				//visible height + pixel scrolled = total height
				else if(widgetContainer.offsetHeight + widgetContainer.scrollTop >= widgetContainer.scrollHeight) {
					isArrowChange = true;
					if (widgetContainerParent.className.indexOf(' slideUp') < 0) {
					 	widgetContainerParent.className += ' slideUp';
					}
				}
				else {
					widgetContainerParent.className = widgetContainerParent.className.replace(/ slideUp/gi , '' );
				}
				//flag to detect if arrow need to change to top or bottom
				if (isArrowChange) {
					scrollButton.getElementsByClassName('trc_button_down')[0].style.display = 'none';
					scrollButton.getElementsByClassName('trc_button_up')[0].style.display = 'block';
				}
				else {
					scrollButton.getElementsByClassName('trc_button_up')[0].style.display = 'none';
					scrollButton.getElementsByClassName('trc_button_down')[0].style.display = 'block';
				}
			};
			/*** Set Scroll After Arrow Visibility ***/
			var scrollHeight = containerHeight/2.6;
			function isScrollEvent(event) {
				var targetContainerRects = targetContainer.getClientRects()[0],
					windowHeight = Math.max(document.documentElement.clientHeight, window.innerHeight),
					constDist = (windowHeight)*0.05,
					targetContainerBottomToBottom =  targetContainerRects.bottom,
					targetContainerTopToBottom =  (targetContainerRects.top) - windowHeight,
					targetContainerTopToTop =  (targetContainerRects.top),
					startScroll = (targetContainerBottomToBottom+constDist) <= windowHeight;
				if (startScroll && (targetContainerBottomToBottom > 0)) {
					passTheBottom = true;
				}
				else {
					passTheBottom = false;
				}
				if ((targetContainerBottomToBottom+targetContainerTopToTop) < 0) {
					passTheBottom = false;
				}
				if ((targetContainerBottomToBottom + constDist) < 0) {
					passTheTop = false;
				}
				if (!passTheTop) {
					if (widgetContainerHeight > windowHeight) {
						if (startScroll && (targetContainerTopToTop+targetContainerBottomToBottom-constDist) > 0) {
							passTheTop = true;
						}
					}
					else {
						if (startScroll && (targetContainerTopToTop+((targetContainerBottomToBottom+constDist)/2.5)) > 0) {
							passTheTop = true;
						}
					}
				}
				if (passTheBottom && passTheTop) {
					widgetContainer.style.overflowY = 'scroll';
					containerRight = widgetContainer.offsetWidth - widgetContainer.clientWidth + 1;
					widgetContainer.style.marginRight  = '-' + containerRight + 'px';
				}
				else {
					widgetContainer.style.overflowY = 'hidden';
					widgetContainer.style.marginRight  = '-' + 0 + 'px';
				}
			}
			//find if browser supports passive event
			var supportsPassive = false;
			!function() {
			    try {
			        var opts = Object.defineProperty({}, 'passive', {
			            get: function () {
			                supportsPassive = true;
			            }
			        });
			        window.addEventListener('test', null, opts);
			    } catch (e) {
			    }
			}(); // jshint ignore:line
			function addEventListenerWithOptions(target, type, handler, options) {
			    var optionsOrCapture = options;
			    if (!supportsPassive) {
			        optionsOrCapture = options.capture;
			    }
			    target.addEventListener(type, handler, optionsOrCapture);
			}
			addEventListenerWithOptions(window, 'scroll', isScrollEvent, {passive:true});
			/*** Set Scroll On Button Click ***/
			function move_up(el) {
		    	el.scrollTop -= scrollHeight;
		  	}
			function move_down(el) {
				el.scrollTop += scrollHeight;
			}
			scrollButton.getElementsByClassName('trc_button_up')[0].onclick = function() {move_up(widgetContainer)};
			scrollButton.getElementsByClassName('trc_button_down')[0].onclick = function() {move_down(widgetContainer)};
		}
	}
	catch(e){
	}
