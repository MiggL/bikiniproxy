/// <reference path="..\..\SignalR.Client.JS\Scripts\jquery-1.6.4.js" />
/// <reference path="jquery.signalR.js" />
(function ($, window, undefined) {
    /// <param name="$" type="jQuery" />
    "use strict";

    if (typeof ($.signalR) !== "function") {
        throw new Error("SignalR: SignalR is not loaded. Please ensure jquery.signalR-x.js is referenced before ~/signalr/js.");
    }

    var signalR = $.signalR;

    function makeProxyCallback(hub, callback) {
        return function () {
            // Call the client hub method
            callback.apply(hub, $.makeArray(arguments));
        };
    }

    function registerHubProxies(instance, shouldSubscribe) {
        var key, hub, memberKey, memberValue, subscriptionMethod;

        for (key in instance) {
            if (instance.hasOwnProperty(key)) {
                hub = instance[key];

                if (!(hub.hubName)) {
                    // Not a client hub
                    continue;
                }

                if (shouldSubscribe) {
                    // We want to subscribe to the hub events
                    subscriptionMethod = hub.on;
                } else {
                    // We want to unsubscribe from the hub events
                    subscriptionMethod = hub.off;
                }

                // Loop through all members on the hub and find client hub functions to subscribe/unsubscribe
                for (memberKey in hub.client) {
                    if (hub.client.hasOwnProperty(memberKey)) {
                        memberValue = hub.client[memberKey];

                        if (!$.isFunction(memberValue)) {
                            // Not a client hub function
                            continue;
                        }

                        subscriptionMethod.call(hub, memberKey, makeProxyCallback(hub, memberValue));
                    }
                }
            }
        }
    }

    $.hubConnection.prototype.createHubProxies = function () {
        var proxies = {};
        this.starting(function () {
            // Register the hub proxies as subscribed
            // (instance, shouldSubscribe)
            registerHubProxies(proxies, true);

            this._registerSubscribedHubs();
        }).disconnected(function () {
            // Unsubscribe all hub proxies when we "disconnect".  This is to ensure that we do not re-add functional call backs.
            // (instance, shouldSubscribe)
            registerHubProxies(proxies, false);
        });

        proxies['publicHub'] = this.createHubProxy('publicHub'); 
        proxies['publicHub'].client = { };
        proxies['publicHub'].server = {
        };

        proxies['tokyProHub'] = this.createHubProxy('tokyProHub'); 
        proxies['tokyProHub'].client = { };
        proxies['tokyProHub'].server = {
            deleteCoachCategory: function (categoryId) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["DeleteCoachCategory"], $.makeArray(arguments)));
             },

            deleteCoachGroupCategory: function (groupCategoryId) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["DeleteCoachGroupCategory"], $.makeArray(arguments)));
             },

            joinCoachCategory: function (categories) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["JoinCoachCategory"], $.makeArray(arguments)));
             },

            joinCoachGroupCategory: function (groupCategories) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["JoinCoachGroupCategory"], $.makeArray(arguments)));
             },

            ping: function () {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["Ping"], $.makeArray(arguments)));
             },

            refreshEmbedJs: function (groupId) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["RefreshEmbedJs"], $.makeArray(arguments)));
             },

            registerFriends: function (friendsIds) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["RegisterFriends"], $.makeArray(arguments)));
             },

            sendAchievement: function (achievement) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendAchievement"], $.makeArray(arguments)));
             },

            sendBotChat: function (userId, groupId, chatRoomId, chatId, text, attachments) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendBotChat"], $.makeArray(arguments)));
             },

            sendBotChatTyping: function (userId, chatRoomId, isTyping) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendBotChatTyping"], $.makeArray(arguments)));
             },

            sendChat: function (chat) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendChat"], $.makeArray(arguments)));
             },

            sendEmail: function (email) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendEmail"], $.makeArray(arguments)));
             },

            sendNotification: function (notification) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendNotification"], $.makeArray(arguments)));
             },

            sendNotificationPostMatchesCustomSearch: function (desktopNotificationModel) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendNotificationPostMatchesCustomSearch"], $.makeArray(arguments)));
             },

            sendStatus: function (status) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendStatus"], $.makeArray(arguments)));
             },

            sendStatusComment: function (statusComment, peopleIds) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["SendStatusComment"], $.makeArray(arguments)));
             },

            statusFeedUpdated: function (userId, newStatusesCount) {
                return proxies['tokyProHub'].invoke.apply(proxies['tokyProHub'], $.merge(["StatusFeedUpdated"], $.makeArray(arguments)));
             }
        };

        return proxies;
    };

    signalR.hub = $.hubConnection("/signalr", { useDefaultPath: false });
    $.extend(signalR, signalR.hub.createHubProxies());

}(window.jQuery, window));