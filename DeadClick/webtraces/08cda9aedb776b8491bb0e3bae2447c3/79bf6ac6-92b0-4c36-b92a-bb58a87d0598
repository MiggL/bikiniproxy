		<script type='text/javascript'>

			$(document).ready(function() {
				$('#calendar').fullCalendar({

					editable:  false,
					height: 600,

					events: "session-dates-feed",

					header: {
					    left:   'prev',
					    center: 'title',
					    right:  'next'
					},

					eventClick: function(event) {
				        if (event.url) {
				            window.open(event.url);
				            return false;
				        }
				    }
				});
			});

		</script>

