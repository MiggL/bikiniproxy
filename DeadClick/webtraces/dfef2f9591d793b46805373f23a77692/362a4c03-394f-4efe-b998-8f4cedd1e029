            -moz-box-sizing: border-box;
            -webkit-box-sizing: border-box;
            box-sizing: border-box;
            display: block;
            width: 1260px;
            height: 600px;
            
        }
    </style>
    <script>
        AdobeEdge.loadComposition('160x600_production', 'EDGE-REMIND', {
            scaleToFit: "none",
            centerStage: "none",
