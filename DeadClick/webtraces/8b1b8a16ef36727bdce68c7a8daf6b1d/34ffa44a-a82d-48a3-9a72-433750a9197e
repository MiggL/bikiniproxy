<script type="text/javascript">
<!--

/**
 * Shows the quick pick overlib.
 * @param text The text to be displayed in the overlib.
 */
function doQuickPickOverlib(elem, text)
{
    if (null != text && text.length > 0)
    {
        var width = $("#jsQuickPick").offset().left + $("#jsQuickPick").width();
        var height = $(elem).offset().top - 5;
        return overlib("<div class='olibArrow'></div><div class='quickPickOverlib'><div class='h'></div><div class='m'><p>" +
                       text + "</p></div><div class='f'></div></div>",
                FULLHTML, FIXX, width, FIXY, height);
    }
    return false;
}

/**
 * Show all content records by showing all the category buckets and all
 * records within those category buckers.
 *
 * @param elem The quick pick element that was selected.
 */
function doQuickPickAll(elem)
{
    // Clear and hide the quick pick category title and show the real category title.
    $(".categoryTitle").show();
    $(".qpCategoryTitle").text("");
    $(".qpCategoryTitle").hide();

    // Show all categories and all records under them.
    $(".categoryBucket").show();
    $(".record").show();
    hilightSelectedQuickPick(elem);
    flashCategoryTitle();
}

/**
 * Show all content records matching the quick pick attribute name. All others are hidden.
 * First, we hide all category buckets, then we iterate through the category buckets and
 * get all the content records. If any content record has the attribute associated with it,
 * that content record is shown and all others are hidden. If any content record within a
 * category bucket is shown, we also make that category bucket shown as well, so we see the
 * category header and any category messages.
 *
 * @param elem The quick pick element that was selected.
 * @fmtHeaderTitle1 The localized title format for one content record
 * @fmtHeaderTitle2 The localized title format for more than one content record
 * @param attrName The name of the attribute.
 * @param attrClassName The attribute classname to match against.
 */
function doQuickPickAttribute(elem, fmtHeaderTitle1, fmtHeaderTitle2, attrName, attrClassName)
{
    // Hide all categories, then iterate through them and show only the content records
    // that match the specified attribute classname.
    $(".categoryBucket").hide();
    $(".categoryBucket").each(
            function()
            {
                $(this).find(".record").each(
                        function()
                        {
                            if ($(this).find(".attributes li").hasClass(attrClassName))
                            {
                                $(this).show();
                                // We add this class here so we can get a count of the number of rows
                                // that we are showing. For some reason, the :visible flag doesn't work.
                                $(this).addClass("showRecord");
                            }
                            else
                            {
                                $(this).hide();
                            }
                        }
                        );

                var count = $(this).find(".record.showRecord").length;
                if (count > 0)
                {
                    // Show this category
                    $(this).show();

                    // Create the quick pick category title.
                    var title = (count > 1 ? fmtHeaderTitle2 : fmtHeaderTitle1);
                    title = title.replace("{CATEGORY}", $(this).find(".categoryTitle").text());
                    title = title.replace("{ATTRIBUTE}", attrName);
                    title = title.replace("{COUNT}", count);

                    // Update the quick pick category title and hide the original category title
                    $(this).find(".qpCategoryTitle").text(title);
                    $(this).find(".qpCategoryTitle").show();
                    $(this).find(".categoryTitle").hide();
                }
                // Remove this class as was needed only to get the record count.
                $(this).find(".record").removeClass("showRecord");
            }
            );
    hilightSelectedQuickPick(elem);
    flashCategoryTitle();
}

/**
 * Shows all content records in the selected category bucket.
 *
 * @param elem The quick pick element that was selected.
 * @fmtHeaderTitle1 The localized title format for one content record
 * @fmtHeaderTitle2 The localized title format for more than one content record
 * @param catId The category ID of the category bucket to be shown.
 */
function doQuickPickCategory(elem, fmtHeaderTitle1, fmtHeaderTitle2, catId)
{
    // Hide all categories, then show the specified category and all records under it.
    $(".categoryBucket").hide();
    $(".categoryBucket.categoryBucketId-" + catId).show();
    $(".categoryBucket.categoryBucketId-" + catId + " .record").show();

    // Get the selected category name and a count of all visible records under it.
    var catName = $(".categoryBucket.categoryBucketId-" + catId + " .categoryTitle").text();
    var count = $(".categoryBucket.categoryBucketId-" + catId + " .record:visible").length;

    // Update the quick pick category title and hide the real category title.
    var title = (count > 1 ? fmtHeaderTitle2 : fmtHeaderTitle1);
    title = title.replace("{CATEGORY}", catName);
    title = title.replace("{COUNT}", count);
    $(".categoryBucket.categoryBucketId-" + catId + " .qpCategoryTitle").text(title);
    $(".categoryBucket.categoryBucketId-" + catId + " .qpCategoryTitle").show();
    $(".categoryBucket.categoryBucketId-" + catId + " .categoryTitle").hide();

    hilightSelectedQuickPick(elem);
    flashCategoryTitle();
}

/**
 * Highlight the selected quick pick.
 * Remove the "selected" class from all quick picks and add it to the one passed in.
 *
 * @param elem The quick pick element to be highlighted.
 */
function hilightSelectedQuickPick(elem)
{
    $(".quickPick").removeClass("selected");
    $(elem).addClass("selected");
}

/**
 * Assign a flash class to the category title. Set a timeout that gets invoked
 * to remove the flas class after a preset timeout.
 */
function flashCategoryTitle()
{
    $(".qpCategoryTitle").addClass("flash");
    setTimeout(unflashCategoryTitle, 250);
}

/**
 * Remove the flash class from the category title.
 */
function unflashCategoryTitle()
{
    $(".qpCategoryTitle").removeClass("flash");
}

/**
 * Hide the goBttn button if we are using JavaScript.
 */
function hideSelectorButton()
{
    $(".goBttn").hide();
}

/**
 * Enables the Show Descriptions toggle to add/remove the "hidden" class.
 * This is only done once at the beginning when the page is loaded.
 * Initially hide the descriptions.
 */
function enableToggleAllDescriptions()
{
    $("#showDescriptions").show();
    $("#showDescriptions").toggle(
            function()
            {
                $(".descriptionContainer").removeClass("hidden");
                $(".hiddenToggle").removeClass("collapsed");
                $(".indicators").removeClass("collapsed");
            },
            function()
            {
                $(".descriptionContainer").addClass("hidden");
                $(".hiddenToggle").addClass("collapsed");
                $(".indicators").addClass("collapsed");
            }
            );

    // Initially hide all descriptions. We hide them here so that if the user does not
    // have JavaScript enabled in their browser, they will be able to see the descriptions.
    $(".descriptionContainer").addClass("hidden");
    $(".hiddenToggle").addClass("collapsed");
    $(".indicators").addClass("collapsed");
}

/**
 * Toggle an individual description by adding/removing the "hidden" class
 * from just the description container for this record.
 * @param elem The element that was selected.
 */
function toggleDescription(elem)
{
    $(elem).siblings(".descriptionContainer").toggleClass("hidden");
    $(elem).children(".indicators").toggleClass("collapsed");
}

/**
 * Show the javascript enabled quick pick list by using javascript.
 * This way, if javascript is enabled by the browser, it will get enabled, but
 * if javascript is not enabled by the browser, it won't get enabled.
 */
function showJavaScriptQuickPicks()
{
    // Update the Recommended quick pick with the number of records that are actually
    // labeled as recommended. Note that this can be different than the number of
    // content records marked as recommended since we only label the first one in
    // each category.
    var recCount = $(".record .recommended").length;
    $("#qpRec").text($("#qpRec").text().replace("{0}", recCount));

    $("#jsQuickPick").show();
    $("#nonjsQuickPick").hide();
}

/**
 * Show the Javascript enabled videos by using javascript. If Javascript is enabled
 * on this browser, then the Javascript videos will be shown and the non-javascript
 * videos will be hidden.
 */
function showJavaScriptVideos()
{
    if (null != $(".nonjsVideo"))
    {
        $(".nonjsVideo").hide();
    }
    if (null != $(".nonjsVideoThumbnail"))
    {
        $(".nonjsVideoThumbnail").hide();
    }
    if (null != $(".jsVideo"))
    {
        $(".jsVideo").show();
    }
    if (null != $(".jsVideoThumbnail"))
    {
        $(".jsVideoThumbnail").show();
    }
}

/**
 * Initialization on page load.
 */
$(document).ready(function()
{
    showJavaScriptQuickPicks();
    showJavaScriptVideos();
    enableToggleAllDescriptions();
    hideSelectorButton();
});

//-->
</script>

