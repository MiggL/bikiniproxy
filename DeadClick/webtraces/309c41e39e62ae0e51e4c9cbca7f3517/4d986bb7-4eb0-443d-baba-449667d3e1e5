        $pc(document).ready(function()
        {
			function TurnOnUpdateDiv() {  
				var obj = document.getElementById("overlay");  
				if(getURLParam("atc") == 1) {                  
                    openDialog('<a href="viewcart.asp?cs=1" role="button" class="btn btn-default">View Cart</a><a role="button" class="btn btn-default" data-dismiss="modal">Browse Products</a>', '<div id="OverlayMsgDialog" title="Item(s) added to the cart" style="display:; text-align: left;"><div class="ui-main"><div style="margin: 5px 0 5px 0;">Your item(s) were added</div>Cart contains 0 Item(s) | Subtotal: $0.00</div></div>', 'Item(s) added to the cart', false);
				}  
			}		
			function addOnLoadEvent(func)
			{
				var oldonload = window.onload ;
				if (typeof window.onload  != 'function') {
				    window.onload  = func;
				}
				else
				{
					window.onload  = function() {
						oldonload();
						func();
					}
				}
			}
			addOnLoadEvent(TurnOnUpdateDiv);			
		});
	</script>  

