$("a").on("click",function(){

var sociaMed = $(this).attr('href');

if(sociaMed.match("facebook.com/TraneCommercial")){

					s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event17';
					s.eVar8 = 'facebook';
s.tl(this, 'o', 'facebook' );

}

else if(sociaMed.match("facebook.com")){
s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event16';
					s.eVar8 = 'facebook';
s.tl(this, 'o', 'facebook' );


}

else if(sociaMed.match("twitter.com/TraneCommercial")){

					s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event17';
					s.eVar8 = 'twitter';
s.tl(this, 'o', 'twitter' );

}

else if(sociaMed.match("twitter.com")){
s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event16';
					s.eVar8 = 'twitter';
s.tl(this, 'o', 'twitter' );


}

else if(sociaMed.match("linkedin.com/company")){

					s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event17';
					s.eVar8 = 'linkedin';
s.tl(this, 'o', 'linkedin' );

}

else if(sociaMed.match("linkedin.com")){
s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event16';
					s.eVar8 = 'linkedin';
s.tl(this, 'o', 'linkedin' );


}


else if(sociaMed.match("plus.google.com/u/")){

					s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event17';
					s.eVar8 = 'google plus';
s.tl(this, 'o', 'google plus' );

}

else if(sociaMed.match("plus.google.com")){
s.linkTrackVars = 'eVar8,events';
					s.linkTrackEvents = s.events = 'event16';
					s.eVar8 = 'google plus';
s.tl(this, 'o', 'google plus' );


}
})
