			function detectIE() {
				var match = navigator.userAgent.match(/(?:MSIE |Trident\/.*; rv:)(\d+)/);
				var matchResult = match ? parseInt(match[1]) : undefined;
				if (matchResult == undefined || matchResult < 11) {
					// IE 10 or older => return version number
					return matchResult;
				}
				return false;
			}

