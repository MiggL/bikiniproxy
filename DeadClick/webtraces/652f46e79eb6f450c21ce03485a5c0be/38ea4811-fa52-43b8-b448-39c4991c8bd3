$(window).load(function() {  
    var $container = $('#product-area, #special-listing');
    $container.masonry({
        columnWidth: 237,
  		gutter: 30,
  		itemSelector: '.grid-list, .specialsListBoxContents',
  		isFitWidth: true,
    });
});
