      <script type="text/javascript">
        (function($){
          var $mcMailFormFooter = $('#mc-embedded-subscribe-form');
          $('.newsletter__submit').on('click', function(e){
            e.preventDefault();
            registerSignup($mcMailFormFooter);
          });

          $('input').on('input', function(){
            $('.no-content').removeClass('error');
          });
          $('.CoverPop-close').click(function(){
            CoverPop.close();
          });
          function registerSignup($mcMailFormFooter) {
            $.ajax({
              type: $mcMailFormFooter.attr('method'),
              headers: {
                'X-Requested-With': 'XMLHttpRequest'
              },
              xhrFields: {
                withCredentials: true
              },
              crossDomain: true,
              url: $mcMailFormFooter.attr('action'),
              data: $mcMailFormFooter.serialize(),
              dataType    : 'json',
              error       : function(err) { console.log('connection error contact info@dearfrances.com') },
              success     : function(data) {
                $('.site-footer .input-group').html('Thanks! Shop now with code VIP10 for 10% off your first purchase!');
                $.cookie("exitIntent", "signedup", { expires: 365 , path: '/' });
              }
            });
          };
        })(jQuery);
      </script>
      
      
