(lib.bridgestone_FR_970x250_eurosport = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_0 = function() {
		/*
		
		this.exit.on("click", function () {
			window.open(window.clickTag, '_blank')
		});
		
		*/
		
		if(!this.loopsPlayed) {
		    this.loopsPlayed = 1;
		}
	}
	this.frame_363 = function() {
		if(this.loopsPlayed >= 2) {
		    this.stop();
		}else{
			this.loopsPlayed ++;	
		}
	}

	// actions tween:
	this.timeline.addTween(cjs.Tween.get(this).call(this.frame_0).wait(363).call(this.frame_363).wait(12));

	// keyline
	this.shape = new cjs.Shape();
	this.shape.graphics.f().s("#000000").ss(1,1,1).p("EhLxgThMCXjAAAMAAAAnDMiXjAAAg");
	this.shape.setTransform(485,125);

	this.timeline.addTween(cjs.Tween.get(this.shape).wait(375));

	// BSLogo
	this.instance = new lib.bridgestoneLogo();
	this.instance.parent = this;

	this.timeline.addTween(cjs.Tween.get(this.instance).wait(375));

	// fadeIn
	this.instance_1 = new lib.whiteFade();
