<script>
  if (!document.cookie.match(/logged_in=true/)) {
    ready(function(){
      $("#discussion_title").focus(function(event) {
        askToLogin(function(){
          $("#discussion_title").unbind("focus");
          $("#discussion_body").unbind("focus");
        });
      });
      $("#discussion_body").parent().find(".redactor_form--textarea").focus(function(event) {
        askToLogin(function(){
          $("#discussion_title").unbind("focus");
          $("#discussion_body").unbind("focus");
        });
      });
    })
    var projectPhotoAskAQuestion;
    projectPhotoAskAQuestion = function(photo_id){
      var sign_in_input = $("#sign_in_user input[name=photo_id]")[0]
      if (sign_in_input === undefined){
        sign_in_input = $("<input>").attr("type", "hidden").attr("name", "photo_id").val(photo_id);
        $('#sign_in_user').append($(sign_in_input));
      }else{
        $(sign_in_input).val(photo_id);
      }
      var sign_up_input = $("#sign_up_user input[name=photo_id]")[0]
      if (sign_up_input === undefined){
        sign_up_input = $("<input>").attr("type", "hidden").attr("name", "photo_id").val(photo_id);
        $('#sign_up_user').append($(sign_up_input));
      }else{
        $(sign_up_input).val(photo_id);
      }
      openLightbox("sign-in");
      $(".sign-in--page--facebook-connect--link, .sign-up--form--facebook-connect--link").attr("href", "/users/auth/facebook?photo_id=" + photo_id);
    }
  }
</script>


