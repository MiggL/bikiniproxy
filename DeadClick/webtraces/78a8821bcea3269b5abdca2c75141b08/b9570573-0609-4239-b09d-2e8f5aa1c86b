            <p class="overlay-or-text">Ou</p>
        <input type="button" class="btn button-action"  onclick="try { omniSiteCat.setOmnitureLoginClickEvent('Make An Offer', ECAT.makeOffer);} catch (e) {} Sothebys.util.registerTransactional('resource', '{{LotPath}}', '{{makeanofferUrl}}',true, false,'{{guaranteeLine}}');" value="Create an Account" />
        </div>
    </a>
  {{/if}}
  {{#if showMakeanofferButtonAuth}}
      {{#if isTransactional}}
          <a class="button-action inline-overlay click-show" href="javascript:void(0)">
            MAKE A POST SALE OFFER
            <div class="overlay-content register-login-overlay makeanoffer-overlay">
              <div class="offer-content">
