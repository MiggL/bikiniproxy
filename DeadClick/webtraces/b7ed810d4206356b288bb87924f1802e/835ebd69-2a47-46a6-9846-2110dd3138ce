<script type="text/javascript">
    $("img").each(function () {
        $(this).on('error', function () {
            var NoImageUrl = "/~/media/KCP/Products/Category/221X221KCNoImageAvailable.ashx";
                var image = $(this);
                var ThisHeight = $(image).css("height");
                var ThisWidth = $(image).css("width");
                if (image.context.naturalWidth == 0 || image.readyState == 'uninitialized') {
                    $(image).unbind("error").attr("src", NoImageUrl).css(
                    {
                        height: ThisHeight,
                        width: ThisWidth,
                    });
                };
            //console.log("error loading image");
        });
        //$(this).on('load', function () { console.log("image loaded correctly"); })
    });
</script>
