f.define("select2/data/base",["../utils"],function(h){function g(i,j){g.__super__.constructor.call(this)
}h.Extend(g,h.Observable);
g.prototype.current=function(i){throw new Error("The `current` method must be defined in child classes.")
};
g.prototype.query=function(i,j){throw new Error("The `query` method must be defined in child classes.")
};
g.prototype.bind=function(i,j){};
g.prototype.destroy=function(){};
g.prototype.generateResultId=function(i,j){var k=i.id+"-result-";
k+=h.generateChars(4);
if(j.id!=null){k+="-"+j.id.toString()
}else{k+="-"+h.generateChars(4)
}return k
};
return g
});
f.define("select2/data/select",["./base","../utils","jquery"],function(g,j,i){function h(k,m){this.$element=k;
this.options=m;
h.__super__.constructor.call(this)
}j.Extend(h,g);
h.prototype.current=function(n){var m=[];
var k=this;
this.$element.find(":selected").each(function(){var p=i(this);
var o=k.item(p);
m.push(o)
});
n(m)
};
h.prototype.select=function(m){var k=this;
m.selected=true;
if(i(m.element).is("option")){m.element.selected=true;
this.$element.trigger("change");
return
}if(this.$element.prop("multiple")){this.current(function(o){var q=[];
m=[m];
m.push.apply(m,o);
for(var p=0;
p<m.length;
p++){var r=m[p].id;
if(i.inArray(r,q)===-1){q.push(r)
}}k.$element.val(q);
k.$element.trigger("change")
})
}else{var n=m.id;
this.$element.val(n);
this.$element.trigger("change")
}};
h.prototype.unselect=function(m){var k=this;
if(!this.$element.prop("multiple")){return
}m.selected=false;
if(i(m.element).is("option")){m.element.selected=false;
this.$element.trigger("change");
return
}this.current(function(n){var p=[];
for(var o=0;
o<n.length;
o++){var q=n[o].id;
if(q!==m.id&&i.inArray(q,p)===-1){p.push(q)
}}k.$element.val(p);
k.$element.trigger("change")
})
};
h.prototype.bind=function(k,n){var m=this;
this.container=k;
k.on("select",function(o){m.select(o.data)
});
k.on("unselect",function(o){m.unselect(o.data)
})
};
h.prototype.destroy=function(){this.$element.find("*").each(function(){i.removeData(this,"data")
})
};
h.prototype.query=function(o,p){var n=[];
var m=this;
var k=this.$element.children();
k.each(function(){var s=i(this);
if(!s.is("option")&&!s.is("optgroup")){return
}var q=m.item(s);
var r=m.matches(o,q);
if(r!==null){n.push(r)
}});
p({results:n})
};
h.prototype.addOptions=function(k){j.appendMany(this.$element,k)
};
h.prototype.option=function(m){var k;
if(m.children){k=document.createElement("optgroup");
k.label=m.text
}else{k=document.createElement("option");
if(k.textContent!==undefined){k.textContent=m.text
}else{k.innerText=m.text
}}if(m.id){k.value=m.id
}if(m.disabled){k.disabled=true
}if(m.selected){k.selected=true
}if(m.title){k.title=m.title
}var n=i(k);
var o=this._normalizeItem(m);
o.element=k;
i.data(k,"data",o);
return n
};
h.prototype.item=function(p){var o={};
o=i.data(p[0],"data");
if(o!=null){return o
}if(p.is("option")){o={id:p.val(),text:p.text(),disabled:p.prop("disabled"),selected:p.prop("selected"),title:p.prop("title")}
}else{if(p.is("optgroup")){o={text:p.prop("label"),children:[],title:p.prop("title")};
var k=p.children("option");
var m=[];
for(var r=0;
r<k.length;
r++){var n=i(k[r]);
var q=this.item(n);
m.push(q)
}o.children=m
}}o=this._normalizeItem(o);
o.element=p[0];
i.data(p[0],"data",o);
return o
};
h.prototype._normalizeItem=function(k){if(!i.isPlainObject(k)){k={id:k,text:k}
}k=i.extend({},{text:""},k);
var m={selected:false,disabled:false};
if(k.id!=null){k.id=k.id.toString()
}if(k.text!=null){k.text=k.text.toString()
}if(k._resultId==null&&k.id&&this.container!=null){k._resultId=this.generateResultId(this.container,k)
}return i.extend({},m,k)
};
