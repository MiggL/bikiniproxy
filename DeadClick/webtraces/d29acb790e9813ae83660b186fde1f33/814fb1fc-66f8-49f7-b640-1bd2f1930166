jQuery(document).ready(function(){function e(){jQuery(".collection-wrap .accordion-section-title").removeClass("go-active"),jQuery(".collection-wrap .collection-items-box").slideUp(300).removeClass("go-open")}var o=jQuery(window).width();return 768>o?void jQuery(".accordion-section-title").click(function(o){var t=jQuery(this).attr("href");jQuery(o.target).is(".go-active")?e():(e(),jQuery(this).addClass("go-active"),jQuery(".collection-wrap "+t).slideDown(300).addClass("go-open")),o.preventDefault()}):!1});