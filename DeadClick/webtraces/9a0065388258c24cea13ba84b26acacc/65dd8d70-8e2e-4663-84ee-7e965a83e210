define(["jquery","foundation.abide"],function(b,a){(function(g,e,c,h){var d="validateCheckboxes";var f=function(j,i){this.element=j;this.options=i;this.metadata=g(this.element).data("validatecheckboxes-options")};f.prototype={init:function(){this.config=g.extend({},this.defaults,this.options,this.metadata);var i=this;g(c).foundation("abide");i.extendFoundation();return i},extendFoundation:function(){var i=this;Foundation.libs.abide.valid_checkbox=function(k,m){var l=false,j=g(k);if(j.prop("checked")===true){l=true;j.removeAttr("data-invalid").parent().removeClass("error")}else{j.attr("data-invalid","").parent().addClass("error")}return l};Foundation.libs.abide.check_validation_and_apply_styles=function(l){var o=l.length,k=[];for(var m=o-1;m>=0;m--){var j=l[m][0],q=l[m][2],r=j.value,s=j.type==="radio",n=j.type==="checkbox",p=(q)?(j.value.length>0):true;if(n&&q){k.push(this.valid_checkbox(j,q))}else{if(s&&q){k.push(this.valid_radio(j,q))}else{if(l[m][1].test(r)&&p||!q&&j.value.length<1){g(j).removeAttr("data-invalid").parent().removeClass("error");k.push(true)}else{g(j).attr("data-invalid","").parent().addClass("error");k.push(false)}}}}return k}}};f.defaults=f.prototype.defaults;g.fn.validateCheckboxes=function(i){return this.each(function(){var j=g(this).data(d);if(!j){j=new f(this,i).init();g(this).data(d,j)}})}})(jQuery,window,document)});