<script type="text/javascript">
  $(function(){

    /********************************************************
     ## Welcome Newsletter
     ********************************************************/

    if($.cookie("welcome_newsletter") == null) {
      $('#welcome-newsletter').toggleClass('newsletter-show');
    }
    $(document).on("click", "#welcome-newsletter .close", function() {
      $.cookie("welcome_newsletter", "no", { expires: 1 });
      $('#welcome-newsletter').hide();

    });

  });
</script>
<!--------------- popup Newsletter ---------------->
  
<script type="text/javascript">
  (function($){
    var $mcMailFormFooter2 = $('#mc-embedded-subscribe-form2');
    $('.newsletter__submit2').on('click', function(e){
      e.preventDefault();
      registerSignup2($mcMailFormFooter2);

    });
    function registerSignup2($mcMailFormFooter2) {
      $.ajax({
        type: $mcMailFormFooter2.attr('method'),
        headers: {
          'X-Requested-With': 'XMLHttpRequest'
        },
        xhrFields: {
          withCredentials: true
        },
        crossDomain: true,
        url: $mcMailFormFooter2.attr('action'),
        data: $mcMailFormFooter2.serialize(),
        dataType    : 'json',
        error       : function(err) {
          $('.col-3 .from-message').html('<p class="error-max">Thank you for subscribing. Use the code VIP10 at checkout to enjoy 10% off your first order</p>');
          $.cookie("welcome_newsletter", "no", { expires: 1, path: '/' });
        },
        success     : function(data) {
          $('.col-3').html('Thanks! Shop now with code VIP10 for 10% off your first purchase!');
          $.cookie("exitIntent", "signedup", { expires: 365 , path: '/' });
        }
      });
    };

  })(jQuery);
</script>
  
  <script>
jQuery(function() {
  jQuery('.swatch :radio').change(function() {
    var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
    var optionValue = jQuery(this).val();
    jQuery(this)
      .closest('form')
      .find('.single-option-selector')
      .eq(optionIndex)
      .val(optionValue)
      .trigger('change');
  });
});
</script>
  
  
