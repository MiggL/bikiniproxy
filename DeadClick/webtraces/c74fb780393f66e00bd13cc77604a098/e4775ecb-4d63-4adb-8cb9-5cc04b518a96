  // debug.log("Condition audience_rmprivi_mig");
    audience = 'audience_rmprivi_mig';
    flag_audience = true;
  } else if ((arrayIndexOf(audiences_joined, 'SFR-SEGMENT-CHANGE-FIBRE')!=-1 || audience_change_fibre==1 )&& Flag_audience_change_fibre===true){
