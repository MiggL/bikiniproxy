(window.onload = function() {
 
    var hostname = "emuseumkiosktest";
    var kioskClass = "kiosk";
 
    if (window.location.href.search(hostname) >= 0) {
        var body = document.getElementsByTagName("body")[0];
        document.body.className = document.body.className + " " + kioskClass;
    }
})();

(window.onload = function() {
 
    var hostname = "emuseumkiosk";
    var kioskClass = "kiosk";
 
    if (window.location.href.search(hostname) >= 0) {
        var body = document.getElementsByTagName("body")[0];
        document.body.className = document.body.className + " " + kioskClass;
    }
})();