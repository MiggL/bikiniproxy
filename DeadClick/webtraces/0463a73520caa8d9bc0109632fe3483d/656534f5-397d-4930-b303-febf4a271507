(lib.text = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// slash
	this.slash = new lib.slash();
	this.slash.parent = this;
	this.slash.setTransform(109,28,1,1,0,0,0,25,1);

	this.timeline.addTween(cjs.Tween.get(this.slash).wait(1));

	// sale_price
	this.sale_price = new cjs.Text("$138", "bold 12px 'Montserrat'", "#FF0000");
    this.sale_price.set({ designHeight: 18.4});
    this.sale_price.name = "sale_price";
	this.sale_price.textAlign = "center";
	this.sale_price.lineHeight = 14;
	this.sale_price.lineWidth = 212;
	this.sale_price.parent = this;
	this.sale_price.setTransform(108,2);

	this.timeline.addTween(cjs.Tween.get(this.sale_price).wait(1));

	// text
	this.title = new cjs.Text("Green Freak Subscription", "12px 'Montserrat'");
    this.title.set({ designHeight: 18.4});
    this.title.name = "title";
	this.title.textAlign = "center";
	this.title.lineHeight = 14;
	this.title.lineWidth = 212;
	this.title.parent = this;
	this.title.setTransform(108,39);

	this.price = new cjs.Text("$138", "bold 12px 'Montserrat'");
    this.price.set({ designHeight: 18.4});
    this.price.name = "price";
	this.price.textAlign = "center";
	this.price.lineHeight = 14;
	this.price.lineWidth = 212;
	this.price.parent = this;
	this.price.setTransform(108,20);

	this.timeline.addTween(cjs.Tween.get({}).to({state:[{t:this.price},{t:this.title}]}).wait(1));

}).prototype = p = new cjs.MovieClip();
