	#submenu .date {display:none}
	.mastheadnav li ul, .mainnav li ul, .subnav li ul {display:none !important}
	
	.col-3col, .right-3col {display:none !important}
	.mid-3col {margin:0!important;text-align:center;width:100%!important}
}
