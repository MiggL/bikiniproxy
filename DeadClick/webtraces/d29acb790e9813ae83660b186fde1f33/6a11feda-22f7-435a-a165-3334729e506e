$(document).ready(function() {
    $(window).load(function() {
        $(".drop").click(function(){
            $("#top-mobile").slideToggle();
        });

        $("#ico-search").click(function(){
            $(".TrimarMobileHeader form").toggleClass("show-form");
        });
        $(".option-content").hide();
        $(".arrow-up").hide();
        $(".option-heading").click(function(){
                $(this).next(".option-content").slideToggle(500);
                $(this).find(".arrow-up, .arrow-down").toggle();
        });

            $(".md_click").click(function(){
            $(".mview_manufacturers").slideToggle();
        });
            });
     
    });
    $(".TrimarkMainMenu ul li:nth-child(2)").css('display','none');
    $(".TrimarkMainMenu ul .submenu ul li:nth-child(2)").css("display", "block");
  //Map Display
    $(".show-map").click(function(){
      var add = $(this).text();
      var link = "location.php?q="+add;
      link = link.replace("#","");
       window.open("https://www.rwsmithco.com/"+link+"");
    });
</script>
