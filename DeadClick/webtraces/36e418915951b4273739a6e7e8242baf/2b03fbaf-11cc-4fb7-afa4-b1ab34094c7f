(function () {
    var queue = window['friendbuy'];
    if (queue.track_callback) {
        queue.track_callback({
            store_host: "site-c14db779-www.underwearexpert.com",
            embed_load_code: "20180215094334a2af854551"
        });
    }

    var uuid = '0d0157570d000057155a00010c1401020a09180a0e0d071e0d5b070a0e0a570b0f5d020a';
    if (queue.set_fbuy_cookie && uuid) {
        queue.set_fbuy_cookie(uuid);
    }

})();