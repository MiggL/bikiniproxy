<script type="text/javascript">
document.addEventListener("DOMContentLoaded", function(event) {
    var el = $('#vertical_menu [href="'+window.location.pathname+'"]');
    el.css({'color':'#D25829'});
    $('.m-industries').parent().toggle();
//    if (el.length) $('.vertical-menu-current').text(el.text());
    if (el.hasClass('m-industries')) {
        $('.p-industries').css({'color':'#D25829'});
    }
    $(window).on('click',function(e){
//
    });
    $('.scroller').click(function(){
        $("html, body").animate({scrollTop: $('#'+$(this).attr('rel')).offset().top +'px'}, 500);
        return false;
    });
});
</script>
