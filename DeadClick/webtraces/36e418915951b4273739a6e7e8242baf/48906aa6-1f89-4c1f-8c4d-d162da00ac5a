if(typeof ue_zendesk!=='undefined'&&typeof ue_zendesk.enabled!=='undefined'&&ue_zendesk.enabled==='1'){enableZenChat();}
$(function(){setZenIdentity();onPermamentSubCancellation();});function setZenIdentity(){if(typeof zE!=='undefined'){if(typeof user_data!=='undefined'){zE(function(){zE.identify(user_data);$zopim(function(){$zopim.livechat.addTags("name: "+ user_data.name,"email: "+ user_data.email);if($('.subscription').length>0){var cSub=$('.subscription').data('current-sub');$zopim.livechat.addTags("subscription: "+ cSub);}});});}}}
function enableZenChat(){zenChatEnabled=true;}
function embedZenChat(){if(typeof zenChatEnabled!=='undefined'&&typeof zenChatEmbeded==='undefined'){window.zEmbed||function(e,t){var n,o,d,i,s,a=[],r=document.createElement("iframe");window.zEmbed=function(){a.push(arguments)},window.zE=window.zE||window.zEmbed,r.src="javascript:false",r.title="",r.role="presentation",(r.frameElement||r).style.cssText="display: none",d=document.getElementsByTagName("script"),d=d[d.length-1],d.parentNode.insertBefore(r,d),i=r.contentWindow,s=i.document;try{o=s}catch(e){n=document.domain,r.src='javascript:var d=document.open();d.domain="'+n+'";void(0);',o=s}o.open()._l=function(){var e=this.createElement("script");n&&(this.domain=n),e.id="js-iframe-async",e.src="https://assets.zendesk.com/embeddable_framework/main.js",this.t=+new Date,this.zendeskHost="underwearexpert.zendesk.com",this.zEQueue=a,this.body.appendChild(e)},o.write('<body onload="document._l();">'),o.close()}();zenChatEmbeded=true;window.zESettings={webWidget:{helpCenter:{suppress:true},contactForm:{suppress:true},launcher:{chatLabel:{'*':'Questions?'}}}};}
setZenIdentity();}
function embedZendeskMyAccount(){window.zEmbed||function(e,t){var n,o,d,i,s,a=[],r=document.createElement("iframe");window.zEmbed=function(){a.push(arguments)},window.zE=window.zE||window.zEmbed,r.src="javascript:false",r.title="",r.role="presentation",(r.frameElement||r).style.cssText="display: none",d=document.getElementsByTagName("script"),d=d[d.length-1],d.parentNode.insertBefore(r,d),i=r.contentWindow,s=i.document;try{o=s}catch(e){n=document.domain,r.src='javascript:var d=document.open();d.domain="'+n+'";void(0);',o=s}o.open()._l=function(){var o=this.createElement("script");n&&(this.domain=n),o.id="js-iframe-async",o.src=e,this.t=+new Date,this.zendeskHost=t,this.zEQueue=a,this.body.appendChild(o)},o.write('<body onload="document._l();">'),o.close()}("https://assets.zendesk.com/embeddable_framework/main.js","underwearexpert.zendesk.com");zE(function(){zE.hide();});window.zESettings={webWidget:{contactOptions:{enabled:true,contactButton:{'*':'Contact Us'},chatLabelOnline:{'*':'Live Chat'},chatLabelOffline:{'*':'Chat is unavailable'}}}};initializeContactButton();setZenIdentity();}
function initializeContactButton(){if($('.contac-form-toggle').length>0&&typeof zE!=='undefined'){$(document).on('click','.contac-form-toggle',function(){zE.activate();return false;});}}
function onPermamentSubCancellation(){$(document).on('change','input[name=stop_time]',function(){var stop_months=$('[name="stop_time"]:checked').data('stop-months');if(stop_months==='permament-1'){var sub_id=$(this).closest('form').find('input[name="subscription_id"]').val();$zopim(function(){$zopim.livechat.addTags("cancellation_attempt");$zopim.livechat.setNotes("Cancelling Attempt: "+ sub_id);$zopim.livechat.sendVisitorPath({url:'https://www.underwearexpert.com/my-account/#cancelling'});});}});}
function zenChatUpdateUsersPath(path){if(typeof zE!=='undefined'){zE(function(){$zopim(function(){$zopim.livechat.sendVisitorPath({url:path});});});}}