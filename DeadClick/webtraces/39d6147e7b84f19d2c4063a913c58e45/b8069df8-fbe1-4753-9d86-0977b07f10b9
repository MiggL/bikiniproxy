//dropkick.jquery.js
jQuery.fn.dropkick = function () {
  var args = Array.prototype.slice.call( arguments );
  return $( this ).each(function() {
    if ( !args[0] || typeof args[0] === 'object' ) {
      new Dropkick( this, args[0] || {} );
    } else if ( typeof args[0] === 'string' ) {
      Dropkick.prototype[ args[0] ].apply( new Dropkick( this ), args.slice( 1 ) );
    }
  });
};
<!-- results tables -->
$( "#results-tables" ).dropkick({ mobile: true, change: function (value, label) {location.href = this.value; }});

<!-- style mailchimp signup drodpown -->
$( "#signup-language" ).dropkick({
  mobile: true,
  //change the form action on selection of language for mailchimp signup form
  change: function (value, label) {
    var selected = this.value;

    //var selected = $(this).children(":selected").text();
    switch (selected) {
    case "English":
      $("#mc-embedded-subscribe-form").attr('action', '//extremesailingseries.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=4f6ebe13b8');
      break;
    case "French":
      $("#mc-embedded-subscribe-form").attr('action', '//extremesailingseries.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=01ae0df104');
      break;
    case "German":
      $("#mc-embedded-subscribe-form").attr('action', '//octhirdpole.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=4cec59de7b');
      break;
    case "Italian":
      $("#mc-embedded-subscribe-form").attr('action', '//extremesailingseries.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=def68e2a46');
      break;
    case "Chinese":
      $("#mc-embedded-subscribe-form").attr('action', '//ocsport.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=a62499fa1b');
      break;
    case "Spanish":
      $("#mc-embedded-subscribe-form").attr('action', '//extremesailingseries.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=b30a25096a');
      break;
    default:
      $("#mc-embedded-subscribe-form").attr('action', '//extremesailingseries.us4.list-manage.com/subscribe/post?u=1cb43a27c782a9f30e7855e16&amp;id=4f6ebe13b8');
    }
  }
});


