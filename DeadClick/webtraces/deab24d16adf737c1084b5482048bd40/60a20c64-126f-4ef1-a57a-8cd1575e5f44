var Ejst={};Ejst.CustomWidget=CQ.Ext.extend(CQ.form.CompositeField,{hiddenField:null,itemName:null,itemUrl:null,constructor:function(a){a=a||{};var b={border:false,layout:"table",columns:4};a=CQ.Util.applyDefaults(a,b);Ejst.CustomWidget.superclass.constructor.call(this,a)},initComponent:function(){Ejst.CustomWidget.superclass.initComponent.call(this);this.hiddenField=new CQ.Ext.form.Hidden({name:this.name});this.add(this.hiddenField);this.add(new CQ.Ext.form.Label({text:"Title",style:{marginRight:"5px",marginLeft:"5px"}}));this.itemName=new CQ.Ext.form.TextField({cls:"ejst-customwidget-1",width:307,maxLength:150,listeners:{change:{scope:this,fn:this.updateHidden}}});this.add(this.itemName);this.itemUrl=new CQ.Ext.form.TextField({cls:"ejst-customwidget-2",width:490,listeners:{change:{scope:this,fn:this.updateHidden}}});this.add(new CQ.Ext.form.Label({text:"URL",style:{marginRight:"5px",marginLeft:"5px"}}));this.add(this.itemUrl)},processPath:function(a){console.log("CustomWidget#processPath",a);this.itemName.processPath(a)},processRecord:function(a,b){this.itemName.processRecord(a,b)},setValue:function(a){var b=a.split("|");this.itemName.setValue(b[0]);this.itemUrl.setValue(b[1]);this.hiddenField.setValue(a)},getValue:function(){return this.getRawValue()},getRawValue:function(){if(!this.itemName){return null}return this.itemName.getValue()+"|"+this.itemUrl.getValue()},updateHidden:function(){this.hiddenField.setValue(this.getValue())}});CQ.Ext.reg("videoPathMultiField",Ejst.CustomWidget);