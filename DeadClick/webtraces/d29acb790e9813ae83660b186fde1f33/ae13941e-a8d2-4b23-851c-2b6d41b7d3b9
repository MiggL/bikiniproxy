function FieldSuggest(e,t,s,a){base=this,base.FILE_XSLT_LAYOUT=t,base.FILE_XML_DATA=s,base._OBJ=document.getElementById(e),base._OBJ&&(base.createXmlHttpRequest=function(){var e=!1;if(window.XMLHttpRequest)e=new XMLHttpRequest,e.overrideMimeType&&e.overrideMimeType("text/xml");else if(window.ActiveXObject)try{e=new ActiveXObject("MSXML2.XMLHTTP.6.0")}catch(t){try{e=new ActiveXObject("Msxml2.XMLHTTP")}catch(t){try{e=new ActiveXObject("Microsoft.XMLHTTP")}catch(t){}}}return e},base.loadDocument=function(e,t){var s=base.createXmlHttpRequest();s.open("GET",e,!0),s.onreadystatechange=function(){4==s.readyState&&200==s.status?t(s):4==s.readyState},s.send(null)},base.parseXmlDocument=function(e,t){if(document.all)return t.transformNode(e);var s=new XSLTProcessor;s.importStylesheet(e);var a=s.transformToDocument(t),n=new XMLSerializer;return n.serializeToString(a)},base.getDocumentOffsetTop=function(e){return parseInt(e.offsetTop)+(e.offsetParent?base.getDocumentOffsetTop(e.offsetParent):0)},base.getDocumentOffsetLeft=function(e){return parseInt(e.offsetLeft)+(e.offsetParent?base.getDocumentOffsetLeft(e.offsetParent):0)},base.show=function(){base._OBJ_panel.style.visibility="visible"},base.hide=function(){base._OBJ_panel.style.visibility="hidden"},base.suggestList=function(){base.loadDocument(base.FILE_XML_DATA+"?"+base._OBJ.name+"="+base._OBJ.value+(a?"&ecosid="+a:""),function(e){base._OBJ_panel.innerHTML=base.parseXmlDocument(base._xsltSheet,e.responseXML),base._OBJ_panel.style.top=base.getDocumentOffsetTop(base._OBJ)+base._OBJ.offsetHeight+"px",base._OBJ_panel.style.left=base.getDocumentOffsetLeft(base._OBJ)+"px",base.show()})},base.loadDocument(base.FILE_XSLT_LAYOUT,function(e){base._xsltSheet=e.responseXML}),base._OBJ_panel=document.createElement("div"),base._OBJ_panel.style.visibility="hidden",base._OBJ_panel.style.position="absolute",base._OBJ_panel.style.overflow="hidden",base._OBJ_panel.style.zIndex="99999",base._OBJ_panel.style.top="0px",base._OBJ_panel.style.left="0px",base._OBJ.parentNode.appendChild(base._OBJ_panel),base._OBJ.onkeyup=function(){base._OBJ.value.length>0&&base.suggestList()},base._OBJ.onblur=function(){setTimeout(function(){base.hide()},500)},base._OBJ.onfocus=function(){base._OBJ.value.length>0&&base.suggestList()})}function FieldSuggest2(e,t,s,a){base2=this,base2.FILE_XSLT_LAYOUT=t,base2.FILE_XML_DATA=s,base2._OBJ=document.getElementById(e),base2._OBJ&&(base2.createXmlHttpRequest=function(){var e=!1;if(window.XMLHttpRequest)e=new XMLHttpRequest,e.overrideMimeType&&e.overrideMimeType("text/xml");else if(window.ActiveXObject)try{e=new ActiveXObject("MSXML2.XMLHTTP.6.0")}catch(t){try{e=new ActiveXObject("Msxml2.XMLHTTP")}catch(t){try{e=new ActiveXObject("Microsoft.XMLHTTP")}catch(t){}}}return e},base2.loadDocument=function(e,t){var s=base2.createXmlHttpRequest();s.open("GET",e,!0),s.onreadystatechange=function(){4==s.readyState&&200==s.status?t(s):4==s.readyState},s.send(null)},base2.parseXmlDocument=function(e,t){if(document.all)return t.transformNode(e);var s=new XSLTProcessor;s.importStylesheet(e);var a=s.transformToDocument(t),n=new XMLSerializer;return n.serializeToString(a)},base2.getDocumentOffsetTop=function(e){return parseInt(e.offsetTop)+(e.offsetParent?base2.getDocumentOffsetTop(e.offsetParent):0)},base2.getDocumentOffsetLeft=function(e){return parseInt(e.offsetLeft)+(e.offsetParent?base2.getDocumentOffsetLeft(e.offsetParent):0)},base2.show=function(){base2._OBJ_panel.style.visibility="visible"},base2.hide=function(){base2._OBJ_panel.style.visibility="hidden"},base2.suggestList=function(){base2.loadDocument(base2.FILE_XML_DATA+"?"+base2._OBJ.name+"="+base2._OBJ.value+(a?"&ecosid="+a:""),function(e){base2._OBJ_panel.innerHTML=base2.parseXmlDocument(base2._xsltSheet,e.responseXML),base2._OBJ_panel.style.top=base2.getDocumentOffsetTop(base2._OBJ)+base2._OBJ.offsetHeight+"px",base2._OBJ_panel.style.left=base2.getDocumentOffsetLeft(base2._OBJ)+"px",base2.show()})},base2.loadDocument(base2.FILE_XSLT_LAYOUT,function(e){base2._xsltSheet=e.responseXML}),base2._OBJ_panel=document.createElement("div"),base2._OBJ_panel.style.visibility="hidden",base2._OBJ_panel.style.position="absolute",base2._OBJ_panel.style.overflow="hidden",base2._OBJ_panel.style.zIndex="99999",base2._OBJ_panel.style.top="0px",base2._OBJ_panel.style.left="0px",base2._OBJ.parentNode.appendChild(base2._OBJ_panel),base2._OBJ.onkeyup=function(){base2._OBJ.value.length>0&&base2.suggestList()},base2._OBJ.onblur=function(){setTimeout(function(){base2.hide()},500)},base2._OBJ.onfocus=function(){base2._OBJ.value.length>0&&base2.suggestList()})}