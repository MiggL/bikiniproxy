function topBanner(){
	var windowHeight = $(window).height();		
	$('#topBanner,#homeBanner .dataBg ,#homeBanner1 .dataBg').css({height:windowHeight});
	$('#topBanner1 .dataBg').css({height:windowHeight - 68});
	$('.welcomeBlock').css({minHeight:windowHeight});
	/*var topHgt = $('#topBanner2').height();
	if(topHgt < windowHeight){
	$('#topBanner2, .patternDec').css({minHeight:windowHeight});
	$('.welcomeBlock').css({minHeight:windowHeight});
	} else { 
	$('#topBanner2, .patternDec').css({minHeight:topHgt});
	$('.welcomeBlock').css({minHeight:topHgt});
	}*/
}



function common(){
	var windowWidth = $(window).width();
	var windowHeight = $(window).height();	
	var contaierWidth = $('.container').innerWidth();
	var outContWidth = Math.round(windowWidth-contaierWidth)/2;
	var introDec = $('.introDesc').innerHeight();
	$('.introDesc').css({marginTop:-(introDec)/2});
	/*if($('#aboutBlk').innerHeight()<windowHeight-80) {
		$('#aboutBlk').css({minHeight:windowHeight-60});
	} else {
		$('#aboutBlk').css({minHeight:$('#aboutBlk').innerHeight()});
	}*/
	
	$('.menuWrap ul li figure img').each(function(index, element) {
        var imgHeight = $(this).height();
		$(this).parents('li').find('.recipeTxt').css({height:imgHeight});
    });
	
	/* recipe section home page */
	if(windowWidth>640){
		$('.menuWrap ul li').each(function(index, element) {
			var movList = $('.menuWrap ul li.movelist');
			if($(this).hasClass('movelist')){
				$(this).find('.recipeTxt').insertAfter($(this).find('figure'));
			} else {
				$(this).find('.recipeTxt').insertBefore($(this).find('figure'));
			}
    	});
	} else {
		$('.menuWrap ul li').each(function(index, element) {
			$(this).find('.recipeTxt').insertBefore($(this).find('figure'));
    	});
	}
	if(windowWidth<=640){
		$('.copyContent').insertAfter('.policy');
$('.blogBlk').each(function(index, element) {
            $(this).find('.date').insertAfter($(this).find('.blogDesc'));
        });
	}else{
		$('.policy').insertAfter('.copyContent');
$('.blogBlk').each(function(index, element) {
            $(this).find('.date').insertBefore($(this).find('.authorName'));
        });
	}
}
/* Height */
function ulHgt(){
var winHgt = $(window).height();
var landCont = $('.landingCont').innerHeight();
var botHgt = $('.moreLocat').innerHeight();
var ulLocat = $('.locatList').innerHeight();
var totHgt = winHgt - (landCont + botHgt + 66);
var countLi =$('.locatList li').size();
countLi = countLi * 45;
if(countLi > totHgt){
$('.locatList').height(totHgt);
} else {
$('.locatList').css({'height':'100% !important'});	
}
/*if(countLi < totHgt){
$('.moreLocat').css('visibility','hidden');
} else {
$('.moreLocat').css('visibility','visible');
}*/
}

if(!Modernizr.touch) { 
