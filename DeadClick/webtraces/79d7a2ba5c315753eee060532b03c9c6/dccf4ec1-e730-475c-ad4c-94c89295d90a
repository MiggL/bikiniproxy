$(".AdvancedListSettingsModule .Controls").hover(function() {
$(this).find("Presets").show();
},function() {

})
$(".AdvancedListSettingsModule .Attributes label").each(function() {
$(this).click(function() {
if ($(this).find(":checked").length > 0) {
$(this).addClass("Selected");
}
else {
$(this).removeClass("Selected");
}
});
if ($(this).find(":checked").length > 0) {
$(this).addClass("Selected");
}
});