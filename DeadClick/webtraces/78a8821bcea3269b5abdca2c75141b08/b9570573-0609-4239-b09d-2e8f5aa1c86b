<script id="registerToBidTemplate" type="text/x-handlebars-template">
    {{#if register}}
        
        
        
          <a class="register-to-bid  button-action  inline-overlay logged-out click-show" href="javascript:void(0)">Enregistrement pour enchérir
        
        <div class="overlay-content register-login-overlay unregistered-user-overlay">
          <h5>Vous devez d'abord vous inscrire ou vous connecter pour participer aux enchères.</h5>
          <input type="button" class="btn button-action" onclick="try { omniSiteCat.setOmnitureLoginClickEvent('Register To Bid', 'false');} catch (e) {} Sothebys.util.loginAndForward('resource', '/en/attend-auction.html/{{shortPath}}');" value="Log In" />
          <p class="overlay-or-text">Ou</p>
          <input type="button" class="btn button-action" onclick="try { omniSiteCat.setOmnitureLoginClickEvent('Register To Bid', 'false');} catch (e) {} Sothebys.util.registerTransactional('resource', '{{shortPath}}', '/en/attend-auction.html/{{shortPath}}', {{booleanToString canRegisterForTransactional}}, {{booleanToString isLessThan24Hours}}, '{{eventTitle}}');" value="Create an Account" />
        </div>
        </a>
        {{#if isTransactionalUser}}
          
          
            <!-- Send blocked Transacational people to the registerTransacational function -->
            {{#if canRegisterForTransactional}}
               <a x-cq-linkchecker="skip" class="register-to-bid button-action" href="/en/attend-auction.html/{{shortPath}}">Enregistrement pour enchérir</a>
            {{else}}
                <a x-cq-linkchecker="skip" class="register-to-bid button-action" href="#" onclick="Sothebys.util.blockNewTransacational('{{eventTitle}}');">Enregistrement pour enchérir</a>
            {{/if}}
          
        {{else}}
          
          
            <a x-cq-linkchecker="skip" class="register-to-bid button-action  click-show logged-in" href="javascript:void(0)" onclick="try { omniSiteCat.setOmnitureLoginClickEvent('Register To Bid', 'false');} catch (e) {} Sothebys.util.registerTransactional('resource', '{{shortPath}}', '/en/attend-auction.html/{{shortPath}}', {{booleanToString canRegisterForTransactional}}, {{booleanToString isLessThan24Hours}}, '{{eventTitle}}');">Enregistrement pour enchérir</a>
          
        {{/if}}
    {{/if}}
    {{#if registerWine}}

      <a x-cq-linkchecker="skip" class="register-to-bid button-action click-show" href="http://www.sothebyswinelive.com/main.cfm?P=WebcastRegistrationForm">Enregistrement pour enchérir</a>
    {{/if}}
    {{#if bidnow}}
        
        
        <a x-cq-linkchecker="skip" class="register-to-bid button-action bid-now" href="javascript:void(0)" onclick="if (!Sothebys.auction.authorizedUser()) { try { omniSiteCat.setOmnitureLoginClickEvent('BIDnow | Watch Live', 'false');} catch (e) {} }Sothebys.auction.bidNow('{{shortPath}}', '{{saleNumber}}', {{booleanToString isBidnowRegistrationAvailable}}, {{booleanToString isTransactionalUser}});">Suivre en direct</a>
        
    {{/if}}
    {{#if bidnowWine}}
        <a x-cq-linkchecker="skip" class="register-to-bid button-action" href="javascript:void(0)" onclick="if (!Sothebys.auction.authorizedUser()) { try { omniSiteCat.setOmnitureLoginClickEvent('BIDnow | Watch Live', 'false');} catch (e) {} } Sothebys.auction.bidNow('{{shortPath}}', '{{saleNumber}}', {{booleanToString isBidnowRegistrationAvailable}}, {{ booleanToStringtransactional}});">Suivre en direct</a>
    {{/if}}
    {{#if showCalendar}}
      <a class="button-action-large add-to-calendar"  href="javascript:Sothebys.catalogue.addToCalendar('/data/events/{{shortPath}}')">
        add to<span class="post">to calendar</span>
      </a>
    {{/if}}
</script>
