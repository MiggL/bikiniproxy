  'STRAT'   : Strategy, 
  'AD'        : eadCreative,
  'EADPUBLISHER'    : eadPublisher.replace('GDN','Google'),
  'EADNAME'         : eadName,
  'EADLOCATION'     : eadLocation.replace('GDN','Google'),
