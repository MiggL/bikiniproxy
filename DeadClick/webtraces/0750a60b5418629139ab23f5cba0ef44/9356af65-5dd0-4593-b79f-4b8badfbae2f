;
// Avoid `console` errors in browsers that lack a console.
(function() {
    var method;
    var noop = function () {};
    var methods = [
        'assert', 'clear', 'count', 'debug', 'dir', 'dirxml', 'error',
        'exception', 'group', 'groupCollapsed', 'groupEnd', 'info', 'log',
        'markTimeline', 'profile', 'profileEnd', 'table', 'time', 'timeEnd',
        'timeStamp', 'trace', 'warn'
    ];
    var length = methods.length;
    var console = (window.console = window.console || {});

    while (length--) {
        method = methods[length];

        // Only stub undefined methods.
        if (!console[method]) {
            console[method] = noop;
        }
    }
}());

function carouselResize(){
	$('#carousel').each(function(){
		var currentCarousel = $(this);
		var slideWidth = currentCarousel.find('.carousel-container').width();
		currentCarousel.find('.slide').width(slideWidth);
	});
}
function carouselRDM(){
	var slideCount = $('#triggers li').length;
	var rdm = Math.floor(Math.random()*(slideCount)+0);
	var currentSlide = $('#carousel .slide:eq('+(rdm-1)+')');
	$('#carousel .slide').removeClass('current-slide');
	$('#triggers li').removeClass('current-trigger');
	currentSlide.addClass('current-slide');
// 	if((rdm-1) != 0)
// 		currentSlide.insertBefore($('#carousel .slide:eq(0)'));
	$('#triggers li:eq('+(rdm-1)+')').addClass('current-trigger');
}
function refineScroll(){

}

$(function(){

	//carouselResize();
	carouselRDM();
	

	/*
		cross browser adjustments
	*/
	var stores = $('.store-locator .parts-stores .three-boxes').length;
	var isOdd;
	if (stores%2 == 0) {
		isOdd = false;
	}else{
		isOdd = true;
	};
	if (isOdd) {
		$('.store-locator .parts-stores .three-boxes').last().addClass('last');	
	};
	// console.log(isOdd);
	$('.store-locator .parts-stores .three-boxes:odd').each(function(){
		$(this).addClass('noBorder');
	});

	$('.product-list .four-boxes:nth-child(4n)').css({'margin-right':0});

	if ( !("placeholder" in document.createElement("input")) ) { // test if the input items support the placeholder attribute
		$('.contact-form input[placeholder], .contact-form textarea[placeholder], #search').each(function(){ // cycle through each place holder
			var placeholder = $(this).attr('placeholder'); // get the placeholder value

			$(this).val(placeholder); // set each input to the placeholder value that is stored
			$(this).focus(function(){
				var currentVal = $(this).val(); // get the current value
				if(currentVal === placeholder)
					$(this).val(''); // when an input is selected make it blank
			}).blur(function(){
				if($(this).val()=='') // if the user un-selects an input box and didn't enter any content then replace it's value with the placeholder
					$(this).val(placeholder);
			});
		});

	}

// 	if($('.parts-stores').length > 0){
// 		$('.parts-stores .three-boxes:nth-child(n+5)').css({'border-bottom':'none'});
// 		$('.parts-stores .three-boxes:nth-child(4n)').css({'border-right':'none'});
// 	}
	
