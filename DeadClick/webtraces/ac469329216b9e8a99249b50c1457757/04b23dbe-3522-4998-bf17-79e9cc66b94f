    <script type="text/javascript">
        var config = {
            '.chosen-select': {},
            '.chosen-select-deselect': { allow_single_deselect: true },
            '.chosen-select-no-single': { disable_search_threshold: 10 },
            '.chosen-select-no-results': { no_results_text: 'Oops, nothing found!' },
            '.chosen-select-width': { width: "95%" }
        }
        for (var selector in config) {
        //    debugger;
            $(selector).chosen(config[selector]);
        }
      </script>
<script>
    $(function () {
        var startPage = 1;
      //  alert(SearchPageCookie);
        if (SearchPageCookie != "") {
            if ($.cookie(SearchPageCookie) != "" && $.cookie(SearchPageCookie)!="NaN") {
                startPage = parseInt($.cookie(SearchPageCookie));
                
            }
        }
        if (isNaN(startPage)) startPage = 1;
        //alert(startPage);
        $("div.holder").jPages({
            containerID: "itemContainer",
            perPage: 20,
            startPage: startPage,
            startRange: 1,
            midRange: 5,
            endRange: 1,
            callback: function (pages, items) {
                //alert(SearchPageCookie);
                if (SearchPageCookie != "") {
                    $.cookie(SearchPageCookie, pages.current, { expires: 1 });
                }
                //$("#legend1").html("Page " + pages.current + " of " + pages.count);
                //$("#legend2").html(items.range.start + " - " + items.range.end + " of " + items.count);
            }
        });

        /* on select change */
        $("select#change_pro").change(function () {

           // debugger;
            /* get new nº of items per page */
            var newPerPage = parseInt($(this).val());
            //var showAll = $(this).val();

            //if (showAll == "All") {
            //    $('#itemContainer').addClass('all');
            //}
            //else {
            //    $('#itemContainer').removeClass('all');
            //}

            /* destroy jPages and initiate plugin again */
            $("div.holder").jPages("destroy").jPages({
                containerID: "itemContainer",
                perPage: newPerPage
            });
        });



        //$("div.holder").jPages({
        //    containerID: "itemContainer",
        //    perPage: 20,
        //    startPage: 1,
        //    startRange: 1,
        //    midRange: 5,
        //    endRange: 1
        //});
    });
</script>
<script type="text/javascript">

    function changelang(val) {
        alert(val);
        var url = "";
        if (val == "FR") {
            url = '/fr-FR' + '/Product/Detail/31072' + "?SearchKey=" + $('input[name="SearchKey"]').val();
        }
        else {
            url = '/en-GB' + '/Product/Detail/31072' + "?SearchKey=" + $('input[name="SearchKey"]').val();
        }
        //$.post(url, function () {
        //    window.location.href = url;
        //});
        $.post(url);
    }
