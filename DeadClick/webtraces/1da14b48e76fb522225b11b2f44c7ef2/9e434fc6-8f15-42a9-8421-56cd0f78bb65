var getCookie = function getCookie(name) {
    if (typeof window === 'undefined') {
        return '';
    }
    var cookies = window.document.cookie.split('; ');
    return cookies.reduce(function (r, v) {
        var parts = v.split('=');
        return parts[0] === name ? decodeURIComponent(parts[1]) : r;
    }, '');
};

var setCookie = function setCookie(name, value) {
    var days = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 14;
    var path = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : '/';
    var domain = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : 'cnn.com';

    var expires = new Date(Date.now() + days * 864e5).toUTCString();
    window.document.cookie = name + '=' + value + '; expires=' + expires + '; path=' + path + '; domain=' + domain + ';';
};
