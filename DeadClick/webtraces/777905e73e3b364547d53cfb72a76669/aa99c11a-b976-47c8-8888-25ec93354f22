<script>
				var message = '<div style="margin-bottom: 5px;border: 1px solid #E6DB55;background-color: #FFFFE0;color: #444444;border-radius: 4px;padding: 15px;margin: 5px 0 15px;font-size: 14px;" class="frm_message" id="message"><p>Thank you for signing up for our email newsletter.</p></div>';

				function sendNewsletter2()
				{
					var targetUrl = jQuery( '#form_newsletter2' ).attr( 'action' );
					var postData = jQuery( '#form_newsletter2' ).serialize();
					console.log(targetUrl);
					console.log(postData);
					jQuery.ajax({
						url: targetUrl ,
						type: 'POST',
						async: false,
						crossDomain: true,
						context: document.body,
						data: postData,
						dataType: 'jsonp'
					})
						.done(function(data) {
        						jQuery('#popmake-4555').popmake('close');
 				
						})
						.fail(function() {
							console.log("failed");
						});

					return false;
				}
			</script>

