        for (i = 0; i < types.length; i++) {toJSONs[i] && (types[i].prototype.toJSON = toJSONs[i]);}
        return result;
      }
      return stringify.apply(this, arguments);
    };
  })(JSON.stringify);

  
  _JSON = JSON;
}, 'json2', 'AdErrorEnv');


