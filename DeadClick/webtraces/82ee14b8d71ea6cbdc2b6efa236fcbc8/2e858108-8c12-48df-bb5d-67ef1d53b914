<script type="text/javascript">
(function (window, document, $) {
  
	$('.header-more a').click(function(){
	
	    if (!$(".header-more ul.content").is(":visible"))
	        $('.header-more').addClass("open");
	
	    $(".header-more ul.content").animate({
	            height: "toggle",
	            opacity: "toggle"
	        }, "fast", function() { 
	        if (!$(".header-more ul.content").is(":visible"))
	            $('.header-more').removeClass("open");
	    });
	});

	$('#value-menu-links .value-menu-link').not('#nav-tentpole').slice(0,6).removeClass('last_four');
	$('#value-menu-container').valuemenu();
 
})(this, document, jQuery);
</script></div><!-- End header_container -->	
