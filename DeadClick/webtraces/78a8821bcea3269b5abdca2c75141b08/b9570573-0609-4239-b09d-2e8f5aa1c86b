<div id="saleroomNoticeModal" class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
  <div class="modal-header clearfix">
    <a class="close" data-dismiss="modal" aria-hidden="true"><i class="glyph-x"></i></a>
  </div>
  <div class="modal-body">
    <table class="saleroom-notice">
      <caption></caption>
      <thead>
        <tr>
          <th>Lot Number</th>
          <th>Saleroom Notice</th>
        </tr>
      </thead>
    </table>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function() {
      var salenumber, lotID;
      var alreadyRanSaleroomNotice = false;

      if (datalayer) {  //make sure datalayer exists
        Sothebys.auction.showSrnLink(); //show the saleroom link
        saleNumber = datalayer.auction.saleNumber;
        lotID = datalayer.auction.lotId;
      }
      
      $("#srnlink").click(function() {
          if (!alreadyRanSaleroomNotice && datalayer) { //only run once
          var saleroomNoticeUrl = "/api/ajax.saleroomnotice.json?salenumber=" + saleNumber;
          
          if(lotID != null) { //don't set the lotID unless on lot page
            saleroomNoticeUrl = saleroomNoticeUrl + "&lotnumber=" + lotID;
          } 

          $.ajax({
              url : saleroomNoticeUrl,
              type : 'GET',
              dataType : 'json',
              success: function(data) {
                  //Create table and inject it into the saleroom modal
                  var injectedTbody = $("<tbody />"); 

                  if(data.saleRoomLots.length === 0) {
                    var currentTr = $("<tr/>");
                    currentTr.append("<td>There is currently no saleroom notice for this lot/sale.</td><td></td>");
                    injectedTbody.append(currentTr); 
                  }
                  else {
                    for (var i = 0; i < data.saleRoomLots.length; i++) { 
                      var currentTr = $("<tr/>");
                      currentTr.append("<td>" +  data.saleRoomLots[i].saleRoomlotNumber + "</td>");
                      currentTr.append("<td>" +  data.saleRoomLots[i].saleRoomLotNotice + "</td>");
                      injectedTbody.append(currentTr); 
                    }
                  }
                  $("#saleroomNoticeModal .saleroom-notice").append(injectedTbody);
                  alreadyRanSaleroomNotice = true; 
                }
            });
          }
      }); 
  });
</script><div id="conditionsOfSaleModal" class="modal center-vert hide fade lotdetail-condition-modal" tabindex="-1" role="dialog" aria-hidden="true">
