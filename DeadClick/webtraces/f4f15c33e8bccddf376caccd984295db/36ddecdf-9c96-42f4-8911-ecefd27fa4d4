<script type="text/javascript" id="JQueryStickyComponentBehavior-id4"><!--/*--><![CDATA[/*><!--*/
$(window).load(function(){

	var query = responsive.mdg;
var options = {offset_top:15,bottoming:true,parent:$('.container'),spacer:false,recalc_events:['lngallery:show'],off_callbacks:[$.VideoPlayerOutOfViewport.on]};
var offOptions = function() {
if (options.off_callbacks && options.off_callbacks.length > 0) {
for (var i = 0; i < options.off_callbacks.length; i++) {
options.off_callbacks[i]();
}
}
}
var updateSticky = function() {

 if ($('#id4').height() < $(window).height()) {
$('#id4').stick_in_parent(options);

 if (options.recalc_events && options.recalc_events.length > 0) {
for (var i = 0; i < options.recalc_events.length; i++) {
$(document.body).on(options.recalc_events[i], function() {
$('#id4').trigger("sticky_kit:recalc");
});
}
}
if (options.bottoming) {
$('#id4').on('sticky_kit:bottom', function(e) {
$(this).parent().css('position', 'static');
})
.on('sticky_kit:unbottom', function(e) {
$(this).parent().css('position', 'relative');
});
}
} else {
$('#id4').trigger("sticky_kit:detach");

 if (options.recalc_events && options.recalc_events.length > 0) {
for (var i = 0; i < options.recalc_events.length; i++) {
$(document.body).off(options.recalc_events[i]);
}
}
offOptions();
}
};
query.addListener(updateSticky);
if (query.matches){
updateSticky();
} else {
offOptions();
}
responsive.stickyAd = query;
});

