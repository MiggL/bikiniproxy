	/*
		add remove product details from the contact form
	*/
	$('.add-product').live('click',function(){
		if(!$(this).parent().parent().hasClass('disabled')){
			var productInfo = $('#product-bank').html();
			$(this).parent().after(productInfo);
		}
	});

	$('.remove-product').live('click',function(){
		if(!$(this).parent().parent().hasClass('disabled')){
			$(this).parent().remove();
		}
	});

	/* submit form */
	$('.contact-form').submit(function(){

	});

	$('#register').submit(function(e){
        checkRegister();


	});

  function checkRegister() {
	  $('input.form_field_text').each( function() {

         if($(this).attr('id')=="register_rep:userId"){

         if($(this).val()=='') {
          $(this).after("<span class='error-message'>Enter UserName</span>");
           $(this).css("margin-bottom", "0px");
           document.getElementById($(this).attr('id')).style.borderColor = "#cf1b24"
           $(this).parent().prev('label').css("color", "#cf1b24");
           $(this).parent().prev('label').children('span').css("color", "#cf1b24");
           $(this).focus();
            event.preventDefault();
         }
         }
         if($(this).attr('id')=="register_rep:password"){

         if($(this).val()=='') {
          $(this).after("<span class='error-message'>Enter Password</span>");
           $(this).css("margin-bottom", "0px");
           document.getElementById($(this).attr('id')).style.borderColor = "#cf1b24"
           $(this).parent().prev('label').css("color", "#cf1b24");
           $(this).parent().prev('label').children('span').css("color", "#cf1b24");
           $(this).focus();
            event.preventDefault();
         }
         }
     } );

}

