function ajaxOnLoad() {
    clientAPI.postPageUnloadExpiryCommand(false);
    clientAPI.startEventQueue();
    GrahamTechnology.ui.Form.processAllPopupForms();
if (GrahamTechnology.ui.FormParagraph) {
    GrahamTechnology.ui.FormParagraph.initialiseParagraphs();
