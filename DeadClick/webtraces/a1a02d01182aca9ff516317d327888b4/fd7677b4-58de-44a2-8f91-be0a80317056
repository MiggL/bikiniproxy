    <script>
        var params = window.location.search.substr(1).split('&');
        window.adpath = params[0] == '_ADPATH_' ? '' : params[0];
        window.adclick = params[1] == '_ADCLICK_' ? '' : params[1];

        function fixHrefs(){
            var links = document.querySelectorAll('a[href^=http]:not([data-adtech-click])');
            for (var i = 0; i < links.length; i++) {
                links[i].addEventListener('click', function(e) {
                    window.open(this.href, '_blank');
                    e.preventDefault();
                });
                links[i].setAttribute('data-adtech-click', true);
            }
        }

        function initClicks() {
            fixHrefs();
            window._open = window.open;
            window.open = function(url, target) {
                if (url !== 'undefined' && url !== '' && url !== null) {
                    window._open(window.adclick + url.replace(/(?:_ADCLICK_|_ADCLICKESC_|_ADCLICKDEC_|_ADCLICKDECESC_)/, ''), target);
                }
            }
        }
        if (window.addEventListener) {
            window.addEventListener('load', initClicks);
        } else {
            window.attachEvent('onload', initClicks);
        }
    </script>
<meta charset="UTF-8">
