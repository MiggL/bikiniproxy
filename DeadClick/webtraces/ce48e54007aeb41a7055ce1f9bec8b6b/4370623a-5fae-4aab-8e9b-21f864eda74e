<script type="text/javascript">
	var imgs, numImgs, startZIndex, next;

	jQuery(window).load(function() {
		jQuery('#slider').children('img').click(function(){
			window.location = jQuery(this).attr('alt');
		});
		imgs = jQuery('#slider').children('img');
		numImgs = imgs.length;
		startZIndex = jQuery(imgs[0]).css('z-index');
		for(var i = 0;i<imgs.length;i++){
		jQuery(imgs[i]).css('z-index',startZIndex-i);
		}
		setTimeout('fadeSlide(0)', 6000);
	});
	
	function fadeSlide(n) {
		if(n+1 >= numImgs) next = 0;
		else next = n+1;
		
		jQuery(imgs[next]).css('z-index',startZIndex-1);
		jQuery(imgs[n]).fadeOut(1000,function(){
			jQuery(this).css('z-index',startZIndex-2);
			jQuery(imgs[next]).css('z-index',startZIndex);
			jQuery(imgs[n]).fadeIn(0);
			setTimeout('fadeSlide('+next+')', 6000);
		});
		
	}
</script>

