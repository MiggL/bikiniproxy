	var navItems = ' https://s3.amazonaws.com/aurora-cms-static/mobile/prod/homepage-header.json';
	var jqxhr = $.getJSON( navItems, function(data) {
		$.each( data, function( i, item ) {	        
			var counter = 1;
			$.each( item.result.items, function( i, linkItem ) {
				console.log("linkItem.url=" + linkItem.url);
				$("#menu-subnav ul").append('<li><a href=\"' + linkItem.url + '\"' + ' data-omtr-intcmp=subnav' + counter+  '"\">' + linkItem.text + '</a></li>');
				counter ++ ;
			});
		});
	})
</script>


    
   	
