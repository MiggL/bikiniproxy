    $( "#header .nav-box .drop" ).click(function() {
        if($('#top-mobile').css('display') == 'block'){
            //$('#top-mobile').css('display', 'none');
            $("#top-mobile").slideToggle();
        }else{
            //$('#top-mobile').css('display', 'block');
            $("#top-mobile").slideToggle();
        }
    });
    $("#ico-search").click(function(){
        $(".nav-box form").toggleClass("show-form");
    }); 
</script>
