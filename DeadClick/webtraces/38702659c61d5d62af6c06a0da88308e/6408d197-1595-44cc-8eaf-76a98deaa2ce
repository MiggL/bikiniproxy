						<h3>VALENTINE'S DAY DINNER CRUISE</h3>
						<p class="mention">Cruise on the Seine about 2 hours </p>
<p class="red">&#8211;</p>
<p><strong>Musical animation with singer<br />
 1 rose offered per couple<br />
 Menu</strong>: Starter | Main course | Dessert<br />
 <b>Drinks</b>: Glass of Champagne + 1 Bottle of Wine for 2 (75 cl) + Mineral Water (50 cl) + Coffee</p>
					</div>
				</div>
			</div>
							<!--</div>-->
			
				
		<script>
			/* Thanks to CSS Tricks for pointing out this bit of jQuery
			https://css-tricks.com/equal-height-blocks-in-rows/
			It's been modified into a function called at page load and then each time the page is resized. One large modification was to remove the set height before each new calculation. */

			equalheight = function(container){

			var currentTallest = 0,
			     currentRowStart = 0,
			     rowDivs = new Array(),
			     $el,
			     topPosition = 0;
			 $(container).each(function() {

			   $el = $(this);
			   $($el).height('auto')
			   topPostion = $el.position().top;

			   if (currentRowStart != topPostion) {
			     for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
			       rowDivs[currentDiv].height(currentTallest);
			     }
			     rowDivs.length = 0; // empty the array
			     currentRowStart = topPostion;
			     currentTallest = $el.height();
			     rowDivs.push($el);
			   } else {
			     rowDivs.push($el);
			     currentTallest = (currentTallest < $el.height()) ? ($el.height()) : (currentTallest);
			  }
			   for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
			     rowDivs[currentDiv].height(currentTallest);
			   }
			 });
			}

			$(window).load(function() {
				var window_width = $(window).width();
				
				if (window_width > 767) {
			  		equalheight('.container .formule');
			  	}
			});


			$(window).resize(function(){
			  	var window_width = $(window).width();
			  	
			  	if (window_width > 767) {
			    		equalheight('.container .formule');
			    	}
			});


		</script>
	
</div>
	<section id="calendar_v2" style="display: none;">
	<hr class="grey">
	<div class="container">
		<div class="row">
			<div class="col-xs-12 col-sm-12 col-md-12 col-lg-8 col-lg-push-2">
				<iframe id="iframe_calendar_product"
					src= ""
					frameborder = "0"
					style="width: 100%;"
				></iframe>
				<script>
										$(function(){
						var css = "catalogue-fra";
						var url_iframe = "http://www.beenbiz.com/cds75/prd.php";
						var lg = "en";
						var url_panier = "http://www.lecapitainefracasse.com/en/cart/";
						var act = 'calendrier-mois';
						var autoOpen = 0;
						var dateParam = "";
						var hasScroll = false;

						var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
						var eventer = window[eventMethod];
						var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";
						var section_calendar_offer = $('#offrir_calendar_v2');

						// Listen to message from child window
						eventer(messageEvent,function(e) {
						  if(document.getElementById('iframe_calendar_product')){

						  	if ((typeof e.data === 'string' || e.data instanceof String)) {
						  			var message = (typeof e.data === 'string' || e.data instanceof String)? e.data.split("|") : parseInt(e.data);
						  			console.log(message);
						  			message = (typeof e.data === 'number')? message : parseInt(message[1]);
						  		   document.getElementById('iframe_calendar_product').style.height = (message + 20) + "px";

						  		   var section_calendar = $('#calendar_v2'); // Page cible
						  		   var speed = 750; // Durée de l'animation (en ms)

						  		   var origHeight = section_calendar.css('height');
						  		   if (section_calendar_offer.css('display') != 'none') {
						  		   	section_calendar_offer.animate( { height: 0+'px' }, speed, function(){
						  		   		section_calendar_offer.css('display', 'none');
						  		   	} ); // Go
						  		   }
						  		   if (section_calendar.css('display') == 'none') {
						  		   	section_calendar.css('height', '0px');
						  		   	section_calendar.css('display', 'block');
						  		   	section_calendar.animate( { height: origHeight+'px' }, speed ); // Go
						  		   }
						  		   if (!autoOpen) {
						  		   	if (!hasScroll) {
						  		   		$('html, body').animate( { scrollTop: $(section_calendar).offset().top-70 }, speed );
						  		   		hasScroll = true;
						  		   	}
						  		   	
						  		   }
						  		   else {
						  		   	autoOpen = 0;
						  		   	if (dateParam != '') {
						  		   		$('html, body').animate( { scrollTop: $(section_calendar).offset().top-300 }, speed );
						  		   		hasScroll = true;
						  		   	}
						  		   }
						  	}
						  	
						      
						  }
						},false);

						$('a[data-ids]:not([data-offrir])').on('click', function(){

							hasScroll = false;
							var date = $(this).attr('data-date');
							var src = url_iframe+'?act='+ act+'&selection='+$(this).attr('data-ids')+'&lg='+lg+'&urlpanier='+url_panier+"&css="+css+dateParam;
							if (typeof date !== typeof undefined && date !== false) {
							  src += '&action=choix-produit&d='+date;
							}
							
							$('#iframe_calendar_product').attr('src', src);
							return false;
						});

											});

				</script>
			</div>
		</div>
	</div>
