		$(document).ready(function() {

			$('.jQueryLightBox a').fancybox({
				overlayColor: '#000000',
				overlayOpacity: .4,
				padding: 6,
				width: 640,
				height: 390,
				transitionIn: 'fade',
				transitionOut: 'fade',
				easingIn: 'easeInSine',
				easingOut: 'easeOutSine',
				titlePosition: 'over',
				cyclic: true
			});

		}); // end ready
