	$("#tabs li").click(function(){
	     var activeTab = $(this).attr("id");
	     $("#tabs li").removeClass("active");
	     $(this).addClass("active");
	     $("#tab-target .article").hide();
	     $('#tab-target .' + activeTab).show();
	});
	/*
		product carousel
	*/
	var prodCount = $('.product-container .product-item').length;
	var winWidth = $(window).width();
	if (prodCount < 5 && winWidth > 960) {
		$('.next,.prev').addClass('hide');
	}
	if (prodCount < 4 && winWidth < 960 && winWidth > 571) {
		$('.next,.prev').addClass('hide');
	}
	if (prodCount < 3 && winWidth < 572 && winWidth > 479) {
		$('.next,.prev').addClass('hide');
	}
	if (prodCount < 2 && winWidth < 480) {
		$('.next,.prev').addClass('hide');
	}
	$(window).resize(function(){
		winWidth = $(window).width();
		$('.next,.prev').removeClass('hide');
		if (prodCount < 5 && winWidth > 960) {
			$('.next,.prev').addClass('hide');
		}
		if (prodCount < 4 && winWidth < 960 && winWidth > 571) {
			$('.next,.prev').addClass('hide');
		}
		if (prodCount < 3 && winWidth < 572 && winWidth > 479) {
			$('.next,.prev').addClass('hide');
		}
		if (prodCount < 2 && winWidth < 480) {
			$('.next,.prev').addClass('hide');
		}
	});
	$('.product-carousel .next').click(function(){
		var productItemW = $('.product-item').outerWidth() + 14;
		$('.product-items').animate({'margin-left': -productItemW},function(){
			$('.product-item:last').after($('.product-item:first'));
			$(this).css({'margin-left':0});
		});
	});
	$('.product-carousel .prev').click(function(){
		var productItemW = $('.product-item').outerWidth() + 14;
		$('.product-item:first').before($('.product-item:last'));
		$('.product-items').css({'margin-left': -productItemW});
 		$('.product-items').animate({'margin-left': 0});
	});


