GrahamTechnology.ui.WebLink=function(a){GrahamTechnology.ui.AbstractComponent.call(this,a);this.enable=function(){this.enabled=true;(this.element()).disabled=false;this.removeGlass()};this.disable=function(){this.enabled=false;GrahamTechnology.DOM.Element.updateAttributes(this.element(),{disabled:true});this.addGlass()};this.updateInnerLayout=function(){this.updateGlass()};this.setText=function(b){this.element().innerHTML=b};this.setHref=function(b){this.element().href=b};this.setRel=function(b){this.element().rel=b};this.setTarget=function(b){this.element().target=b};this.setOnClick=function(b){this.element().onclick=Function(b)};this.setDescription=function(c){var b=GrahamTechnology.DOM.getElementById(this.id+"_desc");if(c!==""){b.innerHTML=c;GrahamTechnology.DOM.Element.removeClass(b,"description-hidden")}else{GrahamTechnology.DOM.Element.addClass(b,"description-hidden")}};this.getText=function(){this.element().innerHTML}};GrahamTechnology.ui.WebLink.prototype=new GrahamTechnology.ui.AbstractComponent();