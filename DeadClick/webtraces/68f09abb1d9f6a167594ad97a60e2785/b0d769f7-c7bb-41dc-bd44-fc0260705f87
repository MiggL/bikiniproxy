// setting for Datalayer
var prodName = $('title').html();
var prodID = $('[itemprop="image"]').attr('content');
var prodID = prodID.substring(prodID.length - 13);
var prodID = prodID.substring(0, 9);
var prodPrice ='';
  sale = ($('.product_price').find('.now').length > 0) ? 'sale' : 'normal' ;
  prodPrice = ((sale =='normal') ? $('.product_price').html().substring($('.product_price').html().length - 5) : $('.product_price').find('.now').html().substring(1));
var catID = prodID.substring(0, 3);
console.log(prodName);
console.log(prodID);
console.log(prodPrice);
console.log(catID);


// --Measuring Views of Product Details--
// -- aka reach the PDP

// Measure a view of product details. This example assumes the detail view occurs on pageload,
// and also tracks a standard pageview of the details page.
dataLayer.push({
  'ecommerce': {
    'detail': {
      'actionField': {'list': 'Apparel Gallery'},    // 'detail' actions have an optional list property.
      'products': [{
        'name': prodName,         // Name or ID is required.
        'id': prodID,
        'price': prodPrice,
        'brand': 'Mint Velvet',
        'category': catID,
        'variant': ''
       }]
     }
   }
});


// --Adding a Product to a Shopping Cart
// aka adding to bag

// Measure adding a product to a shopping cart by using an 'add' actionFieldObject
// and a list of productFieldObjects.
$("#add_to_bag").click(function( event ) {
// event.preventDefault();

  console.log(prodName);
  console.log(prodID);
  console.log(prodPrice);
  console.log(catID);


dataLayer.push({
  'event': 'addToCart',
  'ecommerce': {
    'currencyCode': 'GBP',
    'add': {                                // 'add' actionFieldObject measures.
      'products': [{                        //  adding a product to a shopping cart.
        'name': prodName,
        'id': prodID,
        'price': prodPrice,
        'brand': 'Mint Velvet',
        'category': catID,
        'variant': '',
        'quantity': ''
       }]
    }
  }
});
});

</script>


