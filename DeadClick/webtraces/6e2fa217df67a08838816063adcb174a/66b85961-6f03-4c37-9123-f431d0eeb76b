   require(["jquery","lib/jquery/plugin/jquery.waypoint"], function ($) {
      // Chargement defere des scripts du moteur de recherche
      var $recherche_la_centrale = $("#la_centrale_recherche");

      $recherche_la_centrale.waypoint(function () {
         require(
            ["partenaires/bizdev/lacentrale"],
            function (scripts_lacentrale) {
               scripts_lacentrale.initFor("la_centrale_recherche");
            }
         );
      },
      { offset: '130%', triggerOnce : true }
      );
   });
</script>

