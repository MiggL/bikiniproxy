<script>
    jQuery(document).ready(function () {
        var austDay = new Date(2012, 1, 23);
        var dayNow = new Date();

        jQuery('#Countdown').countdown({
            format: 'YOD',
            since: austDay,
            description: 'Since the permitting process began'
        });

        var options = {
            useEasing : true,
            useGrouping : true,
            separator : ',',
            decimal : '.',
            prefix : '',
            suffix : ''
        }

        var setYear = jQuery("#Years").html();
        var setMonth = jQuery("#Months").html();
        var setDay = jQuery("#Days").html();
        var year = new countUp("year", 0, setYear, 0, 2.5, options);
        var month = new countUp("month", 0, setMonth, 0, 2.5, options);
        var day = new countUp("day", 0, setDay, 0, 2.5, options);
        year.start();
        month.start();
        day.start();

        if (setYear < 10){
            jQuery( "#year" ).addClass( "O" );
        }
        if (setMonth < 10){
            jQuery( "#month" ).addClass( "O" );
        }
        if (setDay < 10){
            jQuery( "#day" ).addClass( "O" );
        }
    });
</script>
