return
}var s=r.which;
if(s==g.SHIFT||s==g.CTRL||s==g.ALT){return
}if(s==g.TAB){return
}n.handleSearch(r)
})
};
h.prototype._transferTabIndex=function(k){this.$search.attr("tabindex",this.$selection.attr("tabindex"));
