var OlapicCallback = OlapicCallback || {};
OlapicCallback.olapicSliderAfterRender = function(w){
    
    if (oQuery('#olapic_specific_widget').hasClass('olapic_items_0') || oQuery('#olapic_specific_widget').hasClass('olapic_items_1') || oQuery('#olapic_specific_widget').hasClass('olapic_items_2') || oQuery('#olapic_specific_widget').hasClass('olapic_items_3')) {
        
        oQuery('.empty-partial-1').appendTo('.olapic-carousel');

        oQuery('.empty-partial-2').appendTo('.olapic-carousel');
        
        oQuery('.empty-partial-3').appendTo('.olapic-carousel');
        
        oQuery('.empty-partial-4').appendTo('.olapic-carousel');
        
    }
    
    else if (oQuery('#olapic_specific_widget').hasClass('olapic_items_4')) {
        
        oQuery('.empty-partial-1').appendTo('.olapic-carousel');

        oQuery('.empty-partial-2').appendTo('.olapic-carousel');
        
        oQuery('.empty-partial-3').appendTo('.olapic-carousel');
        
    }
    else if (oQuery('#olapic_specific_widget').hasClass('olapic_items_5')) {
        
        oQuery('.empty-partial-1').appendTo('.olapic-carousel');

        oQuery('.empty-partial-2').appendTo('.olapic-carousel');
        
    }
    /*else if (oQuery('#olapic_specific_widget').hasClass('olapic_items_6')) {
        
        oQuery('.empty-partial-1').appendTo('.olapic-carousel');
        
    }*/
    oQuery( '.olapic-slider-body' ).append(oQuery('.olapic-slider-footer'));
};
OlapicCallback.olapicSliderBeforeInitCarousel = function(w){
   /*
