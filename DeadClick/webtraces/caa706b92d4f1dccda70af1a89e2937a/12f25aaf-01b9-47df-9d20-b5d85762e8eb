<script type="text/javascript">

function onMemberScoreSubmitted(Score)
{

//alert ("Score Submitted");

timeoutID  = window.setTimeout("UpdateLoggedInUserScore()",3000);
//timeoutID  = window.setTimeout("alert('pp')",3000);
 
}

function UpdateLoggedInUserScore()
{
  base_urljs = 'http://www.tremorgames.com/';
  myurl= base_urljs + "achievements/ajax_loggedinuserscore.php?GameID=190";
 // alert (myurl);
 
  $.ajaxSetup({
    cache: false
});


  $.get(myurl, function(userdata, textStatus)
  {
             
		 document.getElementById('user_hs_bg').innerHTML  = userdata;

  });
}

var activetab = 4;

function onHighScorePageChanged(HSFilter,limitstart)
{
        //   alert (limitstart);
        
  	  RefreshHighScores(limitstart,HSFilter,activetab);
  	  
}

function onHighScoreTabChange(tabid)
{
 
  //alert(tabid);
  activetab  = tabid;
	switch (tabid)
	{
	case 1:
	  RefreshHighScores(0,"Today",tabid);
	  break;
	case 2:
	  RefreshHighScores(0,"ThisWeek",tabid);
	  break;
	case 3:
	  RefreshHighScores(0,"ThisMonth",tabid);
	  break;
        case 4:
	  RefreshHighScores(0,"AllTime",tabid);
	  break;
        case 5:
	  RefreshHighScores(0,"Friends",tabid);
	  break;
	
	}
	
	 $("#hs_tab1").removeClass("actv");
  $("#hs_tab2").removeClass("actv");
  $("#hs_tab3").removeClass("actv");
  $("#hs_tab4").removeClass("actv");
  $("#hs_tab5").removeClass("actv");
  $("#hs_tab" + tabid).addClass("actv");
  
}

function RefreshHighScores(limitstart,hsfilter,tabid)
{



$("#HighScoreLoaderImg").show();
//alert ("RefreshHighScores");
   base_urljs = 'http://www.tremorgames.com/';
  myurl= base_urljs + "achievements/ajax_show_highscores.php?GameID=190&limitstart=" + limitstart + "&HSFilter=" + hsfilter;
 // alert (myurl);
 
  $.ajaxSetup({
    cache: false
});


  $.get(myurl, function(highscoredata, textStatus)
  {
              //  alert(textStatus);
             // alert(highscoredata);
              
		//$("#MemberHighScores").html(data);
		 document.getElementById('MemberHighScores').innerHTML  = highscoredata;
		 $("#hs_tab" + tabid).addClass("actv");
  });
}

function setStatValue(StatName,Value)
{

    
  //StatName = StatName.replace(/ /gi,"");
  // StatType = $("#StatType" + StatName).text();
  
  //CurrentStatValue = parseInt($("#ProgressValue" + StatName).text());
  
   // alert("StatName =" + StatName);
 
  //alert("Cur Value =" + Value);
 
   var i = 0;
   var AchievementID;
   var ProgressValueUpdated = false;
   
//alert(AchievementsJS.length);

//ShowAchMessage(1);
  
  while (i < AchievementsJS.length)
  {
     AchievementID= AchievementsJS[i].AchievementID;

     ProgressValueUpdated = false;
     StatType  =  AchievementsJS[i].StatType;
     if (AchievementsJS[i].StatName == StatName)
     {
       //  alert("Stat Found");
       //Update Current Progress
         if (StatType != "Cumulative"  || AchievementsJS[i].ProgressValue == null)
           $("#CurrentProgressValueSidebar" + AchievementID).text(Value);
         

           
         if (AchievementsJS[i].ProgressValue == null)
         {
         	  $("#ProgressValue" + AchievementID).text(Value);
           	  $("#ProgressValueSidebar" + AchievementID).text(Value);
         	  ProgressValueUpdated  = true;
         }
         else
         {
	          if (StatType == "Max")
	 	   
	 	   {
	    		  if  (Value > AchievementsJS[i].ProgressValue)
	    		  {
	        		  $("#ProgressValue" + AchievementID).text(Value);
	        	        $("#ProgressValueSidebar" + AchievementID).text(Value);
	        		  ProgressValueUpdated = true;
	                  }
	   	   }
	    
	 	 if (StatType == "Min")
	  	  {
	      		if  (Value < AchievementsJS[i].ProgressValue)
	      		{
	          		$("#ProgressValue" + AchievementID).text(Value);
	           		$("#ProgressValueSidebar" + AchievementID).text(Value);
	          		ProgressValueUpdated = true;
	          	}
	    	  }
		 if (StatType == "Cumulative")
	    	{
	          Value = parseInt(Value) + parseInt(AchievementsJS[i].ProgressValue) ;
	         
	          
	          //alert (parseInt(AchievementsJS[i].ProgressValue));
	      	  $("#ProgressValue" + AchievementID).text(Value);
	      	  
       		$("#ProgressValueSidebar" + AchievementID).text(Value);
             $("#CurrentProgressValueSidebar" + AchievementID).text(Value);
       		
	      	  ProgressValueUpdated = true;
	    	}
	
    	} // end if  AchievementsJS[i].ProgressValue == null
    	
    //     PercentComplete =  (parseInt(Value)/ AchievementsJS[i].StatValue) * 100;

      	//$("#jsconsole").append("<br/>StatName=" + StatName);
    	//$("#jsconsole").append(" Value=" + parseInt(Value));
    	//$("#jsconsole").append(" StatValue=" + AchievementsJS[i].StatValue);
    	//$("#jsconsole").append(" Percent Complete=" + PercentComplete);

      if (ProgressValueUpdated)
      {
         AchievementsJS[i].ProgressValue = parseInt(Value);
         
          if (StatType != "Min")
          {
            PercentComplete =  (parseInt(Value)/ AchievementsJS[i].StatValue) * 100;
            if (PercentComplete > 100)
              PercentComplete  = 100;
              
            PercentComplete = PercentComplete  + "%" ;
            
           $("#ProgressBarSidebar" + AchievementID).width(PercentComplete);
           $("#ProgressBar" + AchievementID).width(PercentComplete);
			var progresstitle = Value + "/" + AchievementsJS[i].StatValue ;
		   //$("#ProgressBarSidebar" + AchievementID).title(parseInt(Value)/ AchievementsJS[i].StatValue);
		//	alert (progresstitle);
           $("#ProgressBar" + AchievementID).attr('title', progresstitle);
            
          }
	  isloggedin= document.getElementById("loggedin").value;
       //  alert (isloggedin);	 
         if (AchievementsJS[i].AchievementDate == null )  
         {  // Check for Achievement Completion
	         if ((StatType == "Max" && AchievementsJS[i].ProgressValue >= AchievementsJS[i].StatValue) || (StatType == "Min" && AchievementsJS[i].ProgressValue <= AchievementsJS[i].StatValue) || (StatType == "Cumulative" && AchievementsJS[i].ProgressValue >= AchievementsJS[i].StatValue) )
	         {
	             // Achievement Complete
	             if (isloggedin==1)
				 {
					$("#ProgressText" + AchievementID).text("Achievement Complete (Verification Pending)");
					$("#ProgressTextSidebar" + AchievementID).text("Achievement Complete (Verification Pending)");
				 }
     	         $("#AchievementImage" + AchievementID).removeClass("ach_image_greyscale");
	             $("#AchievementImage" + AchievementID).addClass("ach_image_color");
	             $("#AchievementSidebarImage" + AchievementID).removeClass("ach_image_greyscale");
	             $("#AchievementSidebarImage" + AchievementID).addClass("ach_image_color");
	              
	              
	            ShowAchMessage(i);
	             AchievementsJS[i].AchievementDate = new Date() ;
	              //alert (AchievementsJS[i].AchievementDate);
	              if (StatType == "Min")
	              {
           				$("#ProgressBarSidebar" + AchievementID).width('100%');
           				$("#ProgressBar" + AchievementID).width('100%');
           	      }
            
          
	              
	             
	         }
         }
         
        
      }
          
     }
     i++;
     
  }  // end while
 
  
  
  //alert("#ProgressValue" + StatName);

}  // end function setStatValue(StatName,Value)
// $(document).ready(function() {
  //  setStatValue("Spiders Killed",100);
 //});
function ShowAchMessage(AchIndex)
{
   i = AchIndex;
   var diff = AchievementsJS[i].Difficulty;
   var coins = 0;
   isloggedin= document.getElementById("loggedin").value;
   
   if ( diff == 1)
      coins =5 ;
	  
   if ( diff== 2)
        coins =10 ;
		
   if ( diff== 3)
      coins =15 ;
	  
   if ( diff== 4)
      coins =25 ;	  
	  
      
      
     base_urljs = 'http://www.tremorgames.com/';
    MessageText = "<div style='margin-bottom:5px;'><center style='font-weight:bold;font-size:12px;'>Achievement Complete!</center></div>";
    MessageText = MessageText + "<div style='float:left;margin-right:5px;'><img src='" + base_urljs + "achievements/images/" + AchievementsJS[i].AchievementImage + "'/></div>" ;
    MessageText = MessageText + "<div style='font-size:11px;'><b>" +  AchievementsJS[i].AchievementName + "</b><br/>" ;
    MessageText = MessageText +  AchievementsJS[i].Description + "</div>";
	
	if (isloggedin != 1)
	   {
	     MessageText = MessageText + "<div style='clear:both'></div>";
	     MessageText = MessageText + "<div style='line-height:18px;'><span style='color:green;font-size:12px; font-weight:bold;'>" + coins + " Tremor Coins Reward. </span><br/> <a style='text-decoration:underline' href='http://www.tremorgames.com/login.html'>Log In</a> to claim your Tremor Coins.</div>" ;
	   
	   }
	
   $("#AchMessage").html(MessageText);
   
   $('#AchMessage').slideDown('slow', function() {
    // Animation complete.
    timeoutID  = window.setTimeout("FadeOutAchMessage()",7000);

  });
  
  
}

function FadeOutAchMessage()
{
      $('#AchMessage').fadeOut('slow');
}

function AddToFav(gameid)
{
  base_urljs = 'http://www.tremorgames.com/';
  myurl= base_urljs + "achievements/ajax_addtofav.php?GameID=" + gameid;
  //alert (myurl);
 
  $.ajaxSetup({
    cache: false
});


  $.get(myurl, function(myresponse, textStatus)
  {
         $('#AddToFavMessage').html(myresponse);    
		 

  });

  
}
</script>


