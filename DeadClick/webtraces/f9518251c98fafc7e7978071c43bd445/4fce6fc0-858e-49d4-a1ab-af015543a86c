    <script type="text/javascript">
        function quickView(url) {
            mgsjQuery.magnificPopup.open({
                items: {
                    src: 'http://www.celticjewelry.com/quickview/index/show/' + '?url=' + url
                },
                type: 'iframe',
                callbacks: {
                    open: function () {
                        mgsjQuery("iframe").each(function() {
                            mgsjQuery(this).load(function () {
                                mgsjQuery(this).contents().find('body').css('background-color', '#fff').delay(200);
                            });
                        });
                    }
                }
            });
        }
    </script>
