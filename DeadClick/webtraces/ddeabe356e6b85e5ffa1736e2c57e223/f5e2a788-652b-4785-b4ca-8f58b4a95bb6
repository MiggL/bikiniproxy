$('.moreLocat').on('click', function(){
	$('html').niceScroll();
		if(!$(this).hasClass('active')){
		$(this).addClass('active');
		$(this).find('a').text('Less');
		 $('.dropHide').slideDown(function(){  });
		 $('html, body').animate({
                scrollTop: $('#animateDrop').offset().top
            }, 800);
		} else {
			$(this).removeClass('active');
		$(this).find('a').text('More');	
		$('.dropHide').slideUp();
		}
	return false;
});


