}return String(k).replace(/[&<>"'\/\\]/g,function(n){return m[n]
})
};
i.appendMany=function(k,n){if(j.fn.jquery.substr(0,3)==="1.7"){var m=j();
j.map(n,function(o){m=m.add(o)
});
n=m
}k.append(n)
};
return i
});
f.define("select2/results",["jquery","./utils"],function(i,h){function g(j,k,m){this.$element=j;
