define(["jquery","wcs.context"],function(e,c){var b=e("html"),a="vfdp-user-",d=[{name:"registered",contextKey:"R",},{name:"guest",contextKey:"G"}];className=function(f){return a+f};identifyUserGroup=function(){var f=c.getUserType(),g=d.filter(function(h){return h.contextKey===f});if(g.length>0){setGroup(g[0].name);return g[0].name}setGroup("guest");return"guest"};clearGroupClasses=function(){var f=d.map(function(g){return className(g.name)});b.removeClass(f.join(" "))};setGroup=function(g){var f=d.filter(function(h){return(h.name===g)});if(f.length>0){clearGroupClasses();b.addClass(className(g));return true}throw new Error("User group "+g+" does not exist.")};identifyUserGroup();return{clearGroupClasses:clearGroupClasses,identifyUserGroup:identifyUserGroup,setGroup:setGroup}});