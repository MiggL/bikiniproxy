define(["vfdp-s7-viewer.viewer.prototype",],function(a){var b=function(d,c,e){if(s7sdk.Logger.traceLevel>=s7sdk.Logger.CONFIG){console.log("Creating new ZoomViewer")}this.type=[s7sdk.ItemDescType.IMG];this.config=d;this.Container=c;this.params=e;this.SDKDependencies=["s7sdk.common.Button","s7sdk.common.ControlBar","s7sdk.image.ZoomView"];a.call(this,"image.ZoomView","ZoomView")};b.prototype={init:function(){if(s7sdk.Logger.traceLevel>=s7sdk.Logger.FINE){console.log("ZoomView.prototype.init")}var c=this;this.ControlBar.show=function(){this.setCSS(".s7controlbar","display","block");var d=c.config.getConfig(main.screen.size()).Viewer.ZoomView.ControlBar;if(d.fullscreen){this.FullScreenButton.setCSS(".s7fullscreenbutton","display","block");this.FullScreenButton.activate()}else{this.FullScreenButton.setCSS(".s7fullscreenbutton","display","none");this.FullScreenButton.deactivate()}c.ControlBar.setModifier(d.modifiers)};this.ControlBar.ZoomInButton=new s7sdk.common.ZoomInButton(this.ControlBar.getId(),this.params,this.ControlBar.getId()+"-ZoomView-zoomInBtn");this.ControlBar.ZoomOutButton=new s7sdk.common.ZoomOutButton(this.ControlBar.getId(),this.params,this.ControlBar.getId()+"-ZoomView-zoomOutBtn");this.ControlBar.ZoomResetButton=new s7sdk.common.ZoomResetButton(this.ControlBar.getId(),this.params,this.ControlBar.getId()+"-ZoomView-zoomResetBtn");this.ControlBar.ZoomInButton.addEventListener("click",function(){c.Viewer.zoomIn()});this.ControlBar.ZoomOutButton.addEventListener("click",function(){c.Viewer.zoomOut()});this.ControlBar.ZoomResetButton.addEventListener("click",function(){c.Viewer.zoomReset()})},setItem:function(c){this.Viewer.setItem(c)},};return b});