};
g.prototype.option=function(o){var p=document.createElement("li");
p.className="select2-results__option";
var w={role:"treeitem","aria-selected":"false"};
if(o.disabled){delete w["aria-selected"];
w["aria-disabled"]="true"
}if(o.id==null){delete w["aria-selected"]
}if(o._resultId!=null){p.id=o._resultId
}if(o.title){p.title=o.title
}if(o.children){w.role="group";
w["aria-label"]=o.text;
delete w["aria-selected"]
}for(var q in w){var n=w[q];
p.setAttribute(q,n)
}if(o.children){var j=i(p);
