						if (thisHref.match(/\?/)) {
				            jQuery(this).attr('href',thisHref + '&intcmp=gvsl' + sUniqueCode);
						} else {
				            jQuery(this).attr('href',thisHref + '?intcmp=gvsl' + sUniqueCode);
			      		}
