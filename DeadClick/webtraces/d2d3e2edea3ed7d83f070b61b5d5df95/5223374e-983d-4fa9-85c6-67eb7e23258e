function load_creative(creative){tag=document.createElement('div');var target=document.getElementById(creative.before);if(!!target){if(creative.type='static_image'){tag.innerHTML='<div style="text-align:center;"><img src="'+creative.image+'" style="height:'+creative.height+'px;width:'+creative.width+'px;cursor:pointer;margin:15px auto 15px auto;"></div>';tag.addEventListener("click",function(){location.assign(creative.url);});}
target.parentNode.insertBefore(tag,target);}else{console.log('couldnt find target');}};
