<script><!--
	$(function(){
		$.ajax({
			url : 'index.php?route=taobao/taobao/getProductDetails&tm=0&product_id=524284968106&inter_shipping_val=0',
			dataType : 'json',
			beforeSend : function(){
				$('.price_info').append('<img class="loading_details" src="public/image/loading.gif" />');
				
				$('#stock').append('<img class="loading_details" src="public/image/loading.gif" />');
				
				if($('#alter_stock').length){
					$('#alter_stock').append('<img class="loading_details" src="public/image/loading.gif" />');
				}
				
				$('#express_fee').append('<img class="loading_details" src="public/image/loading.gif" />');
				
				$('#total_text').append('<img class="loading_details" src="public/image/loading.gif" />');
			},
			complete : function(){
				$('.loading_details').remove();
			},
			success : function(json){
				if(json['special_range_text']){
					$('.special-info').html(json['special_range_text']);
					
					$('.price-info').addClass('has-special');
					
					$('input[name=\'special_text\']').val(json['special_range_text']);
				}
				
				if(json['sku_special_info_html']){
					$('.skus-info').append(json['sku_special_info_html']);
				}
				
				if(json['domestic_shipping']){
					$('#express_fee').html(json['domestic_shipping']);
				}
				
				if(json['domestic_shipping_val']){
					$("input[name='domestic_shipping_val']").val(json['domestic_shipping_val']);
				}
				
				if(json['total_text']){
					$('#total_text').html(json['total_text']);
				}else{
					getTotal();	
				}
				
				if(json['sold_html']){
					$('#stock').parent('div').after(json['sold_html']);
				}
				
				if(json['stock']){
					$('#stock').html(json['stock']);
					
					if($('#alter_stock').length){
						$('#alter_stock').html(json['stock']);
					}
					$('input[name=\'stock\']').val(json['stock']);
				}
				
				if(json['seller_rate_html']){
					$('.seller-info').prepend(json['seller_rate_html']);	
				}
			}
		 });
		
		if($('.product_tip').length){
			var data = $('.product_tip').html();
			$.ajax({
				url : 'public/php/getProductName.php?product_name='+encodeURI(data)+'&language_code=en&size=1000&config_translation_approach=3&config_translation_develop_key=&config_google_api_url=com',
				dataType: 'json',
				success : function(json){
					if(json['origin_product_name']){
						$('.product_tip').html(json['origin_product_name']);
					}
				}
			});	
		}
		
		var size = 200;
	
		$(".props > dt").each(function(){
			var obj = this;
			var data = $(obj).html();
			$.ajax({
				url : 'public/php/getProductName.php?product_name='+encodeURI(data)+'&language_code=en&size='+size+'&config_translation_approach=3&config_translation_develop_key=&config_google_api_url=com',
				dataType: 'json',
				success : function(json){
					if(json['origin_product_name']){
						$(obj).html(json['origin_product_name']);
					}
				}
			});	
		});
		
		$(".props").find("li").each(function(){
			var obj = this;
			var data = $(obj).attr("title");
			$.ajax({
				url : 'public/php/getProductName.php?language_code=en&size=' + size + '&product_name='+encodeURIComponent(data),
				dataType: 'json',
				success : function(json){
					if(json['origin_product_name']){
						$(obj).attr("title",json['origin_product_name']);
						$(obj).find("span").html(json['origin_product_name']);
					}
				}
			});	
		});
		
		$(".opentopcart_details li").each(function(num_details){
			var product_name = $(this).html();
			var size = 30;
			$.ajax({
				url : 'public/php/getProductName.php?product_name='+encodeURI(product_name)+'&language_code=en&size='+size+'&config_translation_approach=3&config_translation_develop_key=&config_google_api_url=com',
				dataType: 'json',
				success : function(json){
					$(".opentopcart_details li").eq(num_details).html(json['origin_product_name']);
					$(".opentopcart_details li").eq(num_details).attr('title',json['origin_product_name']);
				}
			});
		});
		
		$.ajax({
			url : 'index.php?route=taobao/taobao/related_products&product_id=524284968106&cid=50022501&tm=0',
			dataType : 'html',
			beforeSend : function(){
				$('#related_products').append('<div class="loading_related" style="margin:15px auto;text-align:center"><img src="public/image/load5.gif" /></div>');
			},
			complete : function(){
				$('.loading_related').remove();	
			},
			success : function(html){
				$('#related_products').html(html);
			}	
		});
		
					var seller_nick = $('#seller-shop').attr('data-nick');
				
			$.ajax({
				url : 'index.php?route=taobao/taobao/getUser',
				type : 'post',
				data : 'seller_nick=' + seller_nick,
				dataType : 'json',
				success : function(json){
					if(json['seller_link']){
						$('#seller-shop').attr('href', json['seller_link']);
						 
						$('#seller-shop').css('display', 'inline-block');
					}	
				}		
			});
			});
//--></script>

<script><!--
	$(function(){
		$(".prop_li").live("click",function(){
			if(!$(this).hasClass("no_stock")){
				var attr = $(this).attr("data-attr");
				if($(this).hasClass("selected")){
					$(this).parent("ul").prev("input[name='Properties']").val('');
					$(this).parent("ul").siblings("input[name='prop_value[]']").val('');
					$(this).removeClass("selected");
				}else{
					$(this).siblings("li").removeClass("selected");
					$(this).parent("ul").prev("input[name='Properties']").val(attr);
					$(this).parent("ul").siblings("input[name='prop_value[]']").val($(this).attr("data-name"));
					$(this).addClass("selected");
				}
				check_attr(this,attr);
			}
		});	
	
		$('#button-cart').bind('click', function(e,url) {
			if($(this).hasClass("cant_add_cart")){
				return false;
			}
			
			$("#allPro").removeClass("un_select_options");	
			
			$("#close_option_warning").remove();
			
			var stock = parseInt($('#stock').html());
				
			var quantity = parseInt($("input[name='quantity']").val());
			
			if(stock < quantity){
				alert('Out of stock');
				return;
			}
			
			var skuInfo = getSkuInfo('allPro');
			
			if(!skuInfo){
				if(!$("#allPro").hasClass("un_select_options")){
					$("#allPro").addClass("un_select_options");
				}
				
				if(!$("#close_option_warning").length){
					if($("html").attr("class")=="rtl"){
						$("#allPro").prepend('<div id="close_option_warning" style="padding-bottom:5px"><b style="color:red">Please choose product options!</b><a class="close_option_warning" style="float:left;margin-left:10px;">x</a>');
					}else{
						$("#allPro").prepend('<div id="close_option_warning" style="padding-bottom:5px"><b style="color:red">Please choose product options!</b><a class="close_option_warning" style="float:right;margin-right:10px;">x</a>');
					}
				}
				
				return false;
			}
			
			var product_id='524284968106';
			
			var price = skuInfo.special;
			
			if(!price){
				price = skuInfo.price;			
			}
			
			var domestic_shipping_val = $("input[name='domestic_shipping_val']").val();
	
			var param = '';
			
			if($("#allPro").find(".props").length){
				var len = $("#allPro").find(".props").length;
				$("#allPro").find(".props").each(function(num){
					 var prop_val =  $(this).find("input[name='prop_value[]']").val();
					 
					 var prop_text = $(this).find("input[name='prop_text']").val();
					 
					 if(num < len-1){
						param += prop_text + ":" + prop_val + ',';
					 }
					 else{
						param += prop_text + ":" + prop_val;
					 }
				});
			}
			
			if(!param){
				param = 'price:' + price;
			}else{
				param += ',price:' + price;	
			}
			
			param += ',stock:' + stock;
			
			param += ',weight:0.96,bulk:0.00';
			
			param += ',shipping:' + domestic_shipping_val;
			
			param += ",origin_pname:大余空调孔油烟机扩孔器水钻头金刚石水钻钻头墙壁混凝土开孔器";
			
			param += ',pLogo:http://img.alicdn.com/imgextra/i3/2657817772/TB2k73ynpXXXXayXpXXXXXXXXXX_!!2657817772.jpg_80x80.jpg';
			
			param += ',shop_url:https://detail.tmall.com/item.htm?id=524284968106&skuId=' + skuInfo.sku_id;
			
			param += ',seller_name:冰超人五金机电商城';
			
			param += ',cid:50022501';
	
			if($("input[name='remark']").length){
				var remark = $("input[name='remark']").val();
				if(remark.length){
					param += ',remark:' + encodeURIComponent(remark);
				}
			}
			
			var _replace = '';
			
			if($("#replace_props").find(".props").length){
				var len = $("#allPro").find(".props").length;
		
				$("#replace_props").find(".props").each(function(num){
					 var prop_val =  $(this).find("input[name='prop_value[]']").val();
					 
					 var prop_text = $(this).find("input[name='prop_text']").val();
					 
					 if(prop_val){
						_replace += prop_text + " ( Alternative Replacement )" + ":"+prop_val + ',';
					}
				});
			}
		
			if(_replace){
				_replace = _replace.substring(0,_replace.lastIndexOf(","));
				
				param += ',' + _replace;
			}
			
			param = encodeURIComponent(param);
			
			name = 'Large+Yu+air-conditioning+hole+reaming+reamer+water+bit+diamond+drill+bit+wall+concrete+hole+opener';
			
			data = '&product_id=524284968106&quantity=' + quantity + '&type=topSdk' + '&name=' + name + '&param=' + param;
	
			$.ajax({
				url: 'index.php?route=checkout/cart/add',
				type: 'post',
				data: data,
				dataType: 'json',
				success: function(json) {
					$('.success, .warning, .attention, information, .error').remove();
					
					if (json['error']) {
						alert(json['error']);
					} 
					
					if (json['success']) {
						if(typeof(url) != "undefined"){
							location = url;
						}else{
							$('#notification').html('<div class="success" style="text-align:center;font-weight:bold">' + json['success'] + '<img src="catalog/view/theme/default/image/close.png" alt="" class="close" /></div>');
								
							$('.success').fadeIn('slow');
							
							$('#cart-total').html(json['total']);
							
							$('#cart').load('index.php?route=module/cart #cart > *');
							
							$('html, body').animate({ scrollTop: 0 }, 'slow'); 
						}
					}	
				}
			});
		});
		
		$("input[name='quantity']").bind('change',function(){
			getWeightByQuantity();	
		});
		$('.add-up').bind('click',function(){
			getWeightByQuantity();	
		});
		$('.add-down').bind('click',function(){
			getWeightByQuantity();	
		});
		
		$(".close_option_warning").live("click",function(){
			$("#allPro").removeClass("un_select_options");	
			$("#close_option_warning").remove();
		});
	
		$("#closed").live("click",function(){
			$('#weightInfo').slideUp();
		});
	
		$('.show_desc').bind('click' , function(){
			if(!$("#desc").html()){
				var url="http://www.chinaglobalmall.com/index.php?route=taobao/taobao/description";
				
				url = url + '&product_id=524284968106';
				
				$("#tab-description").html('<iframe name="iframe" id="iframe" src="'+url+'" width="100%" onload="Javascript:SetWinHeight()" frameborder="no" scrolling="no"></iframe>');
				
				$("#tab-description").append('<div id="loading_desc_img" style="cursor:pointer;text-align:center" onclick=SetWinHeight()><img src="./public/image/load6.gif" title="Refresh" /></div>');
			}
		});
	});
//--></script>



<script>
	function buy_now(obj){
		if($(obj).find("#button-buy-now").hasClass("cant_add_cart")){
			return false;
		}
		var url = "http://www.chinaglobalmall.com/index.php?route=checkout/cart";
		$("#button-cart").trigger("click",url);
	}
	
	function check_attr(obj , attr){
		var _div = $(obj).parent().parent().parent().parent("div");
		if($(obj).hasClass("selected")){
			if($(_div).attr("id") == "allPro"){
				var objnode = "stock";	
				$("#button-cart").removeClass("cant_add_cart");
				if($("#button-buy-now").length){
					$("#button-buy-now").removeClass("cant_add_cart");
				}
				
				if($(obj).find("img").length){
					var img = $(obj).find("img").attr("src");
					var BImg = img.substring(0,img.lastIndexOf("_"));
					$('.image > .colorbox > img').attr('src',BImg);
					$('.image > .colorbox').attr('href',BImg);
					if($("#image").length){
						var prop_img_id = attr.substring(attr.lastIndexOf(":")+1);
						if(!$("#prop_img"+prop_img_id).length){
							var htm='<a href="'+BImg+'" title="" class="colorbox" data-zoom-image="'+BImg+'" style="display:none" data-image="'+BImg+'"><img src="'+BImg+'" style="width:80px;display:none" id="prop_img'+prop_img_id+'"  title="" alt="" data-zoom-image="'+BImg+'" class="product-image-zoom img-responsive" /></a>';
							$('#image-additional .carousel-inner .item').append(htm);
							$("#image").elevateZoom({gallery:'image-additional', cursor: 'pointer', galleryActiveClass: 'active'}); 
							
						}
						$('#prop_img'+prop_img_id).parent('a').trigger('click');
					}
				}
			}else{
				var objnode = "alter_stock";
			}
			
			var sku_info = getSkuInfo($(_div).attr("id"));
			
			if(sku_info){								
				if($("#"+objnode).length){
					$("#"+objnode).html(sku_info.qty);
				}
				
				if($(_div).attr("id") == 'allPro'){
					$("#allPro").removeClass("un_select_options");	
			
					$("#close_option_warning").remove();
					
					if(sku_info.qty == 0 && objnode=='stock'){
						$("#button-cart").addClass("cant_add_cart");
						$("#button-buy-now").addClass("cant_add_cart");
					}
					
					var price = sku_info.price_text;
							
					var special = sku_info.special_text;
					
					$('.price-info').html(price);
					
					if(special){
						$('.special-info').removeClass('price-hidden');
						$('.special-info').html(special);
						if(!$('.price-info').hasClass('has-special')){
							$('.price-info').addClass('has-special');
						}
					}else{
						if(!$('.special-info').hasClass('price-hidden')){
							$('.special-info').addClass('price-hidden');
						}
						$('.price-info').removeClass('has-special');
					}
					
					getTotal();
				}
			}
		}else{
			$(obj).parent().parent().parent().siblings("dl").each(function(){
				$(this).find("dd > ul > li").removeClass("no_stock");	
			});
			if($(_div).attr("id")=="allPro"){
				$("#button-cart").removeClass("cant_add_cart");
				if($("#button-buy-now").length){
					$("#button-buy-now").removeClass("cant_add_cart");
				}
				$("#stock").html($('input[name=\'stock\']').val());
				
				var price = $('input[name=\'price_text\']').val();
							
				var special = $('input[name=\'special_text\']').val();
				
				$('.price-info').html(price);
				
				if(special){
					$('.special-info').removeClass('price-hidden');
					$('.special-info').html(special);
					if(!$('.price-info').hasClass('has-special')){
						$('.price-info').addClass('has-special');
					}	
				}else{
					if(!$('.special-info').hasClass('price-hidden')){
						$('.special-info').addClass('price-hidden');
					}
					$('.price-info').removeClass('has-special');
				}
				
				getTotal();
			}else{
				$("#alter_stock").html($('input[name=\'stock\']').val());
			}
		}
	}	

	function getTotal(){
		var skuInfo = getSkuInfo("allPro");
		
		if(skuInfo){
			var price = skuInfo.special;
			
			if(!price){
				price = skuInfo.price;			
			}
			
			var domestic_shipping_val = $("input[name='domestic_shipping_val']").val();	//国内运费
		
			var inter_shipping_val = $("#inter_shipping_fee_val").val();		//国际运费
		
			var qty = $("input[name='quantity']").val();

			$.ajax({
				url: 'index.php?route=taobao/taobao/getTotalText&price=' + price + '&domestic_shipping=' + domestic_shipping_val + '&inter_shipping_val=' + inter_shipping_val + '&qty=' + qty + '&cid=50022501',
				type: 'post',
				data: "",
				dataType: 'text',
				beforeSend : function(){
					$("#total_text").append("<img class='loading_shipping' src='public/image/loading.gif' />");	
				},
				complete : function(){
					$("#loading_shipping").remove();	
				},
				success: function(text){
					$("#total_text").html(text);
				}
			});
		}else{
			var html = "<span style='color:#6f6f6f;font-size:14px;font-weight:100'>Please choose product options!</span>"
			$("#total_text").html(html);
		}
	}
	
	function getSkuInfo(id){
		var data = false;
		
		if($("#allPro").find(".props").length){					//如果商品有属性
			var prop_combine = new Array();
		
			var prop_strs = '';
			
			for(var l=0;l<$('#' + id).find("input[name='Properties']").length;l++){
				prop_combine[l] = $('#' + id).find("input[name='Properties']").eq(l).val();
			}
			
			var all_selected = true;
			
			for(var u=0;u<prop_combine.length;u++){
				if(prop_combine[u]!=""){
					if(u != prop_combine.length-1){
						prop_strs +=prop_combine[u]+";";
					}
					else{
						prop_strs +=prop_combine[u];
					}
				}else{
					all_selected = false;	
				}
			}
			
			var sku_id = false;
			
			if($("input[name='option_combine_to_sku_id[" + prop_strs + "]']").length){
				sku_id = $("input[name='option_combine_to_sku_id[" + prop_strs + "]']").val();
			}
			
			if(sku_id){
				data = new Array();
				
				data['sku_id'] = sku_id;
			
				var price = $('.sku_info_' + sku_id).attr('data-price');
				
				var special = false;
				
				if($('.sku_special_info_' + sku_id).length){
					special = $('.sku_special_info_' + sku_id).attr('data-special');	
				}
				
				var qty = $('.sku_info_' + sku_id).attr('data-stock');
							
				if($('.sku_stock_info_' + sku_id).length){
					qty = $('.sku_stock_info_' + sku_id).val();
				}

				var price_text = $(".sku_info_" + sku_id).attr('data-price-text');
				
				var special_text = '';
				
				if($(".sku_special_info_" + sku_id).length){	
					special_text = $(".sku_special_info_" + sku_id).attr('data-special-text');
				}
				
				data['price'] = price;
				
				data['special'] = special;
				
				data['qty'] = parseInt(qty);
				
				data['price_text'] = price_text;
				
				data['special_text'] = special_text;
			}else if(all_selected){
				data = new Array();
				
				data['sku_id'] = sku_id;
			
				data['price'] = '24.38';
				
				data['special'] = false;
				
				data['qty'] = 0;
				
				data['price_text'] = $('input[name=\'price_text\']').val();
				
				data['special_text'] = '';
				
				return data;
			}
		}else{
			var price = '24.38';
			
			var special = false;
			
			if($('.sku_special_info_def').length){	
				special = $('.sku_special_info_def').attr('data-special');
			}
			
			var qty = $('input[name=\'stock\']').val();
			
			if($('.sku_stock_info_def').length){	
				qty = $('.sku_stock_info_def').val();
			}
			
			var price_text = $('input[name=\'price_text\']').val();
			
			var special_text = '';
				
			if($(".sku_special_info_def").length){	
				special_text = $(".sku_special_info_def").attr('data-special-text');
			}
			
			data = new Array();
			
			data['sku_id'] = false;
			
			data['price'] = price;
			
			data['special'] = special;
			
			data['qty'] = parseInt(qty);
			
			data['price_text'] = price_text;
			
			data['special_text'] = special_text;	
		}
		
		return data;
	}
	
	function getShipping(){
		var country_id = $("#country_id").val();
		
		var shipping_id = $("#shipping_id").val();

		if($("input[name='quantity']").length){
			var quantity = $("input[name='quantity']").val();
		}else{
			var quantity = 1;	
		}
		
		if(country_id && shipping_id){
			$.ajax({
				url : 'index.php?route=taobao/taobao/getShipping&shipping_id=' + shipping_id + '&country_id=' + country_id + '&weight=0.96&bulk=0.00&quantity=' + quantity + '&no_estimate_weight=',
				dataType : 'json',
				beforeSend : function(){
					$("#shipping_fee").html('<img src="./public/image/loading.gif" id="shipping_fee_loading" />');
				},
				complate : function(){
					$("#shipping_fee_loading").remove();
				},
				success : function(json){
					$("#shipping_info").html(json['shipping_html']);
					 $("#inter_shipping_fee_val").val(json['shipping_fee']);
					 getTotal();
				}
			});
		}else{
			getTotal();
		}
	}
	
	function getWeightByQuantity(){
		var num = $("input[name='quantity']").val();
		
		num = parseInt(num);
		
		var weight = Number('0.96');
		
		weight = weight * num;
		
		$("#otcweight").html(weight.toFixed(2));
		
		getShipping();
	}
	
	function getWeightInfo(){
		$('#weightInfo').slideDown();
	}
	
	function SetWinHeight() { 
		var pTar = null; 
		var down="iframe";
		if (document.getElementById){ 
			pTar = document.getElementById(down); 
		} 
		else{ 
			eval('pTar = ' + down + ';'); 
		} 
		if (pTar && !window.opera){ 
		//begin resizing iframe 
			pTar.style.display="block" 
			if (pTar.contentDocument && pTar.contentDocument.body.offsetHeight){ 
				//ns6 syntax 
				pTar.height = pTar.contentDocument.body.offsetHeight+100;
				pTar.contentDocument.body.style.minHeight="960px"; 
				
				//pTar.width = pTar.contentDocument.body.scrollWidth+20; 
			} 
			else if (pTar.Document && pTar.Document.body.scrollHeight){ 
			//ie5+ syntax 
				pTar.Document.body.style.minHeight="960px"; 
				pTar.height = pTar.Document.body.scrollHeight; 
				//pTar.width = pTar.Document.body.scrollWidth; 
			} 
		}
		$("#loading_desc_img").css("textAlign","right"); 
		$("#loading_desc_img > img").attr("src","./public/image/refresh.gif");
		$("#loading_desc_img > img").css("width","50px"); 
		$("#loading_desc_img > img").css("height","50px");
	} 
</script>


