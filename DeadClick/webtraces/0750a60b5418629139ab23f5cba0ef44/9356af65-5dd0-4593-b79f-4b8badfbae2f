		$(window).scroll(function(){
			scrolledPos = $(this).scrollTop();
			//console.log(scrolledPos);
			if(scrolledPos >= refinePos-20){
				$('.blog-refine').addClass('sticky');
				$('.blog-refine .scotty').fadeIn();
