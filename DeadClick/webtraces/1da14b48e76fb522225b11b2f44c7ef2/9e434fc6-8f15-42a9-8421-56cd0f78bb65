// Determines the content context:
//      previewUser = couchDB that will read, external or CMS user personal couchDB
//      edition = international or domestic homepage
var getContentContext = function getContentContext(staticContext) {
    var edition = 'www';
    var previewUser = void 0;

    // client side rendering
    if (typeof window !== 'undefined') {
        // a CMS user requested the international page like money.com/?edition=international
        var editionParamExists = Object(__WEBPACK_IMPORTED_MODULE_0_util_queryParams__["a" /* getQueryParam */])('edition');
        if (editionParamExists) {
            edition = editionParamExists === 'international' ? 'international' : 'domestic';
        } else {
            // determines the edition base on the cookie
            edition = Object(__WEBPACK_IMPORTED_MODULE_1_util_cookies__["a" /* getCookie */])('SelectedEdition') === 'edition' ? 'international' : 'domestic';
        }
        previewUser = Object(__WEBPACK_IMPORTED_MODULE_0_util_queryParams__["a" /* getQueryParam */])('user');
    } else {
        // if server side rendering
        edition = staticContext.edition === 'edition' ? 'international' : 'domestic';
        previewUser = staticContext.previewUser;
    }

    return {
        edition: Object(__WEBPACK_IMPORTED_MODULE_0_util_queryParams__["b" /* sanitizeQueryParam */])(edition),
        previewUser: Object(__WEBPACK_IMPORTED_MODULE_0_util_queryParams__["b" /* sanitizeQueryParam */])(previewUser)
    };
};
