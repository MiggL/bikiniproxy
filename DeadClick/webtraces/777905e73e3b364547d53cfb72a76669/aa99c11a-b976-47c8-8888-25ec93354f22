        <script>
            jQuery(document).ready(function () {
	        jQuery('.carousel-inner .item:first-child').addClass('active');
                jQuery('#footer-timeline .layer-timeline:first-child').addClass('is-expanded');
                jQuery('.bxslider').bxSlider({
                    minSlides: 3,
                    maxSlides: 4,
                    slideWidth: 500,
                    slideMargin: 10,
                    nextText: '>',
                    prevText: '<',
                    useCSS: false
                });
                jQuery('.videos').click(function(event) {
					var href = jQuery(this).attr('href');
					var url = jQuery(this).find("span.permalink").html();
					jQuery.fancybox({
						openEffect  : 'none',
						closeEffect : 'none',
						href : href,
						helpers : {
							media : {},
							overlay: {
								locked: true
							}
						}
					});
					jQuery.ajax({
						type: "GET",
						cache : false,
						url: url
					});
					
					
					event.preventDefault();
					
                });
                jQuery('.images-popup').click(function(event) {
                  var href = jQuery(this).attr('href');
                  jQuery.fancybox({
                    openEffect  : 'none',
                    closeEffect : 'none',
                    href : href,
                    helpers : {
                        media : {},
                        overlay: {
                            locked: true
                        }
                    }
                  });
                  event.preventDefault();
                });

                var $container = jQuery('#footer-timeline');
                $container.isotope({
                    masonry: {
                        columnWidth: 100
                    }
                });
                $container.on( 'click', '.item-content', function() {
                    jQuery( this ).parent('.layer-timeline').toggleClass('is-expanded');
                    $container.isotope('layout');
                });

            });

