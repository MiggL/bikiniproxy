    template: '\n      <button \n        theme="primary" \n        ng-click="direction()"\n        ng-mouseenter="$ctrl.$dfrCarousel.whichSideIsHovering(dir)"\n        ng-mouseleave="$ctrl.$dfrCarousel.whichSideIsHovering(null)"\n        aria-label="{{dir == \'next\' ? \'Next item\' : \'Prev item\'}}"\n        tabindex="-1"\n        ng-disabled="disabled" category="Carousel" action="Button click" \n        label="{{label}}">\n          <dfr-svg icon="{{dir == \'next\' ? \'chevron-right\' : \'chevron-left\'}}" class="stroked" size="{{$ctrl.$dfrCarousel.bp.bpSm ? 13 : $ctrl.$dfrCarousel.bp.bpMd ? 16 : 20 }}"></dfr-svg>\n      </button>\n    ',
    require: {
        '$dfrCarousel': '^dfrCarousel'
    },
    transclude: true,
    controller: ['$scope', '$element', '$attrs', '$transclude', '$locale', function controller($scope, $element, $attrs, $transclude, $locale) {
        'ngInject';

        var $carousel = this;

        $element.addClass('dfr-carousel-control');

        this.$onInit = function () {
            var isNext = angular.lowercase($element[0].tagName) == 'dfr-carousel-next';
            var isPrev = angular.lowercase($element[0].tagName) == 'dfr-carousel-prev';
            if (isNext) $element.addClass('dfr-carousel-next');
            if (isPrev) $element.addClass('dfr-carousel-prev');
            $scope.direction = isNext ? $carousel.$dfrCarousel.next : isPrev ? $carousel.$dfrCarousel.prev : null;
            if ($scope.direction === null) return;
            $scope.dir = isNext ? 'next' : 'prev';
            $scope.label = $element[0].getAttribute("modelId") + '-' + (isNext ? 'Next item' : 'Previous item');
            $transclude($scope, function (clone, scope) {
                $element.append(clone);
            });
        };
    }]
};

