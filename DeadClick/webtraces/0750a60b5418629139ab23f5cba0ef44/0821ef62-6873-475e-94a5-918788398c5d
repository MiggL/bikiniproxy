_satellite.pushAsyncScript(function(event, target, $variables){
    $(document).on('click','#paginationContent a',function(){
var trainingClassId = $(this).parent().parent().find("b").text();
var position = $('#paginationContent a').index($(this));
position = position+1;
dmf.pushData('searchResultClick',{'searchResultName':'training class search', 'positionCliked':position , 'trainingClassId':trainingClassId});

})
});
