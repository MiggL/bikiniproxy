(lib.playIcon = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_19 = function() {
		/* Stop a Movie Clip
		Stops the specified movie clip on stage.
		
		Instructions:
		1. Use this code for movie clips that are currently playing.
		*/
		
		this.stop();
	}

	// actions tween:
	this.timeline.addTween(cjs.Tween.get(this).wait(19).call(this.frame_19).wait(1));

	// btnPanel (mask)
	var mask = new cjs.Shape();
	mask._off = true;
	mask.graphics.p("Aj+DDQg8AAAAg8IAAkNQAAg8A8AAIH9AAQA8AAAAA8IAAENQAAA8g8AAg");
	mask.setTransform(31.5,19.5);

	// arrow
	this.playArrow = new lib.playArrow();
	this.playArrow.parent = this;
	this.playArrow.setTransform(-12.5,20.1,1,1,0,0,0,9,10.3);
	this.playArrow._off = true;

	var maskedShapeInstanceList = [this.playArrow];

	for(var shapedInstanceItr = 0; shapedInstanceItr < maskedShapeInstanceList.length; shapedInstanceItr++) {
		maskedShapeInstanceList[shapedInstanceItr].mask = mask;
	}

	this.timeline.addTween(cjs.Tween.get(this.playArrow).wait(5).to({_off:false},0).to({x:32.5},14,cjs.Ease.get(1)).wait(1));

	// Layer 1
	this.shape = new cjs.Shape();
	this.shape.graphics.f("#FFFFFF").s().p("Aj+DDQg8AAAAg8IAAkNQAAg8A8AAIH9AAQA8AAAAA8IAAENQAAA8g8AAg");
	this.shape.setTransform(31.5,19.5);

	this.timeline.addTween(cjs.Tween.get(this.shape).wait(20));

}).prototype = p = new cjs.MovieClip();
p.nominalBounds = new cjs.Rectangle(0,0,63,39);


