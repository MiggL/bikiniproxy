		<script type="text/javascript">
			$(document).ready(function () {
				setWidgetServiceClicking($("#widgetSiteToolsHeader7571544c-357b-4a0a-a6dd-0fd6e85dd6fe"), true, function () {
					var $widgetBody = $('#widgetSiteToolsBody7571544c-357b-4a0a-a6dd-0fd6e85dd6fe');

					if ($widgetBody.is(':visible')) {
						var menuItems = $('.widgetItem a', $widgetBody);

						$(menuItems[0]).focus();

						$('.widgetItem', $widgetBody).unbind("keydown").bind("keydown", function (e) {

							if (e.which === 9) {
								var index = menuItems.index(e.target);

								if ((index === 0 && e.shiftKey) || (index === (menuItems.length - 1) && !e.shiftKey)) {
									$("#widgetSiteToolsHeader7571544c-357b-4a0a-a6dd-0fd6e85dd6fe").trigger('click').focus();
								}
							}
						});
					} else {
						$('.widgetItem', $widgetBody).unbind("keydown");
					}
				});
			});
		</script>
