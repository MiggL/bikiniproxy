<script>
$(document).ready(function(){
    $(".TrimarkViewButton").click(function(){
        $(".TrimarkColorPanel-hide").toggle();
        if (!$(this).hasClass('less')) {
            $(this).html('View Less <span><img src="../images/viewless-arrow.png" class="trimark-viewless-arrow"/></span>');
            $(this).addClass('less');
        }
        else {
            $(this).html('View More <span><img src="../images/viewmore-arrow.png" class="trimark-viewmore-arrow"/></span>');
            $(this).removeClass('less');
        }
    });
});    
    
$(document).ready(function() {
    var owl = $(".owl-demo");
     $(owl).each(function(index, element){
     			 $(element).owlCarousel({
        		rewindNav : false,	
						pagination : false,        
        		items : 4,
            afterAction: function(){
      if ( this.itemsAmount > this.visibleItems.length ) {
        $(element).parent().find('.next').show();
        $(element).parent().find('.prev').show();

        $(element).parent().find('.next').removeClass('disabled');
        $(element).parent().find('.prev').removeClass('disabled');
        if ( this.currentItem == 0 ) {
          $(element).parent().find('.prev').addClass('disabled');
        }
        if ( this.currentItem == this.maximumItem ) {
          $(element).parent().find('.next').addClass('disabled');
        }

      } else {
        $(element).parent().find('.next').hide();
        $(element).parent().find('.prev').hide();
      }
    }
    });
     
    // Custom Navigation Events
    $(element).parent().find(".next").click(function(){
    	$(element).trigger('owl.next');
    });
    $(element).parent().find(".prev").click(function(){
    	$(element).trigger('owl.prev');
    });
  });
     
});
</script>
        
