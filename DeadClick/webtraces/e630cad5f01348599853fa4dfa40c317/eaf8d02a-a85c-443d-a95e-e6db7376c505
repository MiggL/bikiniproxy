<!--HEADER ANIMATION SCRIPTS-->

<script type="text/javascript">
var t;
var timer_is_on=0;

function timedCount() { 
    var frames = document.getElementById("meerkatAnimation").children;
    var frameCount = frames.length;
    var i = 0;
    var t = setInterval(function () { 
        if(i==34) { 
    		return; //stop the execution of function
		}
		else {
		frames[i % frameCount].style.display = "none";
        frames[++i % frameCount].style.display = "block";
		}
    }, 100);
}

function timedHawk() { 
    var frames = document.getElementById("hawkAnimation").children;
    var frameCount = frames.length;
    var i = 0;
    var t = setInterval(function () { 
        if(i==54) { 
    		return; //stop the execution of function
		}
		else {
        frames[i % frameCount].style.display = "none";
        frames[++i % frameCount].style.display = "block";
		}
    }, 100);
}

function doTimer()
{
if (!timer_is_on)
  {
  timer_is_on=0;
  timedCount();
  }
}

function doHawk()
{
if (!timer_is_on)
  {
  timer_is_on=0;
  timedHawk();
  }
}

function stopCount()
{
clearInterval(t);
}
</script>
<!--END HEADER ANIMATION SCRIPTS-->


