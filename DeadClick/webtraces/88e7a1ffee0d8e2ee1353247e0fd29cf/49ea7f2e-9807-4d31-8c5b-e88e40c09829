    jQuery(document).ready(function(){
        switch ("GBP")
        {
            case "AUD":
                jQuery.post("/checkout/cart/estimatePost/",{ country_id: "AU", estimate_postcode: "-" });
                break;
            case "CAD":
                jQuery.post("/checkout/cart/estimatePost/",{ country_id: "CA", region_id: "66", estimate_postcode: "-" });
                break;
            case "NZD":
                jQuery.post("/checkout/cart/estimatePost/",{ country_id: "NZ", estimate_postcode: "-" });
                break;
            case "USD":
                jQuery.post("/checkout/cart/estimatePost/",{ country_id: "US", region_id: "12", estimate_postcode: "-" });
                break;
            default:
                jQuery.post("/checkout/cart/estimatePost/",{ country_id: "GB" });
                break;
        }
    });
</script>
