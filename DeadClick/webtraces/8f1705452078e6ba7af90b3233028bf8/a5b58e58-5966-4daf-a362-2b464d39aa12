      <script>
        (function () {
          var $container = $('#page-nav');

          // helper function: toggle active class on element and next element
          function toggleNext($el) {
            var height = 0;
            window.setTimeout(function () {
              $el.toggleClass('is-active');
              $el.next().toggleClass('is-active');
              // adjust min-height of dropdown to account for absolutely-positioned elements
              $('.page-nav__dropdown').css('minHeight', 0);
              $('.page-nav__dropdown.is-active ul.is-active').each(function (i, ul) {
                // get a fresh, non-absolute calculation
                ul.style.position = 'static';
                ul.style.width = '100%';
                height = Math.max(height, $(ul).height() + 40);  // add 40px for padding
                ul.style.position = '';
                ul.style.width = '';
              });
              $('.page-nav__dropdown.is-active').css('minHeight', height + 'px');
            }, 10);
          };

          // add main mobile toggle
          $('.page-nav__toggle').on('click', function (ev) {
            ev.preventDefault();
            $(this).add('.page-nav__1').toggleClass('is-active');
          });

          // add primary toggles
          $container.find('.page-nav__2-toggle').each(function (i, toggle) {
            $(toggle).on('click', function (ev) {
              ev.preventDefault();
              toggleNext($(toggle));
            });
          });

          // add deeper toggles
          $('.page-nav__drilldown-toggle').on('click', function (ev) {
            ev.preventDefault();
            toggleNext($(this));
          });

          // clicks affect nav parts even when not in those parts
          document.body.addEventListener('click', function (ev) {
            if ($(ev.target).parents('.page-nav').length > 0) {
              // we're in the main nav, so only remove active on a subset of elements
              $(ev.target).parents('ul').first().find('.is-active').each(function (i, activeEl) {
                $(activeEl).removeClass('is-active');
              })
            } else {
              // we're not in the nav, so close all of it
              $container.find('.is-active').each(function (i, activeEl) {
                $(activeEl).removeClass('is-active');
              });
            }
          });

          $container.removeClass('no-js');
          $container.addClass('js');
        })();
      </script>
    </div>
  </div>
</div>

