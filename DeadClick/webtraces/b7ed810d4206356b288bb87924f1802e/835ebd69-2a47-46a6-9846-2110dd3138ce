<script>
    $(document).ready(function () {
        $('#product-name').on('keyup', function () {
            var key = $('#product-name').val();
            if (key != undefined && key != "" && key.length > 3) {
                $('#results').html("");
                var results = ""
                $.get("/api/Sitecore/HowToBuy/GetSuggestions?keyword=" + key, function (data, status) {
                    //console.log(data);
                    if (data.length > 0) {
                        var search_regexp = new RegExp(key, "g");
                        for (var i = 0; i < data.length; i++) {

                            results += "<li><a href='" + data[i].Url.replace(/^.*\/\/[^\/]+/, '') + "'>" + (data[i].Name.length > 60 ? data[i].Name.replace(search_regexp, "<b>" + key + "</b>").substring(0, 60) + "..." : data[i].Name.replace(search_regexp, "<b>" + key + "</b>")) + " - " + data[i].SKU + "</a></li>";
                        }
                        $('.auto_complete').show();
                    }
                    else {
                        $('.auto_complete').hide();
                    }
                    $('#results').html(results);

                });
            }
            else {
                $('.auto_complete').hide();
            }

        });

        $('#results li').on('click', function () {
            alert($(this).html());
        });
    });

</script>
