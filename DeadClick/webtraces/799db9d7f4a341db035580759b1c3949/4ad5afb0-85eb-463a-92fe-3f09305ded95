_satellite.pushAsyncScript(function(event, target, $variables){
  /* PDP Review Smart Content Banner -- mbox review pdp*/
// Require lib.min.js (jquery) before creating the mbox
head.load(app.urls.LibMinStaticUrl, function(){
  var mboxDiv = document.createElement('div');
  mboxDiv.id = 'pdp-review-smcontent';
  $('#reviews').prepend(mboxDiv);
  mboxDefine('pdp-review-smcontent' ,'PDPReviewSCBanner');
  mboxUpdate('PDPReviewSCBanner');
});
});
