$(document).ready(function(){$('body').on('hidden.bs.modal','.modal',function(){$(this).removeData('bs.modal');});$('#main-nav').affix({offset:{top:$('header').height()}});$('.maps').click(function(){$('.maps iframe').css("pointer-events","auto");});function offsetAnchor(){if(location.hash.length!==0){window.scrollTo(window.scrollX,window.scrollY-100);}}
$(window).on("hashchange",function(){offsetAnchor();});window.setTimeout(function(){offsetAnchor();},2);});