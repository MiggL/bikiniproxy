  /* Permalink - http://colorzilla.com/gradient-editor/#d1d1da+0,dadae4+100 */
  background: #d1d1da;
  /* Old browsers */
  background: -moz-linear-gradient(-45deg, #d1d1da 0%, #dadae4 100%);
  /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right bottom, color-stop(0%, #d1d1da), color-stop(100%, #dadae4));
  /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(-45deg, #d1d1da 0%, #dadae4 100%);
  /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(-45deg, #d1d1da 0%, #dadae4 100%);
  /* Opera 11.10+ */
  background: -ms-linear-gradient(-45deg, #d1d1da 0%, #dadae4 100%);
  /* IE10+ */
  background: linear-gradient(135deg, #d1d1da 0%, #dadae4 100%);
  /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d1d1da', endColorstr='#dadae4',GradientType=1 );
  /* IE6-9 fallback on horizontal gradient */
}
