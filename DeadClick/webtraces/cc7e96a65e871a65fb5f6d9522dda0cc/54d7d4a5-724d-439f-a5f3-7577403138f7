<script type="text/javascript">


jQuery(document).ready(function($) {
  $(document).ready(function(){

    $('.listsong').hide();
    $('.datecquoison').click(function() {


        var souscontent = $(this).next();
        var h2content = souscontent.prev();

      if(souscontent.is(':hidden') == true) {
        souscontent.slideDown();
        h2content.addClass("open");
      } else {
        souscontent.slideUp();
        if( h2content.hasClass("open") == true){
          h2content.removeClass("open");
        }
      }

    });
    

   });

 });



</script>



    
