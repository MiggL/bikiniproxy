<div style="margin-top:5px">Delivery: <span style="display:inline-block" id="shipping_desc">Bill me final accurate shipping cost later</span></div>
<script>
	$("input[name='search_country']").bind("input propertychange",function(){
		var str = $(this).val();
		str = str.toLowerCase();
		$("#country_info .no_result").css({"display":"none"});
		$(".country_li").removeClass("li_show");
		$(".country_li").removeClass("li_hide");
		if(str!=""){
			var sr = false;
			$(".country_li").each(function(){
				var data = $(this).attr("data");
				if(data.indexOf(str)== 0){
					$(this).addClass("li_show");
					sr = true;
				}else{
					$(this).addClass("li_hide");
				}
			});
		}else{
			$(".country_li").addClass("li_show");
			sr = true;
		}
		if(!sr){
			$("#country_info .no_result").css({"display":""});
		}
	});
	
	$("input[name='search_country']").keypress(function(e){
		var e = e || event;
		switch(e.keyCode){					
			case 38:				//up
				
			case 40:				//down
				
			default:	
				return ;
		}	
	});
	
	$(".select_top_country").bind("click",function(){
		if(!$(this).hasClass("selected_top")){
			$(this).addClass("selected_top");
			if(!$("#dropdown-menu-country").hasClass("dropdown_menu_show")){
				$("#dropdown-menu-country").addClass("dropdown_menu_show");
			}
			
			$(".select_top_shipping").removeClass("selected_top");
			$("#dropdown-menu-shipping").removeClass("dropdown_menu_show");
		
		}else{
			$(this).removeClass("selected_top");
			if($("#dropdown-menu-country").hasClass("dropdown_menu_show")){
				$("#dropdown-menu-country").removeClass("dropdown_menu_show");
			}
		}	
	});
	$(".country_li").bind("mouseover",function(){
		$(this).addClass("li_hover");
	});
	$(".country_li").bind("mouseout",function(){
		$(this).removeClass("li_hover");	
	});
	$(".country_li").bind("click",function(){
		$(".country_li").removeClass("li_selected");
		$(this).addClass("li_selected");
		var country_id = $(this).attr("country_id");
		var country_html = $(this).find("a").html();	
		country_html +='<span class="caret"></span>';
		$(".select_top_country").html(country_html);
		$("#dropdown-menu-country").removeClass("dropdown_menu_show");
		$(".select_top_country").removeClass("selected_top");
		$("#country_id").val(country_id);
		getShipping();
	});
	
	$("input[name='search_shipping']").bind("input propertychange",function(){
		var str = $(this).val();
		str = str.toLowerCase();
		$("#shipping_method_info .no_result").css({"display":"none"});
		$(".shipping_li").removeClass("li_show");
		$(".shipping_li").removeClass("li_hide");
		if(str!=""){
			var sr = false;
			$(".shipping_li").each(function(){
				var data = $(this).attr("data");
				if(data.indexOf(str)== 0){
					$(this).addClass("li_show");
					sr = true;
				}else{
					$(this).addClass("li_hide");
				}
			});
		}else{
			$(".shipping_li").addClass("li_show");
			sr = true;
		}
		if(!sr){
			$("#shipping_method_info .no_result").css({"display":""});
		}
	});
	
	$(".select_top_shipping").bind("click",function(){
		if(!$(this).hasClass("selected_top")){
			$(this).addClass("selected_top");
			if(!$("#dropdown-menu-shipping").hasClass("dropdown_menu_show")){
				$("#dropdown-menu-shipping").addClass("dropdown_menu_show");
			}
			$(".select_top_country").removeClass("selected_top");
			$("#dropdown-menu-country").removeClass("dropdown_menu_show");
		}else{
			$(this).removeClass("selected_top");
			if($("#dropdown-menu-shipping").hasClass("dropdown_menu_show")){
				$("#dropdown-menu-shipping").removeClass("dropdown_menu_show");
			}
		}	
	});
	$(".shipping_li").bind("mouseover",function(){
		$(this).addClass("li_hover");
	});
	$(".shipping_li").bind("mouseout",function(){
		$(this).removeClass("li_hover");	
	});
	$(".shipping_li").bind("click",function(){
		$(".shipping_li").removeClass("li_selected");
		$(this).addClass("li_selected");
		var shipping_id = $(this).attr("shipping_id");
		var shipping_html = $(this).find("a").html();
		var shipping_desc = $(this).attr("shipping_desc");	
		shipping_html +='<span class="caret"></span>';
		$(".select_top_shipping").html(shipping_html);
		$("#dropdown-menu-shipping").removeClass("dropdown_menu_show");
		$(".select_top_shipping").removeClass("selected_top");
		$("#shipping_desc").html(shipping_desc);
		$("#shipping_id").val(shipping_id);
		getShipping();
	});
</script>

<script>
	document.body.onclick=function(e){
		if(!e){
			var obj = event.srcElement;
		}
		else{
			var obj = e.target;	
		}
		var len = $(obj).parents("#shipping_info").length;
		if(!len){
			$(".select_top_country").removeClass("selected_top");
			$(".select_top_shipping").removeClass("selected_top");
			$("#dropdown-menu-shipping").removeClass("dropdown_menu_show");
			$("#dropdown-menu-country").removeClass("dropdown_menu_show");
		}
		
	}
</script>                    </div>
                    
                                        	                        
                                                	<div style="margin:10px 0">
                            	Weight:                                  ≈                             	<span id="otcweight">0.96</span>&nbsp;kg
