<script type="text/javascript">
(function (window, document, $, bam) {
	var CalendarCarousel = bam.widget.CalendarCarousel;
	// bind callback function to carousel
	CalendarCarousel.bind('onChange', function (evt, date) {
		var date = bam.datetime.formatDate(date, 'yyyy/MM/dd');
		location.href = '?c_id=hou&date=' + date;
	});
	var str = '2018/02/15'.replace(/^(\d{4})\/(\d{2})\/(\d{2})$/, '$2/$3/$1')
	CalendarCarousel.init('ppCalendarCarousel', new Date(str));
})(this, this.document, this.jQuery, this.bam);
</script>

