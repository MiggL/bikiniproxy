	/*google map on the sales detail page*/
	if($('#map-canvas').length > 0){
       	var address = 'x';
		var latLon = '';
		
		if($('#map-canvas').attr('data-address'))
			address = $('#map-canvas').attr('data-address');
					
		if($('#map-canvas').attr('data-latLong'))
			latLon = $('#map-canvas').attr('data-latLong');
			
		var storeGeo = new google.maps.Geocoder();
		var latitude;
		var longitude;
		var latLong;
		var mapOptions;
		var map;
		var marker;
        var service;

		function storeMap() {
		
			
			storeGeo.geocode( { 'address': address}, function(results, status) {
				if (status == google.maps.GeocoderStatus.OK) {
					
					if(latLon != '') {
						var latLonArray = latLon.split(",");
						latitude = latLonArray[0];
						longitude = latLonArray[1];
					}
					else {
						latitude = results[0].geometry.location.lat();
						longitude = results[0].geometry.location.lng();
					}
					latLong = new google.maps.LatLng(latitude, longitude);
					/*
if(latLong && latLong != '')
						latLong = new google.maps.LatLng(lat, lon);
*/
					mapOptions = {
						zoom: 12,
						center: latLong,
						mapTypeId: google.maps.MapTypeId.ROADMAP,
						scrollwheel: false
					  }
					  map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);
					  var mapIcon = '/etc/designs/trane/commercial/clientlib/images/map_marker.png';
					  var contentString = '<a href="https://maps.google.com/maps?z=12&t=m&saddr='+address+'" >Get Directions</a>';
					  var infowindow = new google.maps.InfoWindow({
						content: contentString,
						maxWidth: 300
					});
					  marker = new google.maps.Marker({
						  position: latLong,
						  map: map,
						  title: 'Hello World!',
						  optimized: false,
						  icon: mapIcon
					  });
					  /*if ($(window).width() < 768) {
						 map.panBy(-100,-50);	
			   		   }else{
						 map.panBy(-240,-50);
					   }*/
					  google.maps.event.addListener(marker, 'click', function() {
					 infowindow.open(map,marker);


					});

                    service = new google.maps.places.PlacesService(map);
					getStoreStatus(service);	
				}
			});



		} 

        function getStoreStatus(service) { 
        var placeId = document.getElementById('googlePlaceId').value;
       	var request = {
    		placeId: placeId
  		};
        service.getDetails(request,function(place, status) {
            	if (status === google.maps.places.PlacesServiceStatus.OK) {
					if(place.opening_hours.open_now){
                		document.getElementById("storeStatus").value = "Open Now";
                      }else{
						document.getElementById("storeStatus").value = "Currently Closed";
                        document.getElementById("storeStatus").style.color = "#f15c41";
                      }
                    }
  			});
		}

		storeMap();
		$('.info-link').parent().removeAttr('style');
		
		// var mapLoader = 0;
		// $(window).resize(function(){
		// 	if($('.story-overview').css("float") == "none" && mapLoader == 0){
		// 		storeMap();
		// 		mapLoader = 1;
		// 	}
		// 	if($('.story-overview').css("float") == "left" && mapLoader == 1){
		// 		storeMap();
		// 		mapLoader = 0;
		// 	}
		// });
		
		/*google.maps.event.addDomListener(window, 'resize', function() {
			
			map.setCenter(latLong);
			if ($(window).width() < 768) {
				map.panBy(-100,-50);	
			}else{
				map.panBy(-240,-50);
			}
		}); */

	}

	
	
	/*drawings search validation*/
	function checkSelect(){
		$('#drawings-search select').each(function(){
			var $this = $(this);
			var nextOption = $this.parent().next().find('select');
			if ($this.val() == "") {
				nextOption.prop('disabled',true).val('').parent().addClass('off');
			}else{
				nextOption.prop('disabled',false).parent().removeClass('off');
			};
			$('#drawings-search select').each(function(){
				var empty = false;
				if ($(this).val() == "") {
					empty = true;
				}
				if (empty) {
					$('#drawings-search input[type="submit"]').attr('disabled', 'disabled');
				}else{
					$('#drawings-search input[type="submit"]').removeAttr('disabled');
				}

			});
		})
	};
	$('#drawings-search select').change(function(){
		checkSelect();
	});

	if($('#drawings-terms').length > 0){
		$('#terms-agree').click(function(e){
			e.preventDefault();
			$('#drawings-terms').animate({'height':0},function(){
				$(this).remove();
				$('#drawings-search-container').fadeIn();
			});
		});
	}
	
	/*add classes to tables on product-details*/
	$('.product-detail table').each(function(){
		var tableMobile = 0;
		var windowTable = $(window).width();
		var currentTable = $(this);
		currentTable.find('tr:nth-child(2n)').addClass('even-row');
		if(windowTable < 960) {
			tableMobile = 1;
			currentTable.find('thead th').each(function(){
				var tableHead = $(this).text();
				var tableHeadIndex = $(this).index();
				currentTable.find('tr').each(function(){
					$(this).find('td:eq('+tableHeadIndex+')').prepend('<span class="mobile-col">'+tableHead+'</span>');
				});
			});
		}
		$(window).resize(function(){
			var newWindowTable = $(window).width();
			if(newWindowTable < 960 && tableMobile == 0){
				tableMobile = 1;
				currentTable.find('thead th').each(function(){
					var tableHead = $(this).text();
					var tableHeadIndex = $(this).index();
					currentTable.find('tr').each(function(){
						$(this).find('td:eq('+tableHeadIndex+')').prepend('<span class="mobile-col">'+tableHead+'</span>');
					});
				});
			}
			if(newWindowTable >= 960){
				tableMobile = 0;
				$('.mobile-col').remove();
			}
		});
	});
	
	/*newsroom and blog landing share slider*/
	if($('.share-box-icons').length > 0){
		$('.share-box').click(function(e){
			e.preventDefault();
		});
		$('.share-box').toggle(function(){
			if ($(window).width() <= 340) {
				$('.share-box-icons').stop().animate({'bottom':-1});
			}else{
				$('.share-box-icons').stop().animate({'bottom':0});
			}
			$(this).addClass('active').html('Share This &ndash;');
		},function(){
			$('.share-box-icons').stop().animate({'bottom':-36});
			$(this).removeClass('active').html('Share This +');
		});
	}
	
	/*scotty for the blog*/
	if($('.blog-refine').length > 0){
		$('.scotty').click(function(e){
			e.preventDefault();
			var scottyScrollTo = $('.left.post').offset().top;
			$('body,html').animate({scrollTop:scottyScrollTo-40});
		});
	}
	
	/*animate open and close for video in post templates*/
	if($('.video').length > 0){
		$('.video .close').click(function(e){
			e.preventDefault();
			var closeBtn = $(this);
			var $video = $('.video');
			var windowW = $(window).width();
			if (closeBtn.hasClass('inMotion')) {
				return false;
			}
			if(closeBtn.hasClass('expanded-video')){
				closeBtn.removeClass('expanded-video').addClass('inMotion');
				$video.addClass('bgSwitch');
				closeBtn.parent().find('.container').animate({'width':'62%'});
				closeBtn.parent().find('iframe').animate({'height':'200px'},function(){
					closeBtn.parent().find('.video-details').fadeIn('slow');
					closeBtn.removeClass('inMotion');
				});
			}
			else {
				closeBtn.addClass('expanded-video inMotion');
				closeBtn.parent().find('.video-details').fadeOut('fast',function(){
					if (windowW < 960) {
						closeBtn.parent().find('.container').animate({'width':'94.4%'});
					}else{
						closeBtn.parent().find('.container').animate({'width':'96.2%'});
					}
					// closeBtn.parent().find('.container').animate({'width':'96.2%'});
					closeBtn.parent().find('iframe').animate({'height':'270px'});
					closeBtn.removeClass('inMotion');
				});
				$video.removeClass('bgSwitch');
			}
		});
	}
	$(window).resize(function(){
		if($('.container a').hasClass('expanded-video')){
			$('.container').removeAttr('style');
		}
	});
	
	/*region country validation on locate sales office*/
    if($('.store-search').length > 0){
    	
          $('#store-state').change(function(){
              validateState();
          });
          
          $('input#store-zip').bind("change keyup input",function() {
              validateZip();
          });

          
          $('#region').change(function(){
              var region=$('#region').val();
              var country=$('#store-country').val();
              if(region == ''){
            	  $('#store-country').attr('disabled','disabled');
            	  $('#store-region-country').attr('disabled','disabled');
              }
              else{
            	  $('#store-country').removeAttr("disabled");
            	 // $('#store-region-country').removeAttr("disabled");
              }
        });
        
        $('#store-country').change(function() {
        	var region=$('#region').val();
            var country=$('#store-country').val();
            if(region == '' || country == ''){
            	$('#store-region-country').attr('disabled','disabled');
            }
            else{
            	$('#store-region-country').removeAttr("disabled");
            }
        });

    }
    function validateState() {        
    	var state=$('#store-state').val();
    	var zip=$('#store-zip').val();
    	 
    	if(state!='') {
    	$('#store-zip').val('');
    	$('#store-state').val(state).prop('selected', true);    	           
    	$('#store-state-zip').removeAttr("disabled");
    }
    	if(state == '' && zip == '') {
    	   $('#store-state-zip').attr('disabled','disabled');
    	}
  }
    
    function validateZip() {        
    	var state=$('#store-state').val();
    	var zip=$('#store-zip').val();

    	if(zip!='') {
    	   $('#store-state').find('option:eq(0)').prop('selected', true);
    	   if(zip.length>4) {
    	       $('#store-state-zip').removeAttr("disabled");
    	   } else {
    	       $('#store-state-zip').attr('disabled','disabled');
    	   }
    	}
    	if(state == '' && zip == '') {
    	    $('#store-state-zip').attr('disabled','disabled');
    	 }
    }


    $('a[rel=overlay]').live('click',function(e){
    	contactUSDivObj = document.getElementById("contactUs-overlay");
        var windowW = $(window).width();
		if (windowW < 768) {
			//go to link
		}else{
			e.preventDefault();
			var posTop = $(this).offset().top + 680;
            var wrap = '<div class="contactus_lightbox"><div class="wrap"><div id="lb_lightbox"><a href="#" id="lb_light_close">X</a><div class="lb-container" style="overflow-y: scroll;overflow-x: scroll;"><div class="lb-slides">';
            var slide = '<div class="lb-slide">'+contactUSDivObj.innerHTML+'</div>';
            wrap += slide;
            wrap += '</div></div></div></div></div>';
            $('body').append('<div id="lb_overlay"></div>')
			.append(wrap);

			$('#lb_lightbox').css('top', -posTop + 'px');
			$(this).removeClass('active');
			$('#lb_overlay').fadeTo(500, 0.75, function(){
				$('#lb_lightbox').fadeTo(300, 1);
				stButtons.locateElements();
			});
			$('#lb_overlay, #lb_light_close').click(function(e){
				e.preventDefault();
				$('#lb_lightbox').fadeTo(100, 0, function(){
					$(this).remove();
					$('#lb_overlay').fadeTo(200, 0, function(){
						$(this).remove();
					});
				});
			});
    }
 });



	/*posts page lightbox*/

	/* popout carousel */
	$('a[rel=lightbox]').live('click',function(e){
		var windowW = $(window).width();
		if (windowW < 768) {
			//go to link
		}else{
			e.preventDefault();
			var posTop = $(this).offset().top - 800;
			var items = $(this).parent().parent().find('.product-item');
			var itemClicked = $(this).parent().index();
			var wrap = '<div class="wrap"><div id="lb_lightbox"><div class="prev"></div><div class="next"></div><a href="#" id="lb_light_close">X</a><span class="rights">&copy; All Rights Reserved</span><div class="lb-container"><div class="lb-slides">';
			var boobs = 0;

			items.each(function(){
				var image = $(this).find('img').attr('src');
				var title = $(this).find('a').attr('data-title');
				var description = $(this).find('a').attr('data-excerpt');
				var shareURL = $(this).find('a').attr('href');
				var share = '<div class="share-boxes"><p>Share This Information</p><ul class="share-this"><li><span class="facebook st_facebook_custom" st_url="'+shareURL+'"></span></li><li><span class="twitter st_twitter_custom" st_url="'+shareURL+'" st_msg="Summary text/description you wish to share" st_via="trane"></span></li><li><span class="googleplus st_googleplus_custom" st_url="'+shareURL+'"></span></li><li><span class="linkedIn st_linkedin_custom" st_url="'+shareURL+'"></span></li></ul></div>';
				var slide = '<div class="lb-slide"><img src="'+image+'" /><h1>'+title+'</h1><p>'+description+'</p>'+share+'</div>';
				wrap += slide;


			});
			wrap += '</div></div></div></div>';
			
			$('body').append('<div id="lb_overlay"></div>')
			.append(wrap);
			for (var i = 0; i < itemClicked; i++) {
				$('.lb-slide:first').insertAfter('.lb-slide:last');
				
			};
		
			$('#lb_lightbox').css('top', -posTop + 'px');
			$(this).removeClass('active');
			$('#lb_overlay').fadeTo(500, 0.75, function(){
				$('#lb_lightbox').fadeTo(300, 1);
				stButtons.locateElements();
			});
			$('#lb_overlay, #lb_light_close').click(function(e){
				e.preventDefault();
				$('#lb_lightbox').fadeTo(100, 0, function(){
					$(this).remove();
					$('#lb_overlay').fadeTo(200, 0, function(){
						$(this).remove();
					});
				});
			});

			$('#lb_lightbox .next').click(function(){
				var slideWidth = $('.lb-slide').width();
				$('.lb-slides').animate({
					'margin-left':-slideWidth
				},function(){
					$('.lb-slide:first').insertAfter('.lb-slide:last');
					$('.lb-slides').css('margin-left',0);
				});
			});


			$('#lb_lightbox .prev').click(function(){
				var slideWidth = $('.lb-slide').width();
				$('.lb-slide:last').insertBefore('.lb-slide:first');
				$('.lb-slides').css('margin-left',-slideWidth);
				$('.lb-slides').animate({
					'margin-left':0
				});
			});
		}

	});


	/*autoplay video*/
	$('#previewImage').click(function(){
	  var videoSrc = $('#thevideo iframe').attr('src');
	  var srcAutoPlay = videoSrc +';autoplay=1';
	  $('#thevideo iframe').attr('src',srcAutoPlay);
	  $(this).fadeOut('fast',function(){
	  	$('#thevideo').fadeIn('fast');
	  });
	});

	

});

//The below function is to arrange the global naviagtion menu

$(function () {
      var initialContainer = $('.columns'),
          columnItems = $('.columns li'),
          columns = null,
          column = 1, 
          itemsPerColumn = null,
          regionname = "",
          continuedText = $('#continuedText').html() != undefined ? $('#continuedText').html().trim() : "(continued)";
      // The below function builds the "li" tag for each column i.e the "ul" defined in the below function
      function updateColumns() {
          column = 0;           
          itemsPerColumn = (columnItems.length / columns.length);
          columnItems.each(function (id, element) {
              if ($(this).hasClass('regionname') )
              {
                  regionname = $(this).text();
              }
              if (id !== 0 && id > itemsPerColumn * (column + 1)) {	
                  column += 1;
                  //The below tag is to appened the contiued region name tag at the start of every column
                  $(columns.get(column)).append("<li class=\"regionname\">"+regionname+" ("+continuedText+")</li>");
              }
              //console.log(column, element, id);
              $(columns.get(column)).append(element);
          });
      }
	   // The below functions builds the "ul" tag by cloning. The cloning is done equal to the column number.
      function setupColumns() {
          columnItems.detach();
          column++;
          while (column <= initialContainer.data('column')) {
          	  	initialContainer.clone().insertBefore(initialContainer);
              	column++;
          }
          columns = $('.columns');
      }
      setupColumns();
      updateColumns();
  });
//called for each Mega Menu section to be converted to multiple columns instead of one.
function initColumnsMM(containerId) {
    var initialContainer = $(containerId),
        columnItems = $(containerId + ' li'),
        columns = 0,
        column = 1, 
        itemsPerColumn = null,
        regionname = "",
        continuedText = $('#continuedText').html() != undefined ? $('#continuedText').html().trim() : "(continued)";

    // The below functions builds the "ul" tag by cloning. The cloning is done equal to the column number.
    columnItems.detach();
    column++;
    //console.log("data-columns: " + initialContainer.data('column'));
    while (column <= initialContainer.data('column')) {
        initialContainer.clone().insertBefore(initialContainer);
        column++;
    }
    columns = $(containerId);

    // The below function builds the "li" tag for each column i.e the "ul" defined in the below function
    column = 0;           
    var itemsPerColumnCalc= (columnItems.length / columns.length);
    itemsPerColumn = Math.ceil(itemsPerColumnCalc);

    var rownum = 0;
    columnItems.each(function (id, element) {
        //console.log(element);
        if ($(this).hasClass('regionname') )
        {
            regionname = $(this).find("p").html();
            //console.log("regionname: " + regionname);
        }
        //console.log("rownum: " + rownum + "; itemsPerColumn: " + itemsPerColumn + "; ");
        if (rownum > itemsPerColumn) {
            column += 1;
            rownum = 1;
            if (!$(this).hasClass('regionname')) {
                //The below tag is to appened the contiued region name tag at the start of every column
                //console.log("(continued) added");
                $(columns.get(column)).append("<li class=\"regionname\"><p>"+regionname+" ("+continuedText+")</p></li>");
                rownum += 1;
            }
        } else if (rownum == itemsPerColumn && $(this).hasClass('regionname')) {
            column += 1;
            rownum = 1;
        } else { 
            rownum += 1; 
        }
        //console.log(column, element, id);
        $(columns.get(column)).append(element);
    });
} 
//Loop through each of the Mega Menu sections to call the method for converting to multiple columns
$(function() {
	var columnsMM = $('.columnsMM');
  columnsMM.each(function (id, element){
		//console.log(id, element, this.className.replace('columnsMM ', '.'));
      initColumnsMM(this.className.replace('columnsMM ', '.'));
  });
}); 

/* Modernizr 2.6.2 (Custom Build) | MIT & BSD
