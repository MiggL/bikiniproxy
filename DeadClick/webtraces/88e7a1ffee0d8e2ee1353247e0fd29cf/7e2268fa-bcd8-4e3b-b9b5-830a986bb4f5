        jQuery(document).ready(function () {
            jQuery.noConflict();
        });

        if (document.cookie.indexOf("colSort") == -1) {
            setSort("Shade Code");
        }

        function setSort(sort, reload) {
            document.cookie = "colSort= " + sort + ";";

            if (reload) {
                window.setTimeout(function () {
                    location.reload()
                }, 1000)
            }
        }
    </script>
