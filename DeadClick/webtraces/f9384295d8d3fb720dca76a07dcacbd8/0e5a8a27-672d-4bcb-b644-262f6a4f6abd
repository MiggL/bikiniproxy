
var uid = 'maris';
var db = '1';
var delta_time = '-18000';
var new_session = 'yes';
var user_status = 'premium';

if ('premium' == 'premium' && 'no' != 'yes'){
   document.getElementById("wtstimer").innerHTML = db+'::'+uid+'::'+delta_time+'::'+new_session+'::WoXrKmiC2rkAAFy4CR4AAAAG::WoXrKmiC2rkAAFy4CR4AAAAG';
}
else {
   document.getElementById("wtstimer").innerHTML = 'no_streaming';
}

if ('yes' != 'yes'){

   if ('en' == 'fr'){
      var host = 'fr.web-stat.com';
   }
   else{
      var host = 'www.web-stat.com';
   }

   if ('old_format' == 'wix_free'){
      var idTag =  'source=WixStats&encID='; 
   }
   else {
      var idTag =  'PublicID=maris';
   }

   if ('stats_menu' == 'visitor_details'){
      var click_target = 'live_visitors.pl';
   }
   else{
      var click_target = 'checkstats.pl';
   }

   var prefix = 'https:' == document.location.protocol ? 'https://wts.one' : 'http://wts.one';

//if ('10.183.251.101' == "74.243.246.87" || '193.51.236.134' == "74.243.246.87"){
//   console.log('http://'+host+'/stats/'+click_target+'?'+idTag);
//}


   if ('stats_menu' == 'no_click'){
      document.getElementById("wtscount").innerHTML = '<span style="display:inline-block; text-align:center;"><img src="'+prefix+'/draw_counter.pl?10032403::::::::" width="0" height="" border="0" style="; padding:0px ! important; margin:0px auto ! important; box-sizing: content-box ! important;"></span>';
   }
   else {
      document.getElementById("wtscount").innerHTML = '<span style="display:inline-block; text-align:center;"><a href="http://'+host+'/stats/'+click_target+'?'+idTag+'" target="new" border="0"><img src="'+prefix+'/draw_counter.pl?10032403::::::::" width="0" height="" border="0" style="; padding:0px ! important; margin:0px auto ! important;  box-sizing: content-box ! important;"></a></span>';
   }

}


