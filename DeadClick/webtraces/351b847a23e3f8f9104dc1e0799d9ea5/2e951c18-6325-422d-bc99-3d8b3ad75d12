var $k=jQuery.noConflict();
	
    $k(document).ready(function(){
 
        $k(window).scroll(function(){
            if ($k(this).scrollTop() > 100) {
                $k('.scrollup').fadeIn();
            } else {
                $k('.scrollup').fadeOut();
            }
        });
 
        $k('.scrollup').click(function(){
            $k("html, body").animate({ scrollTop: 0 }, 600);
            return false;
        });
 
    });
