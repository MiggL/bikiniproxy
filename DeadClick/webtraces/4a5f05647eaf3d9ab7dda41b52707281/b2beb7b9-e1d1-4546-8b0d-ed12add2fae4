            <script type="text/javascript">
                $j(function(){
                    $j('#slides-content-top-3').slides({
                        preload : true,
                        preloadImage: 'skin/frontend/default/default/qaz/qbanner/images/loading.gif',
                        play: 5000,
                        pause: 1000,
                        hoverPause: true,
                        effect : ifBrowserIsIE8 ? "slide" : 'diagonalShowReverse',
                        animationStart: function(current){
                            $j('.qbanner .caption').animate({
                                bottom:-40
                            },200);
                        },
                        animationComplete: function(current){
                            $j('.qbanner .caption').animate({
                                bottom:0
                            },250);
                        },
                        slidesLoaded: function() {
                            $j('.qbanner .caption').animate({
                                bottom:0
                            },250);
                        }
                    });
                });
            </script>


        <style>
                #slides-content-top-3  .caption{width: 1008px;}
                #slides-content-top-3 .pagination {margin:0 auto;left:479px;}
