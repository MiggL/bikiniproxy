/***/ "../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel.component.js":
/*!***************************************************************************************************************************!*\
  !*** ../src/Feature/ContentPresentation/code/Views/ContentPresentation/Content/CarouselSimple/_dfr-carousel.component.js ***!
  \***************************************************************************************************************************/
/*! no static exports found */
/*! all exports used */
/***/ (function(module, exports, __webpack_require__) {

"use strict";


var _bindings;

/* eslint-disable */
module.exports = {
    transclude: true,
    bindings: (_bindings = {
        data: '=?',
        inherit: '=?',
        options: '<?',
        disableWhenOnePage: '&?',
        customElementSize: '&?',
        customTranslateOffset: '&?',
        current: '<?'
    }, _bindings['inherit'] = '<?', _bindings),
    controller: ['$element', '$window', '$scope', '$attrs', '$parse', '$transclude', '$dfrResponsiveSettings', '$dfrUtil', '$rootScope', '$dfrAnimations', '$document', '$location', function controller($element, $window, $scope, $attrs, $parse, $transclude, $dfrResponsiveSettings, $dfrUtil, $rootScope, $dfrAnimations, $document, $location) {
        'ngInject';

        var $carousel = this;

        // shannon - this shouuldn't really ever work, we should throughly test this and remove it
        // I just haven't had the time
        var isolated = $element.parent().isolateScope();

        if (isolated) {
            // when the immediate parent has an isolated scope, use that to transclude so we can pass data
            // to the children correctly, the instance of this controller will
            // be stored under $carousel on this isolated scope
            $scope = isolated;
            $scope.$carousel = $carousel;
        }

        // setup internals        
        $element.addClass('dfr-carousel');
        // store a reference to the current carousel.
        var elem = $element[0];
        $carousel.$scope = $scope;

        // defaults
        $carousel.current = 0;
        $carousel.items = [];
        // by default, it's 1 per page.
        $carousel.customPerPage = angular.isDefined($attrs.itemsPerPage);
        $carousel.itemsPerPage = $carousel.customPerPage ? parseInt($attrs.itemsPerPage) : 1;
        $carousel.itemsPerPageCopy = angular.copy($carousel.itemsPerPage);
        // forces values to update when pre-defined breakoints are matched.
        $carousel.itemsPerPageResponsive = $carousel.itemsPerPage == 6 ? 2 : 1;

        // retreive breakpoint values
        $carousel.bp = $dfrResponsiveSettings.refresh();
        // expose public methods/vars at the top.
        $carousel.init = init;
        $carousel.prev = prev;
        $carousel.next = next;
        $carousel.goTo = goTo;
        $carousel.whichSideIsHovering = whichSideIsHovering;
        $carousel.slideContainer;
        $carousel.setVendor = setVendor;
        $carousel.getMediaCarouselLimits = getMediaCarouselLimits;
        $carousel.dynamicEvent = dynamicEvent;
        $carousel.suffixer = function (num) {
            return $carousel.currentString = (num < 10 ? '0' : '') + num;
        };

        $carousel.$onInit = function () {
            updateQuery();
        };

        // updates controller values if there's query string values matching
        // functionality for this carousel.
        var query = $dfrUtil.convertQueryToJSON();
        if (query.ci) $carousel.current = query.ci;
        try {
            $carousel.current = parseInt($carousel.current);
        } catch (e) {
            // couldn't convert to int
        }

        if (angular.isDefined($attrs.current)) {
            $attrs.$observe('current', function (newV, oldV) {
                var val = $parse(newV)($scope);
                if (angular.isNumber(val)) {
                    goTo(val);
                }
            });
        }

        // listeners
        $rootScope.$on('window:resize', $dfrUtil.debounce(whenResponsiveHelperUpdates, 200));

        // for accessibility, use arrow keys to navigate
        $document.on('keydown', function (e) {
            switch (e.which) {
                case 37:
                    // left
                    prev();
                    break;
                case 39:
                    // right
                    next();
                    break;
            }
        });

        this.$postLink = function () {
            $transclude($scope, function (clone, scope) {
                $element.append(clone); // or however else you want to manipulate the DOM
            });
        };

        /**
         * @function
         * @name toggleClasses
         * @description - Adds helper classes to the current element based on the current index.
         * @see _dfr-carousel-children.directive.js: Called once the dfr-carousel-children directive has registered all children.
         */

        function toggleClasses() {
            $element.toggleClass('is-first', $carousel.current == 0);
            $element.toggleClass('is-last', $carousel.current == $carousel.items.length - 1);
        }

        /**
         * @function
         * @description - Simple initialization method, perform all initial data  & dom manipuplation here.
         * @see _dfr-carousel-children.directive.js: Called once the dfr-carousel-children directive has registered all children.
         */

        // 
        function init() {

            // called from the dfr-carousel-children directive.
            // store all slides in an array.
            // basically forces a dodgy wait for digest to update.
            $carousel.slideContainer = elem.querySelector('.dfr-carousel-slides');

            // This should be removed once legacy is abolished, this component
            // was never intended to be used with an ng-repeat.
            // Added so Carousel Complex can have a chance to load and be detected.
            if ($carousel.slideContainer) {
                $carousel.items = asNodeList.call($carousel.slideContainer.querySelectorAll('.dfr-carousel-slide'));
            }

            if (!$carousel.items.length && $carousel.data && $carousel.data.length) {
                $dfrUtil.nextTick(function () {
                    init();
                });
                return;
            }
            if (!$carousel.items.length) throw new Error("Couldn't locate any items, are you sure you've got dfr-carousel-slide items inside your template?");
            $carousel.thumbs = [];
            if ($carousel.data) {
                $carousel.thumbs = $carousel.data.map(function (i) {
                    return i.imageDesktop && i.imageDesktop.src ? i.imageDesktop.src : i.imageMobile && i.imageMobile.src ? i.imageMobile.src : null;
                });
            } else {
                angular.forEach($carousel.items, function (item) {
                    var fc = item.querySelector('.feature-container');
                    if (!angular.isDefined(angular.element(fc).attr('dfr-bg-img-device')) && angular.isDefined(angular.element(fc).attr('dfr-bg-img'))) $carousel.thumbs.push(angular.element(fc).attr('dfr-bg-img'));
                    if (angular.isDefined(angular.element(fc).attr('dfr-bg-img-device'))) $carousel.thumbs.push(angular.element(fc).attr('dfr-bg-img-device'));
                });
            }

            // call whenResponsiveHelperUpdates to make sure we've initialized breakpoints before we update 
            // the stage.
            whenResponsiveHelperUpdates(null, $carousel.bp);
        }

        /**
         * @function
         * @name setElementSizes
         * @description - This method calculates the expected width if each slide, and applies it to each slide.
         * This is based off the itemsPerPage count, When the page is larger than the maximum stage width, it will basically add
         * a fake 'slide' left and right to provide a better viewport.
         * @see _dfr-carousel-children.directive.js: Called once the dfr-carousel-children directive has registered all children.
         */

        function setElementSizes() {
            // min/max values for the items.
            var pixelSizes = [];

            $carousel.itemsPerPage = forceItemsPerPage();
            // set the width of this container to be nth times the amount of elements * 100%, 4 elements, 4 * 100% == 400% width.

            // need to update the width if the perPage count was 1, 
            // then updates greater, we only want a maximum of 100% width for this style
            // of carousel.

            if ($carousel.customElementSize) {
                pixelSizes = $carousel.customElementSize({
                    instance: $carousel
                });
            } else {
                $carousel.slideContainer.style.width = $carousel.items.length * 100 + '%';
                pixelSizes[0] = '';
                pixelSizes[1] = 100 / $carousel.items.length + '%';
            }

            // set the width of the slides so they're all an even width of 100%. (3 slides, width == 33.33%, 4 slides, width == 25% etc)                / 
            if (!pixelSizes || !pixelSizes.length || pixelSizes.length !== 2) {
                throw new Error('customElementSize: Please make sure you return an array with a minimum and maximum width [20,30], minimum should be the first index, and maximum the second.');
                return;
            }
            angular.element($carousel.items).css({
                minWidth: pixelSizes[0],
                maxWidth: pixelSizes[1]
            });

            return pixelSizes.map(function (i) {
                return parseInt(i) || 0;
            });
        }

        /**
         * @function
         * @name getMediaCarouselLimits
         * @description - Used for the media carousel only, determins content text limits.
         */

        function getMediaCarouselLimits(type) {
            //nbsp adds char to the count
            var filler = 11;

            //actual count for copy
            var limit = 143 + filler;

            // switch (type) {
            //     case 'content':
            //         if ($carousel.bp.bpXlg) limit = 200;
            //         if ($carousel.bp.bpLg) limit = 160;
            //         if ($carousel.bp.bpMd) limit = 100;
            //         if ($carousel.bp.bpSm) limit = 80;
            //         break;
            //     default:
            //         if ($carousel.bp.bpMd) limit = 80;
            //         if ($carousel.bp.bpSm) limit = 60;
            //         break;
            // }

            // if ($carousel.bp.bpXlg) limit = 320;
            // if ($carousel.bp.bpLg) limit = 280;
            // if ($carousel.bp.bpMd) limit = 180;
            // if ($carousel.bp.bpSm) limit = 140;

            return limit;
        }

        /**
         * @function
         * @name forceItemsPerPage
         * @description - Just returns forced integers based
         * on specific breakpoints, fitting too many items inside
         * specific breakpoints breaks the layout.
         * Always check on the ORIGINAL value ($carousel.itemsPerPageCopy), as we need to reset
         * the value when we go back up in scale.
         */

        function forceItemsPerPage() {
            if (!$carousel.customPerPage) return $carousel.itemsPerPageCopy;
            var itemsPerPageDefault = angular.copy($carousel.itemsPerPageCopy);
            if ($carousel.bp.bpLg && $carousel.itemsPerPageCopy > 5) {
                itemsPerPageDefault = 5;
            }
            if ($carousel.bp.bpMd && $carousel.itemsPerPageCopy > 3) {
                itemsPerPageDefault = 3;
            }
            if ($carousel.bp.bpSm) {
                itemsPerPageDefault = 2;
            }
            if ($carousel.bp.bpSmxs) {
                itemsPerPageDefault = 1;
            }
            $carousel.itemsPerPage = itemsPerPageDefault;
            return itemsPerPageDefault;
        }

        /**
         * @function
         * @name whichSideIsHovering
         * @description - Will toggle the class on the main element 
         * when the next or prev button is hovered.
         */

        function whichSideIsHovering(dir) {
            $element.toggleClass('after-hover', dir == 'next');
            $element.toggleClass('before-hover', dir == 'prev');
        }

        /**
         * @function
         * @name whenResponsiveHelperUpdates
         * @description - Method to update values/viewport when the helper refreshes.
         */

        function whenResponsiveHelperUpdates(event, values) {
            $carousel.bp = values;
            $element.toggleClass('horizontal', !$carousel.bp.bpSm);
            $element.toggleClass('vertical', $carousel.bp.bpSm);
            $carousel.items.forEach(function (slide, i) {
                if ($element.hasClass('layout-contained')) {
                    // helps maintain a 16x9 ratio when resizing, 
                    // or whenever the container is chucked inside any other element that has different widths.           
                    var fcw = slide.querySelector('.feature-container-wrapper');
                    angular.element(fcw).css('height', fcw.clientWidth * 9 / 16 + 'px');
                }
            });
            refresh();
        }

        /**
         * @function
         * @name asNodeList
         * @description Creates an array of the child elements of the slide,
         * exposes a css method so we can update each child like we would with jQuery css or jQlite
         */

        function asNodeList() {
            var nodeArray = [];
            for (var i = 0; i < this.length; ++i) {
                nodeArray.push(this[i]);
            }return nodeArray;
        }

        /**
         * @function
         * @name prev
         * @description Simple goes to the previous item
         */

        function prev() {
            // refresh method handles logic
            $carousel.current--;
            refresh();
        }

        /**
         * @function
         * @name next
         * @description Simple goes to the next item
         */

        function next() {
            $carousel.current++;
            // refresh method handles logic
            refresh();
        }

        /**
         * @function
         * @name dynamicEvent
         * @description - This simply toggles the state of the 'feature-cotnainer', it will
         * popup the video if there is one, or it will just take you to that slide.
         */

        function dynamicEvent(current, index) {
            if (current != index) {
                // the current clicked element isn't active, so go to it.
                goTo(index);
            } else {
                // it's not the active state, update the popup.
                $carousel['popup' + index] = true;
            }
        }

        /**
         * @function
         * @name goTo
         * @description Simple goes to the item by an index value, if the value
         * does not exist, or the index is out of range it will bail.
         */

        function goTo(i) {
            if (i < 0 || i > $carousel.items.length - 1) return;
            $carousel.current = i;

            updateQuery();

            // $location.search('ci', $carousel.current).replace();
            angular.element($element).scrollLeft(0);
            refresh();
        }

        function updateQuery() {
            // if query is not defined, don't add it, only add it
            if (angular.isUndefined($dfrUtil.convertQueryToJSON().ci)) return;
            $window.history.replaceState(null, '', $dfrUtil.updateQueryString(window.location.href, 'ci', $carousel.current));
        }

        function setVendor(element, property, value) {
            element.style["webkit" + property] = value;
            element.style["moz" + property] = value;
            element.style["ms" + property] = value;
            element.style["o" + property] = value;
        }

        /**
         * @function
         * @name refresh
         * @description This will do all main updating of css classes and the slide animation. Everything is atuomated
         * with this carousel, very simple but very effective.
         */

        function refresh() {
            // digest is nescessary to update the scope.
            var sizes = setElementSizes();

            // this is how this logic works
            // When current item index, is less than 0
            // it will skip to the end, more of looping logic.
            // If the items per page number is greater than 0, it will jump to the 'last page',
            // if the current item index is less than 0, example 
            //   
            //   Assuming a per-page limit of 3
            //   if there's 9 items, and you started on the first item (0), it will set the active image
            //   to index to 5 (zero based), so you'll be technically viewing 6-9


            $carousel.current = $carousel.current < 0 ? $carousel.items.length - $carousel.itemsPerPage : $carousel.current - 1 + $carousel.itemsPerPage >= $carousel.items.length ? 0 : $carousel.current;
            if (isNaN($carousel.current)) $carousel.current = 0;
            toggleClasses();
            $carousel.items.forEach(function (slide, i) {
                angular.element(slide).removeClass('dfr-slide-active before after');
                if ($carousel.itemsPerPage == 1) {
                    if (i < $carousel.current) angular.element(slide).addClass('before');
                    if (i > $carousel.current) angular.element(slide).addClass('after');
                    if (i == $carousel.current) angular.element(slide).addClass('dfr-slide-active');
                } else {
                    if (i >= $carousel.current && i <= $carousel.current + ($carousel.itemsPerPage - 1)) angular.element(slide).addClass('dfr-slide-active');
                }
            });

            var tx = $carousel.current * (100 / $carousel.items.length) * -1;

            if ($carousel.customTranslateOffset) {
                tx = $carousel.customTranslateOffset({
                    instance: $carousel,
                    sizes: sizes
                });
                if (!tx.custom) {
                    if (angular.isUndefined(tx) || !angular.isNumber(tx)) {
                        throw new Error('customTranslateOffset: translate value returned is not a number or not defined.');
                        return;
                    }
                }
            }

            var onlyOnePage = $carousel.items.length <= $carousel.itemsPerPage;
            $element.toggleClass('only-one-page', onlyOnePage);

            // provide a value to disable-when-one-page, and it will add that value as classes
            // to the slide container.


            if ($carousel.disableWhenOnePage) {
                $carousel.disableWhenOnePage({
                    onlyOnePage: onlyOnePage
                });
            }
            // no point continuing if we only have one page.
            if (angular.isDefined($attrs.disableWhenOnePage) && onlyOnePage) {
                setVendor($carousel.slideContainer, 'Transform', '');
                return;
            }
            if (Modernizr.csstransforms3d) {
                setVendor($carousel.slideContainer, 'Transform', 'translate3d(' + (tx.custom ? tx.custom : tx + '%') + ', 0, 0)');
            } else {
                setVendor($carousel.slideContainer, 'Transform', 'translate( ' + (tx.custom ? tx.custom : tx + '%') + ', 0)');
            }
            $dfrAnimations.waitTransitionEnd(angular.element($carousel.slideContainer), {
                timeout: 700 // fallback
            }).then(function () {
                $rootScope.$emit('lazyload:images');
                $rootScope.$emit('viewport:update');
            });
        }
    }]
};

/***/ }),

