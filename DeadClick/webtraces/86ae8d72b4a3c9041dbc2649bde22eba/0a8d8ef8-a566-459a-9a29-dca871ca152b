function detectIE(){var e=window.navigator.userAgent;
var d=e.indexOf("MSIE ");
if(d>0){return parseInt(e.substring(d+5,e.indexOf(".",d)),10)
}var b=e.indexOf("Trident/");
if(b>0){var g=e.indexOf("rv:");
return parseInt(e.substring(g+3,e.indexOf(".",g)),10)
}var f=e.indexOf("Edge/");
if(f>0){return parseInt(e.substring(f+5,e.indexOf(".",f)),10)
}return false
}(function(g,b,e){var h=g(b),f,d={us:"/content/dam/blackBerry/campaigns/intercept/2016-may-madness/intercept_us.html",ca:"/content/dam/blackBerry/campaigns/intercept/2016-may-madness/intercept_ca.html",uk:"/content/dam/blackBerry/campaigns/intercept/2016-may-madness/intercept_uk.html",de:"/content/dam/blackBerry/campaigns/intercept/2016-may-madness/intercept_de.html"};
h.on("universal_intercept.bb",function(o,p){var j,n=b.cookie,k="no-intercept-shop-20150420",m=new RegExp(k+"=1"),i=e.navigator.userAgent.indexOf("MSIE")==-1?" expires=0;":"";
if(m.test(n)||detectIE()){return
}try{j=g.get(d[p]).done(function(q){f=g(q);
if(f.has("#universal_intercept_cont")){f.appendTo("body").delay(100).queue(function(){f.addClass("open").dequeue()
});
b.cookie=k+"=1;"+i+" path=/";
if(e.dataLayer){e.dataLayer.push({event:"gaEvent",eventCategory:"Shop_Intercept",eventAction:"open",eventLabel:f.find("#universal_intercept_redeem").attr("href"),eventValue:1})
}}})
}catch(o){return
}});
h.on("click","#universal_intercept_close",function(i){i.preventDefault();
i.stopPropagation();
if(e.dataLayer){e.dataLayer.push({event:"gaEvent",eventCategory:"Shop_Intercept",eventAction:"close",eventLabel:f.find("#universal_intercept_redeem").attr("href"),eventValue:1})
}f.on("transitionend webkitTransitionEnd otransitionend",function(){f.remove()
});
f.removeClass("open")
});
h.on("click","#universal_intercept_redeem",function(i){if(e.dataLayer){e.dataLayer.push({event:"gaEvent",eventCategory:"Shop_Intercept",eventAction:"click",eventLabel:f.find("#universal_intercept_redeem").attr("href"),eventValue:1})
}})
})(jQuery,document,window);
$(function(){var n=$("html").attr("lang"),i=$(".customer-card a"),e=window.location.href,f=$(".bb-nav-icon"),m=$('form [type="submit"]'),d=$(".logo"),j=$("#bb-hrmenu"),g=$(".primary-menu"),h=$(".slick-initialized"),b=$(".slick-slide .cta-button a, .slick-slide .link-container a, .cta-button a");
j.on("click","a",function(v){var u=$(this),s=u.text().trim(),p=u.closest(".bb-hrsub").length?"Flyout Nav":"Primary Nav",t=u.attr("href"),r="Nav Link Click",w="Main Nav Click",q="Link Copy: "+s+" | Link Target: "+t+" | Location: "+p+" | Lang: "+n+" | Page: "+e;
o(w,r,q)
});
g.on("click","a",function(u){var w=$(this),p=w.text().trim(),q=function s(){if(w.hasClass("primary-menu--link")){return"Primary Menu"
}else{if(w.hasClass("secondary-menu--link")){return"Secondary Menu"
}else{return"Tertiary Menu"
}}},t=w.attr("href"),x="Navigation Click",v="Main Nav Click",r="Link Copy: "+p+" | Link Target: "+t+" | Location: "+q()+" | Lang: "+n+" | Page: "+e;
o(v,x,r)
});
f.on("click",function(){var s=$(this),r=s.hasClass("open")?"Closed":"Opened",q="Burger Menu Click",t="Main Nav Click",p="Nav State: "+r+" | Lang: "+n+" | Page: "+e;
o(t,q,p)
});
d.on("click",function(s){var r=$(this),q="Logo Click",t="Main Nav Click",p="Lang: "+n+" | Page: "+e;
o(t,q,p)
});
if(h.length){b.on("click",function(t){var w=$(this),u=w.text().trim(),r=w.closest(".carousel"),s=r.children(".bb-carousel-banner").length?"Hero":"On Page",y=parseInt(w.closest(".slick-active").attr("data-slick-index"),10)+1,p=w.closest(".slick-current").find(".heading:first").text().trim(),x="Carousel Panel Button Click",v="Carousel Click Event",q="Carousel Location: "+s+" | Slide#: "+y+" | Heading: "+p+" | Button Copy: "+u+" | Lang: "+n+" | Page: "+e;
if(sessionStorage){sessionStorage.setItem("bb_caraouselData",q)
}o(v,x,q)
})
}i.on("click",function(r){var t=$(this),x=t.attr("href"),u=t.hasClass("disabled"),v=t.attr("data-companyname"),q=t.attr("data-assettype"),s="Customer Success Stories",w="Click Event",p="Company Name: "+v+" | Asset Type: "+q+" | Lang: "+n+" | Page: "+e;
o(s,w,p)
});
function k(q){var s=q+"=";
var p=document.cookie.split(";");
for(var r=0;
r<p.length;
r++){var t=p[r];
while(t.charAt(0)==" "){t=t.substring(1,t.length)
}if(t.indexOf(s)==0){return t.substring(s.length,t.length)
}}return null
}m.on("click",function(v){var u=$(this),s=u.closest("form"),p=s.attr("id"),t=s[0].checkValidity(),w="Form Submission",r="Button Click",q="Tracking Info:";
if(p){q+=" | Form ID: "+p
}if(sessionStorage.bb_caraouselData){q+=" | Source Data -- "+sessionStorage.getItem("bb_caraouselData")
}if(k("BB-UTM")!=null){q+=" | Campaign Info: "+k("BB-UTM").toString()+" | Current Page"+e+document.referrer!=null?("| Previous Page: "+document.referrer):""
}if(t){o(w,r,q)
}});
function o(r,q,p){dataLayer.push({event:"gaEvent",eventCategory:r,eventAction:q,eventLabel:p})
}});