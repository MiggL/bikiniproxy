<script>
    function PopUp(hideOrshow) {
	    if (hideOrshow == 'hide') {
	    	document.getElementById('wrapper').style.display='none';
	    	setCookie('popState','false',(365*24));
	    }
	    else {
	    	document.getElementById('wrapper').removeAttribute('style');
	    	jQuery('body').trigger('email_pop');
	    	setCookie('popState','viewed',(30*24));
	    	setInterval(function(){
	    		$('div.yie-holder').each(function(){
					if($(this).css('display') == 'block'){
						$(this).css('display','none');
					}
				});
	    	}, 500);
	    }
	}
	var popStateStatus = readCookie('popState');
	if(popStateStatus == null || popStateStatus == ''){
		window.addEventListener('load', 
			function() { 
		    	setTimeout(function () {
		        	PopUp('show');
		    	}, 30000);
		  	}, false);
	}
	
	$(document).ready(function(){
		//$('#popup .submission-message').css('display','none');
		var $popEmailForm = $('#popEmailForm'), 
					        $popEmailInput = $('#popEmailForm .emailtxt'),
					        $popMessage = $('#popup .submission-message');

		$('#popEmailForm input[type="email"]').on('focus',function(){
			$('#popEmailForm span.error').text('');
		});

		$("#wrapper").click(function(e){
		  	$(this).fadeOut();
		});

		$('.main-wrapper-popup').click(function(e){
			e.stopPropagation();
		});

		$("body").on("submit","#popEmailForm",function (evt) {
			var validateFormChk = true, 
			$emailValue = $('#popEmailForm input[type="email"]').val();
			if($emailValue == '' || $emailValue == null){
				$('#popEmailForm span.error').text('Ce champ est obligatoire.').css('font-size','13.5px');
        		validateFormChk = false;
        		return false;
	        }
	        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
			if(re.test($emailValue)){
			
			}
			else{
				validateFormChk = false;
				$('#popEmailForm span.error').text('Veuillez spécifier une adresse électronique valide').css('font-size','12.5px');
				return false;
			}
	        var queryString = $popEmailForm.each(function () {
						        return $(this).val( $.trim( $(this).val() ));
						    }).serialize();
			if(validateFormChk){
				$("#popup :input").prop("disabled", true);
				$.ajax({
					type: $popEmailForm.attr('method'),
					url: $popEmailForm.attr('action'),
					data: queryString,
			        success: function (data) {
			        	$popEmailForm.hide();
		           		$popMessage.css('display','inline-block').animate({
				        	opacity: 1
				        }).text('Merci!');
				        setCookie('popState','submitted',(365*24));
			        },
			        error: function (xhr, ajaxOptions, thrownError) {
			        	// technical backend error
				        if(xhr.status == '400' || xhr.status == '404'){
					        $popEmailForm.hide();
					        $popMessage.css({'display':'inline-block','color':'red','font-size':'14px'}).animate({
					        	opacity: 1
					        }).text('Désolés, nous n\'avons pas pu enregistrer votre envoi en raison de problèmes techniques.');
				        }
				    }
				});
			}
			return false;
		});
    });
</script>

	
	
	













	
	








	
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
			

			
				
					
				
				
			
			
		
	
		
	
		
	
		
	
		
	


	
	
	



    
        
        
    




	
			
			
	


	
		
		
			  
				
				
					
				
				
			  
			  
			  
			  
		
		
        
		
			
             
		
		
        
        
        
		
			
			
				
					
					
						
							
						
					
				
            
				
	    			    
	    			    
                    
			
			
	

	
	


	
		
        
	       
        	
	        	
				
				
			
			
			
			
	       
	    
	




	
        
	




	
