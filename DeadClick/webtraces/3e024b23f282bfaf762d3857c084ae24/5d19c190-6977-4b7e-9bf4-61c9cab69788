(function(){function d(a,b){var c=$(".slide-list",a[b]);$(a[b]).show();c.on("init",function(){setTimeout(function(){a.length>b+1&&d(a,b+1)},1E3)});c.slick({infinite:true,arrows:true,slidesToShow:4,slidesToScroll:4})}(function(){var a=$(".related-slide-container"),b=a.find(".slide-list"),c=a.find(".arrow-left"),a=a.find(".arrow-right");b.slick({infinite:true,arrows:true,prevArrow:c,nextArrow:a,slidesToShow:6,slidesToScroll:6})})();(function(){var a=$(".slide-channel-container");a.length>0&&d(a,0)})();
(function(){var a=$(".featured-slide-container"),b=a.find(".slide-list"),c=b.find(".slide"),d=a.find(".arrow-left"),a=a.find(".arrow-right");c.length==1?(d.remove(),a.remove()):b.slick({infinite:true,arrows:true,prevArrow:d,nextArrow:a,slidesToShow:1,slidesToScroll:1})})()})();