<script>
	function login(){
		var redirect = getQueryString('redirect');
		
		var url = 'http://www.chinaglobalmall.com/index.php?route=account/login';
		
		if(redirect != null){
			url += '&redirect=' + encodeURIComponent(redirect);	
		}
		
		window.location = url;
	}
	
	function register(){
		var redirect = getQueryString('redirect');
		
		var url = 'http://www.chinaglobalmall.com/index.php?route=account/register';
		
		if(redirect != null){
			url += '&redirect=' + encodeURIComponent(redirect);	
		}
		
		window.location = url;
	}
	
	function getQueryString(name) {
    	var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
    	
		var r = window.location.search.substr(1).match(reg);
		
		var redirect = null;
		
    	if (r != null){
			redirect = r[2];
			
		}else{
			redirect = window.location.href;
		}
		
		redirect = unescape(redirect);
		
		if(redirect.indexOf('logout') > 0){
			redirect = null;
		}
		
		return redirect;
    }
</script>   


