}
//*****************************************************************
//************************ Gestion du Native Leboncoin ************
var nativeCampagnData = ADventori.data.campaignData.nativeCampagnData;

if(nativeCampagnData && nativeCampagnData[eadCreative]){
  ADventori.conf.creaUrl = ADventori.macros.replace('[PROTOCOL][HOSTNAME]/lp/dd/adventori/lbc/index2.html');
