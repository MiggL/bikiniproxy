function init_multiplegl(url,tab) {
    
    var ur = (("https:" == document.location.protocol) ? "https://gl.hostcg.com/" : "http://gl.hostcg.com/");
    var q = document, l = q.createElement('script'), sa = q.getElementsByTagName('script')[0];
    l.type = 'text/javascript';
    l.defer = true;
    l.async = true;
    l.onload = function () {
        init_multiplegl2(url,tab);
    };
    var date = new Date();
    l.src = ur + 'js/genlead2.js?tmp=' + date.getDate() + date.getMonth() + date.getYear();
    sa.parentNode.insertBefore(l, sa);
}
if( window.ltwAsyncInit != null){
     window.ltwAsyncInit();
}