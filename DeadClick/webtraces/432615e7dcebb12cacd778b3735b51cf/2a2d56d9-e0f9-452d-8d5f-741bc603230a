/* INLINE: Check for any custom errors that have occurred 					*/
/* ******************************************************************		*/

try {
	if (typeof (sOm_AdError) != "undefined") {
		s.eVar42 = sOm_AdError + " - " + s.pageName; // Error Code
	}
} catch (Error) { }


/* ******************************************************************		*/
/* INLINE: ga Track KPIs													*/
/* ******************************************************************		*/
try {
	om_gaTrackKPI(s.eVar30);
} catch (Error) { }


