<script type="text/javascript">
 jQuery(document).ready(function(){
    jQuery.noConflict();
	jQuery("#dropdown-curent-curency").mouseenter(function(){
		jQuery('#supported-currencies').show();
	});
	jQuery("#supported-currencies").hover(function(){
		jQuery(this).show();
	});
	jQuery("#supported-currencies").mouseleave(function(){
		jQuery(this).hide();
	});
	jQuery("#dropdown-curent-curency").mouseleave(function(){
		jQuery('#supported-currencies').hide();
	});
 });
 if (document.cookie.indexOf("dispCurrency") == -1) {
     setCurrencyAndTax("GBP");
 }
 function setCurrencyAndTax(currencycode, reload) {

     document.cookie = 'dispCurrency=' + currencycode + '; path=/; domain=.woolwarehouse.co.uk;';

     switch (currencycode)
     {
         case "AUD":
             jQuery.post("/checkout/cart/estimatePost/",{ country_id: "AU", estimate_postcode: "90210" });
             break;
         case "CAD":
             jQuery.post("/checkout/cart/estimatePost/",{ country_id: "CA", region_id: "66", estimate_postcode: "90210" });
             break;
         case "NZD":
             jQuery.post("/checkout/cart/estimatePost/",{ country_id: "NZ", estimate_postcode: "90210" });
             break;
         case "USD":
             jQuery.post("/checkout/cart/estimatePost/",{ country_id: "US", region_id: "12", estimate_postcode: "90210" });
             break;
         default:
             jQuery.post("/checkout/cart/estimatePost/",{ country_id: "GB" });
             break;
     }

     if(reload) {
         window.setTimeout(function () {
             location.reload()
         }, 1000)
     }
 }
</script>
