<script type="text/javascript">
	$(document).ready(function(){

		$(window).scroll(function() {
			if ($(window).scrollTop() > 50){
				$('#js_CodeBox').fadeIn(500);
			} else {
				$('#js_CodeBox').fadeOut(800);
			}
		});


		$("#js_CodeBox").hover(function(){
			$(this).find("#preview_Box").show();
			},function(){
			$(this).find("#preview_Box").hide();
		});
	});
</script>


	
	<script>
        var COMING_SOON = 'COMING SOON';
        var ON_SALE_NOW = 'ON SALE NOW';
        var STARTS_IN = 'Starts in';
        var ENDS_IN = 'Ends in';
	</script>
	
	<script>
		$('#js_topSeachInput').val('');
		$(function () {
			var $pop = $('.pop-dimmer');
			var $popCont = $('#custom-content');

			$('.pop-close').click(function (event) {
				$pop.fadeOut();
			});

			function popMsg(msg) {
				$popCont.html(msg);
				if (!$pop.is(':visible')) {
					$pop.fadeIn();
				}
			}

			(function () {
				var $category = $('.category-list');
				var $cates = $category.find('li');
				var $lists = $('.sec-deals .item-list');
				$lists.each(function (index) {
					var $lis = $(this).find('li');
					if ($lis.length < 1) {
						//$cates.eq(index).hide();
					}
				});

				$category.on('click', 'li', function (e) {
					var index = $(this).index();
					$category.find('.active').removeClass('active');
					$(this).addClass('active');
					$lists.hide().eq(index).show();
					$(window).trigger('scroll');
				});

			})();

			GLOBAL.util = {
				scroll: function (options) {

					var _doc = $(document),
						oldSTop = _doc.scrollTop(),
						// object
						target = $(options.target);

					if (!target.length) {
						console.info('target is none!');
						return false;
					}

					// parameter 
					var defaults = {
						classes: {
							downScroll: 'hide',
							upScroll: 'show',
							top: 'fixed'
						},
						offset: {
							top: target.offset().top,
							scroll: target.height()
							// response: 0
						}
					}

					var o = $.extend(defaults, options || {});

					if (!target.length) {
						console.warn('Scroll target is none!');
						return false;
					};

					$(window).scroll(function () {
						var currentSTop = _doc.scrollTop(),
							hasTheClass = target.hasClass(o.classes.downScroll);

						if (oldSTop >= o.offset.top) {
							o.offset.top > 0 ? target.addClass(o.classes.top) : '';

							// if (oldSTop > currentSTop) {
							//     target.removeClass(o.classes.downScroll).addClass(o.classes.upScroll); 
							// } else { 
							//     target.addClass(o.classes.downScroll).removeClass(o.classes.upScroll);
							// }
						} else {
							target.removeClass(o.classes.top);
						}
						oldSTop = currentSTop;
					});
				}
			};

			(function () {
				var floorTargetArr = $('.sec-wrap').eq(0).find('.sec-nav'),
					rightNavLiArr = $('#js-main-nav').find('li'),
					firstTargetTop = floorTargetArr.eq(0).offset().top,
					timer = null;

				$(window).scroll(function () {
					if (timer) {
						clearTimeout(timer);
						timer = null;
					}
					timer = setTimeout(function () {
						var windScrollH = $(window).scrollTop()
						if (windScrollH < firstTargetTop) {
							rightNavLiArr.eq(0).addClass('active');
							return;
						}
						$.each(floorTargetArr, function (index, val) {
							var that = $(val);
							if (windScrollH >= that.offset().top - 150) {
								rightNavLiArr.eq(index).addClass('active').siblings('li').removeClass('active');
							} else {
								rightNavLiArr.eq(index).removeClass('active');
							}
						});
					}, 20);

				});
			})();

			//fixed nav
			GLOBAL.util.scroll({
				target: '#js-main-nav'
			});

			//Remove fixed
			var $mainNav = $('#js-main-nav');
			$(window).scroll(function () {
				if ($(window).scrollTop() == 0) {
					$mainNav.removeClass('fixed');
				}
			});

			if ($mainNav.find('li').length != 3) {
				$mainNav.addClass('num-' + $mainNav.find('li').length);
			}

			$mainNav.on('click', 'li', function () {
				var index = $(this).index();
				var top = $('.sec-nav').eq(index).offset().top - 80;
				$('html, body').animate({
					'scrollTop': top
				}, 500, function () {
					//index != 0 && $mainNav.addClass('hide');
				});
			});

			// $('.flash-list').on('mouseenter mouseleave', '.js-lazy', function(e) {
			// 	var cover = $(this).data('cover');
			// 	if (!cover) {
			// 		return false;
			// 	}
			// 	$(this).attr('src', e.type == 'mouseenter' ? cover : $(this).data('original'));
			// });

			//GLOBAL.lazyLoad.scrollLazyLoad(".js-lazy");
			$('.js-lazy').lazyload({
				threshold: 450,
				effect: 'fadeIn',
				failure_limit: 60,
				skip_invisible: true
			});

			function limitTime(time, status) {
				var seconds = time,
					minutes = Math.floor(seconds / 60),
					hours = Math.floor(minutes / 60),
					days = Math.floor(hours / 24),
					CDay = days,
					CHour = hours % 24,
					CMinute = minutes % 60,
					CSecond = Math.floor(seconds % 60),
					returnObj = {};
				if (CDay < 10) {
					CDay = '0' + CDay;
				}
				if (CHour < 10) {
					CHour = '0' + CHour;
				}
				if (CMinute < 10) {
					CMinute = '0' + CMinute;
				}
				if (CSecond < 10) {
					CSecond = '0' + CSecond;
				}

				if (status === 1) { //start
					returnObj = {
						title: 'PROMO ENDS IN:',
						cDay: CDay,
						CHour: CHour,
						CMinute: CMinute,
						CSecond: CSecond
					};
				} else { //not start
					returnObj = {
						title: 'PROMO BEGINS IN:',
						cDay: CDay,
						CHour: CHour,
						CMinute: CMinute,
						CSecond: CSecond
					};
				}

				return returnObj;
			}

			(function () {
				var $time = $('.js-countdown');
				$.each($time, function (index, val) {
					var $this = $(val);
					var $li = $this.closest('li');
					var $text = $li.find('.status');
					var etime = +$this.attr('data-et');
					var stime = +$this.attr('data-bt');
					var status = $this.attr('data-status');
					var now = +$('#time-now-json').val();
					var limitObj = {};
					var time  = 0;
					if(status == 0 ){
						time = stime - now;	
					} else {
						time = etiem - now;
					}
					if (time <= 0) {
						return true;
					}

					var nailInterval = setInterval(function () {
						time--;
						obj = limitTime(time, status);
						if (time > 0) {
							$text.html(status === 1 ? ON_SALE_NOW : COMING_SOON);
							$this.html(obj.cDay + ':' + obj.CHour + ':' + obj.CMinute + ':' + obj.CSecond);
						} else {
							$text.html(ON_SALE_NOW);
							clearInterval(nailInterval);
							$this.html('00:00:00:00');
							if ($this.closest('.sec-nav').hasClass('main-nav')) {
								$.ajax({
										url: '/m-brand-a-ajax_refresh.htm',
										type: 'POST',
									    data: {'lang':JS_LANG.replace('/','')},
										dataType: 'json'
									})
									.done(function (data) {
										var target = MAIN_DOMAIN + window.location.pathname;
										window.location.href = target;
									});
							}
						}
					}, 1000);
				});
			})();

		});
		// time counter for flash
		function Counter(contain, statusText, template, fun, other) {
			var etime = +contain.attr('data-et');
			var stime = +contain.attr('data-bt');
			var now = +$('#time-now-json').val();
			this.contain = contain;
			this.day = 0;
			this.hour = 0;
			this.minute = 0;
			this.second = 0;
			this.status = contain.attr('data-status');
			this.handler = null;
			this.other = other || false;
			this.fun = fun || function () {};
			// this.template = template ? template : "{day}:{hour}:{minute}:{second}";
			this.template = template;
			if (statusText.length) {
				this.text = this.status == 1 || this.status == 2 ? statusText[1] : statusText[0];
			} else {
				this.text = '';
			}
			if(this.status == 0) {
				this.time = stime - now;				
			} else {
				this.time = etime - now;
			}
			this.init();
		}
		Counter.prototype = {
			timing: function () {
				var self = this;
				this.handler = setTimeout(function () {
					if (self.time <= 0 || self.contain.attr('data-status') == 2) {
						clearTimeout(self.handler);
						self.fun(self.contain);
						return false;
					}
					self.time -= 1;
					if (self.template) {
						self.day = Math.floor(self.time / 60 / 60 / 24);
						self.hour = Math.floor(self.time / 60 / 60 % 24);
						self.minute = Math.floor(self.time / 60 % 60);
						self.second = Math.floor(self.time % 60);
						self.render();
					}
					self.timing();
				}, 1000);
			},
			transform: function (data) {
				if (('' + data).length < 2) {
					return '0' + data;
				} else {
					return data
				}
			},
			render: function () {
				var newStr = this.template.replace(/\{day\}/g, this.transform(this.day))
					.replace(/\{hour\}/g, this.transform(this.hour))
					.replace(/\{minute\}/g, this.transform(this.minute))
					.replace(/\{second\}/g, this.transform(this.second));
				if (this.other && this.time > 86400) {
					this.contain.html( this.text + this.transform(this.day) + ' D ' + this.transform(this.hour) + ' H');
				} else {
					if (this.text) {
						this.contain.html(this.text + newStr);
					} else {
						this.contain.html(newStr);
					}
				}
			},
			init: function () {
				this.timing();
			}
		}
		$('.counter').each(function (index, el) {
			new Counter($(el), [STARTS_IN+':', ENDS_IN+': '], '{hour}:{minute}:{second}', function () {
                window.location.href = window.location.href;
			}, true)
		});
		console.log(GLOBAL.FloatObj.goToUp);
		GLOBAL.FloatObj.goToUp();
	</script>
	




