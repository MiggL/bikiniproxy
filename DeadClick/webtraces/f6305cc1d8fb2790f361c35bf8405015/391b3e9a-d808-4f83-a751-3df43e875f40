Bootstrapper.bindImmediate(function(){var Bootstrapper=window["Bootstrapper"];var ensightenOptions=Bootstrapper.ensightenOptions;window.galleryRefresh=function(){console.log("Gallery refresh loaded.");var frequency=5,limit=1;var rate=frequency*1E3,counter=limit,timer_running=false;var checkTimer=function(){if(!timer_running){timer_running=true;refreshAds();var myTimer=setInterval(function(){if(counter>0)refreshAds();else{timer_running=false;window.clearInterval(myTimer);counter=limit;console.log("interval: stopped")}},
rate)}};var refreshAds=function(){console.log("refreshing ads. interval: "+limit);try{if(typeof window.HDN_ResponsiveGallery!=="undefined"&&typeof window.HDN_ResponsiveGallery.fullscreen!=="undefined")hearstRefreshInterstitialAds(["GO300"]);else setTimeout(hearstRefreshAds(),0)}catch(e){console.log("Gallery Refresh Error: "+e)}counter--};$(document).bind("HDNGalleryActionOnChangeState",checkTimer)};$(document).ready(window.galleryRefresh)},1903930,331341);