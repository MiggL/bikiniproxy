define(["jquery","domReady"],function(b,a){a(function(){c()});function c(){var d=b(".auto-arrange-product-details-js > div").length;switch(d){case 1:b(".auto-arrange-product-details-js > div").css("width","100%");break;case 2:if(!b(".auto-arrange-product-details-js hr").length){b(".auto-arrange-product-details-js > div:nth(1)").after("<hr/>")}break;case 3:b(".auto-arrange-product-details-js > div:nth(2)").css("clear","left").css("width","100%");if(!b(".auto-arrange-product-details-js hr").length){b(".auto-arrange-product-details-js > div:nth(1)").after("<hr/>")}break;default:b(".auto-arrange-product-details-js > div:nth(2)").css("clear","left");if(!b(".auto-arrange-product-details-js hr").length){b(".auto-arrange-product-details-js > div:nth(1)").after("<hr/>")}}}});