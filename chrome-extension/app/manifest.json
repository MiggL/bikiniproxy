{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/babel.min.js",
      "scripts/background.js",
      "scripts/repair_strategies/redirect_HTTP2HTTPS.js",
      "scripts/repair_strategies/inject_library.js",
      "scripts/repair_strategies/not_null.js",
      "scripts/babel.min.js"
    ]
  },
  "content_scripts": [
    {
			"all_frames": true,
			"js": [
				"scripts/inject.js"
			],
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "scripts/error_logging.js",
    "scripts/jquery-1.12.4.min.js",
    "scripts/angular.js",
    "scripts/require.js",
    "scripts/underscope.js"
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "BikiniProxy",
    "default_popup": "popup.html"
  }
}
