<script>
	$('.play-btn').click(function(){
		$('.videopane-contaner').html('<iframe src="//www.youtube-nocookie.com/embed/pVGkFfhvhHg?rel=0&vq=hd1080&autoplay=1" frameborder="0" allowfullscreen=""></iframe>');
		$('.videopane').fadeIn(1300);
		return false;
	});
	$('.closevideo,.videopane').click(function(){
		$('.videopane').fadeOut(300);
		$('.videopane-contaner').html('');
		return false;
	});
	if ($('.site').width()>800)  {
		$('.video-box').html('<video preload="auto" autoplay loop="loop" id="hero-video" class="video-playing" poster="/img/main/poster.jpg">'
				+'<source src="/video/Web1600x762HQ.webm" type="video/webm">'
				+'<source src="https://player.vimeo.com/external/140772741.hd.mp4?s=2b53cd4bac12d5ba45391d9ad4f00820&profile_id=113&download=1" type="video/mp4">'
				+'</video>');
		var player = $('#hero-video').get(0);
		if (player.canPlayType && player.canPlayType('video/mp4').replace(/no/, '')) {
			$('.bg-hero').css({'background':'white'});
			player.play();
			console.log('can play');
		} else {
			console.log("can't play");
			$('.video-box').remove();
		}
	} else {
		$('.video-box').remove();
	}
</script>
