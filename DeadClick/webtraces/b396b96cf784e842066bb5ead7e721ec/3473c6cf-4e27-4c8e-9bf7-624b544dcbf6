function storeSREDID(){var a=document.location.search;if(start=a.indexOf("SREDID="),-1!=start){var b=a.indexOf("&",start);-1==b&&(b=a.length);var c=new Date;c.setTime(c.getTime()+2592e6),document.cookie=a.substring(start,b)+"; expires="+c.toGMTString()+"; path=/"}}function getSREDID(){var a="SREDID=",b=document.cookie,c=b.indexOf(a);if(-1==c)return null;c+=a.length;var d=b.indexOf(";",c);return-1==d&&(d=b.length),b.substring(c,d)}