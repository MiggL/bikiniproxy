                { className: __WEBPACK_IMPORTED_MODULE_7__styles_css___default.a.subSection + ' noTextFlicker', key: '' + sectionIndex + i, href: '' + subSection.url },
                subSection.title
            );
        })
    );
};

/*
  Loops through sections and stacks sections if they have
  too few subSections to have their own column.
  The Columns will only stack if the subSections are adjacent
*/
var chunkSections = function chunkSections(sections) {
    var minSubSectionsForCol = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 10;

    var subSectionCount = 0;
    var currentlyStacking = 0;
    return sections.reduce(function (acc, section, i) {
        if (section.subSections.length > minSubSectionsForCol) {
            acc.push(section);
        } else {
            subSectionCount += section.subSections.length;
            if (subSectionCount <= minSubSectionsForCol) {
                if (acc[currentlyStacking]) {
                    acc[currentlyStacking].push(section);
                } else {
                    acc.push([section]);
                    subSectionCount = section.subSections.length;
                    currentlyStacking = i - 1;
                }
            } else {
                acc.push([section]);
                subSectionCount = section.subSections.length;
                currentlyStacking = i;
            }
        }

        return acc;
    }, []);
};

var _default = function _default(props) {
    return __WEBPACK_IMPORTED_MODULE_0_react___default.a.createElement(__WEBPACK_IMPORTED_MODULE_5_modules_query_container__["a" /* default */], _extends({ Template: SubNav, queryConfig: __WEBPACK_IMPORTED_MODULE_3__graphql__["a" /* graphqlConfig */], query: __WEBPACK_IMPORTED_MODULE_3__graphql__["b" /* navFlyoutQuery */] }, props));
};

/* harmony default export */ __webpack_exports__["a"] = (_default);
;

