(function ($){

	$(function(){
	
		var Mylog = {
			
			toConsole :	 function ( text , groupname , collapsed ){
			
				if (typeof text			=== 'undefined' )  text = '';
				if (typeof groupname	=== 'undefined' )  groupname = false;
				if (typeof collapsed	=== 'undefined' )  collapsed = true;
			
				if ( groupname != undefined ){
					this.openGroup( groupname , collapsed );
				}
				window.console.log( text );
			},

			openGroup : function ( groupname , collapsed  ){

				if (typeof groupname	=== 'undefined' )  groupname = false;
				if (typeof collapsed	=== 'undefined' )  collapsed = true;
/*			
				if ( groupname != false ){
					this.closeGroup();
				}
				*/
				if ( groupname != false ){
					window.console.group ( groupname );
				}
			},

			closeGroup: function (){
				window.console.groupEnd();
			}
		};
		window.MyLog = Mylog;
	});
})(jQuery.noConflict());
