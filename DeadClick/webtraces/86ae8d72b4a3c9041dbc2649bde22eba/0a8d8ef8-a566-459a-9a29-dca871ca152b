function xml2json(b,e){var f={toObj:function(h){var p={};
if(h.nodeType==1){if(h.attributes.length){for(var m=0;
m<h.attributes.length;
m++){p["@"+h.attributes[m].nodeName]=(h.attributes[m].nodeValue||"").toString()
}}if(h.firstChild){var g=0,k=0,j=false;
for(var q=h.firstChild;
q;
q=q.nextSibling){if(q.nodeType==1){j=true
}else{if(q.nodeType==3&&q.nodeValue.match(/[^ \f\n\r\t\v]/)){g++
}else{if(q.nodeType==4){k++
}}}}if(j){if(g<2&&k<2){f.removeWhite(h);
for(var q=h.firstChild;
q;
q=q.nextSibling){if(q.nodeType==3){p["#text"]=f.escape(q.nodeValue)
}else{if(q.nodeType==4){p["#cdata"]=f.escape(q.nodeValue)
}else{if(p[q.nodeName]){if(p[q.nodeName] instanceof Array){p[q.nodeName][p[q.nodeName].length]=f.toObj(q)
}else{p[q.nodeName]=[p[q.nodeName],f.toObj(q)]
}}else{p[q.nodeName]=f.toObj(q)
}}}}}else{if(!h.attributes.length){p=f.escape(f.innerXml(h))
}else{p["#text"]=f.escape(f.innerXml(h))
}}}else{if(g){if(!h.attributes.length){p=f.escape(f.innerXml(h))
}else{p["#text"]=f.escape(f.innerXml(h))
}}else{if(k){if(k>1){p=f.escape(f.innerXml(h))
}else{for(var q=h.firstChild;
q;
q=q.nextSibling){p["#cdata"]=f.escape(q.nodeValue)
}}}}}}if(!h.attributes.length&&!h.firstChild){p=null
}}else{if(h.nodeType==9){p=f.toObj(h.documentElement)
}else{alert("unhandled node type: "+h.nodeType)
}}return p
},toJson:function(r,j,q){var p=j?('"'+j+'"'):"";
if(r instanceof Array){for(var k=0,s=r.length;
k<s;
k++){r[k]=f.toJson(r[k],"",q+"\t")
}p+=(j?":[":"[")+(r.length>1?("\n"+q+"\t"+r.join(",\n"+q+"\t")+"\n"+q):r.join(""))+"]"
}else{if(r==null){p+=(j&&":")+"null"
}else{if(typeof(r)=="object"){var h=[];
for(var g in r){h[h.length]=f.toJson(r[g],g,q+"\t")
}p+=(j?":{":"{")+(h.length>1?("\n"+q+"\t"+h.join(",\n"+q+"\t")+"\n"+q):h.join(""))+"}"
}else{if(typeof(r)=="string"){p+=(j&&":")+'"'+r.toString()+'"'
}else{p+=(j&&":")+r.toString()
}}}}return p
},innerXml:function(i){var h="";
if("innerHTML" in i){h=i.innerHTML
}else{var g=function(p){var m="";
if(p.nodeType==1){m+="<"+p.nodeName;
for(var k=0;
k<p.attributes.length;
k++){m+=" "+p.attributes[k].nodeName+'="'+(p.attributes[k].nodeValue||"").toString()+'"'
}if(p.firstChild){m+=">";
for(var o=p.firstChild;
o;
o=o.nextSibling){m+=g(o)
}m+="</"+p.nodeName+">"
}else{m+="/>"
}}else{if(p.nodeType==3){m+=p.nodeValue
}else{if(p.nodeType==4){m+="<![CDATA["+p.nodeValue+"]]>"
}}}return m
};
for(var j=i.firstChild;
j;
j=j.nextSibling){h+=g(j)
}}return h
},escape:function(g){return g.replace(/[\\]/g,"\\\\").replace(/[\"]/g,'\\"').replace(/[\n]/g,"\\n").replace(/[\r]/g,"\\r")
},removeWhite:function(h){h.normalize();
for(var i=h.firstChild;
i;
){if(i.nodeType==3){if(!i.nodeValue.match(/[^ \f\n\r\t\v]/)){var g=i.nextSibling;
h.removeChild(i);
i=g
}else{i=i.nextSibling
}}else{if(i.nodeType==1){f.removeWhite(i);
i=i.nextSibling
}else{i=i.nextSibling
}}}return h
}};
if(b.nodeType==9){b=b.documentElement
}var d=f.toJson(f.toObj(f.removeWhite(b)),b.nodeName,"\t");
return"{\n"+e+(e?d.replace(/\t/g,e):d.replace(/\t|\n/g,""))+"\n}"
}var rss="";
