    var getCookieByName = function(name) {
        var start = document.cookie.indexOf(name + "=");
        var len = start + name.length + 1;
        if ((!start) && (name != document.cookie.substring(0, name.length))) {
            return null;
        }
        if (start == -1) return null;
        var end = document.cookie.indexOf(';', len);
        if (end == -1) end = document.cookie.length;
        return unescape(document.cookie.substring(len, end));
    };

    //Handle clientside url history tracking
    var pageInfo = datalayer.pageInfo;
    pageInfo.currentUrlHash = window.location.href;

    if (window.localStorage) {
        pageInfo.previousUrlHash = decodeURIComponent(localStorage.getItem('currentUrlHash'));
        localStorage.setItem('currentUrlHash', encodeURIComponent(pageInfo.currentUrlHash));
    } else {
        pageInfo.previousUrlHash = null;
    }


    //Track account data via cookie info
    var sessionId = getCookieByName('stb-auth');
    var authenticated = (sessionId !== null);
    var userId = getCookieByName('rep:userId');
    var clientId = getCookieByName('pty_id');
    var clientLevel = getCookieByName('client_level');
    var isPreferredClient = getCookieByName('isPreferredClient');
    var clientType = (isPreferredClient === 'true') ? 'preferred' : 'authorized';

    var accountData;
    if (authenticated) {
        accountData = {
            authenticated: true,
            clientId: clientId,
            clientLevel: clientLevel,
            userId: userId,
            clientType: clientType
        };
    } else {
        accountData = { authenticated: false };
    }
    datalayer.account = accountData;


    //Bootstrapped data from templates must be loaded into the datalayer
    //before the _satellite.pageBottom() call
    if (window.stb.digitalDataBootstrap) {
        var bootstrap = window.stb.digitalDataBootstrap;

        /**
         * Recursively assign data from the bootstrap object to the datalayer.
         * Assigns values from the bootstrap data object to the parent object.
         * If the value is a non-object, it is simply assigned to the parent.
         * If the value is an object, it is recursively assigned.
         * Essentially this is emulating Object.assign behavior
         *
         * Example: if datalayer = {registration: {existing: 1, foo: 'foobar'}}
         * and bootstrap = {registration: {foo: 'bar', baz: {a: 1, b: 2}},
         * Result is {registration: {existing: 1, foo: 'bar', baz: {a:1, b: 2}}
         *
         * @param payload The bootstrapped data to assign
         * @param parentData The parent data object
         *
         * @returns Hydrated object
         **/
        var assignDataReducer = function (payload, parentData) {
            return Object.keys(payload).reduce(function (data, key) {
                var value = payload[key];
                if (typeof value === 'object' && !Array.isArray(value)) {
                    if (!parentData[key]) { parentData[key] = {}};
                    data = assignDataReducer(value, parentData[key]);
                }
                else {
                    data[key] = value;
                }
                return data;
            }, parentData);
        };

        //assign data from bootstrap to the datalayer
        bootstrap.reduce(function (dl, data) {
            var pageInfo = data[0];
            var payload = data[1];
            if (pageInfo) {
                dl.pageInfo.currentActiveScreen = data[0].activeScreen;
            }
            if (payload) {
                dl = assignDataReducer(payload, dl);
            }
            return dl;
        }, datalayer);;

        //remove the bootstrapped data
        delete window.stb.digitalDataBootstrap;
    }

    window.stb.digitalData = datalayer;

    /* DTM code for COM-991 */
    try {
        _satellite.pageBottom();
    }
    catch(err) {
        stb.digitalData.error = { service: err }
    }
</script>
