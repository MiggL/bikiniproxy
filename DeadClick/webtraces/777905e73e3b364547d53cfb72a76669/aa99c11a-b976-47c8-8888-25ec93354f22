            function PlayVideo(aid,vid,cid){
				
				console.log("lsJquery called");
				var data = jQuery('#layerslider_3').layerSlider('data');
				data.paused=false
				jQuery('#layerslider_3').layerSlider('stop');
				
				if(vid == 'ls-video1')
				{
					document.getElementById(vid).parentNode.style.left = '34px';
				}
				else if(vid == 'ls-video2')
				{
					document.getElementById(vid).parentNode.style.left = '37px';
				}
				
				document.getElementById('layout_masking_menu').style.display = "block";
                document.getElementById('layerslider_3').style.zIndex = 1002;
                document.getElementById(vid).style.display = "block";
                document.getElementById(vid).parentNode.style.zIndex = 1003;
                document.getElementById(aid).style.display = "none";
                document.getElementById(cid).style.display = "block";
                document.getElementById(cid).parentNode.style.zIndex = 1004;
                var video = document.getElementById(vid);
                video.play();

                video.addEventListener('ended',function(){
					document.getElementById('layout_masking_menu').style.display = "none";
					document.getElementById('layerslider_3').style.zIndex = 1;
					document.getElementById(vid).style.display = "none";
					document.getElementById(vid).parentNode.style.zIndex = 0;
					document.getElementById(aid).style.display = "block";
					document.getElementById(cid).style.display = "none";
					document.getElementById(cid).parentNode.style.zIndex = 0;
					lsjQuery('#layerslider_3').layerSlider('stop');
					
                }, false);
				/*
                video.addEventListener('pause',function(){
					
					document.getElementById('layout_masking_menu').style.display = "none";
					document.getElementById('layerslider_3').style.zIndex = 1;
					document.getElementById(vid).style.display = "none";
					document.getElementById(vid).parentNode.style.zIndex = 0;
					document.getElementById(aid).style.display = "block";
					document.getElementById(cid).style.display = "none";
					document.getElementById(cid).parentNode.style.zIndex = 0;
                }, false);
				*/
				
				video.onprogress = function()
				{
					if(vid == 'ls-video1')
					{
						document.getElementById(vid).parentNode.style.left = '34px';
					}
					else if(vid == 'ls-video2')
					{
						document.getElementById(vid).parentNode.style.left = '37px';
					}
				}
				
            }
            function stopVideo(aid,vid,cid,mid){
				
                var video = document.getElementById(vid);
                video.pause();
                video.currentTime = 0;
				document.getElementById('layout_masking_menu').style.display = "none";
				document.getElementById('layerslider_3').style.zIndex = 1;
				document.getElementById(vid).style.display = "none";
				document.getElementById(vid).parentNode.style.zIndex = 0;
				document.getElementById(aid).style.display = "block";
				document.getElementById(cid).style.display = "none";
				document.getElementById(cid).parentNode.style.zIndex = 0;
				lsjQuery('#layerslider_3').layerSlider('start');
				
            }
			
        </script>


