  }; 
  
  ADventori.conf.dom.frameAttrs = {'style': 'margin:0; padding:0; border:0; max-width:846px; max-height:153px; width;100%; height:100%;', 'width' : '100%', 'height' : '100%'};
}
//*****************************************************************
//************************ Gestion des Stats **********************
ADventori.conf.scenario = eadCreative;

ADventori.conf.statData = {
  'REDIRECTION' : redirection,
  'STRAT'   : Strategy, 
  'AD'        : eadCreative,
  'EADPUBLISHER'    : eadPublisher,
  'EADNAME'         : eadName,
  'EADLOCATION'     : eadLocation,
  'EADCREATIVE'     : eadCreative+'-'+eadCreativeType,
  'EADCREATIVETYPE' : eadCreativeType,
