					});
					ga("ec:setAction", "click", {"list": t_list});
					ga("send", "event", "Enhanced-Ecommerce","click", "product_click_"+t_action, {"nonInteraction": 1});  
                                   }else{
                                   t_call_fired=false;
				}
                                return t_call_fired;
			}
