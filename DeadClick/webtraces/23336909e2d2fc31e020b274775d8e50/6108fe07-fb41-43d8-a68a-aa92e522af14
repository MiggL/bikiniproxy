var $j = jQuery.noConflict();
window.onload = function(){
    $j(".level0").click(function(){
        $j(this).find(".level-top").toggleClass("top-level-custom");
        $j(this).find(".mm-wrapper").toggleClass("mm-wrapper-custom");
    });
    $j(".level0").focusout(function(){
        $j(this).find(".level-top").addClass("top-level-custom");
        $j(this).find(".mm-wrapper").addClass("mm-wrapper-custom");
    });
    $j(".level0").ready(function(){
        $j(this).find(".level-top").addClass("top-level-custom");
        $j(this).find(".mm-wrapper").addClass("mm-wrapper-custom");
    });
};
