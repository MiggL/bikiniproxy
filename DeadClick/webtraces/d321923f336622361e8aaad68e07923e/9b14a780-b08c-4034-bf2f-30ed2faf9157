<script type="text/javascript">
function initialize() {

	var myLatlng = new google.maps.LatLng(51.45446142342818,-2.5982164281555242);
	
	var mapOptions = {
		zoom: 15,
		center: myLatlng,
		scrollwheel: false,
		mapTypeId: google.maps.MapTypeId.ROADMAP
	}
	
	var map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);
	
	var image = 'http://lightningprocessbristol.com/wp-content/themes/phil-parker-custom-team/images/mapMarker.png';
	
	var marker = new google.maps.Marker({
	  position: myLatlng,
	  map: map,
	  icon: image
	});
  
}

google.maps.event.addDomListener(window, 'load', initialize);

</script>

