          content += '<textarea id="call_to_mod_reason_note"  onkeydown="textCounter(this,500)" ';
          content += 'name="call_to_mod_reason_note" style="width:470px;height:75px;"></textarea>';
          content += '</form>';
          content += '<div id="error_msg" style="font-size:10px;color:red;font-weight:lighter;display:none;">';
          content += 'Details on rules violation are required, and must be at least 10 characters</div>';
          content += '</div>';
          $.prompt(content,{callback:
          function (v,m) {
          },
          submit: function(v,m) {
            if (v) {
              reason_note = $("#call_to_mod_reason_note").val();
              if (reason_note.length<10) {
                $("#error_msg").show();
                setTimeout('$("#error_msg").hide();',3000);
                return false;
              } else document.call_mod_note_form.submit();
            }
            else return true;
          },
          buttons: { 'Submit': true, 'Cancel': false }});
        }
        function textCounter(field, maxlimit) {
          if (field.value.length > maxlimit) field.value = field.value.substring(0, maxlimit);
        }
        </script>
        <script type="text/javascript">
          if (typeof(opera) != 'undefined') {
            if (opera.setOverrideHistoryNavigationMode) opera.setOverrideHistoryNavigationMode('compatible');
          }
        </script>
      <DIV id="tiplayer" style="visibility:hidden;position:absolute;z-index:1000;top:-100;"></DIV>
