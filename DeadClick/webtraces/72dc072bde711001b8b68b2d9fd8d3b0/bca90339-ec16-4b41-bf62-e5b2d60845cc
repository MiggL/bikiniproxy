window.addEvent('domready',function() {

                
               
		var slider0 = new vmMooslide({
			box: $('vmMooslide0'),
			items: $$('#vmMooslide0 .vmMooslide-item'),
                        size: 170,
                        boxheight: 190,
                        mode: 'vertical',
                        autoPlay: 1,
                        interval: 2300,
                        fxOptions: {
				duration: 1000,
				transition: Fx.Transitions.Expo.easeOut,
				wait: false
			},
			addButtons: {
				previous: $('prev0'),
				play: $('play0'),
				stop: $('stop0'),
				next: $('next0')
			}
		});
                
                $('vmMooslider0').addEvent('mouseenter', function(){
              
                                slider0.stop();
               
                                                }); 
                 $('vmMooslider0').addEvent('mouseleave', function(){
              
                                slider0.play(2300,'next','false' );
               
                                                }); 
});

       
window.addEvent('load',function() {    
               
                var list = $$('#vmMooslide0 div .vmMooslide-product-bloc');
                
list.each(function(element) {
        
        var producttitle = element.getElement('.vmMooslide-product-bloc-name a');
        
        var productprice = element.getElement('.vmMooslide-product-bloc-price');
        var font = producttitle.getStyle('font-size');
        var fontprice = productprice.getStyle('font-size');
        var image = element.getElement('img');
        var largeur = image.getSize().size.x;
        var hauteur = image.getSize().size.y;
        var pourcent = (10 / 100) + 1;
        var newlargeur = largeur * pourcent;
        var newhauteur = hauteur * pourcent;
   
       
        //FX's
        var nameChange = new Fx.Style(producttitle, 'font-size', {duration:200});
        var priceChange = new Fx.Style(productprice, 'font-size', {duration:200});
        var largeurChange = new Fx.Style(image, 'width', {duration:200});
        var hauteurChange = new Fx.Style(image, 'height', {duration:200});
    
        
        element.addEvent('mouseenter', function(){
              
               nameChange.stop();
               priceChange.stop();
               largeurChange.stop();
               hauteurChange.stop();
               nameChange.start(font, 16);
               priceChange.start(fontprice, 16);
               largeurChange.start(largeur, newlargeur);
               hauteurChange.start(hauteur, newhauteur);
               
                                                });

        element.addEvent('mouseleave', function(){
     
               nameChange.stop();
               priceChange.stop();
               largeurChange.stop();
               hauteurChange.stop();
               nameChange.start(16, font);
               priceChange.start(16, fontprice);
               largeurChange.start(newlargeur, largeur);
               hauteurChange.start(newhauteur, hauteur);
                
                                                });
     
             });     
                
});
</script>    
<div style="margin: auto; height: 200px;width:170px;" id="vmMooslider0">
