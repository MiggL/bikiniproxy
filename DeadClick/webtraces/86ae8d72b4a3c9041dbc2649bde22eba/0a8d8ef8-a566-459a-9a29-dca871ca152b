f.define("select2/selection/search",["jquery","../utils","../keys"],function(j,i,g){function h(n,k,m){n.call(this,k,m)
}h.prototype.render=function(m){var k=j('<li class="select2-search select2-search--inline"><input class="select2-search__field" type="search" tabindex="-1" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" role="textbox" aria-autocomplete="list" /></li>');
