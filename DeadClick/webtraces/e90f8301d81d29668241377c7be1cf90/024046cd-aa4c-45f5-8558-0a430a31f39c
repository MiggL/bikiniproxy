	});
	$(".widget-share-toggle").click(function(){
		$(".tracker").toggleClass("widget-share-open");
	});
	$(".credit-toggle").click(function(){
		$(".tracker").toggleClass("credit-open");
	})
	$(".how-text-toggle").click(function(){
		$(".tracker").toggleClass("how-open");
	});
	$(".close-pop-up").click(function(){
		$(".tracker").removeClass("how-open");
		$(".tracker").removeClass("credit-open");
		$(".tracker").removeClass("widget-share-open");
	});
	$('.tracker').delay(3000).queue(function(){
	  $(this).addClass("show-graph");
	});

	/* Detect narrow iframe */
	//var section = $('.tracker');
	//var width = section.width();
	//if (width < 400)
    //section.addClass('narrow-tracker');


	$('.count').each(function () {
    $(this).prop('Counter',0).animate({
        Counter: $(this).text()
    }, {
        duration: 5000,
        easing: 'swing',
        step: function (now) {
            $(this).text(Math.ceil(now));
        }
    });
});

</script>
