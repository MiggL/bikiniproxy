(function($,Edge,compId){var Composition=Edge.Composition,Symbol=Edge.Symbol;Edge.registerEventBinding(compId,function($){
//Edge symbol: 'stage'
(function(symbolName){Symbol.bindTriggerAction(compId,symbolName,"Default Timeline",15000,function(sym,e){sym.i=sym.i?(sym.i-1):1;if(sym.i>0){sym.play(0);}
else{sym.stopAll();sym.getSymbol("cta").stop(0);}});
//Edge binding end
})("stage");
//Edge symbol end:'stage'

//=========================================================

//Edge symbol: 'cta'
(function(symbolName){Symbol.bindTriggerAction(compId,symbolName,"Default Timeline",1000,function(sym,e){sym.play(0);});
//Edge binding end
})("cta");
//Edge symbol end:'cta'
})})(AdobeEdge.$,AdobeEdge,"EDGE-REMIND");