            <div class="widget-body body body-none  body-compact-all"><script>
$(document).ready(function(){
  $("#mandeley").hide();
  var url = $("meta[name='DCSext.WT_doi']").attr("content");
  var booktest = $("meta[name='Dcsref']").attr("content").toLowerCase(); 
  if(booktest.indexOf("book") == -1) {
     $("#mandeley").fadeIn("slow");
  }
  $("#mandeley").click(function(e){
      var er = '#Error Value is Undefined';       
      var link = "https://www.mendeley.com/import/?url="; 
      var ttext = '&title=';
      var title = document.title;
      var doi = "https://doi.org/"+url; 
      var full = link+doi+ttext+title;

      full = full.replace(/([\"])/g,'\\"');
      full = full.replace(/([\'])/g,"\\'");

      var dt = url = $("meta[name='dc.Date']").attr("content");
        if(!dt){ dt = er;}  
      var jname = $("meta[name='DCSext.WT_name']").attr("content"); 
        if(!jname) {   
            jname = $('h1').text();
            if(!jname){ jname = er;}
        } 
      var jtitle = $("meta[name='DCSext.WT_title']").attr("content"); 
        if(!jtitle) {   
        jtitle = $('.hlFld-Title').text();
        if(!jtitle){ jtitle = er;}
        } 
        jtitle = jtitle.replace(/([\'])/g,"\\'"); 

      var eveLabel = jname+';;'+jtitle+';;'+dt;
      var eveCategory = 'Share on Mendeley';
      var eveAction = $("meta[name='Dcsuri']").attr("content"); 

      window.ga ('send', 'event', eveCategory, eveAction, eveLabel);
      if (typeof window.dataLayer !== "undefined" && typeof window.dataLayer.push === "function") try {
      window.dataLayer.push({ 'event' : 'ShareOnMendeley', 'eventCategory' : eveCategory, 'eventAction' : eveAction, 'eventLabel' : eveLabel});
      } catch (d) {}
     window.open(full);
     e.preventDefault();          
  });
});
</script>
<div style="margin-top:14px;display:none;" id="mandeley"><a href="#" title="Save to Mendeley - new window."><img width="100" height="27" alt="Save to Mendeley " src="data:image/png;base64," /></a></div></div>
        </div>
    </div>
    



    
    
    
    
    
    
        
    
        
    
        
    
        
    
        
    
        
    
        
            
        
    
        
    
        
    
        
    
        
    
        
    
    
        
    
        
    
        
            
            
                
            
            
                
                
                
            
        
    
        
    
        
    
        
    
        
            
            
            
        
    
        
    
        
    
        
    
        
    
        
    
    
    

    
