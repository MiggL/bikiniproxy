//add_to_wishlist
//TODO Infinite loop
jQuery(".addto-wishlist-icon").click(function (){
    var index = jQuery(".addto-wishlist-icon").index(this);
    var imageUrl;
    if(utag.data.page_type =="product"){
        imageUrl = utag.data.product_image_url[0];
    }else if (utag.data.page_type =="category" || utag.data.page_type =="search"){
    	var imageTag = jQuery(".product-tile .product-image .thumbnail .thumb-link img")[index];
    	if (imageTag) {
    		 imageUrl = imageTag.src;
    	} else{
    		 imageUrl = jQuery(".product-primary-image img")[0].src
    	}
    }

    var data = {
        "category_id"        : utag_data.page_category,
        "ecommerce_action"   : "wishlist",
        "event_name"         : "add_to_wishlist",
        "product_image_url"  : ["" + imageUrl],
        "product_sku"        : ["" + jQuery(this).data("item_id")],
        "product_category"   : ["" + jQuery(this).data("category_name") ],
        "product_id"         : ["" + jQuery(this).data("item_id")],
        "product_name"       : ["" + jQuery(this).data("item_name")]
    };

    utag_data.ecommerce_action = data.ecommerce_action;
    utag_data.event_name = data.event_name;
    utag_data.product_category = data.product_category;
    utag_data.product_id = data.product_id;
    utag_data.product_image_url = data.product_image_url;
    utag_data.product_name = data.product_name;
    utag_data.product_sku = data.product_sku;
    utag.link(data);
});



