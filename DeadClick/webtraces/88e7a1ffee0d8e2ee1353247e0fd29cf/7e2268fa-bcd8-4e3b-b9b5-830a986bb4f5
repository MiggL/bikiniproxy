<script type = "text/javascript">
    function setLightImageHeight() {
        var width = jQuery('.limage img').width();
        var imgHeight = Math.round(width*0.667);
        jQuery('.limage img').css("height", imgHeight+"px");
    }
    function showLightbox(src) {

        jQuery(function ($) {
            var images = ["http:\/\/media.woolwarehouse.co.uk\/media\/catalog\/product\/cache\/1\/image\/9df78eab33525d08d6e5fb8d27136e95\/s\/r\/sr_f218_402_0.jpg"];
            $('.limage').html("<img src= " + src + ">" + "<p class='l-image-count'>Image 1 of "+ images.length +"</p>");
            $('#lightbox').fadeIn();
            setLightImageHeight();
            $(window).resize(function () {
                setLightImageHeight();
            });
            if(images.length === 1)
            {
                $('.right').addClass('hide');
                $('.left').addClass('hide');
            }

            $('.right').on('click',function () {

                var images = ["http:\/\/media.woolwarehouse.co.uk\/media\/catalog\/product\/cache\/1\/image\/9df78eab33525d08d6e5fb8d27136e95\/s\/r\/sr_f218_402_0.jpg"];
                var x;
                if(images.length > 1)
                {
                    for(x=0; x<1-1; x++){
                        if(images[x] == src){
                            break;
                        }
                        else{
                            continue;
                        }
                    }

                    if(x==0){
                        x=-1;
                    }

                    $('.limage').fadeOut(0);
                    $('.limage').html("<img src = " + images[x+1] + ">"+"<p class='l-image-count'>Image " + (x+2) + " of "+ images.length +"</p>");
                    $('.limage').fadeIn(500);
                    setLightImageHeight();
                    src=images[x+1];
                }
            });

            $('.left').on('click',function () {

                var images = ["http:\/\/media.woolwarehouse.co.uk\/media\/catalog\/product\/cache\/1\/image\/9df78eab33525d08d6e5fb8d27136e95\/s\/r\/sr_f218_402_0.jpg"];
                var x;
                if(images.length > 1)
                {
                    for(x=0; x<1-1; x++){
                        if(images[x] == src){
                            break;
                        }
                        else{
                            continue;
                        }
                    }

                    if(x==0){
                        x=1;
                    }

                    $('.limage').fadeOut(0);
                    $('.limage').html("<img src = " + images[x-1] + ">"+"<p class='l-image-count'>Image " + (x) + " of "+ images.length +"</p>");
                    $('.limage').fadeIn(500);
                    setLightImageHeight();
                    src=images[x-1];
                }
            });

            $('.close').on('click',(function () {
                $('#lightbox').fadeOut();
                $('.left').off('click');
                $('.right').off('click');
            }));
            return false;
        });
    }
    jQuery('.product-image img').click(function () {
        showLightbox("http:\/\/media.woolwarehouse.co.uk\/media\/catalog\/product\/cache\/1\/image\/9df78eab33525d08d6e5fb8d27136e95\/s\/r\/sr_f218_402_0.jpg");
        return false;
    })
</script>							</div>

