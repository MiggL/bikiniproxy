    		<script>
            $(function(){
                $(".name").each(function(num_products){
                    var product_name = $(this).find('a').attr('title');
                    $.ajax({
                        url : 'public/php/getProductName.php?product_name='+encodeURI(product_name)+'&language_code=en&size=54&config_translation_approach=3&config_translation_develop_key=&config_google_api_url=com',
                        dataType: 'json',
                        success : function(json){
                            $(".name").eq(num_products).find('a').html(json['product_name']);
                            $(".name").eq(num_products).find('a').attr('title',json['origin_product_name']);
                            $(".box-product .img-responsive").eq(num_products).attr('alt',json['origin_product_name']);
                            $(".box-product .img-responsive").eq(num_products).attr('title',json['origin_product_name']);
                        }
                    });
                });
            });
        </script>
	