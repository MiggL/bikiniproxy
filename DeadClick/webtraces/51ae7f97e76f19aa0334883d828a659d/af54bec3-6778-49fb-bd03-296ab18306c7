<script>
if (typeof(window.onbeforeonload) == 'undefined') {
	$('a').bind('click', function() {
		var target_url = $(this).attr('href');
		
		if (typeof(target_url) != 'undefined') {	
			var reg = /\/products\/\d/; 
			
			if(target_url.toString().match(reg) != null){
				$('#loading_for_product').remove();
				
				var html = $('<div style="position:fixed;top:0;left:0;width:100%;height:100%;text-align:center;padding-top:10%;z-index:9999" id="loading_for_product"><div style="opacity:0.6 !important;filter:alpha(opacity=60);z-index:9999; background:#000;top:0;left:0;width:100%;height:100%;position:fixed;"></div><div style="width:300px;height:100px;background:#fff;margin-right:auto;;margin-left:auto;text-align:center;border:5px solid #bbb; line-height:40px;padding-top:10px;position:relative;z-index:10000;color:#000">loading</div></div>');
				
				$("body").append(html);
				
				window.location = target_url;
				
				return false;
			}
		}
	});
} else {
	$(window).on('beforeunload', function(event){
	   var target_url = event.target.activeElement;
	   
	   var reg = /\/products\/\d/; 
	   
	   if(target_url.toString().match(reg) != null){
			$('#loading_for_product').remove();
				
			var html = $('<div style="position:fixed;top:0;left:0;width:100%;height:100%;text-align:center;padding-top:10%;z-index:9999" id="loading_for_product"><div style="opacity:0.6 !important;filter:alpha(opacity=60);z-index:9999; background:#000;top:0;left:0;width:100%;height:100%;position:fixed;"></div><div style="width:300px;height:100px;background:#fff;margin-right:auto;;margin-left:auto;text-align:center;border:5px solid #bbb; line-height:40px;padding-top:10px;position:relative;z-index:10000;color:#000">loading</div></div>');
			
			$("body").append(html);
	   }
	});	
}
</script>
