<script type="text/javascript">
    function searchV(field, text) {
        if (field.value == text) {
            field.value = '';
        }
    }

    function searchN(field, text) {
        if (field.value == '') {
            field.value = text;
        }
    }
    function clear() {
        document.getElementById('ctl00_Search1_ddlSearchOptions').style = "";
    }
