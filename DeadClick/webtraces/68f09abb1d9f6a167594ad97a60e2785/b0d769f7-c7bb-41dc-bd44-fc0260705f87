$(document).ready(function(){
	// Collect the variables from the current URL
	var edit_url = document.URL; // Collect the whole URL
    var query = edit_url.split("?")[1]; // Remove all information BEFORE the "?"

    function getQueryVariable(variable) // Start the variable selection function
      {
        if (query) {
          var vars = query.split("&");
          for (var i=0;i<vars.length;i++) {
                 var pair = vars[i].split("=");
                 if(pair[0] == variable){return pair[1];}
          }
          return(false);
        }
      }

    var the_prodid = getQueryVariable("magazine"); // Collect the POST ID from the URL

    if (the_prodid == "true") {
    	$('#main.container').css({"width" : "1px", "min-width" : "100%", "*width" : "100%", "margin-top" : "0"  });
    	$('.top,#breadcrumbs,.size_guide_link,.add_to_wishlist,.product_social,.buy_the_outfit,.product_additional,#footer,.superzoom_open,.superzoom_link,.seo__background').hide();
		$("#add_to_bag").click(function () {
			window.parent.jQuery.fancybox.close();
            window.parent.addToBasketSuccess();
		});

    } else {}

});
</script>
