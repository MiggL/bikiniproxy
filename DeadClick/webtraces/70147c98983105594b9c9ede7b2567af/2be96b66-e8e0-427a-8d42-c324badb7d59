
function sshare_sh_button(t,a){$(document).ready(function(){$(a).each(function(){$el=$(this),$el.hide();var a,e="",s="",n="",i="",r="";$el.parents("[data-share-href]").length&&(a=$($el.parents("[data-share-href]")[0]),t=encodeURIComponent(a.data("share-href"))),a&&a.data("share-title")?e+=encodeURIComponent(a.data("share-title")):null!=$(document).attr("title")&&(e+=encodeURIComponent($(document).attr("title"))),a&&a.data("share-desc")?n+=encodeURIComponent(a.data("share-desc")):null!=$('meta[name="description"]').attr("content")?n+=encodeURIComponent($('meta[name="description"]').attr("content")):null!=$(document).attr("title")&&(n+=encodeURIComponent($(document).attr("title"))),a&&a.data("share-img")?i+=a.data("share-img"):null!=$('meta[property="og:image"]').attr("content")&&(i+=$('meta[property="og:image"]').attr("content")),$el.data("share-related")&&(s+="&related="+encodeURIComponent($el.data("share-related"))),$el.data("via-handle")&&(s+="&via="+encodeURIComponent($el.data("via-handle"))),buttons="string"==typeof $el.data("show-only")?$el.data("show-only").split(","):["facebook","twitter","gplus","reddit","linkedin","pinterest","stumbleupon","whatsapp"],$el.data("mobile")?($el.addClass("mobile-sharebar"),-1!==buttons.indexOf("facebook")&&(r+='<p><a href="http://www.facebook.com/sharer/sharer.php?u='+t+'" target="_blank" class="sh_btn sh-facebook" title="Facebook"><i class="fa fa-facebook"></i> </a></p>'),-1!==buttons.indexOf("twitter")&&(r+='<p><a href="https://twitter.com/share?text='+e+"&url="+t+s+'" target="_blank" class="sh_btn sh-twitter" title="Twitter"><i class="fa fa-twitter"></i> </a></p>'),-1!==buttons.indexOf("gplus")&&(r+='<p><a href="https://plus.google.com/share?url='+t+'" target="_blank" class="sh_btn sh-google" title="Google+"><i class="fa fa-google-plus"></i> </a></p>'),-1!==buttons.indexOf("reddit")&&(r+='<p><a href="http://reddit.com/submit?url='+t+"&title="+e+'" target="_blank" class="sh_btn sh-reddit" title="Reddit"><i class="fa fa-reddi