function init_multiplegl2(a,c){if(Array.isArray(c)){window.piwikAsyncInit=function(){try{for(var g=0;g<c.length;g++){var j=c[g];if(typeof j==="function"){j()}else{if(j.length>=3){$clesite="";if(j[2]!=null&&j[2]!=""){$clesite="?clesite="+j[2]}var d=Piwik.getTracker(j[0]+"js/tracker.php"+$clesite,j[1]);d.enableLinkTracking();d.trackPageView()}}}}catch(h){console.log(h)}};var f=document,e=f.createElement("script"),b=f.getElementsByTagName("script")[0];e.type="text/javascript";e.defer=true;e.async=true;e.src=a+"js/tracker.php";b.parentNode.insertBefore(e,b)}};