		_pa.paProduct = {

  findType: "",
  findMatch: "",
  extractAttr: "",
  extractInner: "",
  productId: "",

  run : function(settings) {
    _pa.paProduct.findType = settings.find.type;
    _pa.paProduct.findMatch = settings.find.match;
    switch (_pa.paProduct.findType) {
      case "query":
        var queryString = window.location.search.substring(1);
        _pa.paProduct.productId = _pa.paProduct.findQueryValue(queryString);
        break;
      case "path":
        var filePath = window.location.pathname;
        _pa.paProduct.match_split_1 = settings.find.match_split_1;
        _pa.paProduct.match_split_2 = settings.find.match_split_2;
        var split_result_1 = filePath.split(_pa.paProduct.match_split_1)[1];
        if (split_result_1 !== undefined) {
          var split_result_2 = split_result_1.split(_pa.paProduct.match_split_2)[0];
          if (split_result_2 !== undefined) {
            _pa.paProduct.productId = split_result_2;
          }
        }
        break;
      case "DOM":
        var el = document.querySelector(_pa.paProduct.findMatch);
        _pa.paProduct.productId = _pa.paProduct.extractProductID(el, settings.extract);
        break;
      case "regex":
        var filePath = window.location.pathname;
        _pa.paProduct.findMatch = new RegExp(_pa.paProduct.findMatch);
        var matchingArray = filePath.match(_pa.paProduct.findMatch);
        if (matchingArray.length > 1) {
          _pa.paProduct.productId = matchingArray[1];
        }
        break;
      default:
        break;
    }
    if (_pa.paProduct.productId && _pa.paProduct.productId.replace(/^\s+|\s+$/g, '') != undefined) {
      window._pq = window._pq || [];

      if (settings.transform) {
        for (var transformation in settings.transform) {
          if (settings.transform.hasOwnProperty(transformation)) {
            if (transformation === "before") {
              _pa.paProduct.productId = settings.transform[transformation] + _pa.paProduct.productId;
            } else if (transformation === "after") {
              _pa.paProduct.productId = _pa.paProduct.productId + settings.transform[transformation];
            }
          }
        }
      }
      _pq.push(['trackProduct', _pa.paProduct.productId]);
    }
  },

  extractProductID : function(el, config) {
    var productId;
    if (typeof config === 'string') {
      if (config === 'innerHTML') {
        productId = el.innerHTML;
      }
    } else if (typeof config === 'object') {
      if (el !== null && config.hasOwnProperty('attr')) {
        productId = el.getAttribute(config.attr);
      }
    }
    return productId;
  },

  findQueryValue : function(queryString) {
    if(queryString) {
      var segments = queryString.split("&");
      var pair;
      for(var i = 0; i < segments.length; i++) {
        pair = segments[i].split("=");
        if(pair[0] === _pa.paProduct.findMatch) {
          _pa.paProduct.productId = pair[1];
        }
      }
      return _pa.paProduct.productId;
    }
  }
};

