	$('#triggers li').click(function(){
	
		var current = $('#carousel .slide.current-slide');
		var selectedItem = $(this).index();
		var slideHeight = current.outerHeight();
		var slideWidth = current.outerWidth();
		if(!$('#carousel .slide:eq('+selectedItem+')').hasClass('current-slide')) {
			current.animate({'margin-left':'-100%','width':slideWidth},function(){
				$(this).removeClass('current-slide').attr('style','');
			});
			$('#carousel .slide:eq('+selectedItem+')').css({'position':'absolute','top':0,'width':slideWidth,'display':'block','left':'100%'}).animate({'left':'0%'},function(){
				$(this).addClass('current-slide').attr('style','');
			});
		}
	
		$('.quick-view').css({'display':'block','opacity':0})//reset pop-up boxes
		$('#triggers li').removeClass('overflow');//reset overflow for pop-up triggers
// 		if(!$(this).hasClass('current-trigger')){
// 			var selectedItem = $(this).index();
// 			var current = $('#carousel .slide.current-slide');
// 			current.animate({'left':'-100%'},function(){
// 				$(this).css({'left':'100%'});
// 			}).removeClass('current-slide');
// 			$('#carousel .slide:eq('+selectedItem+')').animate({'left':'0%'}).addClass('current-slide');
			$('#triggers li').stop().animate({'bottom':0}).removeClass('current-trigger').css({'border-color':'#FFFFFF'});
			$(this).addClass('current-trigger').css({'border-color':'#F15C41'});
// 		}
	});

	/*
		page specific menu
	*/
	var pageNavH = $('#page-nav').height();
	$('#page-nav-container').height(pageNavH);
	$(window).scroll(function(){
		var headHeight = $('#head').height();
		var breadCrumbHeight = $('#breadcrumb').outerHeight(true);
			headHeight += breadCrumbHeight;
		var windowTop = $(this).scrollTop();
		var $arrowUp = $('#page-nav .scotty');//arrow to scroll page back to top
		if(windowTop >= headHeight){
			$('#page-nav').addClass('sticky');
			$arrowUp.fadeIn('slow');
		}else{
			$('#page-nav').removeClass('sticky');
			$arrowUp.fadeOut('slow');
		}
	});

	/* scroll menu for about page*/
	$('#page-nav li').on('click', function() {

		var navBarHt = $('#page-nav').height();
	    var scrollAnchor = $(this).attr('data-scroll');
	    var scrollPoint = $('div[data-anchor="' + scrollAnchor + '"]').offset().top - navBarHt;

	    $('body,html').stop().animate({
	        scrollTop: scrollPoint
	    }, 1000);

	    return false;

	})
	$(window).scroll(function() {
	    var windscroll = $(window).scrollTop();
	    var navBarHt = $('#page-nav').height();

	    $('#pulse').fadeOut(1000);

	    if (windscroll >= navBarHt) {
	        $('.about-section').each(function(i) {
	            if ($(this).position().top <= windscroll + (navBarHt+5)) {
	                $('#page-nav li a.highlight').animate('color','#fff').removeClass('highlight');
	                $('#page-nav li a').eq(i).addClass('highlight');
	            }
	        });

	    } else {
	        $('#page-nav li a.highlight').removeClass('highlight');
	        // $('#page-nav li a:first').addClass('highlight');
	    }

	});

	$('#page-nav .scotty').click(function(){
		$('#page-nav li a').removeClass('highlight');
		$('html,body').stop().animate({scrollTop: 0},1000);
	});//end click

	$(window).resize(function(){
		var newPageNavH = $('#page-nav').height();
		if(pageNavH != newPageNavH){
			$('#page-nav-container').height(newPageNavH);
			pageNavH = newPageNavH;
		}
	});
	
	/*pulsating arrow on about page*/
	var $pulse = $('#pulse');
	var count = 0;
    function runIt() {
    	var topStart = parseFloat($pulse.css('top'));
    	var topMove = topStart + 30;
    	if (count == 5) {
    		$pulse.fadeOut(1000);
    		count = 0;
    	}else{
    		$pulse.animate({ 
	            opacity:'.8',
	            top:topMove
	        }, 1000, function() {
	            $pulse.animate({opacity:'0'},200,function(){
	            	$pulse.css('top',topStart);
	            	count++;
        			runIt();
	            });
	        });
    	}
    }

    runIt();

