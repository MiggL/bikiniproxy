function hide_countrylist(divID){
var divObj=document.getElementById(divID)
divObj.style.visibility="hidden"
divObj.style.height="0px"}
function show_countrylist(divID,h){
h = typeof(h) != 'undefined' ? h : 212;
var divObj=document.getElementById(divID)
divObj.style.visibility="visible"
divObj.style.height=h+"px"}
function hidemenu(divID){
var divObj=document.getElementById(divID)
divObj.style.visibility="hidden"
divObj.style.Zindex=4
divObj.style.height="0px"
var divObj2=document.getElementById("formsearch")
divObj2.style.visibility="visible"}
function showmenu(divID){
var divObj=document.getElementById(divID)
divObj.style.visibility="visible"
divObj.style.Zindex=8
divObj.style.height="300px"
var divObj2=document.getElementById("formsearch")
divObj2.style.visibility="hidden"}
