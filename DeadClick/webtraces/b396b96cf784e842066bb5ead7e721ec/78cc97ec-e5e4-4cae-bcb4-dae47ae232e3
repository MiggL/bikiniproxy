<script type="text/javascript">
//Tealium events.

var productClick = function(product){
    utag_data.ecommerce_action = "detail";
    utag_data.event_name = "product_click";

    utag.link({
            "category_id"       : utag_data.page_category,
            "ecommerce_action"  : "detail",
            "event_name"        : "product_click",
            "product_category"  : ["" + jQuery(product).data("category_name") ],
            "product_id"        : ["" + jQuery(product).data("itemid")],
            "product_name"      : ["" + jQuery(product).data("item_name")],
            "product_image_url" : ["" + jQuery(product).data("image_url")]
        });
};

//cart_add
jQuery("#add-to-cart").click(function(){
    // add-to-cart

    var product_quantity = [jQuery("form[id^='dwfrm_product_addtocart_'] #Quantity").val()];

    utag_data.category_id = utag_data.product_category_ids[0];
    utag_data.ecommerce_action = "cart_add";
    utag_data.event_name = "cart_add";
    utag_data.order_currency_code = "USD";
    utag_data.cart_product_id = utag_data.product_id;
    utag_data.cart_product_price = utag_data.product_price;
    utag_data.cart_product_quantity = product_quantity;
    utag_data.cart_product_sku = utag_data.product_sku;
    var orderCurrency = 'USD';


    utag.link({
            "cart_product_id"              : utag_data.cart_product_id,
            "cart_product_price"           : utag_data.cart_product_price,
            "cart_product_quantity"        : utag_data.cart_product_quantity,
            "cart_product_sku"             : utag_data.cart_product_sku,
            "order_currency"               : orderCurrency,
            "category_id"                  : utag_data.product_category_ids[0],
            "ecommerce_action"             : "cart_add",
            "event_name"                   : "cart_add",
            "order_currency_code"          : orderCurrency,
            "product_category"             : utag_data.product_category,
            "product_id"                   : utag_data.product_id,
            "product_image_url"            : utag_data.product_image_url,
            "product_name"                 : utag_data.product_name,
            "product_price"                : utag_data.product_price,
            "product_quantity"             : product_quantity,
            "product_sku"                  : utag_data.product_sku
        });

    utag_data.cart_product_id = undefined;
    utag_data.cart_product_price = undefined;
    utag_data.cart_product_quantity = undefined;
    utag_data.cart_product_id = undefined;
});

//Cart Remove
jQuery("#cart-items-form button[name$='_deleteProduct']").click(function(){
    var index = jQuery("#cart-items-form button[name$='_deleteProduct']").index(this);

    utag_data.ecommerce_action = "cart_remove";
    utag_data.event_name = "cart_remove";

    utag.link({
            "cart_product_id"              : utag_data.cart_product_id,
            "cart_product_price"           : utag_data.cart_product_price,
            "cart_product_quantity"        : utag_data.cart_product_quantity,
            "cart_product_sku"             : utag_data.cart_product_id,
            "ecommerce_action"             : "cart_remove",
            "event_name"                   : "cart_remove", 
            "product_category"             : [utag_data.product_category[index]],
            "product_id"                   : [utag_data.product_id[index]],
            "product_image_url"            : [utag_data.product_image_url[index]],
            "product_name"                 : [utag_data.product_name[index]],
            "product_price"                : [utag_data.product_price[index]],
            "product_quantity"             : [utag_data.product_quantity[index]],
            "product_sku"                  : [utag_data.product_sku[index]]
        });
});

