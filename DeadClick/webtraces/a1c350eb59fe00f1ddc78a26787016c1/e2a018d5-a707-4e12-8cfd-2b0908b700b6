var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];
(function() {
var gads = document.createElement('script');
gads.async = true;
gads.type = 'text/javascript';
var useSSL = 'https:' == document.location.protocol;
gads.src = (useSSL ? 'https:' : 'http:') +
'//www.googletagservices.com/tag/js/gpt.js';
var node = document.getElementsByTagName('script')[0];
node.parentNode.insertBefore(gads, node);
})();

ThisSrc = $("#gad").attr("src");
Src = ThisSrc.split("?");
ThisId = Src[1].split("=");

googletag.cmd.push(function() {
if(ThisId[1] == "index" || ThisId[1] == "history"){
	googletag.defineSlot('/21202031/TT-01-Index-468X60-DISPLAY-DFP', [468, 60], 'div-gpt-ad-1331113780758-0').addService(googletag.pubads());
	googletag.defineSlot('/21202031/TT-02-Index-300X250-DISPLAY', [300, 250], 'div-gpt-ad-1333007677859-0').addService(googletag.pubads());
}else{
	googletag.defineSlot('/21202031/TT-21-news-728X90-DISPLAY-DFP', [728, 90], 'div-gpt-ad-1331113780758-1').addService(googletag.pubads());
	googletag.defineSlot('/21202031/TT-22-news-300X250-DISPLAY-DFP', [300, 250], 'div-gpt-ad-1333007677859-0').addService(googletag.pubads());
}
/*RTA Script TOP*/
var crtg_split = (crtg_content || '').split(';');
var pubads = googletag.pubads();
for (var i=1;i<crtg_split.length;i++){
	pubads.setTargeting("" + (crtg_split[i-1].split('='))[0] + "", "" + (crtg_split[i-1].split('='))[1] + "");
}
/*RTA Script END*/
googletag.pubads().enableSingleRequest();
googletag.enableServices();
});