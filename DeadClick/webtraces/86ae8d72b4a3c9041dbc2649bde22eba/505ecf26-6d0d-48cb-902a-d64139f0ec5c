(function (d, ua, n, s) {
    if (ua.indexOf('MSIE') > -1) return;
    if (ua.indexOf('Trident/') > -1) n = 'ie11';
    if (/(iPhone|iPod|iPad)/i.test(ua) && parseInt(ua.match(/OS [\d]+/i)[0].substr(3)) < 10) n = 'ios9';
    s = d.createElement('script');
    s.src = 'https://load.instinctiveads.com' + '/s/js/platform.' + n + '.js?time=' + new Date().toISOString().slice(0, 13);
    d.head.appendChild(s);
})(document, navigator.userAgent, 'x');
