<script>
	$("input[name='search']").autocomplete({
		delay: 300,
		source: function(request, response) {
			$('input[name=\'search_cid\']').attr('value',"");
			$.ajax({
				url: 'index.php?route=common/header/autocomplete&filter_name=' +  encodeURIComponent(request.term),
				dataType: 'json',
				success: function(json) {
					response($.map(json, function(item) {
						return {
							label: item.name,
							value: item.category_id,
							keyword: item.keyword
						}
					}));
					$(".ui-autocomplete").css("z-index","999");
				}
			});
		}, 
		select: function(event, ui) {
			$('input[name=\'search\']').attr('value', ui.item.label);
			$('input[name=\'search_cid\']').attr('value', ui.item.value);
			$('input[name=\'search_cname\']').attr('value',ui.item.keyword);
			$('.button-search').trigger("click");
			return false;
		},
		focus: function(event, ui) {
		  $('input[name=\'search\']').attr('value', ui.item.label);
		  $('input[name=\'search_cid\']').attr('value', ui.item.value);
		  $('input[name=\'search_cname\']').attr('value',ui.item.keyword);
		  return false;
	   }
	});
	
	$("input[name='search-mobile']").bind("input",function(){
		var filter_name = $(this).val();
		if(filter_name!=""){
			$.ajax({
				url: 'index.php?route=common/header/autocomplete&filter_name=' +  encodeURIComponent(filter_name),
				dataType: 'json',
				success: function(json) {
					var content = "";
					var len = 9;
					if(json.length < len){
						len = json.length;
					}
					if(len>0){
						for(var i=0;i<len;i++){
							content += "<li class='suggest_li'><a href='"+json[i]['href']+"'>"+json[i]['name']+"</a></li>";
						}
						content +="<li><a onclick='$(\'search_content_ul\').hide()' style='width:50px;height:30px;float:right;border-left:1px solid #ddd;text-align:center;line-height:30px;'>Close</a></li>";
						$("#search_content_ul").html(content);
						$("#search_content_ul").show();
					}else{
						$("#search_content_ul").html("");
						$("#search_content_ul").hide();
					}
				}
			});
		}else{
			$("#search_content_ul").html("");
			$("#search_content_ul").hide();
		}
	});
	
	$(".suggest_li").live("click",function(){
		$("#search_content_ul").hide();
		$("input[name='search-mobile']").val($(this).find(a).html());
	});
	$("body").bind("click",function(){
		$("#search_content_ul").hide();	
	});
	
</script>
