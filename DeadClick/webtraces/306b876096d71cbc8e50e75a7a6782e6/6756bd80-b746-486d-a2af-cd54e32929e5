var RELOAD_NOTE='reload=true';var RELOAD_FREQUENCY=600;var DELIM = '?';var reloadPage = document.location.href;if (reloadPage.indexOf(RELOAD_NOTE) != -1)  reloadPage = reloadPage.substring(0, reloadPage.indexOf(RELOAD_NOTE) - 1) +reloadPage.substring(reloadPage.indexOf(RELOAD_NOTE) + RELOAD_NOTE.length + 1);if (reloadPage.indexOf(DELIM) != -1) DELIM='&';document.writeln('<META HTTP-EQUIV="refresh" CONTENT="'+RELOAD_FREQUENCY+';url='+reloadPage+DELIM+RELOAD_NOTE + '">');//document.writeln('<META HTTP-EQUIV="refresh" CONTENT="'+RELOAD_FREQUENCY+';url='+reloadPage+ '">');