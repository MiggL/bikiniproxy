<script>

    // YouTube player
    // ------

    // This code loads the IFrame Player API code asynchronously.
    var tag = document.createElement('script');
    tag.src = "https://www.youtube.com/player_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

    // This function creates an <iframe> (and YouTube player) after the API code downloads.
    var player_intro = null;

    function onYouTubeIframeAPIReady() {

        player_intro = new YT.Player('embed-intro', {
          height: '450',
          width: '800',
          videoId: 'l35e1Ytkwgw',
          playerVars: {rel: 0},
          events: {
            'onError': onIntroPlayerError,
            'onReady': onIntroPlayerReady,
            'onStateChange': onIntroPlayerStateChange
          },
          playerVars: {
            autoplay: 1,
            controls: 0,
            enablejsapi: 1,
            loop: 1,
            playlist: 'l35e1Ytkwgw',
            showinfo: 0,
            modestbranding: 1,
            //origin:
          }
        });
    }

    setTimeout(function() {
        if (player_intro == null) {
            jQuery.publish("homeSkipVideo");
        }
    }, 5000);

    function onIntroPlayerError (argument) {  
        jQuery.publish("homeIntroError", event);
    }

    function onIntroPlayerReady(event) {
        jQuery.publish("homeIntroReady", event);
    }

    function onIntroPlayerStateChange(event) {
        jQuery.publish("homeIntroStateChange", event);
    }
    
</script>

    
