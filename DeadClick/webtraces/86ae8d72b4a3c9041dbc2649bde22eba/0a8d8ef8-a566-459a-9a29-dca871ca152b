for(var s=0;
s<o.children.length;
s++){var m=o.children[s];
var k=this.option(m);
v.push(k)
}var r=i("<ul></ul>",{"class":"select2-results__options select2-results__options--nested"});
r.append(v);
j.append(u);
j.append(r)
}else{this.template(o,p)
}i.data(p,"data",o);
return p
};
g.prototype.bind=function(j,m){var k=this;
