/*----------------------------- HYPER LINKS ------------------------*/

a:link {text-decoration:none;}
a:visited {text-decoration:none;}
a:hover {text-decoration:underline;}
a:active {text-decoration:underline;}

a.point8em
{
  font-style:normal;
  font-family:Arial, Helvetica, sans-serif;
  font-size:0.8em;
}

a.point9em
{
  font-style:normal;
  font-family:Arial, Helvetica, sans-serif;
  font-size:0.9em;
}

a.gold
{
  font-style:normal;
  font-family:Arial, Helvetica, sans-serif;
  color:goldenrod;
}

a.basic:link {text-decoration:none; border:0; }
a.basic:visited {text-decoration:none; border:0; }
a.basic:hover {text-decoration:underline; border:0; }

/*---------------------- GENERIC MAROON MENU BUTTON --------------------*/

.menuButton{
  font: bold 13px arial;
  padding: 6px .075em;
  margin: 0;
  width: 100%;
  background-color: maroon;
  text-align: center; /*set value to "right" for example to align menu to the right of page*/
}

.menuButton li{
  list-style: none;
  display: inline;
}

.menuButton li a{
  /* the text in the button */
  padding: 3px 0.5em;
  text-decoration: none;
  /* Color of default button text in normal state */
  color: #CCA352;
  /* Default background color of button in normal state */
  background-color: maroon;
  /* Default color of border around button */
  border: 2px solid maroon;
}

.menuButton li a:hover{
  /* Text color when hovering */
  color: maroon;
  /* Background color of button when hovering */
  background-color: #CCA352;
  border-style: outset;
  border-color: #CCA352;
}

/* Apply mousedown effect only to NON IE browsers */
.menuButton li a:active{
  border-style: inset;
  color: maroon;
  background-color: #CCA352;
}

.menuButton li a.disabled{
  /* Padding to the left and right and top and bottom of */
  /* the text in the button */
  padding: 3px 0.5em;
  text-decoration: none;
  /* Color of default button text in normal state */
  color: maroon;
  /* Default background color of button in normal state */
  background-color: #CCA352;
  /* Default color of border around button */
  border: 2px solid maroon;
  border-style: inset;
  border-color: #CCA352;
}


