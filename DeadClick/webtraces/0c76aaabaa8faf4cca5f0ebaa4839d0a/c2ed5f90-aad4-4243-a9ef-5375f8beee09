    background: #eee;

    font-family: "Courier 10 Pitch", Courier, monospace;

    font-size: 15px;

    font-size: 1.5rem;

    line-height: 1.6;

    margin-bottom: 1.6em;

    max-width: 100%;

    overflow: auto;

    padding: 1.6em;

}



code,

kbd,

tt,

var {

    font: 15px Monaco, Consolas, "Andale Mono", "DejaVu Sans Mono", monospace;

}



abbr,

acronym {

    border-bottom: 1px dotted #666;

    cursor: help;

}



mark,

ins {

    background: #fff9c0;

    text-decoration: none;

}



sup,

sub {

    font-size: 75%;

    height: 0;

    line-height: 0;

    position: relative;

    vertical-align: baseline;

}



