<script type="text/javascript">
var formNamesArr = new Array();
var currentPageName = 'literature';
var isMobile = false;
// device detection
    var reg1 = /(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|ipad|iris|kindle|Android|Silk|lge|maemo|midp|mmp|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows (ce|phone)|xda|xiino/i;
    var reg2 = /1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i;
    if(reg1.test(navigator.userAgent)|| reg2.test(navigator.userAgent.substr(0,4))) 
    	isMobile = true;
var sysEnv = 'desktop';
if(isMobile)
	sysEnv = 'mobile';
jQuery(document).ready(function(){
	var siteShortName = 'tc';
	var regionName = 'North America';
	if(regionName == undefined || regionName == null || regionName == '')
		regionName = '';
	var langRoot = '/content/trane-commercial/north-america/us/en';
	var countryCode = 'United States';
	if(countryCode == undefined || countryCode == null || countryCode == '')
		countryCode = '';
	var languageCode = 'en';
	var pageType = '';
	var isPageTypeAvailable = false;
	var currentPageTemplatePath = '';
	if(pageType != null && pageType != undefined && pageType != ''){
		currentPageTemplatePath = pageType;
		isPageTypeAvailable = true;
	}
	var primaryCategory = '';
	var secondaryCategory1 = '';
	var secondaryCategory2 = '';
	//console.log('Language Root : /content/trane-commercial/north-america/us/en');
	//console.log('Country Code : United States');
	//console.log('Language Code : en');
	var breadcrumb = '';
	var subCat1Found = false;
	var subCat2Found = false;
	var primaryCatFound = false;
    try {
    jQuery('.breadcrumb li a').each(function(index, obj){
		if(index == 1){
			primaryCategory = $(obj).html().trim().toLowerCase();
			primaryCatFound = true;
		}
		else if(index == 2){
			secondaryCategory1 = $(obj).html().trim().toLowerCase();
			subCat1Found = true;
		}
		else if(index == 3){
			secondaryCategory2 = $(obj).html().trim().toLowerCase();
			subCat2Found = true;
		}
		if(index > 1){
			breadcrumb = breadcrumb + ':';
		}
		if(index > 0)
			breadcrumb = breadcrumb + $(obj).html().trim().toLowerCase();
	});
	/*var activeSubCat = $('.breadcrumb li.active').html();
	if(!primaryCatFound)
		primaryCategory = activeSubCat;
	else if(!subCat1Found)
		secondaryCategory1 = activeSubCat;
	else if(!subCat2Found)
		secondaryCategory2 = activeSubCat;
	breadcrumb = breadcrumb + ':' + activeSubCat;*/
	console.log('Breadcrumb : ' + breadcrumb);
        } catch(err) {
        	console.log('Error setting Breadcrumb : ' + err.message);
    	}
        try {
	if(regionName == '' && countryCode == '')
		dmf.pushValue('page.pageInfo.pageName', siteShortName + ':global:' + languageCode + ':' + breadcrumb);
	else
		dmf.pushValue('page.pageInfo.pageName', siteShortName + ':' + regionName + ':' + countryCode + ':' + languageCode + ':' + breadcrumb);
        } catch(err) { 
            console.log("Error setting pageName:" + err.message); 
    }
	dmf.pushValue('page.pageInfo.language', languageCode);
	dmf.pushValue('page.pageInfo.sysEnv', sysEnv);
	dmf.pushValue('page.category.primaryCategory', primaryCategory);
	dmf.pushValue('page.category.subCategory1', secondaryCategory1);
	dmf.pushValue('page.category.subCategory2', secondaryCategory2);
	dmf.pushValue('user.profile.language', languageCode);
	dmf.pushValue('page.category.pageType', currentPageTemplatePath);
	var isProductDetailPage = 'false';
	if(isProductDetailPage == 'true'){
		dmf.pushValue('product.productInfo.productID', '');
		dmf.pushValue('product.productInfo.productName', 'Product Literature');
		dmf.pushValue('product.category.primaryCategory', '');
		dmf.pushValue('product.attributes.productFindingMethod', '');
	}
	var isSearchPage = 'false';
	if(isSearchPage == 'true'){
		var searchTerm = $('.page-search input[name=q]').val();
		var searchTime = $('.search').text();
		//console.log(searchTime);
		var firstIndex = $('.search').text().indexOf('Results 1 - ') + 12;
		var secondIndex = $('.search').text().indexOf('for ' + searchTerm + '');
		console.log(firstIndex);
		console.log(secondIndex);
		var searchCount = searchTime.substring(firstIndex, secondIndex).trim();
		dmf.pushValue('page.attributes.onsiteSearchTerm', searchTerm);
		dmf.pushValue('page.attributes.onsiteSearchResults', searchCount.split(' ')[2]);
	}
	var isDealerPage = 'false';
	var isBlogPage = 'false';
	var isPartsStorePage = 'false';
	dmf.dumpObject();

	//Form Start Trigger
	jQuery('form').find('input[type="text"]:first, textarea:first, select:first').blur(function(){
		var formEle = $(this).parents('form');
		var formName = formEle.attr('name');
		var formNameAttribPresent = false;
		if(formName != undefined && formName != null && formName != '')
			formNameAttribPresent = true;
		if(!formNameAttribPresent)
			formName = currentPageName;
		if(isPageTypeAvailable)
			formName = pageType;
		console.log('Form Start : Form Name : ' + formName);
		var hasFormAlreadyStarted = false;
		for(var i=0; i<formNamesArr.length; i++){
			if(formName == formNamesArr[i]){
				hasFormAlreadyStarted = true;
				break;
			}
		}
		if(!hasFormAlreadyStarted){
			formNamesArr.push(formName);
			dmf.pushData('formStart', {'formName' : formName.toLowerCase()});
		}
	});

	//Form Submission Trigger
	jQuery('form').submit(function(){
		var formEle = $(this);
		var formName = formEle.attr('name');
		var formNameAttribPresent = false;
		if(formName != undefined && formName != null && formName != '')
			formNameAttribPresent = true;
		if(!formNameAttribPresent)
			formName = currentPageName;
		if(isPageTypeAvailable)
			formName = pageType;
		var searchCriteria = '';
		var productCategory = '';
		if(currentPageName.toLowerCase().indexOf('locate-sales-offices') >= 0){
			console.log('Triggering sales office search type trigger');
			searchCriteria = 'state:' + $('form.store-search select[name="state"]').val();
			searchCriteria = searchCriteria + ',region:' + $('form.store-search select[name="region"]').val();
			dmf.pushData('search', {'searchType' : 'sales office search', 'searchCriteria' : searchCriteria});
		}
		if(currentPageName.toLowerCase().indexOf('literature') >= 0){
			console.log('Triggering literature search type trigger');
			searchCriteria = 'productType:' + $('select[name="productType"]').val();
			searchCriteria = searchCriteria + ',productName:' + $('select[name="productName"]').val();
			searchCriteria = searchCriteria + ',literatureType:' + $('select[name="literatureType"]').val();
			searchCriteria = searchCriteria + ',language:' + $('select[name="language"]').val();
			productCategory = $('form.literature-search select#productType').val();
			dmf.pushData('search', {'searchType' : 'literature search', 'searchCriteria' : searchCriteria});
		}
		if(currentPageName.toLowerCase().indexOf('locate-a-parts-store') >= 0){
			console.log('Triggering parts store search type trigger');
			searchCriteria = 'state:' + $('form.store-search select[name="state"]').val();
			searchCriteria = searchCriteria + ',region:' + $('form.store-search select[name="region"]').val();
			dmf.pushData('search', {'searchType' : 'parts store search', 'searchCriteria' : searchCriteria});
		}
		console.log('Form Submission : Form Name : ' + formName);
		if(productCategory != '')
			dmf.pushData('formSubmission', {'formName' : formName.toLowerCase(), 'productCategory' : productCategory});
		else
			dmf.pushData('formSubmission', {'formName' : formName.toLowerCase()});
	});

	//Training Class Search Trigger
	jQuery('div.classfinder select').change(function(){
		var searchCriteria = 'category:' + $('div.classfinder select.category option:selected').text();
		searchCriteria = searchCriteria + ',role:' + $('div.classfinder select.role option:selected').text();
		searchCriteria = searchCriteria + ',subcategory:' + $('div.classfinder select.subcategory option:selected').text();
		searchCriteria = searchCriteria + ',state:' + $('div.classfinder select.state option:selected').text();
		searchCriteria = searchCriteria + ',classtype:' + $('div.classfinder select.classtype option:selected').text();
		console.log('Triggering training class search type trigger : ' + searchCriteria);
		dmf.pushData('search', {'searchType' : 'training class search', 'searchCriteria' : searchCriteria});
	});
	//Internal Search Trigger
	jQuery('form#searchform, div.search form.page-search').submit(function(){
		console.log('Triggering internal search type trigger');
		dmf.pushData('search', {'searchType' : 'internal search'});
	});
	//Internal Search Result Click Trigger
	jQuery('div.searchLeft .search-result a').click(function(){
		var anchorText = $(this).parents('.search-result').find('p.search-title a').html();
		var positionClicked = -1;
		//console.log('Anchor Text : ' + anchorText);
		$('div.searchLeft .search-result p.search-title a').each(function(index, value){
			//console.log($(this).html());
			if($(this).html() == anchorText){
				positionClicked = index + 1;
			}
		});
		console.log('Triggering search result click type trigger : ' + positionClicked);
		dmf.pushData('searchResultClick', {'searchResultName' : 'internal search', 'positionClicked' : positionClicked});
	});

	//Training Class Search Result Click Trigger
	var checkTrainingResultsLoaded = true;
	window.setTimeout(getTrainingClassId, 6000);
	function getTrainingClassId(){
		console.log('Training Class Id click event listener added.');
		if($('div#paginationContent a.courseDetail') != undefined){
			$('div#paginationContent a.courseDetail').click(function(event){
				//event.preventDefault();
				//event.stopPropagation();
				var item = $(this).parents('.classListItem');
				var trainingClass = $(item).find('.classNameType b').html();
				console.log('Triggering training class search result click trigger : ' + trainingClass);
				//alert('Triggering training class search result click trigger : ' + trainingClass);
				dmf.pushData('searchResultClick', {'searchResultName' : 'training class search', 'trainingClassId' : trainingClass});
				//return false;
			});
		}
	}
	//Sales Office Website Link Click Trigger
	jQuery('div.sales-offices p a.read-more').click(function(){
		console.log('Triggering sales office websute link click trigger.');
		dmf.pushData('click', {'clickType' : 'sales office website link click'});
	});
	//Facet Filter Click Trigger
	jQuery('div.searchRight a').click(function(){
		var anchorText = $(this).html();
		anchorText = anchorText.substring(0, anchorText.indexOf('('));
		console.log('Triggering facet or filter click trigger : ' + anchorText.trim());
		dmf.pushData('click', {'clickType' : 'facet or filter click', 'filterClickName' : anchorText.trim()});
	});
	//Literature search type trigger
	jQuery('form.literature-search .store-submit').click(function(){
		console.log('Triggering literature search type trigger');
		dmf.pushData('search', {'searchType' : 'literature search'});
	});
	//Training Search type Trigger
	jQuery('div.searchPanel .selStyled select').change(function(){
		//console.log('Triggering training class search type trigger.');
		//dmf.pushData('search', {'searchType' : 'training class search'});
	});
	//K-12 Portfolio Tool Uses
	jQuery('form#form1 .zoneTable #imageView').click(function(){
		console.log('K-12 portfolio tool uses type trigger');
		dmf.pushData('toolStart', {'toolName' : 'k12 portfolio tool uses'});
	});
	//Product Tabs click Trigger
	jQuery('.producttabs ul li a').click(function(){
		var tabName = $(this).html();
		console.log('Triggering product page tab used trigger : ' + tabName);
		dmf.pushData('productPageTabsUsed', {'tabName' : tabName});
	});
});
</script>
