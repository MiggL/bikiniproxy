<script type="text/javascript">


function titresplayer(data){

// le titre en cours
$('.titreencours').html(data[0].titre);
$('.artistetitreencours strong').html(data[0].artiste);
$('.radioencours').html(data[0].radio);
$('.albumencours .labelencours').html(data[0].label);
$('.albumencours .nom').html(data[0].album);


// les 3 derniers titres qui viennent de passer
$('.titre1passe em').html(data[1].titre);
$('.titre2passe em').html(data[2].titre);
$('.titre3passe em').html(data[3].titre);

$('.titre1passe .heurepasse').html(data[1].heure);
$('.titre2passe .heurepasse').html(data[2].heure);
$('.titre3passe .heurepasse').html(data[3].heure);


$('.titre1passe strong').html(data[1].artiste);
$('.titre2passe strong').html(data[2].artiste);
$('.titre3passe strong').html(data[3].artiste);

}

jQuery(document).ready(function($) {
  $(document).ready(function(){

$('img.alignright').parent('a').addClass('fancybox');


 $("#newsletter:not('.open_at_load')").hide();
  

$(".hiddenform").hide();


  $('.foundicon-search').click(function() {

    if ($("#searchform:visible").length != 0) {


       $("#searchform").slideUp("normal", function() {         

        $('#foundicon-search').addClass("mess");
} );


      }else{
        $('#foundicon-search').removeClass("mess");
        $("#searchform").slideDown();

      }
         });


$(".newsletter span").each( function () {
    $(this).replaceWith('<a href="" title="s\'inscrire à la newsletter">' + $(this).text() + '<\/a>') ;
    } ) ;

$(".newsletter a").click( function () {
        if ($("#newsletter:visible").length != 0) {

            $('.newsletter a').removeClass("open");
            $("#newsletter").slideUp();
        }

        else {
            $('.newsletter a').addClass("open");
            $("#newsletter").slideDown();
        }

        return false;
    });    



$('.contentNavHide').hide();
$('#principalNav h3').click(function() {


  if ($(".contentNavHide:visible").length != 0) {
            $(".contentNavHide").slideUp(400, function() {$('#principalNav').removeClass("open"); });
              
        }

        else {
            $('#principalNav').addClass("open");
            $(".contentNavHide").slideDown(1000);
        }

        return false;


});

$('.galaxieweb ul').clone().appendTo('.galaxiewebheader');


$('.avecimage .petitcontent').hide();

$('.avecimage').hover(function(){
    $(this).find('.petitcontent').show();
}, function(){ 
    $(this).find('.petitcontent').hide();
});



$('.headercontent').hide();


$('.antenne').click(function() {


  if ($(".headercontent:visible").length != 0) {


            $(".headercontent").slideUp(400, function() {$('#infoheader').removeClass("openheader"); });
              
        }

        else {
            $('#infoheader').addClass("openheader");
            $(".headercontent").slideDown(1000);
        }

        return false;


});

$.getJSON( "http://www.radiogrenouille.com/wp-content/themes/radiogrenouille-new/cestpasse.php", function(data) { 
/// le titre en cours
$('.titreencours').html(data[0].titre);



if(data[0].etatemission != null){
  $('.artistetitreencours strong').html(data[0].etatemission);
}else{
  $('.artistetitreencours strong').html(data[0].artiste);
}


$('.radioencours').html(data[0].radio);
$('.albumencours .labelencours').html(data[0].label);
$('.albumencours .nom').html(data[0].album);


// les 3 derniers titres qui viennent de passer

$('.titre1passe .heurepasse').html(data[1].heure);
$('.titre2passe .heurepasse').html(data[2].heure);
$('.titre3passe .heurepasse').html(data[3].heure);

$('.titre1passe .albumpasse').html(data[1].album);
$('.titre2passe .albumpasse').html(data[2].album);
$('.titre3passe .albumpasse').html(data[3].album);


if(data[1].etatemission != null){
  $('.titre1passe strong').html(data[1].etatemission+' '+data[1].titre);
  $('.titre1passe em').html('');

}else{
  $('.titre1passe em').html(data[1].titre);
  $('.titre1passe strong').html(data[1].artiste);
}

if(data[2].etatemission != null){
  $('.titre2passe strong').html(data[2].etatemission +' '+data[2].titre);
  $('.titre2passe em').html('');

}else{
$('.titre2passe em').html(data[2].titre);
$('.titre2passe strong').html(data[2].artiste);
}


if(data[3].etatemission != null){
  $('.titre3passe strong').html(data[3].etatemission+' '+data[3].titre);
  $('.titre3passe em').html('');

}else{
  $('.titre3passe em').html(data[3].titre);
  $('.titre3passe strong').html(data[3].artiste);
}


         });


setInterval(function() { 


$.getJSON( "http://www.radiogrenouille.com/wp-content/themes/radiogrenouille-new/cestpasse.php", function(data) { 
/// le titre en cours
$('.titreencours').html(data[0].titre);



if(data[0].etatemission != null){
  $('.artistetitreencours strong').html(data[0].etatemission);
}else{
  $('.artistetitreencours strong').html(data[0].artiste);
}


$('.radioencours').html(data[0].radio);
$('.albumencours .labelencours').html(data[0].label);
$('.albumencours .nom').html(data[0].album);


// les 3 derniers titres qui viennent de passer

$('.titre1passe .heurepasse').html(data[1].heure);
$('.titre2passe .heurepasse').html(data[2].heure);
$('.titre3passe .heurepasse').html(data[3].heure);

$('.titre1passe .albumpasse').html(data[1].album);
$('.titre2passe .albumpasse').html(data[2].album);
$('.titre3passe .albumpasse').html(data[3].album);


if(data[1].etatemission != null){
  $('.titre1passe strong').html(data[1].etatemission+' '+data[1].titre);
  $('.titre1passe em').html('');

}else{
  $('.titre1passe em').html(data[1].titre);
  $('.titre1passe strong').html(data[1].artiste);
}

if(data[2].etatemission != null){
  $('.titre2passe strong').html(data[2].etatemission +' '+data[2].titre);
  $('.titre2passe em').html('');

}else{
$('.titre2passe em').html(data[2].titre);
$('.titre2passe strong').html(data[2].artiste);
}


if(data[3].etatemission != null){
  $('.titre3passe strong').html(data[3].etatemission+' '+data[3].titre);
  $('.titre3passe em').html('');

}else{
  $('.titre3passe em').html(data[3].titre);
  $('.titre3passe strong').html(data[3].artiste);
}


         });


},5000);


  });
  });
</script>



