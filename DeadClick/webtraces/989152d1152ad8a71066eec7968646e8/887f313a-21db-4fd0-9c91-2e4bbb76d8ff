// Removes empty results and approximate results - run before jquery.dataTables.min.js

(function ($) {

$(document).ready(function() {
var tableRows = $("#views-aggregator-datatable tbody tr"); //find all the rows
    tableRows.each(function() {

    var remove = true;
     $(this).find(".views-field:last").each(function(){

		if($(this).text().trim() !=="")  // checks for non-empty results
            remove = false;

    });
	
    if(remove == true)
        $(this).remove();

});
});
})(jQuery);

//--><!]]>
