var ADventoriPlacementID =ADventori.data.placement.placementId;
debug.stack.pop();

debug.stack.push('conf_campaign_after');
if (eadCreativeType !='HABILLAGE' && eadPublisher!='Amazon'){
//debug.log('je remonte la visibilité');
ADventori.conf.macros['EULERIAN_DOMAIN'] = 'elr.sfr.fr';
ADventori.conf.macros['EULERIAN_SITEID'] = 'sfr-fr';
ADventori.conf.macros['EULERIAN_PUBLISHER'] = eadPublisher;
ADventori.conf.macros['EULERIAN_NAME'] = eadName;
ADventori.conf.macros['EULERIAN_LOCATION'] = eadLocation;
ADventori.conf.macros['EULERIAN_CREATIVE'] = eadCreative+'-'+eadCreativeType;
ADventori.conf.macros['EULERIAN_CREATIVETYPE'] = eadCreativeType;

/* EULERIAN VIEWABILITY - START */
var eulerianUrl = '[PROTOCOL][EULERIAN_DOMAIN]/evis/[EULERIAN_SITEID]/[RANDOM]?ea-id=[EULERIAN_EAID]&ead-publisher=[EULERIAN_PUBLISHER]&ead-name=[EULERIAN_NAME]&ead-location=[EULERIAN_LOCATION]&ead-creative=[EULERIAN_CREATIVE]&ead-creativetype=[EULERIAN_CREATIVETYPE]';
ADventori.dom.getScript(ADventori.macros.replace(eulerianUrl, {'EULERIAN_EAID': ADventori.id}, [ADventori.conf]));
var eulerianQ = window['EAdVis'+ADventori.id] = eulerianQ || [];

ADventori.fn.onViewUndetermined = function(conf) {
  eulerianQ.push('na');
  //debug.log(eulerianQ);
};

var eulerianTimer = null;

ADventori.fn.onViewStart = function(conf) {
  eulerianTimer = setInterval(function() {
    var currentState = conf.viewability.getCurrentState();
    eulerianQ.push(Math.floor(currentState.area * 100));
    //debug.log(eulerianQ);
  }, conf.viewability.tick, 'view_eulerian', 'AdErrorConf');
};

ADventori.fn.onViewEnd = function(conf) {
  clearInterval(eulerianTimer);
};

// High duration to continue to tick for Eulerian
var eulerianThreshold = {area: 50, duration: 100};

ADventori.conf.viewability.beacon.doubleTrack = true;
ADventori.conf.viewability.tick = 400;
ADventori.conf.viewability.thresholds = [
  {area: 50, duration: 1},
  eulerianThreshold
];
/* DISABLE FLASH detection + beacon */
ADventori.env.flash.getVersion = function() {return '0.0.0';};
/* DISABLE FLASH - END */

}
/* EULERIAN VIEWABILITY - END */
debug.stack.pop();

}, 'conf', 'AdErrorConf');

ADventori.track.attempt(ADventori.autoStart, 'autoStart');
}, 'init');

} catch(_e) {
  (function() {
    var campaignId = 0;
    var placementId = 0;
    var error = '';
    var context = '';
    var userAgent = '';
    try {
      if (_ADventori_data) {
        campaignId = _ADventori_data.campaign['campaignId'];
        placementId = _ADventori_data.placement['placementId'];
      }
    } catch(__e) {}
    try {
      error = (_e && _e.name ? (_e.name + ': ' + (_e.message || '')) : (_e || '')) + '';
    } catch(__e) {}
    try {
      try {
        context = window.top.location.href;
      } catch(__e) {
        context = window.document.referrer;
      }
    } catch(__e) {}
    try {
      userAgent = (window.navigator && window.navigator.userAgent);
    } catch(__e) {}
    document.createElement('img').src = ('https://adventori.com/tracker/trackPrint?tk_type=AdErrorInitJs' + '&tk_campaignId=' + encodeURIComponent(campaignId) + '&tk_cartoucheId=' + encodeURIComponent(placementId) + '&tk_urlClick=' + encodeURIComponent(error) + '&tk_context=' + encodeURIComponent(context) + '&tk_retailer_4=' + encodeURIComponent(userAgent));
    try {
      window.console && window.console.error && window.console.error('[ADventori]', 'AdErrorInitJs', _e);
    } catch(__e) {}
  })();
}

})();
