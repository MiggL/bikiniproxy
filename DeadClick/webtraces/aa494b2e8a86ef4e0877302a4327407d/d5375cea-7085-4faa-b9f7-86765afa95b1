<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.2/TweenLite.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.2/TimelineLite.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.2/easing/EasePack.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.2/plugins/CSSPlugin.min.js"></script>
<script>
window.addEventListener('load', function(e){
    TweenLite.set('#wrapper', {autoAlpha:1});
    var tl = new TimelineLite();
    tl  .add("step1")
        .from('#pack', .8, {autoAlpha:0, y:-150,  ease:Power2.easeOut}, "step1")
        .from('#stabilo1', .2, {x:-800,  ease:Power2.easeOut}, "step1+=.4")
        .from('#text1', .8, {autoAlpha:0, ease:Power2.easeOut}, "step1+=.4")
        .from('#fleche', .4, {autoAlpha:0, x:-20, y:40, rotation:20,  ease:Power2.easeOut}, "step1+=.4")
       
        .add("step2", "+=1.2")
        .set('#text1', {autoAlpha:0}, "step2")
        .set('#stabilo1', {autoAlpha:0})
        .from('#text2', .8, {autoAlpha:0, y:-100,  ease:Power2.easeOut}, "step2")
        .from('#ligne', .2, {x:-500, y:10,  ease:Power2.easeOut}, "step2+=.4")
        .to('#fleche', .2, {autoAlpha:0, y:10,  ease:Power2.easeOut}, "step2+=.4")
        .to('#pack', 1, {autoAlpha:0, scale:1.1, ease:Power2.easeOut}, "step2+=.4")
        .from('#packmod', 1, {autoAlpha:0, scale:.9, ease:Power2.easeOut}, "step2+=.4")
        
        .add("step3")
        .to('#packmod', 1, {autoAlpha:0, scale:1.1,ease:Power2.easeOut})
        .from('#cochon', 1, {autoAlpha:0, scale:.9, ease:Power2.easeOut}, "step3")

        .add("step4")
        .set('#text2', {autoAlpha:0}, "step4")
        .set('#ligne', {autoAlpha:0})
        .set('#note1', {autoAlpha:0})
        .to('#cochon', 1, {autoAlpha:0, scale:1.1,ease:Power2.easeOut})
        .from('#tirelire', 1, {autoAlpha:0, scale:.9, ease:Power2.easeOut}, "step4")
        .to('#piece1', 1.8, {autoAlpha:0, y:40, ease:Power2.easeIn}, "step4+=.2")
        .to('#piece2', 1.8, {autoAlpha:0, y:40, ease:Power2.easeIn}, "step4+=.6")
        .to('#piece3', 1.8, {autoAlpha:0, y:40, ease:Power2.easeIn}, "step4+=.4")
        .from('#note2', .4, {autoAlpha:0, ease:Power2.easeOut}, "step4")
        .from('#text3', .4, {autoAlpha:0, scale:2, ease:Power2.easeOut}, "step4")
        .from('#stabilo2', .2, {x:-850,  ease:Power2.easeOut}, "step4+=.4")

        .add("step5", "+=1.2")
        .set('#text2', {autoAlpha:0}, "step5")
        .set('#text3', {autoAlpha:0})
        .set('#tirelire', {autoAlpha:0})
        .set('#stabilo2', {autoAlpha:0})
        .from('#text4', .4, {autoAlpha:0, scale:2, ease:Power2.easeOut}, "step5")
        .from('#stabilo3', .2, {x:-800,  ease:Power2.easeOut}, "step5+=.4")
        .from('#piece4', 1.2, {autoAlpha:0, y:-500, ease:Power2.easeIn}, "step5+=.6")
        .from('#piece5', 1.2, {autoAlpha:0, y:-500, ease:Power2.easeIn}, "step5+=.4")
        .from('#pictoflamme', .8, {autoAlpha:0, scale:.6, ease:Power2.easeOut}, "step5+=.8")
        .from('#pictolamp', .8, {autoAlpha:0, scale:.6, ease:Power2.easeOut}, "step5+=1")

        .add("step6", "+=1.2")
        .set('#text4', {autoAlpha:0}, "step6") 
        .set('#stabilo3', {autoAlpha:0})
        .set('#pictoflamme', {autoAlpha:0})  
        .set('#pictolamp', {autoAlpha:0})   
        .set('#note2', {autoAlpha:0})    
        .set('#piece4', {autoAlpha:0})   
        .set('#piece5', {autoAlpha:0})   
        .from('#text5', .4, {autoAlpha:0, y:-50, ease:Power2.easeOut})    
        .from('#text6', .2, {autoAlpha:0, scale:2, ease:Power2.easeOut}, "+=.2")    
        .from('#logo', .6, {autoAlpha:0, ease:Power2.easeOut})
        .from('#baseline', .6, {autoAlpha:0, ease:Power2.easeOut}, "-=.6")

});
</script>
