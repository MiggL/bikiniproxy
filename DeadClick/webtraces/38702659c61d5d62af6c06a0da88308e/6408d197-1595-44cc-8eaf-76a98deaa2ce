	<script type="text/javascript">

	$(function() {

		$('a[data-target=#calendar-modal]').on('click', function(){
			$('#calendar-modal').modal();
			
		});


		$('[data-menu]').on('click', function(){

						var slug = $(this).attr('data-menu');
			$.ajax({
			  	url: "/en/menu/"+slug+"/",
			  	context: document.body
			}).done(function(data) {

				
				$('#bModal').css('display', 'block');
			  	$('#modal-body').html(data);

			  	var winHeight = $( window ).height();
			  	var modalHeight = $('#modal-body').height()+54;
			  	console.log(modalHeight)

			  	var margin = (winHeight-modalHeight)/2;
			  	margin = (margin<0)? 0 : margin;
			  	console.log(margin);
			  	$('#bModal .modal-dialog').css('marginTop', margin+'px');

			  
			  	$('#bModal').css('display', 'none');
			  	$('#bModal').modal();


			});
			
		});


		function scrollToSection() {
			$('.next a, .next2 a').click( function() { // Au clic sur un élément
				var page = $(this).attr('href'); // Page cible
				var speed = 750; // Durée de l'animation (en ms)
				$('html, body').animate( { scrollTop: $(page).offset().top-100 }, speed ); // Go
				$('[data-toggle="popover"]').popover('hide');
				return false;
			});
		}

		scrollToSection();
		

		$('#calendar-modal').on('shown.bs.modal', function () {
			$('[data-toggle="popover"]').popover('hide');
		});

		$('[data-toggle="popover"]').on('shown.bs.popover', function () {
			scrollToSection();
		});

		$('a.ajax').click( function() { // Au clic sur un élément
			var page = $(this).attr('href'); // Page cible
			$.ajax({
			  url: page
			}).done(function(msg) {
				$('#modal-body').html($($.parseHTML(msg)).filter("#content"));
				$('#bModal').modal();
			});
			return false;
		});

		$('header .navbar.navbar-default a.dropdown-toggle').on('click', function(){
			window.location.href = $(this).attr('href');
		});
		$('.tooltipf').tooltip();
	});
	
	</script>
	
