  'REFERER'         : referer,
  'DSP'             :"Affiperf-" + AffiperfDSP
};

ADventori.data.metadata.eadCreative = eadCreative;
//*********************** Gestion du clickTag *********************
if (eadPublisher =='AdotMob' || (eadPublisher=='Ecselis' && eadCreativeType=='HABILLAGE') || eadPublisher=='Teads' /*|| eadPublisher=='Lagardere'*/){
  ADventori.conf.clickTag = encodeURIComponent (ADventori.macros.replace(ADventori.data.placement.clickTag))  + encodeURIComponent(redirection + omniture);
  ADventori.conf.track.url.clickTag = false;
} /*else if (eadPublisher=='Lagardere') {
  
  ADventori.conf.clickTag = encodeURIComponent (encodeURIComponent(ADventori.macros.replace(ADventori.data.placement.clickTag))  + encodeURIComponent(redirection + omniture));
  
}*/ else {
    ADventori.conf.clickTag = ADventori.data.placement.clickTag  + encodeURIComponent(redirection + omniture);
  }
  
if (eadCreative=='SFR-TMGP-CARROUSEL-DEVICE'){
  var topItems = ADventori.utils.map(ADventori.data.results.items.related.i_top, function(item) {
    return {
      id : item.id,
      groupName: 'default',
      data : {
        SFR_Mobile_Titre    : item.data.category1,
      	SFR_Mobile_img      : item.data.img,
      	SFR_Mobile_Contenu  : item.data.category2,
