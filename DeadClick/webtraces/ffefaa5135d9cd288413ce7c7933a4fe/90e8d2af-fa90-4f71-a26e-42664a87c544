define(["jquery","foundation.reveal"],function(d){var a="";if(d(".masthead.header").length<1){a=d(".header-container")}else{a=d(".masthead.header")}if(d("[data-reveal]").length<=0){d("body").append('<div id="navigation-modal" class="reveal-modal" data-reveal></div>')}function c(){if(d(".reveal-modal-bg").is(":visible")){Foundation.libs.reveal.toggle_bg()}a.removeClass("nav-overlay")}function b(){if(!d(".reveal-modal-bg").is(":visible")){Foundation.libs.reveal.toggle_bg()}a.addClass("nav-overlay")}return{show:b,hide:c}});