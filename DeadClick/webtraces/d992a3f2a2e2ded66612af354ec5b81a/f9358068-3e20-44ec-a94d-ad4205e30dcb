    var session = false;

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return;
    }

    function getQueryString(q) {
        if (q == "") return {};
        return (function(a) {
            if (a == "") return {};
            var b = {};
            for (var i = 0; i < a.length; ++i) {
                var p = a[i].split('=');
                if (p.length != 2) continue;
                b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
            }
            return b;
        })(q.split("&"));
    }

    function addScriptTag(wmbUrl) {
        var wmbTag = document.createElement("script");
        wmbTag.setAttribute("src",wmbUrl);
        wmbTag.setAttribute("async","async");
        wmbTag.setAttribute("defer","defer");
        document.body.appendChild(wmbTag);
    }

    function getCommonParams(urlParams,wmbSessionId) {
        wmbUrlParams = '&session='+encodeURIComponent(wmbSessionId);

        if(typeof urlParams['referrer'] != "undefined")
            wmbUrlParams = wmbUrlParams+'&referrer='+encodeURIComponent(urlParams['referrer']);

        if(typeof urlParams['track_wmb'] != "undefined")
            wmbUrlParams = wmbUrlParams+'&track_wmb='+encodeURIComponent(urlParams['track_wmb']);

        if(typeof urlParams['emerch'] != "undefined")
            wmbUrlParams = wmbUrlParams+'&emerch='+encodeURIComponent(urlParams['emerch']);

        return wmbUrlParams;
    }

    var query = window.location.search.substring(1);
    var urlParams = getQueryString(query);

    var wmbSessionId = getCookie("WMB_SESSION_ID");

    if(typeof wmbSessionId == "undefined"){
        function guid() {
            function s4() {
                return Math.floor((1 + Math.random()) * 0x10000)
                    .toString(16)
                    .substring(1);
            }
            return s4() + s4() + "-" + s4() + "-" + s4() + "-" +
                s4() + "-" + s4() + s4() + s4();
        }
        wmbSessionId = guid();
        document.cookie = "WMB_SESSION_ID=" + encodeURIComponent(wmbSessionId) + "; max-age=31536000; path=/;";
    }

    function wmbGetVariable(wmbUrlParamsArray, urlParams, wmbSessionId, date, wmbUrl)
    {
        var getParamsUrl = 'https://www.eider.com/en/wmb/tag/getParams/';

        getParamsUrl = getParamsUrl+'?session='+wmbSessionId;
        getParamsUrl = getParamsUrl+'&time='+date.getTime();

        if(typeof wmbUrlParamsArray['page_type'] != "undefined")
            getParamsUrl = getParamsUrl+'&page_type='+wmbUrlParamsArray['page_type'];

        jQuery.post(getParamsUrl)
            .done(function(response) {

                wmbUrlParams = getCommonParams(urlParams,wmbSessionId);

                jQuery.each(response,function(index,value) {
                    wmbUrlParams = wmbUrlParams+'&'+index+'='+encodeURIComponent(value);
                    if(index == 'customer')
                        document.cookie = "WMB_CUSTOMER_EMAIL=" + encodeURIComponent(value) + "; max-age=31536000; path=/;";
                    if(index == 'login_at')
                        document.cookie = "WMB_CUSTOMER_LAST_LOGIN=" + encodeURIComponent(value) + "; max-age=31536000; path=/;";
                });

                wmbUrl = wmbUrl + wmbUrlParams;

                addScriptTag(wmbUrl);
            });
    }

    function wmbUseVariable(urlParams, wmbSessionId, wmbUrl, wmbCustomerEmail, wmbCustomerLastLogin)
    {
        wmbUrlParams = getCommonParams(urlParams,wmbSessionId);
        wmbUrlParams = wmbUrlParams+'&customer='+wmbCustomerEmail;
        wmbUrlParams = wmbUrlParams+'&login_at='+wmbCustomerLastLogin;

        wmbUrl = wmbUrl + wmbUrlParams;

        addScriptTag(wmbUrl);
    }

    function wmbLoad(urlParams, wmbSessionId)
    {
        var date = new Date();
        var referrer = document.referrer;

        var wmbUrlParams = '';
        var wmbUrl = "https://flux.wmb.io/merchandising_v3.php?site=3EE2A5F0DBA813E8&page_type=cms_page_view&page=%2F";
        var wmbUrlParamsArray = getQueryString(wmbUrl.split("?")[1]);
        var wmbCustomerIsGuest = getCookie("WMB_CUSTOMER_IS_GUEST");

        if (typeof window.getWmbVariables != 'undefined') {
            if (typeof wmbCustomerEmail == 'undefined') {
                var params = window.getWmbVariables();
                wmbCustomerEmail = params.wmbCustomerEmail;
            }
            if (typeof wmbCustomerLastLogin == 'undefined') {
                if (typeof params == 'undefined') {
                    var params = window.getWmbVariables();
                }
                wmbCustomerLastLogin = params.wmbCustomerLastLogin;
            }
        }

        if(typeof wmbCustomerEmail == 'undefined')
            var wmbCustomerEmail = getCookie("WMB_CUSTOMER_EMAIL");
        if(typeof wmbCustomerLastLogin == 'undefined')
            var wmbCustomerLastLogin = getCookie("WMB_CUSTOMER_LAST_LOGIN");

        if( ((
            ( (typeof wmbCustomerEmail == 'undefined') || (typeof wmbCustomerLastLogin == 'undefined') )
            && typeof wmbCustomerIsGuest == 'undefined'
            )
            || wmbUrlParamsArray['page_type'] == 'checkout_cart_index'
            || wmbUrlParamsArray['page_type'] == 'checkout_onepage_index'
            )
        ) {
            wmbGetVariable(wmbUrlParamsArray, urlParams, wmbSessionId, date, wmbUrl)
        }
        else { // We get all params we needs in cookie
            wmbUseVariable(urlParams, wmbSessionId, wmbUrl, wmbCustomerEmail, wmbCustomerLastLogin);
        }
    }

    document.observe("dom:loaded", wmbLoad(urlParams, wmbSessionId));
</script>