    //
}


/* ******************************************************************		*/
/* INLINE: Remove invalid characters from all variables 					*/
/* ******************************************************************		*/

function om_cleanData(data) {
	// remove illegal characters and new lines
	try {
		if (typeof (data) != "undefined") {
			data = data.replace(/\r|\n/g,'');
		}
		return data;
	} catch (Error) {
		return data;
	}
}

