	/*
		prevent main menu from linking
	*/
	$('#main-nav a, #page-nav a').click(function(e){
		var winW = $(window).width();
		if(winW >= 768)
			e.preventDefault();
	});

	/*
		country drop down
	*/
	var countryOpen = 0;
	$('#country, #country-list .close-menu').click(function(e){
		var getWinW = $(window).width();
		if(getWinW > 767) {
			e.preventDefault();
			if(countryOpen == 0) {
				$('#country-list').slideDown();
				$('#top-nav .triangleDown').removeClass('triangleDown').addClass('triangleUp');
				countryOpen = 1;
			}
			else {
				$('#country-list').slideUp();
				$('#top-nav .triangleUp').removeClass('triangleUp').addClass('triangleDown');
				countryOpen = 0;
			}
		}
	});

	/*
		main menu
	*/
	$('#menu-links li').click(function(){
		var windowW = $(window).width();
		if(windowW >= 767)
		{
			var selectedMenu = $(this).index();
			var clicked = $(this);
			var closeMenu = $('.menu-container .close-menu');
			//console.log(selectedMenu);
			$('#main-nav a').removeClass('active');
			$('#main-nav').find('.menu-marker').animate({'bottom':'-12px'});

			if(!$('#submenu .menu-container:eq('+selectedMenu+')').is(":visible")){
				closeMenu.fadeOut(100);
				$('#submenu .menu-container').slideUp(600);
				$('#submenu .menu-container:eq('+selectedMenu+')').slideDown(600,function(){
					closeMenu.fadeIn();
				});
				clicked.find('a').addClass('active');
				clicked.find('.menu-marker').show().animate({'bottom':'0px'});
			}
			else if($('#submenu .menu-container:eq('+selectedMenu+')').is(":visible")){
				closeMenu.fadeOut(100);
				$('#submenu .menu-container').slideUp(600,function(){
				});
				$('.menu-marker').animate({'bottom':'-12px'},function(){
					$(this).hide();
				});
			}
		}
	});
	
	$('#submenu .close-menu').click(function(){
		$('#submenu .menu-container').slideUp(600);
	});

	$('#mobile-nav a').click(function(e){
		e.preventDefault();
		$(this).toggleClass('active');
		$('#main-nav').slideToggle();
	});

	$('#submenu .menu-tabs a').click(function(e) {
		e.preventDefault();
		$(this).parent().parent().parent().find('a').each(function(){
			$(this).removeClass('active');
		});
		$(this).addClass('active');
		var selectedSubmenu = $(this).parent().index();
		$(this).parent().parent().parent().parent().find('.menu-listing').each(function(){
			$(this).removeClass('active');
		});
		$(this).parent().parent().parent().parent().find('.menu-listing:eq('+selectedSubmenu+')').addClass('active');
	});

