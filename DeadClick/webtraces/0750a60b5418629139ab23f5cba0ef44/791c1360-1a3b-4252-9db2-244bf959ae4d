<script type="text/javascript">

function populateValues(searchType) {

	var url = "http://internal.trane.com/Commercial-WCF/LitSearch.svc/";
    var responseType = "json";
    var baseUrl = "/content/trane-commercial/north-america/us/en/products-systems/literature/jcr:content/content/productlitsearch_ceb.productlitsearch.json";
    var searchVar="";
    var selectType="";
	
    if(searchType=='1'){
        searchVar="prodTypes";
        selectType = "productType";
        url=url+searchVar;
    } else if(searchType=='2'){
    	var prodType = $('#productType').val();
        selectType = "productName";
        searchVar="prodNames/"+prodType;
        url=url+searchVar;
    }else if(searchType=='3'){
        selectType = "literatureType";
        searchVar="LitTypes";
        url=url+searchVar;
    }else if(searchType=='4'){
        selectType = "language";
        searchVar="LitLanguages";
        url=url+searchVar;
    }
    else if(searchType=='5'){
        //Search here
        var producttype = $('#productType').val();
        var productname = $('#productName').val();
        var litType = $('#literatureType').val();
        
        //Setting the value as ALL if All Option is selected.
        if(litType=='0' || litType == null || litType == ''){
            litType="ALL";
        }
        if(producttype=='0' || producttype == null || producttype == ''){
            producttype="ALL";
        }
        if(productname=='0' || productname == null || productname == ''){
            productname="ALL";
        }

        var language = $('#language').val();
        var prodSearchText = $('#searchText').val();
        if (language == null) language = 'EN';        

        searchVar="search/";
        url=url+searchVar+producttype+"/"+productname+"/"+litType+"/"+language;

        if(prodSearchText!=''){
            url=url+"?keywords="+ encodeURIComponent(prodSearchText);
        }
        
        //alert(url);

        responseType = "html";
        baseUrl="/content/trane-commercial/north-america/us/en/products-systems/literature/jcr:content/content/productlitsearch_ceb.productlitsearchresults.html";

        $("#product-lit-search").empty().append('Loading....');
    }   

    $.ajax({
        type: 'POST',   
        url:baseUrl,
        dataType: responseType,
        data:'url='+url,    
        error: function(slingResponse){
        alert('Failed to load the languages'+ex);
        },
        beforeSend: function() {
        	if(searchType=='5'){
        	    $('#product-lit-search').html("<div id='loadingDiv'></div>");
        	}
        },
        success : function(data) {
            if(searchType=='5'){
                //if (data != null && data != '') {
                	$("#product-lit-search").empty().append(data);
                //}else{
                //    $("#product-lit-search").empty().append('<h3>No Product Literature Found</h3>');
                //}
                checkValues();
                checkresults();
            }else{
            	if(searchType=='2'){
            		$('#'+selectType).prop('disabled',false);
                    $('#'+selectType).empty();
                    $('#'+selectType).append($('<option/>').attr("value", "").text("Select Product Name"));
                }
                $.each(data, function(i, option) {
                    $('#'+selectType).append($('<option/>').attr("value", option.value).text(option.text));
                    });
            }

            if(searchType=='4'){
                var currentLanguage=$('#currentLang').val().toUpperCase();
                var defaultLanguage=$('#defaultLang').val().toUpperCase();

                var status = $("#language").find("option").filter(function (){return $(this).val()== currentLanguage }).length;
                if(status==0){
                    $('#language').val(defaultLanguage);
                }else{
                    $('#language').val(currentLanguage);
                }
            }

            
            }
    });
  
}


$(document).ready(function(){
    populateValues(1);
    populateValues(3);
    populateValues(4);
    var currentLanguage="EN";    
    $('#language').val(currentLanguage);
});


function openPdf(url) {
    window.open(url);
}

function checkValues(){
    $('.literature-section .required').each(function(){        
        var formRequiredValue = document.getElementById("language").value;
        var searchText = document.getElementById("searchText").value;
        //alert('searchText');
        if(formRequiredValue == '') 
        {
            $(this).addClass('error'); 
                if ($("#language").parent().next(".validation").length == 0) // only add if not added
                {
                    $("#language").parent().after("<div class='validation' style='color:#D8000C;width:49.73418%;float:right;background-color:white'>Please select a language</div>");
                }
        } else {
            $(this).removeClass('error');
             $("#language").parent().next(".validation").remove(); 
            $("#searchText").removeClass('error');
             $("#searchText").parent().next(".validation").remove(); 
        }
        if(formRequiredValue == '' && searchText != '')
        {
			$(this).addClass('error'); 
                if ($("#searchText").parent().next(".validation").length == 0) // only add if not added
                {
                    $("#searchText").parent().after("<div class='validation' style='color:#D8000C;background-color:white;position:absolute;top:62%;left:15px'>Please select a language before entering the keyword</div>");
                }
        }
    });
}

function checkresults(){
	if (! $(".top-level").length ){
         $("#product-lit-search").append('<div id="noresults"><h3>No Results Found</h3></div>');
    }
}  

$(document).on("keypress", "#searchText", function (e) {
    if (e.keyCode == 13 || e.which == '13') {
        event.preventDefault()
        populateValues(5);
    }
});
