function vswReady(a){"loading"!=document.readyState?a():document.addEventListener?document.addEventListener("DOMContentLoaded",a):document.attachEvent("onreadystatechange",function(){"loading"!=document.readyState&&a()})}function vswActivate(){for(var a=document.getElementsByTagName("script"),b=0;b<a.length;b++)if(a[b].src.indexOf("/widget.js?wid=")>0){var c=a[b].src.split("=")[1];if(c){var d=document.getElementById("vsw"+c);d||(d=document.getElementById(c)),d&&(d.onclick=function(){return window.location.href="https://www.viator.com/operator/"+d.dataset.sid+"?aid="+c,!1})}}}vswReady(vswActivate);