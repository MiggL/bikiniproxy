//YMAL & recently viewed temp fix
$(document).ajaxComplete(function(event, xhr, settings) {
	if (settings.url.indexOf("RecentlyViewed.ice") !== -1) {
		if ($('.recently-viewed').length === 0 && $('#recently_viewed').length > 0) {
			$('.ymal-products').before(
				'<li class="active recently-viewed col-lg-3 col-md-4 col-sm-4 col-xs-6" data-toggle="tab">' +
					'<a class="col-lg-6 col-md-8 col-sm-8 col-xs-12" href="#recently_viewed">Recently Viewed</a>' +
				'</li>'
			);
			$('.product_additional .nav-tabs li > a').off().on('click', function (e) {
				$('.product_additional > ul > li').removeClass('active').attr('style', '');
				$(this).parent().addClass('active');
				return btf.global.reveal(this, 'acc-reveal');
			}).first().click();
		}
	}
});



	/* Workaround for MINTV-2301 - fixes facbook social share button 
var facebookElement = document.querySelectorAll(".fb-like.social_button"),
	brokenFacebookURL = facebookElement[0].getAttribute('data-url'),
	facebookElementHref = facebookElement[0].getAttribute('href'),
	correctFaceBookURL = window.location.origin + window.location.pathname,
	facebookElementHrefReplaced = facebookElementHref.replace(brokenFacebookURL, correctFaceBookURL);

facebookElement[0].setAttribute("data-url", facebookElementHrefReplaced);
facebookElement[0].setAttribute("href", facebookElementHrefReplaced);
 EOF Workaround for MINTV-2301 */


  set_delivery_content()

  function set_delivery_content() {
    console.log('setting delivery content');
    var country_iso = $.cookie('country_iso');
	
    if (country_iso !== undefined) {
    	var $country_delivery_element = $('#delivery_' + country_iso.toLowerCase());
    	if ($country_delivery_element.length > 0) {
    		$country_delivery_element.show();	
    	} else {
    		$('#delivery_international').show();
    	}
    }
  }
});

// size guide modal X quick fix from fresca
btf.utils.modal = function() {
	//caching objects
	var $modalContent = $('<div id="modal-content"/>'),
		$modalBlock = $('<div id="modal-block"/>'),
		$closeButton,
		loadContent = function () {
		},
		showModal = function () {
			$modalBlock.css({'display' : 'none'})
			$('body').append($modalBlock);
			$modalBlock.addClass('modal-open');
			$modalBlock.fadeIn();
		},
		modalSubmitHandler = function(event) {
			if (event.type == 'submit' && $('input.error', $(this)).length == 0 && window.matchMedia(btf.mediaBreaks.maxsm).matches == false) {
				if (event.target.id == 'registerProspect_form') {
					var url = event.target.action + '?' + $(event.target).serialize(),
						content = $('<iframe src="'+ url + '" width="100%" height="730" id="lyrisForm"></iframe>'),
						$closeButton = $('<img src="https://mintvelvet.btxmedia.com/pws/images/btn_enews-close.gif" class="modal-close" />');
					btf.utils.hideKeyboard();
				}

				$modalBlock.append($modalContent);
				$modalContent.css({'max-width' : '470px'});
				$modalContent.html(content);
				$modalContent.append($closeButton);
				toggleClosehandler('on');
				showModal();
				event.preventDefault();
			}
		},
		appendClose = function (element) {
			element.append('<div><i class="mv-modal__close-icon-size-guide"></i></div>');
		},
		modalClickHandler = function(event) {
			if (this.nodeName == 'A') {
				var href = $(this).attr('href');
				if (href.indexOf('#') > -1 && $(href).length > 0) {
					$modalContent.append($(href));
					appendClose($modalContent);
				} else {
					$modalContent.load(this.href + ' #cms_modal_content', function(){
						appendClose($modalContent);
					});
				}
				$modalBlock.append($modalContent); //Append Data to modal holder
				toggleClosehandler('on'); //toggle Close Event Handlers
				$modalContent.css({'max-height' : window.innerHeight / 2 * 1.5});
				showModal();
				event.preventDefault();
				event.stopPropagation();
			}
		},
		toggleClosehandler = function (type) {
			if (type === 'off') {
				//remove after being called
				$(document).off('click', '#modal-block', closeModal);
				$(document).off('keyup', keyUpCloseFunc);
			} else {
				//add close modal listeners
				$(document).on('click', '#modal-block', closeModal);
				$(document).on('keyup', keyUpCloseFunc);
			}
		},
		keyUpCloseFunc = function (e) {
			if (e.keyCode == 27 && $closeButton.length > 0) {
				closeModal();
			}
		}, //function to handle key press
		closeModal = function () {
			toggleClosehandler('off');
			$modalBlock.fadeOut(function() {
				$modalBlock.remove();
				$modalBlock.removeClass('modal-open');
			});
		};

		//Handles Modal Submit form
		$('.modal').submit(modalSubmitHandler);

		//Handles Click Event to build modal
		$('.modal').click(modalClickHandler);
};

var prod_id = $('div[data-product-id]').attr('data-product-id');
var prod_price = $('.product_price').text().trim();
var prod_currency = $.cookie('currency_iso');

// Facebook track of products browsed
fbq('track', 'ViewContent', {
    content_ids: prod_id,
    content_type: 'product',
    value: prod_price,
    currency: prod_currency
});

// To track products added to bag jhenry 19.07.17
$('#add_to_bag').click(function() {
    fbq('track', 'AddToCart', {
        content_ids: $('.custom_input.selected').prev().val(),
        content_type: 'product',
        value: prod_price,
        currency: prod_currency
    });
});

// jhen YMAL to use the bigger image, to fix blury adobe image batching iss
$(document).ready(function() {
    if ($("#recently_viewed li").length > 0) {
        $('#recently_viewed li img').each(function() {
            console.log($(this).attr("src"));
            var uri = $(this).attr("src").replace('/80px/', '/150px/');
            $(this).attr("src", uri);
        });
    }
});

// out of stock prod, change display and upsell
$(document).ready(function() {
if ($(".size_select_list").children().length == $(".size_select_list").children(".no_stock").length) {
    $('#sizes, #quantity_selector, #add_to_bag').hide();
    $('.product_features').before('<div class="product_oos" style="margin-top: -40px;"><h4>Sorry this product is out of stock - to see other items you may also like <a class="product_oos-click" href="#ymal_products">CLICK HERE</a></h4></div>');

    $(".product_oos-click").click(function() {
        $('html, body').animate({
            scrollTop: $("#dd-ymal").offset().top
        }, 2000);
    });
}
});


