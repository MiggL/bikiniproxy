function rcom_scrollTracker(){var e=0,a=0;return function(){var d=$(document).height(),b=$(window).height(),c=d-b,d=$(this).scrollTop(),b=0;e>=0&&c>0&&(c=d/c,d>e&&c>=0.25&&(c>=0.5?c>=0.75?c>=0.98?a!==100&&(a=100,b=1):a!==75&&(a=75,b=1):a!==50&&(a=50,b=1):a!==25&&(a=25,b=1)));e=d;b>0&&(dataLayer.push({analyticsAttributes:""}),a==25?dataLayer.push({event:"GA_event",category:"Scroll Down",action:a+"%",analyticsAttributes:{pageScrollDown25Percent:"1"}}):a==50?dataLayer.push({event:"GA_event",category:"Scroll Down",
action:a+"%",analyticsAttributes:{pageScrollDown50Percent:"1"}}):a==75?dataLayer.push({event:"GA_event",category:"Scroll Down",action:a+"%",analyticsAttributes:{pageScrollDown75Percent:"1"}}):a==100&&dataLayer.push({event:"GA_event",category:"Scroll Down",action:a+"%",analyticsAttributes:{pageScrollDown100Percent:"1"}}))}}function inAppleMobileDevice(){return navigator&&navigator.userAgent&&navigator.userAgent!=null&&navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad)/)!=null?true:false};