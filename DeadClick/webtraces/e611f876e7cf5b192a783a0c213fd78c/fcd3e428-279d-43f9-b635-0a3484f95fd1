</script>
<script type="text/javascript">
    var gptAdSlots = gptAdSlots || {};
    gptPreEnableEvent = document.createEvent('Event');
    gptPreEnableEvent.initEvent('gptPreEnable', true, false);

    var gptQuery = location.search.substr(1).split('&');
    var gptClientWidth = document.documentElement.clientWidth;
    var gptPath = window.location.pathname.toLowerCase() + (window.location.pathname == '/' ? 'home/' : '');
    gptPath = gptPath.substring(0, gptPath.lastIndexOf('/'));


    var list = new Array;
	var renderOverlay = true;
	var renderBottomfloater = true;

    function dfpMessageReceived(event) {
        var allowed = false;
        var domains = ['doubleclick.net', 'googlesyndication.com', 'googleadservices.com'];
        domains[domains.length] = window.location.hostname;

        for (var i=0; i < domains.length; i++) { if (event.origin.indexOf(domains[i]) >= 0) { allowed = true; break;}}

        if (allowed) {
            try { window.dfpMessageData = JSON.parse(event.data); } catch(err) { return; }
            if (typeof window.dfpMessageData.scriptUrl == 'undefined')  return;

            //window.removeEventListener('message', dfpMessageReceived, false); 
            if(window.dfpMessageData.command == 'bottomfloater')
            {
                window.dfpMessageDataBottomfloater = window.dfpMessageData;
                list.push(window.dfpMessageDataBottomfloater);
            }
            if(window.dfpMessageData.command == 'overlay')
            {
                window.dfpMessageDataOverlay = window.dfpMessageData;
                list.push(window.dfpMessageDataOverlay);

            }
            for(var i=0;i<list.length;i++)
			{
                var obj = list[i];
                var script = document.createElement('script');
                script.async = true;
                script.type = 'text/javascript';
                if(obj.command == 'overlay' && renderOverlay)
                {
                    window.dfpMessageDataOverlay = obj;				   
				    script.src = window.dfpMessageDataOverlay.scriptUrl;
				    var node = document.getElementsByTagName('script')[0];
				    node.parentNode.insertBefore(script, node);
				    renderOverlay = false;
                }
                else if(obj.command == 'bottomfloater' && renderBottomfloater)
                {
                    window.dfpMessageDataBottomfloater = obj;
				    script.src = window.dfpMessageDataBottomfloater.scriptUrl;
					var node = document.getElementsByTagName('script')[0];
				    node.parentNode.insertBefore(script, node);
					renderBottomfloater = false;
                }
                if(list.length > 2)
				   break;

            }//end for loop
        }//end allow
    }

    window.addEventListener('message', dfpMessageReceived, false);    
googletag.cmd.push(function() {
        gptAdSlots.gptAd0 = googletag.defineSlot('/6177/slm.wzab' + gptPath, [120,60], 'gpt-ad-0').addService(googletag.pubads()).setTargeting('pos', 'onairsponsor');
        gptAdSlots.gptAd1 = googletag.defineSlot('/6177/slm.wzab' + gptPath, [970,90], 'gpt-ad-1').addService(googletag.pubads()).setTargeting('pos', 'header');
        gptAdSlots.gptAd2 = googletag.defineSlot('/6177/slm.wzab' + gptPath, [300,250], 'gpt-ad-2').addService(googletag.pubads()).setTargeting('pos', 'topbox');
        gptAdSlots.gptAd3 = googletag.defineSlot('/6177/slm.wzab' + gptPath, [300,250], 'gpt-ad-3').addService(googletag.pubads()).setTargeting('pos', 'bottombox');
        gptAdSlots.gptAd4 = googletag.defineSlot('/6177/slm.wzab' + gptPath, [728,90], 'gpt-ad-4').addService(googletag.pubads()).setTargeting('pos', 'footer');
        gptAdSlots.gptAdOop5 = googletag.defineOutOfPageSlot('/6177/slm.wzab' + gptPath, 'gpt-ad-oop5').addService(googletag.pubads()).setTargeting('pos', 'takeover');
        gptAdSlots.gptAdOop6 = googletag.defineOutOfPageSlot('/6177/slm.wzab' + gptPath, 'gpt-ad-oop6').addService(googletag.pubads()).setTargeting('pos', 'promo');
        gptPreEnableEvent.slots = gptAdSlots;
        window.dispatchEvent(gptPreEnableEvent);

        for (var i in gptQuery)
        {
            var kv = gptQuery[i].split('=');
            if (kv[0].indexOf('utm_') == 0)
              googletag.pubads().setTargeting(kv[0], kv[1]);
        }

        var gptKV = document.querySelector("meta[name='ad:keyvalue']");
        if (gptKV !== null)
        {
            gptKV = gptKV.getAttribute("content").split(';');
            for (var i in gptKV)
            {
                var kv = gptKV[i].split('=');
                if (kv.length == 2)
                    googletag.pubads().setTargeting(kv[0], kv[1]);
            }
        }

        if (document.referrer.indexOf('facebook') >= 0)
            googletag.pubads().setTargeting('ref', 'fb');

        googletag.pubads().enableSingleRequest();
         googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();

            googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                if( document.getElementById(event.slot.getSlotElementId()) != null){
                    var slotSize = event.size[0] + 'x' + event.size[1];
                    document.getElementById(event.slot.getSlotElementId()).setAttribute('style', 'width: auto; height: auto;' );	
                    var divId = 'google_ads_iframe_' + event.slot.getAdUnitPath() + '_' + event.slot.getSlotElementId().slice(-1) + '__container__';

                    if( document.getElementById(divId) != null ){
                        if( slotSize == '300x250')
                        {                           
                            document.getElementById(divId).setAttribute('style', 'border: 0pt none; display: inline-block; margin: 0 auto 0 auto;');
                        }
                        else if(slotSize == '120x60')
                        {
                            document.getElementById(divId).setAttribute('style', 'border: 0pt none; padding: 0 1.81em 0 1.81em;');
                        }	
                        var childEle = document.getElementById(divId).children;
                        for(var i=0;i<childEle.length;i++)
                        {
                            if(childEle[i].tagName == 'IFRAME'){
                                childEle[i].width = '100%';
                                //childEle[i].height = '100%';
                                var innerDoc = childEle[i].contentDocument || childEle[i].contentWindow.document;
                                if(innerDoc != null){
                                    if(innerDoc.getElementsByTagName('IMG')[0] != null){

                                        var imgWidth  = parseInt(innerDoc.getElementsByTagName('IMG')[0].width);
                                        var slotWidth = parseInt(event.size[0]);
                                        var newImgWidth = '100%';
                                        if(imgWidth < slotWidth && imgWidth > 0)
                                        {
                                            newImgWidth = Math.round((imgWidth/slotWidth)*1000)/10;
                                            newImgWidth = newImgWidth.toString() + '%';
                                        }
                                                                                                             
                                        innerDoc.getElementsByTagName('IMG')[0].removeAttribute('width');
                                        innerDoc.getElementsByTagName('IMG')[0].removeAttribute('height');	
                                        innerDoc.getElementsByTagName('IMG')[0].style.width=newImgWidth;
                                        innerDoc.getElementsByTagName('IMG')[0].style.height='auto';
                                    }
                                }
                            }

                        }//end for loop

                    }//end divId if        

               }//end getSlotElementId if

               //set image width/height interstitial ad for responsive
               setTimeout(function(){
                   var adtype = {'dfpBottomFloater': '', 'dfpTakeover': ''};                   
                 
                                  
                   var setOnce = true;
                   for(var k in adtype)
                   {
                       
                        if(document.getElementById(k) != null){
                           document.getElementById(k).style.display = 'inline-block';
                            var ele = document.getElementById(k).children;
                            for(var j=0; j<ele.length; j++)
                            {
                                if(ele[j].tagName == 'A'){
                                   
                                    var subEle = ele[j].children;
                                    if(subEle.length > 0){                                       
                                       for(var k=0; k<subEle.length; k++)
                                       {
                                            if(subEle[k].tagName == 'IMG'){
                                                subEle[k].removeAttribute('width');
                                                subEle[k].removeAttribute('height');
                                                subEle[k].style.width='100%';
                                                subEle[k].style.height= 'auto';
                                                break;
                                            }
                                       }

                                    }//end subEle
                                }//end tagName


                            }//end for loop
                            
                        }
                        
                   }//end for loop adtype
               }, 600);

           });
});
</script>
