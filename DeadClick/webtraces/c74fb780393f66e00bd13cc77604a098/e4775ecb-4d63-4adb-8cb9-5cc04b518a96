  audience_xsell_mobile = p.audience_xsell_mobile || (ADventori.data.results.userData.dmp ? ADventori.data.results.userData.dmp.audience_xsell_mobile : '0'),
  audience_multi = p.audience_multi || (ADventori.data.results.userData.dmp ? ADventori.data.results.userData.dmp.audience_multi : '0'),
  audience_xsell_adsl = p.audience_xsell_adsl || (ADventori.data.results.userData.dmp ? ADventori.data.results.userData.dmp.audience_xsell_adsl : '0');

