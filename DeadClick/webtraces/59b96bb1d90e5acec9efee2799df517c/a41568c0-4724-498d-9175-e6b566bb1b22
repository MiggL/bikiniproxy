<script>            
	$(document).ready(function() {
		var offset = 100;
		var duration = 1000;
		if($(window).width()<500){
		$(window).scroll(function() {
			if ($(this).scrollTop() > offset) {
				$('#go_to_top_img').fadeIn(duration);
			} else {
				$('#go_to_top_img').fadeOut(duration);
			}
		});
		
		$('#go_to_top_img').click(function(event) {
			event.preventDefault();
			$('html, body').animate({scrollTop: 0}, duration);
			return false;
		})
	}
	});
</script>
