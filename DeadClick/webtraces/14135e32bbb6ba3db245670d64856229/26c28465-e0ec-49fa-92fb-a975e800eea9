(function(window,document,$,undefined){$.extend({omnitureIcid:function(config){config=config||{};var allowedDomains=["fan.football.sony.net",config.domain];function appendIcid($anchor,params){var parts;var delimiter;var href;var hostname=$anchor[0].hostname;var isDomainAllowed=false;parts=$anchor.attr("href").split("#");if(/^javascript:/.test(parts[0])){return;}$.each(allowedDomains,function(){if(RegExp(this+"$").test(hostname)){isDomainAllowed=true;return false;}});if(!isDomainAllowed){return;}if(/ICID=/.test(parts[0])){return;}if(parts[0]===""){return;}delimiter=(/\?/).test(parts[0])?"&":"?";href=parts[0]+delimiter+"ICID="+params.join("_");if(parts.length>1){href+="#"+parts[1];}$anchor.attr("href",href);}function extractIcids($elements){var icidParams=[];$elements.each(function(){icidParams.unshift($(this).attr("data-omniture-icid"));});return icidParams;}$('body[data-omniture-icid!=""]').delegate("a[href]","mousedown focus",function(){var $clickedElement=$(this);var $icidParents=$(this).parents("[data-omniture-icid]");if($icidParents.length===1){appendIcid($(this),extractIcids($icidParents));}else{var $module=$(this).parents("[data-omniture-icid]:first");var tags;var index=1;var items=$module.find('[data-omniture-role="item"]');if(items.length){items.each(function(){$('a[href!="#"]',$(this)).each(function(){tags=extractIcids($icidParents);tags.push(index);if($clickedElement.get(0)===this){appendIcid($(this),tags);}});index++;});}else{$('a[href!="#"]',$module).each(function(){tags=extractIcids($icidParents);tags.push(index);if($clickedElement.get(0)===this){appendIcid($(this),tags);}index++;});}}});}});}(this,this.document,this.jQuery));