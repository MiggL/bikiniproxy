//product_quick_view
jQuery("#quickviewbutton").click(function(){
    var product = this.closest(".product-tile");
    var index = jQuery("#quickviewbutton").index(this);

    var data = {
        "ecommerce_action"   : "detail",
        "event_name"         : "product_quick_view",
        "product_category"   : ["" + product.data("category_name")],
        "product_id"         : ["" + product.data("itemid")],
        "product_image_url"  : ["" + product.data("image_url")],
        "product_name"       : ["" + product.data("item_name")],
        "product_price"      : ["" + product.data("price")],
        "product_sku"        : ["" + product.data("itemid")]
    };
    utag_data.ecommerce_action = data.ecommerce_action;
    utag_data.event_name = data.event_name;
    utag_data.product_category = data.product_category;
    utag_data.product_id = data.product_id;
    utag_data.product_name = data.product_name;
    utag_data.product_price = data.product_price;
    utag_data.product_sku = data.product_sku;
    utag.view(data);
});

