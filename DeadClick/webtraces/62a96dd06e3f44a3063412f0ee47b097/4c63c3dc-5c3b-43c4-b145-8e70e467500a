			<script type="text/javascript">
				$(function(){
					
					
					var $content = $("#cg-anons img").eq(0); 
					if ($.cookie('cg-anons') !== 'ok'){
						$.fancybox($content, {
							hideOnContentClick: true,
							autoDimensions: true,
							autoScale: false,
							centerOnScroll: false,
							titlePosition: 'none',
							padding: 10,
							overlayShow: true, 
							onComplete: function(){
								if($content.attr('rel')){
									$content.wrap($("<a />", {href: $content.attr('rel'), target: "_blank"}));
								}
								$.cookie('cg-anons', 'ok', { expires: '' });
							}
						});
					}
				});
			</script>
		  

		
